@EVENT_COUNTE_MESSAGE5041
SELECTCASE TCVAR:MASTER:21
	CASE 1
		IF TALENT:MASTER:胸圍 > 0
			PRINTFORMW %CALLNAME:MASTER%用胸部挟住了%CALLNAME:TARGET%的%TINKO(TARGET)%…
		ELSE
