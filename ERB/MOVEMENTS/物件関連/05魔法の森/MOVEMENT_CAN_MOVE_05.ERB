;-----------------------------------------------------------
;地点の接続設定
;ARG    基準の地点
;ARG:1  対象の地点
;RETURNF 0=移動できない,見えない
;        1=移動できる,見えない
;        2=移動できない,見える
;        3=移動できる,見える（直通）
;-----------------------------------------------------------
@CAN_MOVE_5(ARG, ARG:1)
#FUNCTION
SELECTCASE ARG
	CASE P501魔法之森入口
		SIF GROUPMATCH(ARG:1, P503岔道)
			RETURNF 3
		SIF GROUPMATCH(ARG:1, P502香霖堂, P534輕飄飄的艾倫魔法商店)
			RETURNF 1
		SIF ARG:1 == MAXROOM
			RETURNF 2
	CASE P502香霖堂
		SIF GROUPMATCH(ARG:1, P501魔法之森入口)
			RETURNF 1
	CASE P503岔道
		SIF GROUPMATCH(ARG:1, P501魔法之森入口)
			RETURNF 3
		SIF GROUPMATCH(ARG:1, P508魔法之広場, P511魔理沙宅・居間, P515魔理沙宅・倉庫, P520人偶洋館前, P534輕飄飄的艾倫魔法商店, P540古木的大樹, P544成美の小屋)
			RETURNF 1
	CASE P506無縁塚
		SIF GROUPMATCH(ARG:1, P509再思之道)
			RETURNF 3
	CASE P507菌菇群生地
		SIF GROUPMATCH(ARG:1, P508魔法之広場, P540古木的大樹)
			RETURNF 1
	CASE P508魔法之広場
		SIF GROUPMATCH(ARG:1, P503岔道, P507菌菇群生地, P509再思之道, P510霧雨魔法店, P544成美の小屋)
			RETURNF 1
	CASE P509再思之道
		SIF GROUPMATCH(ARG:1, P506無縁塚)
			RETURNF 3
		SIF GROUPMATCH(ARG:1, P508魔法之広場)
			RETURNF 1

	CASE P510霧雨魔法店
		SIF GROUPMATCH(ARG:1, P508魔法之広場, P511魔理沙宅・居間)
			RETURNF 1
	CASE P511魔理沙宅・居間
		SIF GROUPMATCH(ARG:1, P503岔道, P510霧雨魔法店, P512魔理沙宅・寝室, P513魔理沙宅・研究部屋, P514魔理沙宅・風呂, P515魔理沙宅・倉庫)
			RETURNF 1
	CASE P512魔理沙宅・寝室, P513魔理沙宅・研究部屋, P514魔理沙宅・風呂
		SIF GROUPMATCH(ARG:1, P511魔理沙宅・居間)
			RETURNF 1
	CASE P515魔理沙宅・倉庫
		SIF GROUPMATCH(ARG:1, P503岔道, P511魔理沙宅・居間)
			RETURNF 1

	CASE P520人偶洋館前
		SIF GROUPMATCH(ARG:1, P503岔道, P540古木的大樹, P525人偶洋館・生活間)
			RETURNF 1
	CASE P521人偶洋館・愛麗絲的房間, P522人偶洋館・神綺的房間, P523人偶洋館・夢子的房間, P524人偶洋館・研究室, P526人偶洋館・風呂
		SIF GROUPMATCH(ARG:1, P525人偶洋館・生活間)
			RETURNF 1
	CASE P525人偶洋館・生活間
		SIF GROUPMATCH(ARG:1, P520人偶洋館前, P521人偶洋館・愛麗絲的房間, P522人偶洋館・神綺的房間, P523人偶洋館・夢子的房間, P524人偶洋館・研究室)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P526人偶洋館・風呂, P529人偶洋館・露台, P530人偶洋館・二階)
			RETURNF 1
	CASE P528人偶洋館・倉庫, P531人偶洋館・空的部屋
		SIF GROUPMATCH(ARG:1, P530人偶洋館・二階)
			RETURNF 1
	CASE P529人偶洋館・露台
		SIF GROUPMATCH(ARG:1, P525人偶洋館・生活間, P530人偶洋館・二階)
			RETURNF 1
	CASE P530人偶洋館・二階
		SIF GROUPMATCH(ARG:1, P525人偶洋館・生活間, P528人偶洋館・倉庫, P529人偶洋館・露台, P531人偶洋館・空的部屋)
			RETURNF 1

	CASE P534輕飄飄的艾倫魔法商店
		SIF GROUPMATCH(ARG:1, P501魔法之森入口, P503岔道, P535艾倫宅・寝室, P536艾倫宅・倉庫, P537艾倫宅・風呂)
			RETURNF 1
	CASE P535艾倫宅・寝室, P536艾倫宅・倉庫, P537艾倫宅・風呂
		SIF GROUPMATCH(ARG:1, P534輕飄飄的艾倫魔法商店)
			RETURNF 1

	CASE P540古木的大樹
		SIF GROUPMATCH(ARG:1, P503岔道, P507菌菇群生地, P520人偶洋館前)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P541朱鷺子的棲息処, P542莉格露的棲息処, P543露米亜的棲息処)
			RETURNF 1
	CASE P541朱鷺子的棲息処, P542莉格露的棲息処, P543露米亜的棲息処
		SIF GROUPMATCH(ARG:1, P540古木的大樹)
			RETURNF 1
	CASE P544成美の小屋
		SIF GROUPMATCH(ARG:1, P503岔道, P508魔法之広場)
			RETURNF 1
	CASE P545慧ノ子の棲み処
		SIF GROUPMATCH(ARG:1, P508魔法之広場)
			RETURNF 1

	CASE MAXROOM
		SIF ARG:1 == P501魔法之森入口
			RETURNF 1
ENDSELECT
RETURNF 0
