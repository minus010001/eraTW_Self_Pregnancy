;-----------------------------------------------------------
;地点の接続設定
;ARG    基準の地点
;ARG:1  対象の地点
;RETURNF 0=移動できない,見えない
;        1=移動できる,見えない
;        2=移動できない,見える
;        3=移動できる,見える（直通）
;-----------------------------------------------------------
@CAN_MOVE_9(ARG, ARG:1)
#FUNCTION
#DIM 参照先
SELECTCASE ARG
CASE P901地霊殿前
	SIF GROUPMATCH(ARG:1, P905一階廊下, P931大街)
		RETURNF 1
CASE P904大會堂, P906一層廁所, P911阿空私室, P912阿燐私室
	SIF GROUPMATCH(ARG:1, P905一階廊下)
		RETURNF 1
CASE P905一階廊下
	SIF GROUPMATCH(ARG:1, P901地霊殿前, P904大會堂, P906一層廁所, P909厨房, P911阿空私室, P912阿燐私室)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P913食堂, P914二階廊下, P923後院, P926中庭)
		RETURNF 1
CASE P909厨房
	SIF GROUPMATCH(ARG:1, P905一階廊下, P910食料庫, P913食堂, P925薔薇園)
		RETURNF 1
CASE P910食料庫
	SIF GROUPMATCH(ARG:1, P909厨房)
		RETURNF 1
CASE P913食堂
	SIF GROUPMATCH(ARG:1, P905一階廊下, P909厨房, P923後院, P926中庭)
		RETURNF 1
CASE P914二階廊下
	SIF GROUPMATCH(ARG:1, P905一階廊下, P915露台, P916庫房, P917二層廁所)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P918恋私室, P919覚私室, P920書斎, P922脱衣所)
		RETURNF 1
CASE P915露台, P916庫房, P917二層廁所, P918恋私室
	SIF GROUPMATCH(ARG:1, P914二階廊下)
		RETURNF 1
CASE P919覚私室
	SIF GROUPMATCH(ARG:1, P914二階廊下, P920書斎)
		RETURNF 1
CASE P920書斎
	SIF GROUPMATCH(ARG:1, P914二階廊下, P919覚私室)
		RETURNF 1
CASE P921風呂
	SIF GROUPMATCH(ARG:1, P922脱衣所)
		RETURNF 1
CASE P922脱衣所
	SIF GROUPMATCH(ARG:1, P914二階廊下, P921風呂)
		RETURNF 1
CASE P923後院
	SIF GROUPMATCH(ARG:1, P905一階廊下, P913食堂, P925薔薇園)
		RETURNF 1
CASE P925薔薇園
	SIF GROUPMATCH(ARG:1, P909厨房, P923後院)
		RETURNF 1
CASE P926中庭
	SIF GROUPMATCH(ARG:1, P905一階廊下, P913食堂, P928灼熱地獄跡)
		RETURNF 1
CASE P928灼熱地獄跡
	SIF GROUPMATCH(ARG:1, P926中庭, P929地獄的人工太陽)
		RETURNF 1
CASE P929地獄的人工太陽
	SIF GROUPMATCH(ARG:1, P928灼熱地獄跡, P930旧血の池地獄)
		RETURNF 1
CASE P930旧血の池地獄
	SIF GROUPMATCH(ARG:1, P929地獄的人工太陽, P947ちやりの棲み処)
		RETURNF 1
CASE P947ちやりの棲み処
	SIF GROUPMATCH(ARG:1, P930旧血の池地獄)
		RETURNF 1
CASE P931大街
	SIF GROUPMATCH(ARG:1, P901地霊殿前, P932後街, P933酒場, P937空屋, P944街道)
		RETURNF 1
CASE P932後街
	SIF GROUPMATCH(ARG:1, P931大街, P933酒場, P934温泉, P935情人旅館, P936勇儀的家, P938鬼的聚集場所, P939小胡同)
		RETURNF 1
CASE P933酒場
	SIF GROUPMATCH(ARG:1, P931大街, P932後街)
		RETURNF 1
CASE P934温泉, P935情人旅館, P936勇儀的家, P938鬼的聚集場所, P939小胡同
	SIF GROUPMATCH(ARG:1, P932後街)
		RETURNF 1
CASE P937空屋
	SIF GROUPMATCH(ARG:1, P931大街)
		RETURNF 1
CASE P940帕露西的家, P941山女的家, P942琪斯美的家
	SIF GROUPMATCH(ARG:1, P945川岸)
		RETURNF 1
CASE P943茅屋
	SIF GROUPMATCH(ARG:1, P944街道)
		RETURNF 1
CASE P944街道
	SIF GROUPMATCH(ARG:1, P946橋)
		RETURNF 3
	SIF GROUPMATCH(ARG:1, P931大街, P943茅屋, P945川岸)
		RETURNF 1
CASE P945川岸
	SIF GROUPMATCH(ARG:1, P940帕露西的家, P941山女的家, P942琪斯美的家, P944街道, P946橋)
		RETURNF 1
CASE P946橋
	SIF GROUPMATCH(ARG:1, P944街道, P948縦穴底部)
		RETURNF 3
	SIF GROUPMATCH(ARG:1, P945川岸)
		RETURNF 1
CASE P948縦穴底部
	SIF GROUPMATCH(ARG:1, P946橋)
		RETURNF 3
	SIF GROUPMATCH(ARG:1, MAXROOM)
		RETURNF 2
CASE MAXROOM
	SIF ARG:1 == P948縦穴底部
		RETURNF 1
ENDSELECT

RETURNF 0
