;-----------------------------------------------------------
;地点の接続設定
;ARG    基準の地点
;ARG:1  対象の地点
;RETURNF 0=移動できない,見えない
;        1=移動できる,見えない
;        2=移動できない,見える
;        3=移動できる,見える（直通）
;-----------------------------------------------------------
@CAN_MOVE_10(ARG, ARG:1)
#FUNCTION
SELECTCASE ARG
CASE P1001槐安通路
	SIF GROUPMATCH(ARG:1, P1002月之都, P1050哆来咪宅・居間)
		RETURNF 1
	SIF ARG:1 == MAXROOM
		RETURNF 2
CASE P1002月之都
	SIF GROUPMATCH(ARG:1, P1001槐安通路, P1003玉兎街, P1005探女宅・居間, P1007闲置的房子)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P1020綿月亭・門)
		RETURNF 3
CASE P1003玉兎街
	SIF GROUPMATCH(ARG:1, P1002月之都, P1008鈴瑚宅, P1009清蘭宅, P1010吃飯処, P1011甘味処, P1014桃林)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P1004広場)
		RETURNF 3
CASE P1004広場
	SIF GROUPMATCH(ARG:1, P1010吃飯処, P1011甘味処, P1012酒屋, P1013八百屋)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P1003玉兎街)
		RETURNF 3
CASE P1005探女宅・居間
	SIF GROUPMATCH(ARG:1, P1002月之都, P1006探女宅・寝室)
		RETURNF 1
CASE P1006探女宅・寝室
	SIF GROUPMATCH(ARG:1, P1005探女宅・居間)
		RETURNF 1
CASE P1007闲置的房子
	SIF GROUPMATCH(ARG:1, P1002月之都)
		RETURNF 1
CASE P1008鈴瑚宅
	SIF GROUPMATCH(ARG:1, P1003玉兎街)
		RETURNF 1
CASE P1009清蘭宅
	SIF GROUPMATCH(ARG:1, P1003玉兎街)
		RETURNF 1
CASE P1010吃飯処
	SIF GROUPMATCH(ARG:1, P1003玉兎街, P1004広場)
		RETURNF 1
CASE P1011甘味処
	SIF GROUPMATCH(ARG:1, P1003玉兎街, P1004広場)
		RETURNF 1
CASE P1012酒屋
	SIF GROUPMATCH(ARG:1, P1004広場)
		RETURNF 1
CASE P1013八百屋
	SIF GROUPMATCH(ARG:1, P1004広場)
		RETURNF 1
CASE P1014桃林
	SIF GROUPMATCH(ARG:1, P1003玉兎街)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P1015砂浜)
		RETURNF 3
CASE P1015砂浜
	SIF GROUPMATCH(ARG:1, P1014桃林)
		RETURNF 3
	SIF GROUPMATCH(ARG:1, P1016入海口)
		RETURNF 1
CASE P1016入海口
	SIF GROUPMATCH(ARG:1, P1015砂浜, P1060純狐邸・玄関)
		RETURNF 1

CASE P1020綿月亭・門
	SIF GROUPMATCH(ARG:1, P1021綿月亭・玄関, P1023門番詰め所, P1024玉兎食堂)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P1002月之都)
		RETURNF 3
CASE P1021綿月亭・玄関
	SIF GROUPMATCH(ARG:1, P1020綿月亭・門, P1024玉兎食堂, P1028食料庫)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P1022一階回廊)
		RETURNF 3
CASE P1022一階回廊
	SIF GROUPMATCH(ARG:1, P1026食堂, P1029綿月亭・脱衣所, P1031宝物庫, P1032鈴仙二號私室, P1033綿月亭・空的部屋, P1040二階回廊)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P1021綿月亭・玄関, P1035中庭)
		RETURNF 3
CASE P1023門番詰め所
	SIF GROUPMATCH(ARG:1, P1020綿月亭・門)
		RETURNF 1
CASE P1024玉兎食堂
	SIF GROUPMATCH(ARG:1, P1020綿月亭・門, P1021綿月亭・玄関, P1025玉兎詰め所)
		RETURNF 1
CASE P1025玉兎詰め所
	SIF GROUPMATCH(ARG:1, P1024玉兎食堂)
		RETURNF 1
CASE P1026食堂
	SIF GROUPMATCH(ARG:1, P1022一階回廊, P1027厨房)
		RETURNF 1
CASE P1027厨房
	SIF GROUPMATCH(ARG:1, P1026食堂, P1028食料庫)
		RETURNF 1
CASE P1028食料庫
	SIF GROUPMATCH(ARG:1, P1021綿月亭・玄関, P1027厨房)
		RETURNF 1
CASE P1029綿月亭・脱衣所
	SIF GROUPMATCH(ARG:1, P1022一階回廊, P1030綿月亭・風呂)
		RETURNF 1
CASE P1030綿月亭・風呂
	SIF GROUPMATCH(ARG:1, P1029綿月亭・脱衣所)
		RETURNF 1
CASE P1031宝物庫
	SIF GROUPMATCH(ARG:1, P1022一階回廊)
		RETURNF 1
CASE P1032鈴仙二號私室
	SIF GROUPMATCH(ARG:1, P1022一階回廊)
		RETURNF 1
CASE P1033綿月亭・空的部屋
	SIF GROUPMATCH(ARG:1, P1022一階回廊)
		RETURNF 1
CASE P1035中庭
	SIF GROUPMATCH(ARG:1, P1022一階回廊, P1036四阿)
		RETURNF 3
CASE P1036四阿
	SIF GROUPMATCH(ARG:1, P1035中庭)
		RETURNF 3

CASE P1040二階回廊
	SIF GROUPMATCH(ARG:1, P1022一階回廊, P1041豊姫私室, P1042依姫私室, P1043書斎, P1044會客廳)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P1035中庭)
		RETURNF 2
CASE P1041豊姫私室
	SIF GROUPMATCH(ARG:1, P1040二階回廊)
		RETURNF 1
CASE P1042依姫私室
	SIF GROUPMATCH(ARG:1, P1040二階回廊)
		RETURNF 1
CASE P1043書斎
	SIF GROUPMATCH(ARG:1, P1040二階回廊)
		RETURNF 1
CASE P1044會客廳
	SIF GROUPMATCH(ARG:1, P1040二階回廊)
		RETURNF 1

CASE P1050哆来咪宅・居間
	SIF GROUPMATCH(ARG:1, P1001槐安通路, P1051哆来咪宅・寝室, P1052哆来咪宅・倉庫, P1053哆来咪宅・洗面所)
		RETURNF 1
CASE P1051哆来咪宅・寝室
	SIF GROUPMATCH(ARG:1, P1050哆来咪宅・居間)
		RETURNF 1
CASE P1052哆来咪宅・倉庫
	SIF GROUPMATCH(ARG:1, P1050哆来咪宅・居間)
		RETURNF 1
CASE P1053哆来咪宅・洗面所
	SIF GROUPMATCH(ARG:1, P1050哆来咪宅・居間, P1054哆来咪宅・風呂, P1055哆来咪宅・廁所)
		RETURNF 1
CASE P1054哆来咪宅・風呂
	SIF GROUPMATCH(ARG:1, P1053哆来咪宅・洗面所)
		RETURNF 1
CASE P1055哆来咪宅・廁所
	SIF GROUPMATCH(ARG:1, P1053哆来咪宅・洗面所)
		RETURNF 1

CASE P1060純狐邸・玄関
	SIF GROUPMATCH(ARG:1, P1016入海口)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P1061居間)
		RETURNF 3
CASE P1061居間
	SIF GROUPMATCH(ARG:1, P1063純狐邸・空的部屋, P1064克勞恩皮絲私室, P1065庫房, P1066純狐邸・脱衣所, P1070屋上)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P1060純狐邸・玄関, P1062廚房, P1068純狐邸・中庭)
		RETURNF 3
CASE P1062廚房
	SIF GROUPMATCH(ARG:1, P1061居間)
		RETURNF 3
CASE P1063純狐邸・空的部屋
	SIF GROUPMATCH(ARG:1, P1061居間)
		RETURNF 1
CASE P1064克勞恩皮絲私室
	SIF GROUPMATCH(ARG:1, P1061居間)
		RETURNF 1
CASE P1065庫房
	SIF GROUPMATCH(ARG:1, P1061居間)
		RETURNF 1
CASE P1066純狐邸・脱衣所
	SIF GROUPMATCH(ARG:1, P1061居間, P1067純狐邸・風呂)
		RETURNF 1
CASE P1067純狐邸・風呂
	SIF GROUPMATCH(ARG:1, P1066純狐邸・脱衣所)
		RETURNF 1
CASE P1068純狐邸・中庭
	SIF GROUPMATCH(ARG:1, P1061居間)
		RETURNF 3
CASE P1070屋上
	SIF GROUPMATCH(ARG:1, P1061居間, P1071純狐私室, P1072赫喀提亞私室)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P1068純狐邸・中庭)
		RETURNF 2
CASE P1071純狐私室
	SIF GROUPMATCH(ARG:1, P1070屋上)
		RETURNF 1
CASE P1072赫喀提亞私室
	SIF GROUPMATCH(ARG:1, P1070屋上)
		RETURNF 1

CASE MAXROOM
	SIF GROUPMATCH(ARG:1, P1001槐安通路)
		RETURNF 1
ENDSELECT

RETURNF 0