IF ARGS == "OUTPUT"
	RESULTS = %LOCALS%
	RETURN
ENDIF

;効果の無いものは消しておく
;稗田家秘伝の味
SIF STRCOUNT(LOCALS, "稗田家秘伝の味/") && !STRCOUNT(DISH_TASTE, "和食/")
	LOCALS '= REPLACE(LOCALS, "稗田家秘伝の味\/", "")
;得意料理
IF STRCOUNT(LOCALS, "得意料理")
	;得意料理を参照する(RESULT:1)
	SPLIT LOCALS, "＊", RESULTS
	SIF !STRCOUNT(DISH_TASTE, RESULTS:1 + "/")
		LOCALS '= REPLACE(LOCALS, "得意料理＊.+＊\/", "")
ENDIF
;切れ味
IF STRCOUNT(LOCALS, "切れ味/")
	SIF !STRCOUNT(DISH_TASTE, "野菜/") && !STRCOUNT(DISH_TASTE, "肉/") && !STRCOUNT(DISH_TASTE, "魚/")
		LOCALS '= REPLACE(LOCALS, "切れ味\/", "")
ENDIF
;美味的水
IF STRCOUNT(LOCALS, "美味的水/")
	SIF !STRCOUNT(DISH_TASTE, "鍋物/") && !STRCOUNT(DISH_TASTE, "汁物/") && !STRCOUNT(DISH_TASTE, "饮用/") && !STRCOUNT(DISH_TASTE, "扑鼻/")
		LOCALS '= REPLACE(LOCALS, "美味的水\/", "")
ENDIF
;大火力
SIF STRCOUNT(LOCALS, "大火力/") && STRCOUNT(DISH_TASTE, "冷/")
	LOCALS '= REPLACE(LOCALS, "大火力\/", "")
;氷結
SIF STRCOUNT(LOCALS, "氷結/") && STRCOUNT(DISH_TASTE, "温暖/")
	LOCALS '= REPLACE(LOCALS, "氷結\/", "")

;付与効果（中立付与は強化も兼ねる）
SIF STRCOUNT(LOCALS, "冥界風/") && !STRCOUNT(DISH_TASTE, "洋食/") && !STRCOUNT(DISH_TASTE, "中華/")
	DISH_TASTE += "和食/"
SIF STRCOUNT(LOCALS, "地底風/") && !STRCOUNT(DISH_TASTE, "冷/")
	DISH_TASTE += "温暖/"
SIF STRCOUNT(LOCALS, "月都風/") && !STRCOUNT(DISH_TASTE, "和食/") && !STRCOUNT(DISH_TASTE, "洋食/")
	DISH_TASTE += "中華/"
SIF STRCOUNT(LOCALS, "魔界風/") && !STRCOUNT(DISH_TASTE, "和食/") && !STRCOUNT(DISH_TASTE, "中華/")
	DISH_TASTE += "洋食/"
SIF STRCOUNT(LOCALS, "夢幻風/") && !STRCOUNT(DISH_TASTE, "和食/") && !STRCOUNT(DISH_TASTE, "中華/")
	DISH_TASTE += "洋食/"
IF STRCOUNT(LOCALS, "未知の味/")
	SELECTCASE RAND:10
	CASE 0
		LOCAL = 4
	CASE 1, 2
		LOCAL = 3
	CASE 3, 4, 5
		LOCAL = 2
	CASEELSE
		LOCAL = 1
	ENDSELECT
	SELECTCASE RAND:4
	CASE 1
		DISH_TASTE += "甜/" * LOCAL
	CASE 2
		DISH_TASTE += "辣/" * LOCAL
	CASE 3
		DISH_TASTE += "咸/" * LOCAL
	CASEELSE
		DISH_TASTE += "酸/" * LOCAL
	ENDSELECT
ENDIF
SIF STRCOUNT(LOCALS, "浓郁/") && !STRCOUNT(DISH_TASTE, "濃厚/")
	DISH_TASTE += "濃厚/"
SIF STRCOUNT(LOCALS, "薄味/") && !STRCOUNT(DISH_TASTE, "淡白/")
	DISH_TASTE += "淡白/"
