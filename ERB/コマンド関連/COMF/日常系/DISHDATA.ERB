ENDIF

;変数の補完設定
IF TYPECHECK == ""
	SELECTCASE DISH_NUM
	CASE 101 TO 200
		TYPECHECK = 軽食
	CASE 201 TO 300
		TYPECHECK = 主食
	CASE 301 TO 350
		TYPECHECK = 甜点
	CASEELSE
		IF DISH_NAME != ""
			PRINTL 
			PRINTFORML TYPECHECKが設定されてないよ！ DISH_NUM:{DISH_NUM} DISH_NAME:%DISH_NAME%
			THROW
		ENDIF
	ENDSELECT
ENDIF
IF !SKILL_LV
	SELECTCASE TYPECHECK
	CASE "軽食"
		SKILL_LV = 0
	CASE "主食"
		SKILL_LV = 2
	CASE "甜点"
		SKILL_LV = 3
	ENDSELECT
ENDIF
IF !DISH_TIME
	SELECTCASE TYPECHECK
	CASE "軽食"
		DISH_TIME = 20
	CASE "主食"
		DISH_TIME = 40
	CASE "甜点"
		DISH_TIME = 40
	ENDSELECT
ENDIF
SIF !DISH_MAXLIMIT
	DISH_MAXLIMIT = 120
SIF ITEM:保温壺
	TIMES DISH_MAXLIMIT, 1.5

;RESULT設定,默认は1（材料不足）
VARSET RESULT
RESULT = 1
;材料判定,いずれかの材料を1つでも持っていれば作れる
FOR LOCAL, 1, 11
	SIF DISH_ING:LOCAL < 0
		BREAK
	SIF ITEM:(DISH_ING:LOCAL) > 0
		RESULT = 10
NEXT

;必要技能判定
SIF ABL:MASTER:料理技能 < SKILL_LV
	RESULT = 2

;練習時は判定を無視してDISH_TYPEを決定する
IF 練習モード
	DISH_TYPE = %TYPECHECK%
ELSE
	;TYPECHECK判定
	SIF TYPECHECK != ARGS
		RESULT = 0

	;固有料理判定
	IF INRANGE(DISH_NUM, 400, 400 + CHARANUM)
		SIF LEARNED_MENU:SP_CHARA < 100
			RESULT = 0
	ENDIF
ENDIF

;存在判定
SIF DISH_NAME == ""
	RESULT = 0

RESULT:1 = SKILL_LV
RESULTS:1 = %TYPECHECK%
RETURN RESULT
;-------------------------------------------------
;料理の材料を設定する関数
;材料は１つで作れるので多いほど作る手段が多い≒作りやすい
;"汎用"を入れると汎用食材がセットされる
;"魚"を入れると魚食材がセットされる
;"作成不能"を入れるとどうやっても作れないようになる（表示はされる）
;-------------------------------------------------
@材料設定(ARGS:1, ARGS:2, ARGS:3, ARGS:4, ARGS:5, ARGS:6, ARGS:7, ARGS:8, ARGS:9, ARGS:10)
VARSET DISH_ING, -1
IF ARGS:1 == "汎用"
	DISH_ING:1 = GETNUM(ITEM, "高级食材")
	DISH_ING:2 = GETNUM(ITEM, "优质食材")
	DISH_ING:3 = GETNUM(ITEM, "平价食材")
	RETURN
ELSEIF ARGS:1 == "魚"
	DISH_ING:1 = GETNUM(ITEM, "好吃的魚")
	DISH_ING:2 = GETNUM(ITEM, "魚")
	RETURN
ELSEIF ARGS:1 == "虫"
	DISH_ING:1 = GETNUM(ITEM, "稀有昆虫")
	DISH_ING:2 = GETNUM(ITEM, "虫")
	RETURN
ELSEIF ARGS:1 == "作成不能"
	FOR LOCAL, 1, 11
		DISH_ING:LOCAL = -1
	NEXT
	RETURN
ENDIF
FOR LOCAL, 1, 11
	IF ARGS:LOCAL == ""
		;跳蛋が0番なので無いものは-1を入れておく
		DISH_ING:LOCAL = -1
	ELSE
		DISH_ING:LOCAL = GETNUM(ITEM, ARGS:LOCAL)
		IF GETNUM(ITEM, ARGS:LOCAL) == -1
			PRINTFORML 道具名が間違ってるよ！＞%ARGS:LOCAL%
			THROW
		ENDIF
	ENDIF
NEXT

;-------------------------------------------------
;DISH_TASTEにあとから付け加える関数
;ARG    食材のID
;ARGS   加える文字列,使ってないけど一応用意した
;-------------------------------------------------
@DISH_TASTE_ADD(ARG, ARGS)
VARSET RESULTS
LOCALS = 
IF ARGS != ""
	DISH_TASTE += ARGS + "/"
	;角色依存のものは@DISH_TASTE_FROM_CHARAでやる
ENDIF

IF ARG >= 0 && ITEMNAME:ARG != ""
	SELECTCASE ITEMNAME:ARG
	;肉は野雞肉以外追加しない
	CASE "獣肉", "鳥肉"
	;冰淇淋は追加しない
	CASE "冰淇淋"
	;蘑菇素材は[蘑菇]を追加する
	CASE "普通的菌菇", "毒菌菇", "粘性菌菇", "冬虫夏草", "可疑菌菇", "魔法菌菇"
		DISH_TASTE += "蘑菇/"
	;草莓と西瓜と仙桃は[果物]と両方追加する
	CASE "草莓", "西瓜", "仙桃"
		DISH_TASTE += ITEMNAME:ARG + "/"
		DISH_TASTE += "果物/"
	;(酒造パッチ)紅酒は[家庭的]を陈酿紅酒は[上品]を優質陈酿紅酒は[上品]と[豪華]を追加する
	CASE "紅酒"
		DISH_TASTE += "家庭的/"
	CASE "陈酿紅酒"
		DISH_TASTE += "上品/"
	CASE "優質陈酿紅酒"
		DISH_TASTE += "上品/"
		DISH_TASTE += "豪華/"
	CASEELSE
		DISH_TASTE += ITEMNAME:ARG + "/"
	ENDSELECT
ENDIF

;-------------------------------------------------
;角色に依存したTASTEを付与する関数
;ARGS OUTPUT用,角色依存TASTEのみをRESULTSに出力する
;-------------------------------------------------
@DISH_TASTE_FROM_CHARA(CHARA, ARGS)
#DIM CHARA
;まずLOCALSにパラメータを入れる
LOCALS = 

;助手効果によるもの
LOCALS += GET_STR(, "角色データ", CHARA, "料理：助手効果")

;TALENTによるもの
;人类
SELECTCASE GET_TRIBENAME(CHARA, GETNUM(TALENT, "人类"))
CASE "仙人"
	LOCALS += "仙術/"
;天人,月人は助手効果
CASE "魔界人"
	LOCALS += "魔力/"
CASE "外界人"
	LOCALS += "外界風/"
ENDSELECT
;妖怪
SELECTCASE GET_TRIBENAME(CHARA, GETNUM(TALENT, "妖怪"))
CASE "妖怪", "河童", "妖獣", "妖鳥", "妖蟲", "座敷童子", "山童"
	LOCALS += "妖力/"
CASE "鬼"
	LOCALS += "妖力/"
CASE "吸血鬼"
	LOCALS += "妖力/"
CASE "天狗"
	LOCALS += "妖力/天狗流/"
CASE "悪魔"
	LOCALS += "妖力/魔力/"
CASE "小人"
	LOCALS += "妖力/小人族秘伝的味道/"
ENDSELECT
;妖精
SIF TALENT:CHARA:妖精
	LOCALS += "生命力/"
;神霊
SIF TALENT:CHARA:神霊
	LOCALS += "神力/"
;幽霊
SIF TALENT:CHARA:幽霊
	LOCALS += "霊障/"
;付喪神
SIF TALENT:CHARA:付喪神
	LOCALS += "道具の共鳴/"
;[人形]はとくに無
;SELECTCASE GET_TRIBENAME(CHARA, GETNUM(TALENT, "人形"))
;CASE ""
;	LOCALS += /
;ENDSELECT
;追加種族
SELECTCASE GET_TRIBENAME(CHARA, GETNUM(TALENT, "追加種族"))
CASE "巫女"
	LOCALS += "神力/"
CASE "魔法使"
	LOCALS += "魔力/"
CASE "女僕"
	LOCALS += "女僕流/"
ENDSELECT
;闪耀
IF GROUPMATCH(GET_WEATHER(), "晴天", "晴朗")
	IF TIME:2 >= 2 && TIME:2 <= 4
		SIF TALENT:CHARA:日光浴
			LOCALS += "闪耀/"
	ELSE
		SIF TALENT:CHARA:月光浴
			LOCALS += "闪耀/"
	ENDIF
ENDIF

;母亲的味道
SIF TALENT:CHARA:母性 || EXP:CHARA:出産経験
	LOCALS += "母亲的味道/"

;愛情
IF TALENT:CHARA:恋人
	LOCALS += "愛情/" * 5
ELSEIF TALENT:CHARA:恋慕
	LOCALS += "愛情/" * 3
ELSEIF TALENT:CHARA:思慕
	LOCALS += "愛情/"
ENDIF

