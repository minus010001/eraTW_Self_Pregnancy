	LOCAL = 3
CASE "高级食材"
	LOCAL = 5
CASE "甜味料"
	LOCAL = 3
CASE "魚"
	LOCAL = 2
CASE "好吃的魚"
	LOCAL = 4
CASE "虫"
	LOCAL = 2
CASE "稀有昆虫"
	LOCAL = 4
CASE "青蛙"
	LOCAL = 2
CASE "跳蛋"
	LOCAL = 4
CASE "捲心菜", "芜菁", "黄瓜", "番茄", "洋葱", "茄子", "南瓜", "白菜", "胡蘿卜", "葱", "草莓", "西瓜"
	;小作人だったりするとランク上がる予定,漁師とかMASTER側にロールを設定するのもいいかもしれない
	IF 0
		LOCAL = 5
	ELSE
		LOCAL = 3
