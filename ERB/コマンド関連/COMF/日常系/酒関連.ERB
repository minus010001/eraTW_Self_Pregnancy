;酒番号から名称を取得
@ALC_NAME(ARG)
#FUNCTIONS
#DIM CHARA
RETURNF GET_STR(0, "酒データ", ARG, "名称")

;名称から酒番号を取得
@ALC_ID(ARGS)
#FUNCTION
SIF ARGS == ""
	ARGS = %VAR_FERMENT%
FOR LOCAL,1,100
	SIF GET_STR(0, "酒データ", LOCAL, "名称") == ARGS
		RETURNF LOCAL
NEXT
RETURNF 0

@IS_ALC(ARG)
#FUNCTION
#DIM 酒ID
LOCALS = %ITEMNAME:ARG%
FOR 酒ID, 1, 100
	IF GET_STR(0, "酒データ", 酒ID, "名称") == LOCALS
		IF GET_INT(0, "酒データ", 酒ID, "酒気増加量") > 0
			RETURNF 1
		ELSE
			RETURNF 0
		ENDIF
	ENDIF
NEXT
RETURNF 0

;廃止
;@醸造日数(ARG)
;@醸造できた量(ARG)


@酒造
#DIMS ALTER
DAY:10 ++
LOCAL = GET_INT(0, "酒データ", ALC_ID(), "醸造日数")
IF LOCAL > 0 && LOCAL <= DAY:10
	ALTER = %GET_STR(0, "酒データ", ALC_ID(), "変化先")%
	CALL COLORMESSAGE(@"酒虫の瓶の中の%VAR_FERMENT%は%ALTER%に変化した",C_YELLOW, 2)
	DAY:10 = 0
	VAR_FERMENT = %ALTER%
ENDIF

@DRUNKEN(C_ID, itemnum)
#DIM C_ID
#DIM 酒ID
#DIM itemnum
#DIM 酒気増加量

LOCALS = %ITEMNAME:itemnum%

酒ID = ALC_ID(LOCALS)

酒気増加量 = GET_INT(C_ID, "酒データ", 酒ID, "酒気増加量")
;一夜奇稻田を除く
IF LOCALS != "一夜奇稻田"
	IF TALENT:C_ID:酒耐性 == -1
		TIMES 酒気増加量, 1.2
	ELSEIF TALENT:C_ID:酒耐性 == 1
		TIMES 酒気増加量, 0.9
	ELSEIF TALENT:C_ID:酒耐性 == 2
		TIMES 酒気増加量, 0.75
	ELSEIF TALENT:C_ID:酒耐性 == 3
		TIMES 酒気増加量, 0.5
	ELSEIF TALENT:C_ID:酒耐性 == -2
		IF CFLAG:C_ID:不會喝酒 >= 5
			TIMES 酒気増加量, 2.0
		ELSEIF CFLAG:C_ID:不會喝酒 >= 2
			TIMES 酒気増加量, 1.5
		ELSE
			TIMES 酒気増加量, 1.0
		ENDIF
	ENDIF
ENDIF
SIF C_ID == MASTER
	酒気増加量 = 酒気増加量 / (1 + TFLAG:醒酒药)
BASE:C_ID:酒気 = MIN(BASE:C_ID:酒気 + 酒気増加量 ,MAXBASE:C_ID:酒気)

@酒一覧表示(ARG, ARGS, 割合)
;所持数がARG未満だと非表示、ARG以上所有してる酒を選択するとその道具番号をRETURNする
#DIM ITEM_ID
#DIM 割合
$LOOP

FOR ITEM_ID, 0, 600
	IF IS_ALC(ITEM_ID) && ITEM:ITEM_ID >= ARG
		IF ARGS == "売却"
			PRINTFORML [{ITEM_ID}]%ITEMNAME:ITEM_ID% (\\{ITEMPRICE:ITEM_ID * 割合 / 100})  	(所持:{ITEM:ITEM_ID})
		ELSE
			CALL PRINT_ITEM_EXPLANATION(ITEM_ID)
		ENDIF
	ENDIF
NEXT
DRAWLINE 
PRINTL [999] 中止
INPUT
IF IS_ALC(RESULT) && ITEM:RESULT >= ARG
	RETURN RESULT
ELSEIF RESULT == 999
	RETURN 0
ELSE
	GOTO LOOP
ENDIF

@CAN_DRINK(ARG)
#FUNCTION
SIF TALENT:ARG:酒耐性 == -2
	RETURNF 0
SIF BASE:ARG:酒気 >= MAXBASE:ARG:酒気 * 2 / 3
	RETURNF 0
RETURNF 1

@ALCOHOL_TASTE(ARG)
#FUNCTION
#DIM 酒ID
LOCALS = %ITEMNAME:ARG%
FOR 酒ID, 1, 100
	IF GET_STR(0, "酒データ", 酒ID, "名称") == LOCALS
		LOCAL = GET_INT(TARGET, "酒データ", 酒ID, "TASTE")
		LOCAL += TFLAG:口上特殊補正
		RETURNF LOCAL
	ENDIF
NEXT
RETURNF 0

