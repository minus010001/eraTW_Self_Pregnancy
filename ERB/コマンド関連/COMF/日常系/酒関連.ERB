@EXIST_酒データ50
#LOCALSIZE 1
#LOCALSSIZE 1

;オブジェクト本体
@酒データ50(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "名称"
	CALLF MAKE_STR(V_NAME, "一夜奇稻田")
CASE "変化先"
	CALLF MAKE_STR(V_NAME, "")
CASE "醸造日数"
	CALLF MAKE_INT(V_NAME, )
;CASE "醸造数"
;	CALLF MAKE_INT(V_NAME, )
CASE "酒気増加量"
	LOCAL = 500 + RAND:200
	;一夜奇稻田は酒耐性が高いほど酔いが回りやすい
	IF ARG != MASTER
		IF TALENT:ARG:酒耐性 == -1
			TIMES LOCAL, 1.1
		ELSEIF TALENT:ARG:酒耐性 == 1
			TIMES LOCAL, 1.2
		ELSEIF TALENT:ARG:酒耐性 == 2
			TIMES LOCAL, 1.7
		ELSEIF TALENT:ARG:酒耐性 == 3
			TIMES LOCAL, 2.5
		ELSEIF TALENT:ARG:酒耐性 == -2
			IF CFLAG:ARG:不會喝酒 >= 5
				TIMES LOCAL, 2.0
			ELSEIF CFLAG:ARG:不會喝酒 >= 2
				TIMES LOCAL, 1.5
			ELSE
				TIMES LOCAL, 1.0
			ENDIF
		ENDIF
	ENDIF
	CALLF MAKE_INT(V_NAME, LOCAL)
CASE "TASTE"
	IF TALENT:ARG:酒耐性 < 0
		LOCAL = 8
	ELSE
		LOCAL = 25
	ENDIF
	CALLF MAKE_INT(V_NAME, LOCAL)
ENDSELECT


