;蔵書閲覧判定・改＠下見パッチ
;戻り値はPLACE_IDにすること
;MASTERの現在位置に関係なく判定可能に
;PERMISSIONが真なら許可フリー
@IS_LIBRARY(PLACE_ID, PERMISSION)
#FUNCTION
#DIM PLACE_ID
#DIM PERMISSION
;外にいる場合、拠点用のPLACE_IDに置換
IF !IN_HOME(PLACE_ID)
	SELECTCASE PLACE_ID
		CASE 夢幻遺跡 
			PLACE_ID = P49夢幻遺跡・研究室
		CASE 命蓮寺境内
			PLACE_ID = P105経蔵
		CASE 神霊廟道場
			PLACE_ID = P126道場
		CASE 商家町
			PLACE_ID = P213鈴奈庵
		CASE 寺子屋
			PLACE_ID = P222寺子屋
		CASE 長屋街
			PLACE_ID = P221稗田邸
		CASE 大図書館
			PLACE_ID = P310図書館
		CASE 永遠亭的内院
			PLACE_ID = P414輝夜私室
		CASE 霧雨魔法店
			PLACE_ID = P513魔理沙宅・研究部屋
		CASE 人偶的洋館
			PLACE_ID = P524人偶洋館・研究室
		CASE 迷途之家
			PLACE_ID = P633紫私室
		CASE 白玉楼
			PLACE_ID = P623幽幽子私室
		CASE 仙人的宅邸
			PLACE_ID = P724仙人的宅邸・書斎
		CASE 天界
			PLACE_ID = P842比那名居邸庫房
		CASE 地霊殿
			PLACE_ID = P920書斎
		CASE 綿月亭
			PLACE_ID = P1043書斎
		CASE 寧靜海
			PLACE_ID = P1065庫房
		CASEELSE
			RETURNF 0
	ENDSELECT
ENDIF
;PLACE_IDを返す
SELECTCASE PLACE_ID
	CASE P48夢幻遺跡・第一資料室, P49夢幻遺跡・研究室, P50夢幻遺跡・第二資料室
		RETURNF PLACE_ID
	CASE P105経蔵, P126道場
		RETURNF PLACE_ID
	CASE P213鈴奈庵, P222寺子屋
		RETURNF PLACE_ID
	CASE P221稗田邸
		SIF PERMISSION
			RETURNF PLACE_ID
		;扮演/思慕/恋慕で開放
		SIF FLAG:扮演 == [[阿求]]
			RETURNF PLACE_ID
		SIF TALENT:[[阿求]]:恋慕 || TALENT:[[阿求]]:思慕
			RETURNF PLACE_ID
	CASE P224慧音宅
		SIF PERMISSION
			RETURNF PLACE_ID
		;扮演/信賴度30以上同室で許可
		SIF FLAG:扮演 == [[慧音]]
			RETURNF PLACE_ID
		SIF CFLAG:[[慧音]]:同室 && CFLAG:[[慧音]]:信賴度 >= 30
			RETURNF PLACE_ID
	CASE P310図書館
		RETURNF PLACE_ID
	CASE P414輝夜私室
		SIF PERMISSION
			RETURNF PLACE_ID
		;扮演/思慕以上同室で許可
		SIF GROUPMATCH(FLAG:扮演, [[鈴仙]], [[帝]], [[輝夜]], [[永琳]])
			RETURNF PLACE_ID
		SIF CFLAG:[[輝夜]]:同室 && (TALENT:[[輝夜]]:恋慕 || TALENT:[[輝夜]]:思慕)
			RETURNF PLACE_ID
	CASE P424庫房, P427倉庫
		SIF PERMISSION
			RETURNF PLACE_ID
		;扮演/同室で許可
		SIF GROUPMATCH(FLAG:扮演, [[鈴仙]], [[帝]], [[輝夜]], [[永琳]])
			RETURNF PLACE_ID
		SIF CFLAG:[[鈴仙]]:同室 || CFLAG:[[帝]]:同室 || CFLAG:[[輝夜]]:同室 || CFLAG:[[永琳]]:同室
			RETURNF PLACE_ID
	CASE P513魔理沙宅・研究部屋
		SIF PERMISSION
			RETURNF PLACE_ID
		;扮演/信賴度30以上同室で許可
		SIF FLAG:扮演 == [[魔理沙]]
			RETURNF PLACE_ID
		SIF CFLAG:[[魔理沙]]:同室 && CFLAG:[[魔理沙]]:信賴度 >= 30
			RETURNF PLACE_ID
	CASE P524人偶洋館・研究室
		SIF PERMISSION
			RETURNF PLACE_ID
		;扮演/住んでる/思慕以上同室で許可
		SIF GROUPMATCH(FLAG:扮演, [[愛麗絲]], [[神綺]])
			RETURNF PLACE_ID
		SIF INRANGE(CFLAG:MASTER:初期位置, 521, 531)
			RETURNF PLACE_ID
		SIF (CFLAG:[[愛麗絲]]:同室 || CFLAG:[[神綺]]:同室) && (TALENT:[[愛麗絲]]:恋慕 || TALENT:[[愛麗絲]]:思慕 || TALENT:[[神綺]]:恋慕 || TALENT:[[神綺]]:思慕)
			RETURNF PLACE_ID
	CASE P623幽幽子私室
		SIF PERMISSION
			RETURNF PLACE_ID
		;扮演/思慕以上同室で許可
		SIF GROUPMATCH(FLAG:扮演, [[妖夢]], [[幽幽子]])
			RETURNF PLACE_ID
		SIF CFLAG:[[幽幽子]]:同室 && (TALENT:[[幽幽子]]:恋慕 || TALENT:[[幽幽子]]:思慕)
			RETURNF PLACE_ID
	CASE P633紫私室
		SIF PERMISSION
			RETURNF PLACE_ID
		;扮演/思慕以上同室で許可
		SIF GROUPMATCH(FLAG:扮演, [[橙]], [[藍]], [[紫]])
			RETURNF PLACE_ID
		SIF CFLAG:[[紫]]:同室 && (TALENT:[[紫]]:恋慕 || TALENT:[[紫]]:思慕)
			RETURNF PLACE_ID
	CASE P724仙人的宅邸・書斎
		SIF PERMISSION
			RETURNF PLACE_ID
		;扮演/住んでる/思慕以上同室で許可
		SIF FLAG:扮演 == [[華扇]]
			RETURNF PLACE_ID
		SIF INRANGE(CFLAG:MASTER:初期位置, 720, 730)
			RETURNF PLACE_ID
		SIF CFLAG:[[華扇]]:同室 && (TALENT:[[華扇]]:恋慕 || TALENT:[[華扇]]:思慕)
			RETURNF PLACE_ID
	CASE P842比那名居邸庫房
		SIF PERMISSION
			RETURNF PLACE_ID
		;扮演/住んでる/同室で許可
		SIF GROUPMATCH(FLAG:扮演, [[天子]], [[衣玖]])
			RETURNF PLACE_ID
		SIF INRANGE(CFLAG:MASTER:初期位置, 834, 842)
			RETURNF PLACE_ID
		SIF CFLAG:[[天子]]:同室 || CFLAG:[[衣玖]]:同室
			RETURNF PLACE_ID
	CASE P920書斎
		SIF PERMISSION
			RETURNF PLACE_ID
		;扮演/住んでる/思慕以上同室で許可,ただし信賴度50未満はダメ
		SIF FLAG:扮演 == [[覚]]
			RETURNF PLACE_ID
		SIF CFLAG:[[覚]]:信賴度 < 50
			RETURNF 0
		SIF INRANGE(CFLAG:MASTER:初期位置, 902, 926)
			RETURNF PLACE_ID
		SIF CFLAG:[[覚]]:同室 && (TALENT:[[覚]]:恋慕 || TALENT:[[覚]]:思慕)
			RETURNF PLACE_ID
	;綿月亭書斎
	CASE P1043書斎
		SIF PERMISSION
			RETURNF PLACE_ID
		;扮演/住んでるで許可
		SIF GROUPMATCH(FLAG:扮演, [[鈴仙二號]], [[依姫]], [[豊姫]])
			RETURNF PLACE_ID
		SIF INRANGE(CFLAG:MASTER:初期位置, 1020, 1049)
			RETURNF PLACE_ID
	;純狐邸庫房
	CASE P1050哆来咪宅・居間
		SIF PERMISSION
			RETURNF PLACE_ID
		;扮演/住んでる/思慕以上同室で許可,ただし信賴度50未満はダメ
		SIF FLAG:扮演 == [[純狐]]
			RETURNF PLACE_ID
		SIF CFLAG:[[純狐]]:信賴度 < 50
			RETURNF 0
		SIF INRANGE(CFLAG:MASTER:初期位置, 1060, 1072)
			RETURNF PLACE_ID
		SIF CFLAG:[[純狐]]:同室 && (TALENT:[[純狐]]:恋慕 || TALENT:[[純狐]]:思慕)
			RETURNF PLACE_ID
ENDSELECT
RETURNF 0

