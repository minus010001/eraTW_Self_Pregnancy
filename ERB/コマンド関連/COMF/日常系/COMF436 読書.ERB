;-------------------------------------------------
;読書
;日常系指令、レベル1
;-------------------------------------------------
@COM436
#DIM READING_EXP
READING_EXP = 1


PRINTL 要读什么？ 
CALL ASK_M("文学", 1, "実用書", 1, "小黄书", 1, "中止", 1)
IF RESULT == 0
	SELECTCASE RAND:10
		;忍殺
		CASE 0
			TFLAG:193 = 1
		;失敗
		CASE 1 TO 3
			TFLAG:193 = 2
		;文学
		CASEELSE
			TFLAG:193 = 3
	ENDSELECT
ELSEIF RESULT == 1
	SELECTCASE RAND:10
		;失敗
		CASE 0 TO 2
			TFLAG:193 = 4
		;実用
		CASEELSE
			TFLAG:193 = 5
	ENDSELECT
ELSEIF RESULT == 2
	SELECTCASE RAND:9
		;ｳｽｲﾀｶｲﾎﾝ
		CASE 0 TO 2
			TFLAG:193 = 6
		;保健体育
		CASE 3 TO 5
			TFLAG:193 = 7
		;カーマスートラ
		CASE 6 TO 8
			TFLAG:193 = 8
		;やばい本
;		CASEELSE
;		IF !TALENT:MASTER:禁断的知識
;			TFLAG:193 = 9
;		ELSE
;			GOTO LOOP
;		ENDIF
	ENDSELECT
ELSEIF RESULT == 3
	RETURN -1
ENDIF

IF TARGET == 54 && CFLAG:325 == 5
	IF TALENT:恋慕
		PRINTFORM 和帕秋莉一起享受着读书的快乐
	ELSE
		PRINTFORM 在帕秋莉停止読書之前你也把时间打发在了読書上
	ENDIF
ENDIF
PRINTFORM %CALLNAME:MASTER%把书架上的
SELECTCASE TFLAG:193
	CASE 1
		PRINT 『忍者杀手』
	CASE 2 TO 3
		PRINT 文芸書
	CASE 4 TO 5
		PRINT 実用書
	CASE 6
		PRINT 又薄又贵的书
	CASE 7
		PRINT 保健体育的书
	CASE 8
		PRINT 『印度爱经』
;	CASE 9
;		PRINT タイトルのない皮装丁の湿り気を帯びた本
ENDSELECT
PRINTFORML 拿下読了起來

;3分の1で追加イベント
SIF !RAND:3 && TARGET == 54
	TFLAG:194 = 1
;TARGETが無知かつＨな本読みかつMASTERがTARGETより教養が高くTARGETの教養が2以上かつ五分の一
SIF !RAND:5 && TALENT:TARGET:無知 && TFLAG:193 >= 6 && TFLAG:193 <= 8 && ABL:MASTER:教養 > ABL:教養 && ABL:教養 >= 2
	TFLAG:194 = 2

SELECTCASE TFLAG:193
	CASE 1
		EXP:MASTER:戦闘経験 += 5
	CASE 3
	;文学
		EXP:MASTER:学習経験 += READING_EXP
	CASE 5
	;実用
		SELECTCASE RAND:7
			CASE 0
				EXP:MASTER:会話経験 += READING_EXP
			CASE 1
				EXP:MASTER:撮影経験 += READING_EXP
			CASE 2
				EXP:MASTER:料理経験 += READING_EXP
			CASE 3
				EXP:MASTER:釣魚経験 += READING_EXP
			CASE 4
				EXP:MASTER:採集経験 += READING_EXP
			CASE 5
				EXP:MASTER:調合経験 += READING_EXP
			CASEELSE
				EXP:MASTER:伐採経験 += READING_EXP
		ENDSELECT
	CASE 6
		CALL RECOVER(MASTER,100,"精力")
		CALL RECOVER(MASTER,100,"勃起")
ENDSELECT

IF TARGET > 0 && CFLAG:行動 != 5
	;固定で獲得するソース
	SOURCE:歓楽 = 200
	;信頼
	TFLAG:98 = 3

	;ABL:従順をみる
	IF ABL:従順 <= 1
		SOURCE:歓楽 += (ABL:従順 * 80)
	ELSEIF ABL:従順 <= 3
		SOURCE:歓楽 += 300 + (ABL:従順 * 80)
	ELSEIF ABL:従順 <= 5
		SOURCE:歓楽 += 700 + (ABL:従順 * 80)
	ELSEIF ABL:従順 <= 8
		SOURCE:歓楽 += 950 + (ABL:従順 * 100)
	ELSEIF ABL:従順 <= 11
		SOURCE:歓楽 += 1200 + (ABL:従順 * 90)
	ELSE
		SOURCE:歓楽 += 2000 + (ABL:従順 * 50)
	ENDIF

	;好感度をみる
	IF CFLAG:2 <= 500
		SOURCE:歓楽 += CFLAG:2 * 2
	ELSEIF CFLAG:2 <= 5000
		SOURCE:歓楽 += 1000 + (CFLAG:2 - 500) / 2
	ELSE
		SOURCE:歓楽 += 5000 + (CFLAG:2 - 5000) / 3
	ENDIF
	SIF SOURCE:歓楽 < 0
		SOURCE:歓楽 = 0
ENDIF

DOWNBASE:MASTER:体力 = 50
DOWNBASE:MASTER:気力 = 200
;賢者タイムは読書がはかどる
IF IS_WISEMAN()
	PRINTFORML 由于煩悩消失了、学習的効率提高了
	TIME += 45
ELSE
	TIME += 60
ENDIF
RETURN 1


