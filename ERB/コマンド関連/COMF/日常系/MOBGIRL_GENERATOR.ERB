@REVEAL_YOUKAI(ARG)
#DIMS ACC_EAR
#DIMS ACC_TAIL
#DIMS ACC_LEAF
SIF !TALENT:ARG:人类
	RETURN
SELECTCASE CFLAG:ARG:妖怪変化
	CASE 0
		RETURN
	CASE 1
		CSTR:ARG:路人子種族 = 妖怪狸
		ACC_EAR = 狸猫耳
		ACC_TAIL = 狸猫尾巴
		ACC_LEAF = 装飾_狸猫叶子
		TALENT:ARG:動物耳 = 1
	CASE 2
		CSTR:ARG:路人子種族 = 妖怪狐
		ACC_EAR = 狐耳
		ACC_TAIL = 狐尾巴
		TALENT:ARG:動物耳 = 1
		TALENT:ARG:妖狐 = 1
ENDSELECT
TALENT:ARG:人类 = 0
CALL COLORMESSAGE(@"外道照身灵波光线!看你的真面目！",C_YELLOW,2)
PRINTFORML %CALLNAME:ARG%的脑袋上蹦出一个妖怪的耳朵!
PRINTFORML %CALLNAME:ARG%竟然是变成人类的妖怪。！

FOR LOCAL,30,36
	CSTR:ARG:LOCAL = %REPLACE(@"%CSTR:ARG:LOCAL%","[装飾1:装飾_無][装飾2:装飾_無]", @"[装飾1:装飾_%ACC_EAR%][装飾2:装飾_%ACC_TAIL%]")%
	SIF CFLAG:ARG:妖怪変化 == 1
		CSTR:ARG:LOCAL = %DIC_SET(CSTR:ARG:LOCAL,"装飾3",ACC_LEAF)%
NEXT

CSTR:ARG:路人子プロフィール１ = %REPLACE(@"%CSTR:ARG:路人子プロフィール１%","相遇的町娘", @"相遇的变化成町娘的%CSTR:ARG:路人子種族%")%

CALL 路人子画像リセット(ARG)

;路人子の一人称/二人称を設定する
@SET_MOB_PERSON(ARG, 敵対)
#DIM 年齢差
#DIM 体格差
#DIM 呼称
#DIM 敵対
呼称 = (TALENT:MASTER:性別 == 2)
年齢差 = TALENT:MASTER:年齢
体格差 = (TALENT:MASTER:体型 + 2 <= TALENT:ARG:体型)
IF TALENT:MASTER:幼稚 || TALENT:MASTER:幼児／幼児退行 || 年齢差 == -1
	IF TALENT:ARG:幼稚 || TALENT:ARG:幼児／幼児退行
		年齢差 = 0
	ELSE
		年齢差 = -1
	ENDIF
ELSEIF (TALENT:ARG:幼稚 || TALENT:ARG:幼児／幼児退行) && 年齢差 != 2
	年齢差 = 1
ENDIF
路人子一人称 = %TEXTR(@"咱/我/我/我/%CALLNAME:ARG%")%
IF CFLAG:ARG:性格傾向 == 2 || (TALENT:ARG:胆量 > 0 && TALENT:ARG:自尊心 > 0)
	SIF !RAND:5 && CSTR:ARG:路人子種族 == "鬼"
		路人子一人称 = %TEXTR("俺们/俺")%
	SIF !RAND:4
		路人子一人称 = %TEXTR(@"我/我/咱/%CALLNAME:ARG%")%
	路人子二人称 = %TEXTR("汝/你/你/你/您/您")%
	SIF STRCOUNT(路人子二人称, "(你)") && (!RAND:4 || TALENT:ARG:風俗嬢)
		路人子二人称 = %路人子二人称%桑
	SIF RAND:3
		GOTO SKIP
	SIF !RAND:3 && !TALENT:ARG:人妻
		路人子二人称 = %\@ 呼称 ? %TEXTR("兄桑/哥哥酱/哥哥")% # %TEXTR("姉桑/姉酱/姐姐")% \@%
	SIF RAND:2
		GOTO SKIP
	IF 年齢差 > 0
		IF 呼称
			IF CSTR:ARG:路人子種族 == "鬼" && 敵対
				路人子二人称 = %\@ 年齢差 == 2 ? 老头 # 大叔 \@%
			ELSEIF RAND:2
				路人子二人称 = %\@ 年齢差 == 2 ? 爷爷 # 叔叔 \@%
			ELSE
				路人子二人称 = %\@ 年齢差 == 2 ? 老爷爷 # 叔叔桑 \@%
			ENDIF
		ELSE
			IF CSTR:ARG:路人子種族 == "鬼" && 敵対
				路人子二人称 = %\@ 年齢差 == 2 ? 老太婆 # 阿姨 \@%
			ELSEIF RAND:2
				路人子二人称 = %\@ 年齢差 == 2 ? 奶奶 # 老姐 \@%
			ELSE
				路人子二人称 = %\@ 年齢差 == 2 ? 老奶奶 # 老姐 \@%
			ENDIF
		ENDIF
	ELSEIF 体格差 > 0
		IF 敵対 && CSTR:ARG:路人子種族 == "鬼"
			路人子二人称 = %\@ 呼称 ? 小不点 # 小不点 \@%
		ELSE
			路人子二人称 = %\@ 呼称 ? 小不点桑 # 小不点酱 \@%
		ENDIF
	ELSEIF 年齢差 < 0 && 敵対 && CSTR:ARG:路人子種族 == "鬼"
		路人子二人称 = %\@ 呼称 ? 小孩 # 小女孩 \@%
	ENDIF
ELSE
	路人子二人称 = %TEXTR("你/您")%
	SIF TALENT:ARG:風俗嬢
		路人子二人称 = 您
	IF CFLAG:ARG:性格傾向 == 4
		路人子一人称 = %TEXTR("人家/我/咱")%
		路人子二人称 = 您
		RETURN
	ELSEIF (RAND:2 && CFLAG:ARG:性格傾向 == 3) || !RAND:5
		路人子一人称 = %TEXTR(@"我/我/咱/%CALLNAME:ARG%")%
	ENDIF
	SIF RAND:3
		GOTO SKIP
	SIF !RAND:3 && !TALENT:ARG:人妻
		路人子二人称 = %\@ 呼称 ? %TEXTR("兄桑/欧尼桑")% # %TEXTR("姉桑/欧涅桑")% \@%
	SIF RAND:2
		GOTO SKIP
	IF 年齢差 > 0 
		IF 呼称
			SELECTCASE CFLAG:ARG:性格傾向
				CASE 1
					路人子二人称 = %\@ 年齢差 == 2 ? 爷爷桑 # 叔叔桑 \@%
				CASE 3
					路人子二人称 = %\@ 年齢差 == 2 ? 爷爷 # 叔叔 \@%
					SIF 年齢差 == 1 && RAND:3
						路人子二人称 = %TEXTR("哥哥酱/欧尼酱/哥哥")%
				CASE 5
					路人子二人称 = %\@ 年齢差 == 2 ? 老爷爷 # 叔叔桑 \@%
			ENDSELECT
		ELSE
			SELECTCASE CFLAG:ARG:性格傾向
				CASE 1
					路人子二人称 = %\@ 年齢差 == 2 ? 老奶奶 # 老姐 \@%
				CASE 3
					路人子二人称 = %\@ 年齢差 == 2 ? 奶奶 # 老姐 \@%
					SIF 年齢差 == 1 && RAND:3
						路人子二人称 = %TEXTR("姉ちゃん/お姉ちゃん/おねえちゃん")%
				CASE 5
					路人子二人称 = %\@ 年齢差 == 2 ? 老婆婆 # 阿姨 \@%
			ENDSELECT
		ENDIF
	ELSEIF 体格差 > 0 || 年齢差 < 0 
		路人子二人称 = %\@ 呼称 ? 公子 # 小姐姐 \@%
	ENDIF
ENDIF
$SKIP
SIF 路人子一人称 == CALLNAME:ARG && !RAND:3
	路人子一人称 += "酱"
RETURN
