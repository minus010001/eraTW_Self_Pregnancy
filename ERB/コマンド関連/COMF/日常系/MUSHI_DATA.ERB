;-------------------------------------------------
;ムシの出現条件をひとまとめにした関数
;-------------------------------------------------
@ムシ出現条件(ARGS)
#FUNCTION
SELECTCASE ARGS
CASE "昼間"
	SIF INRANGE(TIME, 360, 1080)
		RETURNF 1
CASE "夜間"
	SIF !INRANGE(TIME, 360, 1080)
		RETURNF 1
CASE "深夜"
	SIF INRANGE(TIME, 0, 300)
		RETURNF 1
CASE "暁"
	SIF INRANGE(TIME, 240, 360) || INRANGE(TIME, 840, 1080)
		RETURNF 1
CASE "春"
	SIF GET_MONTH() == "春之月"
		RETURNF 1
CASE "夏"
	SIF GET_MONTH() == "夏之月"
		RETURNF 1
CASE "秋"
	SIF GET_MONTH() == "秋之月"
		RETURNF 1
CASE "雨天"
	SIF GROUPMATCH(天候, 4, 5, 6, 7) || FLAG:異常気象 == 9
		RETURNF 1
CASE "晴朗"
	SIF GROUPMATCH(天候, 0)
		RETURNF 1
CASE "0の付く日"
	SIF GROUPMATCH(DAY:3, 10, 20, 30)
		RETURNF 1
CASE "月初め"
	SIF GROUPMATCH(DAY:3, 1, 2, 3)
		RETURNF 1
CASEELSE
	THROW
ENDSELECT
