+ ABL:TARGET:親密 + RAND:(ABL:TARGET:親密) + (TALENT:TARGET:心情 * 2) + (TALENT:TARGET:献身的 * 5) + (CFLAG:TARGET:清い交際 * 5)
- TALENT:TARGET:懒散 - TALENT:TARGET:傲嬌 - (MARK:TARGET:反発刻印 * 5) + RAND:20 - RAND:15 + StoneKoukando + 5
}

SELECTCASE Stoneday
	CASE 1
	;春
		IF STRCOUNT(GET_STR(, "角色データ", TARGET, "感性：好き"), "(春の|春風の|四季の)")
			StoneSense += 35
		ELSEIF STRCOUNT(GET_STR(, "角色データ", TARGET, "感性：好き"), "(樱花の|百合の)")
			StoneSense += 20
		ELSEIF STRCOUNT(GET_STR(, "角色データ", TARGET, "感性：好き"), "(粉色|黄|虹|自然)")
			StoneSense += 10
		ENDIF
		
		SIF STRCOUNT(GET_STR(, "角色データ", TARGET, "感性：好き"), "(夏の|秋の|冬の|自然)")
			StoneSense -= 20

		SIF STRCOUNT(GET_STR(, "角色データ", TARGET, "感性：嫌い"), "(春の|春風の|四季の)")
			StoneSense -= 30

	CASE 2
	;お皿を積もう
		StoneSense += ABL:MASTER:料理技能 + ABL:TARGET:料理技能
		SIF STRCOUNT(GET_STR(, "角色データ", TARGET, "感性：好き"), "(食器|皿|女僕的)")
			StoneSense += 25

		SIF STRCOUNT(GET_STR(, "角色データ", TARGET, "感性：嫌い"), "(食器|皿|女僕的)")
			StoneSense -= 25

	CASE 3
	;ホラー
		IF STRCOUNT(GET_STR(, "角色データ", TARGET, "感性：好き"), "(闇の|闇|血の|可怕的|毒|毒の)")
			StoneSense += 35
		ELSEIF STRCOUNT(GET_STR(, "角色データ", TARGET, "感性：好き"), "(妖怪の|妖魔の|幽霊の|亡霊の|不思議|奇妙)")
			StoneSense += 20
		ENDIF

		SIF STRCOUNT(GET_STR(, "角色データ", TARGET, "感性：嫌い"), "(闇の|闇|血の|可怕的|毒|毒の)")
			StoneSense -= 30

	CASE 4
	;音楽的
		StoneSense += ABL:MASTER:音楽技能 + ABL:TARGET:音楽技能
		
		SIF STRCOUNT(GET_STR(, "角色データ", TARGET, "感性：好き"), "(音楽|楽器)")
			StoneSense += 25
		SIF STRCOUNT(GET_STR(, "角色データ", TARGET, "感性：嫌い"), "(音楽|楽器)")
			StoneSense -= 25
		SIF FLAG:约会的对象 == 127
			StoneSense += 25
		
	CASE 5
	;メルヘン
		IF STRCOUNT(GET_STR(, "角色データ", TARGET, "感性：好き"), "(可愛的|活潑|洛麗塔)")
			StoneSense += 35
		ELSEIF STRCOUNT(GET_STR(, "角色データ", TARGET, "感性：好き"), "(粉色|心形の|夢の)")
			StoneSense += 20
		ENDIF
		
