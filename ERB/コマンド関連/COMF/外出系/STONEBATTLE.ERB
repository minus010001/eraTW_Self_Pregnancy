@STONEBATTLE(difficulty, C_ID)
#DIM C_ID
#DIM FIRSTONE
#DIM SECONE, 3
#DIM LASTONE, 3
#DIM FIRSTONE_E
#DIM SECONE_E
#DIM LASTONE_E
#DIM STONE_YOSOKU
#DIM KUISHIBARI
#DIM Skill
#DIM OUTCOME	;0=双方失敗 1=你成功 2=相手成功 3=双方成功
#DIM PLAYER_TOTAL
#DIM ENEMY_TOTAL
#DIM difficulty

{
FIRSTONE = 1 + RAND:5 + (TALENT:MASTER:擅長用針 * 2) + (TALENT:MASTER:靈活手指 * 2) + (TALENT:MASTER:採集Lv / 2 )
+ ( ABL:MASTER:清掃技能 / 2 ) + (TALENT:MASTER:調合Lv / 2 ) + (TALENT:MASTER:積み石ランカー * 2) 
}

;持ち込み道具
SIF STONE_ITEM:1 == 2
	FIRSTONE += TALENT:MASTER:積み石ランカー + 1

Skill = ABL:MASTER:技巧 + ABL:MASTER:指

SECONE:0 = RAND:(6 + Skill) + Skill + RAND:8 + 7
SECONE:1 = RAND:(4 + Skill) + Skill + RAND:5 + 4
SECONE:2 = RAND:(2 + Skill) + (Skill / 2) + RAND:2 + 2
LASTONE:0 = RAND:9 + 2
LASTONE:1 = RAND:7 + 4
LASTONE:2 = RAND:5 + 7
IF difficulty == 0
	FIRSTONE_E = RAND:4 + 1
	SECONE_E = RAND:4 + RAND:5 + 1
	LASTONE_E = RAND:5 + 1
ELSEIF difficulty == 1
	FIRSTONE_E = RAND:5 + 2
	SECONE_E = RAND:6 + RAND:6 + 2
	LASTONE_E = RAND:6 + 2
ELSEIF difficulty == 2
	FIRSTONE_E = RAND:6 + 3
	SECONE_E = RAND:6 + RAND:7 + 3
	LASTONE_E = RAND:7 + 3
ELSEIF difficulty == 3
	FIRSTONE_E = RAND:7 + 4
	SECONE_E = RAND:7 + RAND:7 + 4
	LASTONE_E = RAND:8 + 4
ELSEIF difficulty == 4
	FIRSTONE_E = RAND:8 + 5
	SECONE_E = RAND:7 + RAND:8 + 5
	LASTONE_E = RAND:9 + 5
ELSEIF difficulty == 5
	FIRSTONE_E = RAND:10 + 6
	SECONE_E = RAND:8 + RAND:8 + 6
	LASTONE_E = RAND:10 + 6
ELSEIF difficulty == 6
	LOCAL = RAND:3
	FIRSTONE_E = RAND:(10 + LOCAL) + 5 + LOCAL
	SELECTCASE RAND:3
		CASE 0
			SECONE_E = RAND:8 + RAND:8 + 5
		CASE 1
			SECONE_E = RAND:9 + RAND:9 + 6
		CASE 2
			SECONE_E = RAND:10 + RAND:10 + 7
	ENDSELECT
	LASTONE_E = RAND:5 + 8
ELSEIF difficulty == 7
	SELECTCASE RAND:3
		CASE 0
			FIRSTONE_E = RAND:10 + 6
		CASE 1
			FIRSTONE_E = RAND:11 + 7
		CASE 2
			FIRSTONE_E = RAND:12 + 8
	ENDSELECT
	SELECTCASE RAND:3
		CASE 0
			SECONE_E = RAND:8 + RAND:8 + 6
		CASE 1
			SECONE_E = RAND:9 + RAND:9 + 7
		CASE 2
			SECONE_E = RAND:10 + RAND:10 + 8
	ENDSELECT
	LASTONE_E = RAND:6 + 9
ELSEIF difficulty == 8
	FIRSTONE_E = RAND:6 + 10
	SECONE_E = RAND:7 + RAND:7 + 10
	LASTONE_E = 20
ELSE 
	FIRSTONE_E = RAND:7 + 12
	SECONE_E = RAND:8 + RAND:8 + 12
	LASTONE_E = 22
ENDIF

;相手の積み石数予測
IF STONE_ITEM:1 == 4
	STONE_YOSOKU = SECONE_E
ELSE
	IF SECONE_E < 6
		STONE_YOSOKU = SECONE_E + RAND:3
	ELSE
		SELECTCASE 1
			CASE 0
				STONE_YOSOKU = SECONE_E - RAND:6
			CASEELSE
				STONE_YOSOKU = SECONE_E + RAND:6
		ENDSELECT
	ENDIF
ENDIF


DRAWLINE

PRINTFORMW 積み石勝負がはじまった！
	SIF STONE_ITEM:1 == 2
		PRINTFORML 肉体強化地蔵発動！
	PRINTFORML 序盤の斯塔トダッシュを決めた%CALLNAME:MASTER%が現在積めた石の数は{FIRSTONE}個
PRINTFORML 一方%CALLNAME:C_ID%は{FIRSTONE_E}個積んでいる…
IF STONE_ITEM:1 == 4
	PRINTFORMW 正直すぎる石しるべ発動！
	PRINTFORMW 事故が無ければ相手はここから{STONE_YOSOKU}個積み上げる…！
ELSE
	PRINTFORMW おそらく相手はここから大体{STONE_YOSOKU}個積み上げるだろう…
ENDIF
{
CALL ASK_M(@"急いで積む（石を崩す松鼠ク{12 + (ABL:欲望 * 2)}％＆積み石数多め）",1,@"普通に積む（石を崩す松鼠ク{10 + ABL:欲望}％）",1,
@"慎重に積む（石を崩す松鼠ク{7 + (ABL:欲望 / 2)}％＆積み石数少なめ）",1, @"什麼也不做（石を崩す松鼠ク0＆積み石数0）",1)
}

IF RESULT == 0
	PRINTFORMW %CALLNAME:MASTER%は多少の松鼠クを負ってでも早く石を積み上げた！
	PRINTFORMW 
	SELECTCASE RAND:100
		CASE 0 TO 6
			OUTCOME = 1
		CASE 7 TO 13
			OUTCOME = 0
		CASE 14 TO 18 + (ABL:欲望 * 2)
			OUTCOME = 2
		CASEELSE
			OUTCOME = 3
		ENDSELECT
ELSEIF RESULT == 1
PRINTFORMW %CALLNAME:MASTER%は石を積んだ
PRINTFORMW 
	SELECTCASE RAND:100
		CASE 0 TO 6
			OUTCOME = 1
		CASE 7 TO 13
			OUTCOME = 0
		CASE 14 TO 16 + ABL:欲望
			OUTCOME = 2
		CASEELSE
			OUTCOME = 3
	ENDSELECT
ELSEIF RESULT == 2
	PRINTFORMW %CALLNAME:MASTER%は慎重に石を積んだ
	PRINTFORMW 
	SELECTCASE RAND:100
		CASE 0 TO 6
			OUTCOME = 1
		CASE 7 TO 13
			OUTCOME = 0
		CASE 13 TO 13 + (ABL:欲望 / 2)
			OUTCOME = 2
		CASEELSE
			OUTCOME = 3
	ENDSELECT
ELSEIF RESULT == 3
	PRINTFORMW %CALLNAME:MASTER%は石を積まなかった
	PRINTFORMW 
	PLAYER_TOTAL = FIRSTONE
	SELECTCASE RAND:100
		CASE 0 TO 13
			PRINTFORMW %CALLNAME:MASTER%は何もせずに待っている…
			PRINTFORMW 哎呀！%CALLNAME:C_ID%似乎在结束前石崩了！
			PRINTFORMW 对手的成果是{LASTONE_E}個……
			ENEMY_TOTAL = LASTONE_E
		CASEELSE
			IF STONE_ITEM:1 == 3 && RAND:9 == 0
				PRINTFORMW %CALLNAME:MASTER%は何もせずに待っている…
				PRINTFORMW 気ままな積み石の効果発動！
				PRINTFORMW %CALLNAME:C_ID%\@ OUTCOME % 2 == 1? は終了間際に# も\@石を崩したみたいだ！
				PRINTFORMW 拼命想把已经崩掉的石堆重新積起的对手的結果是{LASTONE_E}個……
				ENEMY_TOTAL = LASTONE_E
			ELSE
				PRINTFORMW %CALLNAME:MASTER%は何もせずに待っている…
				PRINTFORMW 一息ついた%CALLNAME:MASTER%が隣を見ると…
				PRINTFORMW 对手似乎又稳稳地積了{SECONE_E}個石头……
				ENEMY_TOTAL = FIRSTONE_E + SECONE_E
			ENDIF
	ENDSELECT
ENDIF

IF GROUPMATCH(RESULT, 0, 1, 2)
	IF OUTCOME % 2 == 1
		PRINTFORMW %CALLNAME:MASTER%は{SECONE:RESULT}個を追加で積み上げた！
		PLAYER_TOTAL = FIRSTONE + SECONE:RESULT
	ELSE
		PRINTFORMW 啊！
		PRINTFORMW %CALLNAME:MASTER%在不经意见石崩了！
		KUISHIBARI = RAND:CHARANUM
		;全角色から随机に判定して恋慕か恋人にヒットしたら食いしばりが発動する
		;仕様上全員を恋慕にしてるプレイヤーは絶対発動
		IF TALENT:(KUISHIBARI):恋人 || TALENT:(KUISHIBARI):恋慕
			PRINTFORMW ……頑張って！！！
			PRINTFORMW 
			PRINTFORMW %CALLNAME:MASTER%は幻聴かもしれないが%CALLNAME:KUISHIBARI%から声援を受けた気がした…
			PRINTFORMW %CALLNAME:MASTER%は全力で積み直した！
			PRINTFORMW %CALLNAME:MASTER%は{FIRSTONE + SECONE:RESULT}個を積み上げた！
			PLAYER_TOTAL = FIRSTONE + SECONE:RESULT
		ELSEIF STONE_ITEM:1 == 1
			SELECTCASE RAND:5
				CASE 0
					PRINTFORMW 小石ころ帽子の効果発動！！
					PRINTFORMW 石は一度崩れたような気がしたが見えない何かによって防がれた…！
					PRINTFORMW %CALLNAME:MASTER%は{FIRSTONE + SECONE:RESULT}個を積み上げた！
					PLAYER_TOTAL = FIRSTONE + SECONE:RESULT
				CASEELSE
					PRINTFORMW 仓促積石的结果、是{LASTONE:RESULT}個
					PLAYER_TOTAL = LASTONE:RESULT
			ENDSELECT
		ELSE
			PRINTFORMW 仓促積石的结果、是{LASTONE:RESULT}個
			PLAYER_TOTAL = LASTONE:RESULT
		ENDIF
	ENDIF
	IF OUTCOME >= 2
		IF STONE_ITEM:1 == 3
			SELECTCASE RAND:9
				CASE 0
					PRINTFORMW 気ままな積み石の効果発動！
					PRINTFORMW %CALLNAME:C_ID%\@ OUTCOME % 2 == 1? は終了間際に# も\@石を崩したみたいだ！
					PRINTFORMW 拼命想把已经崩掉的石堆重新積起的对手的結果是{LASTONE_E}個……
					ENEMY_TOTAL = LASTONE_E
				CASEELSE
					PRINTFORMW 另一边、对手似乎又稳稳地積了{SECONE_E}個石头……
					ENEMY_TOTAL = FIRSTONE_E + SECONE_E
			ENDSELECT
		ELSE
			PRINTFORMW 另一边、对手似乎又稳稳地積了{SECONE_E}個石头……
			ENEMY_TOTAL = FIRSTONE_E + SECONE_E
		ENDIF
	ELSE
		PRINTFORMW 哎呀！%CALLNAME:C_ID%似乎\@ OUTCOME % 2 == 1? 在结束前 # 也\@石崩了！
		PRINTFORMW 拼命想把已经崩掉的石堆重新積起的对手的結果是{LASTONE_E}個……
		ENEMY_TOTAL = LASTONE_E
	ENDIF
ENDIF
CALL COLORMESSAGE(@"%CALLNAME:MASTER%：{PLAYER_TOTAL}　VS　%CALLNAME:C_ID%：{ENEMY_TOTAL}",C_YELLOW,2)
PRINTFORML 
IF PLAYER_TOTAL > ENEMY_TOTAL
	CALL COLORMESSAGE(@"YOU WIN!",C_YELLOW,1)
	CALL COLORMESSAGE(@"人生的勝利者！",C_YELLOW,1)
	PRINTFORMW
	;ラスボス特殊分岐
	IF difficulty == 8
		TRYCALLFORM StoneMessage_{C_ID}("lastbattle_w")
	ELSE
		TRYCALLFORM StoneMessage_{C_ID}("win")
	ENDIF
	RETURN 0
ELSEIF PLAYER_TOTAL == ENEMY_TOTAL
	PRINTFORML
	PRINTFORMW 因为平局、所以需要再開始一场比赛
	JUMP STONEBATTLE(difficulty, C_ID)
ELSE
	CALL COLORMESSAGE(@"YOU LOSE!",C_YELLOW,1)
	CALL COLORMESSAGE(@"败犬！",C_YELLOW,1)
	PRINTFORMW
	;ラスボス特殊分岐
	IF difficulty == 8
		TRYCALLFORM StoneMessage_{C_ID}("lastbattle_l")
	ELSE
		TRYCALLFORM StoneMessage_{C_ID}("lose")
	ENDIF
	RETURN 1
ENDIF

RETURN 0


