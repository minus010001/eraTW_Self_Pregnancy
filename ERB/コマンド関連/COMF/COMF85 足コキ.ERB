;-------------------------------------------------
;足交
;奉仕系指令、レベル4
;-------------------------------------------------
@COM85
;-------------------------------------------------
;COM別FLAGの設定
;-------------------------------------------------
;行為者
TCVAR:116 = TARGET
;奉仕快楽フラグ
TCVAR:108 = 1

CSTR:PLAYER:10 += @"{TARGET}/"
;TCVAR:2 射精部位フラグ（-1=避孕套 1=膣内 2=肛門 3=手淫 4=口淫 5=乳交 6=素股 7=足交 8=体表 9=肛門奉仕 10=道具 11=触手 12=Ａ責め 13=Ｖ責め）
TCVAR:PLAYER:2 = 7
;強度
TCVAR:PLAYER:4 = 600

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
IF FLAG:70 == 1
	BASE:MASTER:TSP -= 70
	SOURCE:PLAYER:快Ｃ = 250

ELSE

	DOWNBASE:体力 += 10
	DOWNBASE:気力 += 150
	SOURCE:PLAYER:快Ｃ = 250

	SOURCE:加虐 = 800
	SOURCE:征服 = 500
	SOURCE:性行動 = 700
	SOURCE:達成 = 500
	SOURCE:屈従 = 500
	SOURCE:逸脱 = 400
	SOURCE:反感 = 400

	;上のほうで計算した汚れデータ
	SOURCE:不潔 = Y*10 + 50

	;-------------------------------------------------
	;特殊ソース処理
	;-------------------------------------------------
	;圧抑、抵抗持ちには、抑鬱ソース追加
	SIF TALENT:自己愛 < 0 || TALENT:抵抗
		SOURCE:鬱屈 += 600
ENDIF
;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;同性愛　レズなら戻り値1ゲイなら戻り値2
IF FLAG:70 == 1 || CFLAG:恶作剧
	IF HETEROSEX(TARGET,PLAYER)
		EXP:PLAYER:(50 + HETEROSEX(TARGET,PLAYER)) += 4
	ENDIF
ELSE
	IF HETEROSEX(TARGET,PLAYER)
		EXP:(50 + HETEROSEX(TARGET,PLAYER)) += 4
		EXP:PLAYER:(50 + HETEROSEX(TARGET,PLAYER)) += 4
	ENDIF

	;施虐狂は嗜虐快楽
	SIF TALENT:施虐狂 || ABL:施虐属性
		EXP:嗜虐快楽経験 ++
ENDIF
RETURN 1

@CAN_COM85(ARG)
SKIPDISP ARG
PRINTL 足交
;-------------------------------------------------
;実行判定処理
;-------------------------------------------------
IF FLAG:70 == 1 || CFLAG:恶作剧

ELSE

A = 0
S = 0

;全部の命令に共通の要素を考慮
;(従順が高いと命令に従いやすいなど)
CALL COM_ORDER

;ABL:欲望
IF ABL:欲望
	SIF S
		PRINT  + 
	A += ABL:欲望 * 1
	PRINT 欲望
	PRINTV 'LV,ABL:欲望,'(,ABL:欲望 * 1,')
	S = 1
ENDIF
;ABL:奉仕精神
IF ABL:奉仕精神
	SIF S
		PRINT  + 
	A += ABL:奉仕精神 * 4
	PRINT 奉仕精神
	PRINTV 'LV,ABL:奉仕精神,'(,ABL:奉仕精神 * 4,')
	S = 1
ENDIF
;ABL:精液中毒
IF ABL:精液中毒
	SIF S
		PRINT  + 
	A += ABL:精液中毒 * 1
	PRINT 精液中毒
	PRINTV 'LV,ABL:精液中毒,'(,ABL:精液中毒 * 1,')
	S = 1
ENDIF

IF MARK:快楽刻印
	SIF S
		PRINT  + 
	A += MARK:快楽刻印 * 1
	PRINT 快楽刻印
	PRINTV 'LV,MARK:快楽刻印,'(,MARK:快楽刻印 * 1,')
	S = 1
ENDIF
;PALAM:欲情
IF GETPALAMLV(PALAM:欲情,5)
	SIF S
		PRINT  + 
	A += GETPALAMLV(PALAM:欲情,5) * 1
	PRINT 欲情
	PRINTV 'LV,GETPALAMLV(PALAM:欲情,5),'(,GETPALAMLV(PALAM:欲情,5) * 1,')
	S = 1
ENDIF
;害羞
IF TALENT:羞恥心 > 0
	PRINT  - 
	A -= 3
	PRINT 害羞
	PRINTV '(,1,')
	S = 1
ENDIF
;汚臭鈍感
IF TALENT:汚臭耐性 > 0
	SIF S
		PRINT  + 
	A += 1
	PRINT 汚臭鈍感
	PRINTV '(,1,')
	S = 1
ENDIF
;汚臭敏感
IF TALENT:汚臭耐性 < 0
	PRINT  - 
	A -= 1
	PRINT 汚臭敏感
	PRINTV '(,1,')
	S = 1
ENDIF
;献身的
IF TALENT:献身的
	SIF S
		PRINT  + 
	A += 6
	PRINT 献身的
	PRINTV '(,6,')
	S = 1
ENDIF
;否定快感（淫乱、娼婦で無効）
IF TALENT:快感応答 < 0 && !TALENT:淫乱
	PRINT  - 
	A -= 3
	PRINT 否定快感
	PRINTV '(,1,')
	S = 1
ENDIF
;討厭男人（恋慕、親愛で無効）
IF OTOKOGIRAI(TARGET)
	PRINT  - 
	A -= 8
	PRINT 討厭男人
	PRINTV '(,5,')
	S = 5
ENDIF
;恋慕
IF TALENT:恋慕 && ASSIPLAY == 0
	SIF S
		PRINT  + 
	A += 5
	PRINT 恋慕
	PRINTV '(,5,')
	S = 5
ENDIF
;親愛
IF TALENT:恋慕 > 1 && ASSIPLAY == 0
	SIF S
		PRINT  + 
	A += 5
	PRINT 親愛
	PRINTV '(,5,')
	S = 5
ENDIF

;倒錯的
IF TALENT:倒錯的
	SIF S
		PRINT  + 
	A += 4
	PRINT 倒錯的
	PRINTV '(,4,')
	S = 4
ENDIF
;施虐狂
IF TALENT:施虐狂
	SIF S
		PRINT  + 
	A += 4
	PRINT 施虐狂
	PRINTV '(,4,')
	S = 4
ENDIF
;小悪魔
IF TALENT:小悪魔
	SIF S
		PRINT  + 
	A += 4
	PRINT 小悪魔
	PRINTV '(,4,')
	S = 4
ENDIF
;調教者が扶她
IF (TALENT:PLAYER:2 & 3) == 3
	SIF S
		PRINT  + 
	A += 8
	PRINT 扶她
	PRINTV '(,8,')
	S = 1
ENDIF

Y = 0
;愛液的污垢
SIF STAIN:PLAYER:2 & 1
	Y += 1
;精液的污垢
SIF STAIN:PLAYER:2 & 4
	Y += 3
;肛門的污垢
SIF STAIN:PLAYER:2 & 8
	Y += 7
SIF STAIN:PLAYER:2 & 16
	Y += 15
;足交では汚れがそれほど影響しない
Y /= 3
Y = Y * (2 - TALENT:汚臭耐性) / 2

;有污垢
IF Y
	PRINT  - 
	A -= Y
	PRINT 有污垢
	;汚臭鈍感
	IF TALENT:汚臭耐性 > 0
		PRINT 、汚臭鈍感
	ELSEIF TALENT:汚臭耐性 < 0
		PRINT 、汚臭敏感
	ENDIF
	PRINTV '(,Y,')
	S = 1
ENDIF


;合計を表示(22以上で実行)
PRINT  = 
PRINTV A

V = 22
SIF A < V
	PRINT  < 
SIF A == V
	PRINT  = 
SIF A > V
	PRINT  > 
PRINT 実行値
PRINTV V

WAIT
SKIPDISP 0
;実行できない
SIF A < V
	RETURN 0
ENDIF
RETURN 1

;-------------------------------------------------
;実行判定
;-------------------------------------------------
@COM_ABLE85
;足交実行判定
SIF !TFLAG:100
	RETURN 0
;一括管理
SIF GLOBAL_COMABLE(85)
	RETURN RESULT
;調教者が男人か半人半妖か扶她じゃないとダメ
SIF !(TALENT:PLAYER:2 & 2)
	RETURN 0
;時間停止中は許可
IF FLAG:70 != 1
	;縄使用中は不可
	SIF TEQUIP:縄
		RETURN 0
ENDIF
;浴室PLAY中は塑料地墊が無いとダメ
IF TEQUIP:31
	SIF ITEM:20 == 0 && NOITEM == 0
		RETURN 0
ENDIF
RETURN 1

