;-------------------------------------------------
;排卵誘発剤
;特殊指令
;-------------------------------------------------
@COM192
IF TCVAR:強制排卵
	PRINTL 已經服用完畢
	RETURN -1
ENDIF
ITEM:排卵誘発剤 -= 1
TCVAR:強制排卵 = 180
;-------------------------------------------------
;COM別FLAGの設定
;-------------------------------------------------
;行為者
TCVAR:116 = PLAYER
;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
IF FLAG:70 == 1

ELSE

;調教者が調合知識を持っていれば消費を抑えられる
IF TALENT:PLAYER:調合知識
	DOWNBASE:体力 += 200
	DOWNBASE:気力 += 200
ELSE
	DOWNBASE:体力 += 100
	DOWNBASE:気力 += 100
ENDIF
	SOURCE:逸脱 = 1000
ENDIF

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;同性愛　レズなら戻り値1ゲイなら戻り値2
IF FLAG:70 == 1
	IF HETEROSEX(TARGET,PLAYER)
		EXP:PLAYER:(50 + HETEROSEX(TARGET,PLAYER)) += 1
	ENDIF
ELSE
	IF HETEROSEX(TARGET,PLAYER)
		EXP:(50 + HETEROSEX(TARGET,PLAYER)) += 1
		EXP:PLAYER:(50 + HETEROSEX(TARGET,PLAYER)) += 1
	ENDIF
ENDIF
RETURN 1

@CAN_COM192(ARG)
SKIPDISP ARG
;-------------------------------------------------
;実行判定処理
;-------------------------------------------------
IF FLAG:70 == 1 || CFLAG:恶作剧

ELSE
A = 0
S = 0

;全部の命令に共通の要素を考慮
;(従順が高いと命令に従いやすいなど)
CALL COM_ORDER

;ABL:欲望
IF ABL:欲望
	SIF S
		PRINT  + 
	A += ABL:欲望 * 1
	PRINT 欲望
	PRINTV 'LV,ABL:欲望,'(,ABL:欲望 * 1,')
	S = 1
ENDIF
;ABL:Ｖ感覚
IF ABL:Ｖ感覚
	SIF S
		PRINT  + 
	A += ABL:Ｖ感覚 * 2
	PRINT Ｖ感覚
	PRINTV 'LV,ABL:Ｖ感覚,'(,ABL:Ｖ感覚 * 2,')
	S = 1
ENDIF
;ABL:精液中毒
IF ABL:精液中毒
	SIF S
		PRINT  + 
	A += ABL:精液中毒 * 3
	PRINT 精液中毒
	PRINTV 'LV,ABL:精液中毒,'(,ABL:精液中毒 * 3,')
	S = 1
ENDIF

IF MARK:快楽刻印
	SIF S
		PRINT  + 
	A += MARK:快楽刻印 * 1
	PRINT 快楽刻印
	PRINTV 'LV,MARK:快楽刻印,'(,MARK:快楽刻印 * 1,')
	S = 1
ENDIF
;PALAM:欲情
IF GETPALAMLV(PALAM:欲情,5)
	SIF S
		PRINT  + 
	A += GETPALAMLV(PALAM:欲情,5) * 1
	PRINT 欲情
	PRINTV 'LV,GETPALAMLV(PALAM:欲情,5),'(,GETPALAMLV(PALAM:欲情,5) * 1,')
	S = 1
ENDIF
;害羞
IF TALENT:羞恥心 > 0
	PRINT  - 
	A -= 1
	PRINT 害羞
	PRINTV '(,1,')
	S = 1
ENDIF
;討厭男人（恋慕、親愛で無効）
IF OTOKOGIRAI(TARGET)
	PRINT  - 
	A -= 12
	PRINT 討厭男人
	PRINTV '(,12,')
	S = 1
ENDIF
;恋慕
IF TALENT:恋慕 && ASSIPLAY == 0
	SIF S
		PRINT  + 
	A += 5
	PRINT 恋慕
	PRINTV '(,5,')
	S = 5
ENDIF
;愛欲
IF TALENT:愛欲 && ASSIPLAY == 0
	SIF S
		PRINT  + 
	A += 5
	PRINT 愛欲
	PRINTV '(,5,')
	S = 5
ENDIF
;親愛
IF TALENT:恋慕 > 1 && ASSIPLAY == 0
	SIF S
		PRINT  + 
	A += 5
	PRINT 親愛
	PRINTV '(,5,')
	S = 5
ENDIF

;允许无套
IF CFLAG:允许无套 == 2
	SIF S
		PRINT  + 
	A += 5
	PRINT 中出し合意
	PRINTV '(,5,')
	S = 1
ELSE
	SIF S
		PRINT  - 
	A -= 200
	PRINT 中出し未合意
	PRINTV '(,200,')
	S = 1
ENDIF

;調教者が扶她
IF (TALENT:PLAYER:2 & 3) == 3
	SIF S
		PRINT  + 
	A += 8
	PRINT 扶她
	PRINTV '(,8,')
	S = 1
ENDIF

;合計を表示(100以上で実行)
PRINT  = 
PRINTV A

V = 100
SIF A < V
	PRINT  < 
SIF A == V
	PRINT  = 
SIF A > V
	PRINT  > 
PRINT 実行値
PRINTV V

WAIT
SKIPDISP 0
;実行できない
SIF A < V
	RETURN 0
ENDIF
RETURN 1

