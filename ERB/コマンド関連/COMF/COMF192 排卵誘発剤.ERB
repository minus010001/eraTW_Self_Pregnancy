@COM_ABLE192
;排卵誘発剤実行判定
SIF !TFLAG:100
	RETURN 0
;一括管理
SIF GLOBAL_COMABLE(192)
	RETURN RESULT
;道具を持っていないとダメ
SIF ITEM:排卵誘発剤 == 0
	RETURN 0
IF DRUG == 0
	;薬毒耐性だとダメ
	SIF TALENT:薬毒耐性
		RETURN 0
	;停止中は不可
	SIF FLAG:70 == 1
		RETURN 0
ENDIF
SIF SHIRAHU(TARGET) && CFLAG:允许无套 != 2
	RETURN 0
RETURN 1

