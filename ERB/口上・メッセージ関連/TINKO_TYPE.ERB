;-------------------------------------------------
;関数名:TINKO
;概　要:二番煎じ上等
;引　数:ARG…角色登録番号
;戻り値:
;備　考:文中関数
;使用例:%TINKO(PLAYER)%
;-------------------------------------------------
@TINKO(ARG)
#FUNCTIONS
VARSET LOCALS
SIF !ARG
	ARG = MASTER
;地の文と辻褄合わせが必要なのでとりあえず汚れ描写は保留
[SKIPSTART]
IF GETBIT(STAIN:ARG:2,64)
	LOCALS = 破瓜血
	LOCAL ++
ENDIF
IF GETBIT(STAIN:ARG:2,8)
	SIF LOCAL
		LOCALS += "和"
	LOCALS += "腸液"
	LOCAL ++
ENDIF
IF GETBIT(STAIN:ARG:2,4)
	SIF LOCAL
		LOCALS += "和"
	LOCALS += "精液"
	LOCAL ++
ENDIF
IF GETBIT(STAIN:ARG:2,1)
	SIF LOCAL
		LOCALS += "和"
	LOCALS += "愛液"
	LOCAL ++
ENDIF
SIF LOCAL
	LOCALS += "充满的"
[SKIPEND]
;確率で形容詞がつく
IF !RAND:4 && ARG == MASTER && TCVAR:ARG:100 != TARGET && TCVAR:ARG:101 != TARGET
	IF BASE:ARG:勃起 > 1000 
		SELECTCASE RAND:4
			CASE 0
				LOCALS += "兇猛翹起的"
			CASE 1
				LOCALS += "不断滴落著前列腺液的"
			CASE 2
				LOCALS += "浮现血管的"
			CASE 3
				LOCALS += "一股浓浓的精臭味的"
		ENDSELECT
	ELSEIF BASE:ARG:勃起 > 500
		SELECTCASE RAND:3
			CASE 0
				LOCALS += "抬起頭的"
			CASE 1
				LOCALS += "硬度漸増的"
			CASE 2
				LOCALS += "半勃起的"
		ENDSELECT
	ELSE
		LOCALS += "还很柔软的"	
	ENDIF
ELSEIF !RAND:3
	SELECTCASE TALENT:ARG:形状
		CASE 1
			;未熟
			SELECTCASE RAND:3
				CASE 0
					LOCALS += "可愛的"
				CASE 1
					LOCALS += "小孩模様的"
				CASE 2
					LOCALS += "还不够成熟的"
			ENDSELECT
		CASE 2
			;粗末
			LOCALS += "寒酸的"
		CASE 3
			;普通
			LOCALS += "健壮的"
		CASE 4
			;光る
			SELECTCASE RAND:3
				CASE 0
					LOCALS += "散发着模糊的光的"
				CASE 1
					LOCALS += "忽明忽暗闪烁着的"
				CASE 2
					LOCALS += "亮的刺眼的"
			ENDSELECT
		CASE 5
			SELECTCASE RAND:3
				CASE 0
					LOCALS += "奇形怪状的"
				CASE 1
					LOCALS += "凶悪的"
				CASE 2
					LOCALS += "布满突起的"
			ENDSELECT
		CASE 6
			;巨根
			LOCALS += "粗大的"
		CASE 10
			;馬並
			LOCALS += "像馬似的"
	ENDSELECT
ELSEIF ARG == MASTER && TARGET
	IF !RAND:3
		IF TALENT:TARGET:妊娠
			LOCALS += "让她怀孕的"
		ELSEIF STRFIND(CSTR:TARGET:処女喪失履歴, "阴茎") > 0
			LOCALS += "让她变成女人的"
			
		ENDIF
	ELSEIF !RAND:2 && 妊娠可否(TARGET) && CFLAG:TARGET:生理周期 == 7 && TEQUIP:TARGET:50 == MASTER
		LOCALS += "能確實讓人懷孕的"
	ENDIF
ENDIF
SELECTCASE RAND:12
	CASE 0
		LOCALS += "雄性象征"
	CASE 1
		LOCALS += "男根"
	CASE 2
		LOCALS += "怒張"
	CASE 3
		LOCALS += "陰茎"
	CASE 4
		LOCALS += "肉棒"
	CASE 5
		LOCALS += "剛直"
	CASEELSE
		LOCALS += "陰莖"
ENDSELECT
SIF !TALENT:ARG:2 & 2
	LOCALS = "穿戴式假陰莖"
RETURNF LOCALS


@TINKO_DECIDE(ARG)
SIF !HAS_PENIS(ARG)
	RETURN
$LOOP
PRINTFORML 请设定%CALLNAME:ARG%的男性器形状。
PRINTL [1] 未成熟
PRINTL [2] 短小的
PRINTL [3] 普通
PRINTL [4] 発光的
PRINTL [5] 长疣的
PRINTL [6] 巨根
;PRINTL [7] 小不点
;PRINTL [8] ドリル
;PRINTL [9] 派プカット済み
INPUT
IF RESULT == 5
	PRINTW 那様的东西就算插进去也只会痛苦不会感到舒服哦？
	PRINTW 这不是单纯的自我满足吗？不去考虑对方的感受吗？
	PRINTL [0] 那算了
	PRINTL [1] 多管闲事
	INPUT
	IF RESULT == 1
		PRINTW 你个鬼畜！
		PRINTFORMW %CALLNAME:ARG%的男性器上长出了疣
		TALENT:ARG:形状 = 5
	ELSE
		GOTO LOOP
	ENDIF
ELSEIF RESULT == 4
	PRINTL 这有什么意义吗
	PRINTL [0] 对不起点错了
	PRINTL [1] 和萤鱿发光吸引猎物一个原理
	INPUT
	IF RESULT == 1
		PRINTW 是这様吗。
		PRINTFORMW %CALLNAME:ARG%的男性器变得能発光了
		TALENT:ARG:形状 = 4
	ELSE
		GOTO LOOP
	ENDIF
ELSEIF RESULT == 6
	PRINTL 认为一味的大就行了、这种想法欠考虑吧？太大会导致血液难以到达而变软的。。
	PRINTL 而且最重要的不是尺寸的大小而是与对象的小穴的相性。尺寸不合适的话彼此都会感到痛苦哦。
	PRINTL [0] 我想插萝莉的XXX、还是算了
	PRINTL [1] 我即使自己痛苦也想要看到在痛苦中扭曲的少女的表情
	INPUT
	IF RESULT == 1
		PRINTW 搞什么啊。
		PRINTFORMW %CALLNAME:ARG%的男性器变大了
		TALENT:ARG:形状 = 6
	ELSE
		GOTO LOOP
	ENDIF
ELSEIF RESULT == 3
	PRINTFORMW %CALLNAME:ARG%的男性器是非常普通的形状
	TALENT:ARG:形状 = RESULT
ELSEIF RESULT == 2
	PRINTFORMW %CALLNAME:ARG%的男性器看起来很可怜的様子
	TALENT:ARG:形状 = RESULT
ELSEIF RESULT == 1
	PRINTFORMW %CALLNAME:ARG%的男性器是很可爱的尺寸
	TALENT:ARG:形状 = RESULT
ENDIF
@TINKO_VSOURCE(ARG)

SELECTCASE TALENT:(TEQUIP:ARG:50):形状
	CASE 5
	;长疣的
		CUP:ARG:快Ｖ /= 4
	CASE 2
	;粗末
		CUP:ARG:快Ｖ /= 2
	CASE 10
	;馬並
		CUP:ARG:快Ｖ *= 2
ENDSELECT
SIF 挿入不可(ARG) == -1 && ABL:ARG:膣 < 2
	CUP:ARG:快Ｖ = CUP:ARG:快Ｖ * 4 / 5
SIF 挿入不可(ARG) == -2 && ABL:ARG:膣 < 3
	CUP:ARG:快Ｖ /= 2
@TINKO_ASOURCE(ARG)

SELECTCASE TALENT:(TEQUIP:ARG:51):形状
	CASE 5
	;长疣的
		CUP:ARG:快Ａ /= 4
	CASE 2
	;粗末
		CUP:ARG:快Ａ /= 2
	CASE 10
	;馬並
		CUP:ARG:快Ａ *= 2
SIF 挿入不可(ARG) == -1 && ABL:ARG:肛門 < 2
	CUP:ARG:快Ａ = CUP:ARG:快Ｖ * 4 / 5
SIF 挿入不可(ARG) == -2 && ABL:ARG:肛門 < 3
	CUP:ARG:快Ａ /= 2
ENDSELECT

@TINKO_SIMPLE(ARG)
#FUNCTIONS
VARSET LOCALS
SELECTCASE TALENT:ARG:形状
	CASE 1
		LOCALS = 幼小的
	CASE 2
		LOCALS = 寒酸的
	CASE 3
		LOCALS = 健壮的
	CASE 4
		LOCALS = 發光的
	CASE 5
		LOCALS = 兇猛的
	CASE 6
		LOCALS = 粗壯的
	CASE 10
		LOCALS = 和馬一樣的
ENDSELECT
LOCALS += "陰莖"
RETURNF LOCALS