;-------------------------------------------------
;350,推倒
;-------------------------------------------------
@MESSAGE_COM350
IF FLAG:70 == 1
	IF GET_TARGETNUM() > 1
		PRINTFORML %CALLNAME:PLAYER%猥褻起時停中的%CALLNAME:TARGET%们…
	ELSE
		PRINTFORML %CALLNAME:PLAYER%猥褻起時停中的%CALLNAME:TARGET%…
	ENDIF
;ELSEIF TFLAG:194 == 2
;	PRINTFORML %CALLNAME:TARGET%に拒絶された
;	PRINTFORML 流石に野外では抵抗があるようだ
ELSEIF TFLAG:194  == 100
	;口上内特殊判定の場合地の文非表示
ELSEIF TFLAG:194 == 1
	PRINTL 这里有人……
;ELSEIF TFLAG:194 == 3
;	PRINTFORML 雖然想和%CALLNAME:TARGET%們亂交、但是被拒絕了……
ELSEIF TFLAG:194 == 4
	PRINTFORML 想要抱%CALLNAME:TARGET%、但是被沒有那種心情的%CALLNAME:TARGET%拒絕了
;ELSEIF TFLAG:194 == 5
;	PRINTL 有清醒的目撃者在所以无法实施酔姦
ELSEIF TCVAR:TARGET:烂醉 == 1
	PRINTFORML %CALLNAME:PLAYER%猥褻起醉倒的%CALLNAME:TARGET%…
	PRINTFORML 烂醉如泥的%CALLNAME:TARGET%、无法做出正常判断的様子…
ELSEIF GET_TARGETNUM() > 1
	PRINTFORML %CALLNAME:PLAYER%在确认附近没人之后抱住了%CALLNAME:TARGET%们…
ELSEIF (CFLAG:TARGET:現在位置 == 情人用宿屋 || CFLAG:TARGET:現在位置 == 情人旅館 || CFLAG:TARGET:現在位置 == OMANEKIBEYA()) && CFLAG:TARGET:诶嘿嘿 != 2
	PRINTFORML 兩人獨処時%CALLNAME:PLAYER%摟住了%CALLNAME:TARGET%的肩膀…
ELSEIF CFLAG:現在位置 == 寺子屋
	PRINTFORML 「得给勤奋努力的%CALLNAME:PLAYER%一些奖励呢…」
	PRINTFORML 在只剩兩人的教室中、慧音老师的特別授業開始了…
ELSEIF !TFLAG:194 && TCVAR:TARGET:初次拥抱 == 10
	PRINTFORML 中奖的%CALLNAME:MASTER%強迫%CALLNAME:TARGET%在自己眼前脫掉衣服
	PRINTFORML %CALLNAME:TARGET%帯著憤怒敞開了衣服…
ELSEIF TCVAR:TARGET:初次拥抱 == 99
	;口上内特殊判定の場合地の文非表示
ELSE
	PRINTFORML %CALLNAME:PLAYER%在确认附近没人之后抱住了%CALLNAME:TARGET%…
ENDIF

IF !FLAG:70 && !TFLAG:194
	SELECTCASE TCVAR:TARGET:初次拥抱
		CASE 99
			;口上内特殊判定の場合地の文非表示
		CASE 98
			;口上内特殊判定の場合地の文非表示
		CASE 2
			PRINTFORML %CALLNAME:TARGET%虽然接受了拥抱但似乎有些不满
			PRINTFORML …话说回来还没接吻过呢
		CASE 3
			PRINTFORML %CALLNAME:TARGET%按捺不住身体的躁动、默许了%CALLNAME:MASTER%的行动…
			CALL ASK_DIARY(@"体の疼きを抑えきれなくなった%CALLNAME:TARGET%と、強引に行為に及んだ")
		CASE 4
			PRINTFORML %CALLNAME:TARGET%懵懵懂懂地、接受了%CALLNAME:MASTER%的拥抱…
			SIF CFLAG:体目当て
				CALL COLORMESSAGE(@"因为奇迹之石、我和那个一直关注我的孩子做爱。！（%GET_MAPNAME(MAIN_MAP)%在住　%CALLNAME:MASTER%氏）",C_YELLOW,2,2)
			CALL ASK_DIARY(@"%CALLNAME:MASTER%を拒めなくなってしまった%CALLNAME:TARGET%と、強引に行為に及んだ")
		CASE 5
			PRINTFORML 容易屈服的%CALLNAME:TARGET%、无法对%CALLNAME:MASTER%提出拒绝…
			CALL ASK_DIARY(@"気の小さい%CALLNAME:TARGET%と、強引に行為に及んだ")
		CASE 6
			PRINTFORML 被%CALLNAME:TARGET%拒绝了…
			PRINTFORML 但是%CALLNAME:TARGET%看起來並不是不高興的樣子
		CASE 7
			IF TALENT:TARGET:処女 == 0
				PRINTFORML 虽想抱住%CALLNAME:TARGET%、但被冷淡对待了…
			ELSE
				PRINTFORML 被%CALLNAME:TARGET%拒绝了…
			ENDIF
			SIF CFLAG:体目当て
				CALL COLORMESSAGE(@"明明用了愛欲獨佔的奇跡之石、卻根本不湊效、一點愛欲都不能沾！（%GET_MAPNAME(MAIN_MAP)%在住　%CALLNAME:MASTER%氏）",C_YELLOW,2,2)
		CASE 8
			IF TALENT:TARGET:胆量 < 0 || OTOKOGIRAI(TARGET)
				PRINTFORML %CALLNAME:TARGET%发出了尖叫将%CALLNAME:MASTER%撞飞了
			ELSE
				PRINTFORML %CALLNAME:TARGET%冷淡的拒绝了%CALLNAME:MASTER%
			ENDIF
			SIF CFLAG:体目当て
				CALL COLORMESSAGE(@"明明用了愛欲獨佔的奇跡之石、卻根本不湊效、一點愛欲都不能沾！（%GET_MAPNAME(MAIN_MAP)%在住　%CALLNAME:MASTER%氏）",C_YELLOW,2,2)
		CASE 9
			PRINTFORML 按捺不住身体骚动的%CALLNAME:TARGET%、在禁止挿入的条件下允許了%CALLNAME:MASTER%的行為…
		CASE 1
			IF TALENT:恋慕 && TALENT:恋人
				PRINTFORML %CALLNAME:TARGET%也伸出手臂回抱了%CALLNAME:MASTER%… 
			ELSEIF TALENT:恋慕 && TALENT:胆量 < 0
				PRINTFORML %CALLNAME:TARGET%用似乎做好覚悟又带着些犹豫的眼神看着%CALLNAME:MASTER%…
			ELSEIF TALENT:調合知識
				PRINTFORML %CALLNAME:TARGET%惊慌失措着…
			ELSEIF TALENT:無知
				PRINTFORML %CALLNAME:TARGET%不是很明白的様子…
			ELSEIF TALENT:胆量 < 0
				PRINTFORML %CALLNAME:TARGET%还没做好觉悟的様子…
			ELSE
				PRINTFORML %CALLNAME:TARGET%用做好覚悟的眼神看着%CALLNAME:MASTER%…
			ENDIF
			IF TALENT:処女 == -1 || TALENT:処女 == 1
				PRINTFORM %CALLNAME:TARGET%
				IF TARGET == 57 && !TALENT:恋慕
					PRINTFORM 轮流切换着老翁、猿猴和童子的面具
				ELSE
					PRINTFORM 露出了混杂着不安与期待的表情
				ENDIF
				PRINTFORML …
			ENDIF
			CALL ASK_DIARY(@"%CALLNAME:TARGET%と初めて合意の下で行為に及んだ")
		CASE 20
			PRINTFORM 虽然%CALLNAME:TARGET%很抗拒、但是我命令她给我乖乖地抱着
			IF TALENT:TARGET:胆量 > 0 || TALENT:態度 > 0 || TALENT:自尊心 > 0
				PRINTFORM 屈辱得咬牙切齿
			ELSE
				PRINTFORM 眼里含着泪
			ENDIF
			PRINTFORML 接受了要求…
			CALL ASK_DIARY(@"弾幕勝負で屈服させた%CALLNAME:TARGET%と、強引に行為に及んだ")
		ENDSELECT
ENDIF

IF !FLAG:70 && HAS_PENIS(MASTER) && !TFLAG:194
	IF TALENT:恋人
		CALL COLORMESSAGE(@"只要感觉到%CALLNAME:TARGET%的体温、%CALLNAME:MASTER%就已经鸡儿梆硬了！",C_PINK,1)
		BASE:MASTER:勃起 = MAXBASE:MASTER:勃起
	ELSEIF TCVAR:TARGET:初次拥抱 == 20
		CALL COLORMESSAGE(@"想到现在可以对%CALLNAME:TARGET%为所欲为了、感到血液忍不住的聚集起来了…",C_PINK,1)
		BASE:MASTER:勃起 = MAX(BASE:MASTER:勃起, 1000)
	ELSEIF CFLAG:积攒度 >= 500
		CALL COLORMESSAGE(@"被沉在情慾的%CALLNAME:TARGET%注视着、让%CALLNAME:MASTER%抑制不住的興奮",C_PINK,1)
		BASE:MASTER:勃起 = MAX(BASE:MASTER:勃起, 1000)
	ENDIF
ENDIF