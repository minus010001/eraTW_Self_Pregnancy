;==================================================
@M_KOJO_MESSAGE_SCOM_K90_12
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_SCOM_K90_12_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K90_12_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;恋慕
	ELSEIF TALENT:恋慕
		IF RAND:3 == 0
			PRINTFORMW 「ん…胸部口交気持ちいい？」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「唔%UNICODE(0x2665) *1%下流的味道都沾到胸部上了%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「啊唔%UNICODE(0x2665) *1%请快点让我嚯到浓厚的精液嘛%UNICODE(0x2665) *1%」
		ENDIF
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		IF RAND:3 == 0
			PRINTFORMW 「真是的%UNICODE(0x2665) *1%请快点把欧金金汁射出乃%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「唔啾%UNICODE(0x2665) *1%嗯%UNICODE(0x2665) *1%请快点让我嚯到欧金金汁嘛%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「先走汁…都在乳沟里面堆积起来了啊%UNICODE(0x2665) *1%也请来一些本気汁嘛%UNICODE(0x2665) *1%」
		ENDIF
	;それ以外
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「唔ー有点难呢…%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「啊啊唔%UNICODE(0x2665) *1%将乱跳的欧金金压下来…%UNICODE(0x2665) *1%唔%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「舔%UNICODE(0x2665) *1%嗯%UNICODE(0x2665) *1%雄臭味闻起来好奇怪啊%UNICODE(0x2665) *1%」
		ENDIF
	ENDIF
ENDIF
;未記入
;CALLFORM M_KOJO_MESSAGE_COM_K90_{SELECTCOM}_1
RETURN 1

