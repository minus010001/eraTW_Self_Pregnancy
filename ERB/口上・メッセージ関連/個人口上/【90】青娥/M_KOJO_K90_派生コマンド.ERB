;==================================================
@M_KOJO_MESSAGE_SCOM_K90_1
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_SCOM_K90_1_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K90_1_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;出す直前
	ELSEIF BASE:PLAYER:2 > 8000
		IF RAND:3 == 0
			PRINTFORMW 「唔～～～%UNICODE(0x2665) *1%」
			PRINTFORMW 青娥灵活的舌头巧妙地促进着%CALLNAME:MASTER%的射精感
			PRINTFORMW 「快点，浓烈的射出来吧%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「啾噜…%UNICODE(0x2665) *1%…唔…%UNICODE(0x2665) *1%…出来吧…%UNICODE(0x2665) *1%」
			PRINTFORMW 青娥灵活的舌头巧妙地促进着%CALLNAME:MASTER%的射精感
		ELSE
			PRINTFORMW 「…啾噜…%UNICODE(0x2665) *1%」
			PRINTFORMW 青娥灵活的舌头巧妙地促进着%CALLNAME:MASTER%的射精感
			PRINTFORMW 「…唔…舔…啾…不要忍着啊，将浓厚的精液射出来吧%UNICODE(0x2665) *1%」
		ENDIF
	;恋慕
	ELSEIF TALENT:恋慕
		IF RAND:3 == 0
			PRINTFORMW 「唔…%UNICODE(0x2665) *1%…rero…%UNICODE(0x2665) *1%」
			PRINTFORMW 「…这个…好美味…%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「…啾噜…%UNICODE(0x2665) *1%…唔…%UNICODE(0x2665) *1%」
			PRINTFORMW 「…我慢汁…%UNICODE(0x2665) *1%…光是这样还不够…%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「啾噜…%UNICODE(0x2665) *1%…舔…%UNICODE(0x2665) *1%」
			PRINTFORMW 「请在嘴巴里满满的填满精液…%UNICODE(0x2665) *1%」
		ENDIF
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		IF RAND:3 == 0
			PRINTFORMW 「唔…好好吃%UNICODE(0x2665) *1%」
			PRINTFORMW 青娥气势汹汹地吸吮着阴茎
		ELSEIF RAND:2 == 0
			PRINTFORMW 「唔啾っ…%UNICODE(0x2665) *1%」
			PRINTFORMW 「舔%UNICODE(0x2665) *1%请射出来吧%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「在嘴巴里出来也可以哦%UNICODE(0x2665) *1%」
		ENDIF
	;それ以外
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「…唔…啾噜噜…%UNICODE(0x2665) *1%」
			PRINTFORMW 「…濃厚的雄性味道%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「…舔…欧金金…马上让你变干净%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「…唔…」
			PRINTFORMW 青娥的将肉棒不自然的含在了嘴里
		ENDIF
	ENDIF
ENDIF
;未記入
;CALLFORM M_KOJO_MESSAGE_COM_K90_{SELECTCOM}_1
RETURN 1

