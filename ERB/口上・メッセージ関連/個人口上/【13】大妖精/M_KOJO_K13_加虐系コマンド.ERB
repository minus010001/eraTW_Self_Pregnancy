;100,打屁股
;==================================================
@M_KOJO_MESSAGE_COM_K13_100
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K13_100_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K13_100_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;恋慕
	ELSEIF TALENT:恋慕
		PRINTFORMW 「哎呀…好痛啊、啊啊啊！」
		RETURN 1
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		PRINTFORMW 「啊啊啊啊…感觉变得很奇怪」
		PRINTFORMW 「啊、咿」
		RETURN 1
	;それ以外
	ELSE
		PRINTFORMW 无视了哭喊着道歉的大妖精，继续拍打着柔软的屁股。
		RETURN 1
	ENDIF
ENDIF
RETURN 1

