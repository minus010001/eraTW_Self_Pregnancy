;302,身体接觸
;TFLAG:193(1=大成功 0=成功 -1=失敗)
;==================================================
@M_KOJO_MESSAGE_COM_K52_302
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K52_302_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K52_302_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL

	IF TALENT:恋人
		SELECTCASE RAND:4
			CASE 3
				PRINTFORML 
				PRINTFORMW 「%CALLNAME:MASTER%……嗯,可以随你喜欢的摸哦……」
				PRINTFORMW 鈴仙的身体蹭了过来和%CALLNAME:MASTER%的身体进行着肌肤之亲.
				PRINTFORMW 她那光滑的白色肌肤又冷又柔软。简直像雪一样。
				PRINTFORMW 少女的脸颊染成樱花色,将自己的身体交给了%CALLNAME:MASTER%。
				
			CASE 2
				PRINTFORML 
				PRINTFORMW 「我的头发？那还是很有自信的」
				PRINTFORMW 确实,铃仙的长发非常顺滑,发丝像水流一般淌过手指之间
				PRINTFORMW 你嗅着玲仙的头发，感觉散发着如同桃子一般淡淡甘甜的清香
				PRINTFORMW 「我换了洗发水,感觉怎样呐？」
				PRINTFORMW 就这样享受了一会儿头发香甜的气味
			CASE 1
				PRINTFORML 
				PRINTFORMW %CALLNAME:MASTER%对鈴仙的耳朵吮咬起来。
				PRINTFORMW 「唉？等……」
				PRINTFORMW 「等等,耳朵很敏感的啊……」
				PRINTFORMW 鈴仙的表情恍惚了起来,但在她的耳朵上只能感受到布的味道和触感
			CASE 0
				PRINTFORML 
				PRINTFORMW 「揉……肩…？」
				PRINTFORMW 「唔嗯ー,是没什么问题」
				PRINTFORMW 「不过不要太用力哦？」
				PRINTFORMW 得到了铃仙的同意,%CALLNAME:MASTER%便揉起了她的肩膀
				PRINTFORMW 「嗯,哈啊……」,铃仙不断发出感觉会被别人误会的声音。
		ENDSELECT
	ELSEIF TALENT:恋慕
		SELECTCASE RAND:4
			CASE 3
				PRINTFORML 
				PRINTFORMW 「噫,不要……」
				PRINTFORMW 一碰到铃仙的身体,她就吓得喊了出来。
				PRINTFORMW 担心铃仙的%CALLNAME:MASTER%刚打算放手,便被少女握住了手。
				PRINTFORMW 「啊,不用……放手,已经没关系了」
				PRINTFORMW 鈴仙这样说着,%CALLNAME:MASTER%便继续開始肌肤之亲。
			CASE 2
				PRINTFORML 
				PRINTFORMW 「噫,呜……」
				PRINTFORMW 每当%CALLNAME:MASTER%触碰到铃仙的身体，她都会发出似乎很痛苦的声音。
				PRINTFORMW 「再,摸一会吧……」
				PRINTFORMW 但是，好像并不是很讨厌。
			CASE 1
				PRINTFORML 
				PRINTFORMW %CALLNAME:MASTER%轻轻地咬了铃仙的耳朵…有种布的味道。你将舌头慢慢舔入玲仙的耳朵，玲仙颤抖着底下了头
				PRINTFORMW 「呜！　耳朵……」
				PRINTFORMW 现在拉一下耳朵，是不是能取下来呢。一瞬产生了这样的想法。
				PRINTFORMW 「啊咧。怎么突然停下来了？」
			CASE 0
				PRINTFORML 
				PRINTFORMW 「唔……头发」
				PRINTFORMW %CALLNAME:MASTER%顺着铃仙的长发抚摸起来。散发出一股雨后竹林的清香。
				PRINTFORMW 「那个,用了师匠做的洗发水。感觉……怎样呐？」
				PRINTFORMW 很香呢……这样说了之后她便安心下来了。
		ENDSELECT
	ELSEIF TALENT:思慕
		SELECTCASE RAND:4
			CASE 3
				PRINTFORML 
				PRINTFORMW 「那个,%CALLNAME:MASTER%,手」
				PRINTFORMW %CALLNAME:MASTER%的手和她的手重叠在了一起。
				PRINTFORMW …像雪一样冰冷,光滑的白手
				PRINTFORMW 「月亮上呐,又冷又干燥,还经常晒不到太阳」
				PRINTFORMW 「所以,变成这样很正常呐……%CALLNAME:MASTER%觉得怎样？　很奇怪吗？」
				PRINTFORMW %CALLNAME:MASTER%告诉她很漂亮,鈴仙露出了微笑。
			CASE 2
				PRINTFORML 
				PRINTFORMW 「啊%CALLNAME:MASTER%……」
				PRINTFORMW 「那个,在朋友之间……这样也很平常吧」
				PRINTFORMW 鈴仙承受着%CALLNAME:MASTER%触碰自己身体的手。
				PRINTFORMW 「唔……啊……」
				PRINTFORMW 铃仙并没有抵抗的样子。
			CASE 1
				PRINTFORML 
				PRINTFORMW %CALLNAME:MASTER%摸了鈴仙的那对耳朵。
				PRINTFORMW 「啊,呀！」
				PRINTFORMW 「耳朵,耳朵哈啊……」
				PRINTFORMW 鈴仙的脸上染了一层红晕,但你只有摸着布的感觉。
			CASE 0
				PRINTFORML 
				PRINTFORMW 「唔……怎样？」
				PRINTFORMW %CALLNAME:MASTER%十分喜欢铃仙头发的香味,就像牛奶和什么香甜的味道混在一起……有种柔和的香味。
				PRINTFORMW 「因为的永恒亭的各位…都喜欢的香味…」
				PRINTFORMW 那股甘甜的味道恐怕就是胡萝卜吧。
		ENDSELECT
	ELSE
		SELECTCASE RAND:2
			CASE 1
				PRINTFORML 
				PRINTFORMW 「噫呜,请不要碰奇怪的地方……」
				PRINTFORMW 鈴仙咬着嘴唇微微发抖。
			CASE 0
				PRINTFORML 
				PRINTFORMW 「噫,耳朵,讨厌, 不要,啊！」
				PRINTFORMW %CALLNAME:MASTER%玩弄着铃仙的耳朵。
				PRINTFORMW 但是摸起来和布差不多,这到底是什么呢
				PRINTFORMW 「那个,求求你……住手,真的啦！」
				PRINTFORMW 铃仙不断的喘着粗气…这个耳朵上布满了神经吗。
		ENDSELECT
	ENDIF
ENDIF
RETURN 1
