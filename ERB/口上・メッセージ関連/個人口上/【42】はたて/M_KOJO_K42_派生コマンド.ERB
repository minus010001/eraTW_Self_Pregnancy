CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_SCOM_K42_1_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K42_1_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		IF FLAG:70
			RETURN 1
		ELSE
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;恋慕
	ELSEIF TALENT:恋慕
		PRINTFORMW 
		RETURN 1
	ELSEIF TALENT:愛欲 || TALENT:炮友
		;強制舐陰←→舐陰は六九式対象外らしい
		;強制舐陰←→给对方口交も×
		;给对方口交←→口交はＯＫ
		;極扶她
		IF HAS_PENIS(TARGET) == 1
			;你もちんこついてる（男または扶她）
			IF HAS_PENIS(MASTER) == 1
				IF PREVCOM == 1;舐陰
					;極口交中
					IF TEQUIP:TARGET:口接触部位 == 1
						PRINTFORML 「Hi呀唔嗯！？　啊、Kora、现在舔的话、不行的啦…」
						PRINTFORML 从阴茎的根部到会阴、用舌头一口气描绘玩弄着、极颤栗着屁股止不住地颤抖
						PRINTW 
					;極舐陰中
					ELSE
						PRINTFORML 被眼前不断微微开合的牝穴诱惑着、轻轻地吻了一下
						PRINTFORML 「呼啊嗯%UNICODE(0x2665) *1%　等下、现在是我的回合呀…%UNICODE(0x2665) *1%」
						PRINTW 
					ENDIF
				ELSEIF PREVCOM == 2;给对方口交
					;極口交中
					IF TEQUIP:TARGET:口接触部位 == 1
						SELECTCASE RAND:2
						CASE 0
							PRINTFORML 「咿呀呜！？」
							PRINTFORML 含住在眼前晃来晃去的极的阴茎后、它在口腔内颤抖个不停
							PRINTW 
						CASE 1
							PRINTFORM 被极的口交技术玩弄着的同时、眼前
							SELECTCASE TALENT:TARGET:形状
							CASE 6,10;巨根、馬並み
								PRINTFORM 的是和少女娇小的体型毫不相称、几乎有人类手腕那么粗的
							CASE 1,2;粗チン
								PRINTFORM 实在是十分可爱的
							CASE 5;长疣的
								PRINTFORM 和主人完全相反拥有凶恶形状的
							CASEELSE
								PRINTFORM 看起来和她不太相称的健壮的
							ENDSELECT
							PRINTFORML 阴茎进入了视野
							PRINTW 
							PRINTFORML 啪库…
							PRINTFORML 「唉！？　等下、你在做什、恩恩！！%UNICODE(0x2665) *1%」
							PRINTW 
							PRINTFORML 「看、看谁先会高潮ー来一决胜负ー吗…正好啊」
							PRINTFORML 「就算是我、也不会输的啊…%UNICODE(0x2665) *1%　哈姆、噗啵、噗啵…%UNICODE(0x2665) *1%」
							PRINTW 
						ENDSELECT
					;極舐陰中
					ELSE
						PRINTFORM 爱抚着极娇嫩牝穴的同时、眼前
						SELECTCASE TALENT:TARGET:形状
						CASE 6,10;巨根、馬並み
							PRINTFORM 的是和少女娇小的体型毫不相称、几乎有人类手腕那么粗的
						CASE 1,2;粗チン
							PRINTFORM 实在是十分可爱的
						CASE 5;长疣的
							PRINTFORM 和主人完全相反拥有凶恶形状的
						CASEELSE
							PRINTFORM 看起来和她不太相称的健壮的
						ENDSELECT
						PRINTFORML 阴茎进入了视野
						PRINTW 
						PRINTFORML 呸咯…
						PRINTFORML 「Hi呀！？　等、等下、前端、很弱的啦、库噫%UNICODE(0x2665) *1%」
						PRINTFORML 「讨厌、好舒服%UNICODE(0x2665) *1%　不要那样、呸咯呸咯地舔着呀…%UNICODE(0x2665) *1%」
						PRINTW 
					ENDIF
				ELSEIF PREVCOM == 81;口交
					IF TEQUIP:MASTER:口接触部位 == 1
						PRINTFORML 「我也要、开始舔你的了呢…%UNICODE(0x2665) *1%　哈姆%UNICODE(0x2665) *1%」
					ELSEIF TEQUIP:MASTER:口接触部位 == 6
						PRINTFORML 「这样的太刺激啦、不要只舔着那里呀…%UNICODE(0x2665) *1%」
						PRINTFORML 「我也、要这么做了呢…%UNICODE(0x2665) *1%　哈姆%UNICODE(0x2665) *1%」
					ENDIF
					PRINTFORML 「嗯、嗯、嗯…%UNICODE(0x2665) *1%　又坚硬、又美味…%UNICODE(0x2665) *1%」
					PRINTW 
				ELSE
				ENDIF
			;你は女。対応指令がないよ！
			ELSE
			ENDIF
		;極女
		ELSE
			IF PREVCOM == 1;舐陰
				SELECTCASE RAND:5
				CASE 0
					PRINTFORML 「啊、Kora、现在是我要让你舒服起来的…啊%UNICODE(0x2665) *1%　嗯%UNICODE(0x2665) *1%」
					PRINTFORML 「嗯啾%UNICODE(0x2665) *1%　哈啊%UNICODE(0x2665) *1%　啊啊%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%」
					PRINTW 
				CASE 1
					PRINTFORML Nu啾、啾啪、啾噜噜…
					PRINTFORML 「嗯%UNICODE(0x2665) *1%　呼%UNICODE(0x2665) *1%　呼%UNICODE(0x2665) *1%　呼～～～～嗯%UNICODE(0x2665) *1%」
					PRINTW 
				CASE 2
					PRINTFORML 「啊、这样子做的话、就会太舒服没法做…%UNICODE(0x2665) *1%」
					PRINTW 
				CASE 3
					PRINTFORML 「啊、那种地方、不要扩开来呀！」
					PRINTFORML （讨厌…那里全部都被%MASTERNAME:42%看光了…%UNICODE(0x2665) *1%）
					PRINTW 
					PRINTFORML 「嘴没动了什么的、这都怪你ー说了那种话啊…恩恩%UNICODE(0x2665) *1%　嗯嗯%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%」
					PRINTFORML 「已经湿漉漉了什么的、那种事情ー不说也行的啊…笨蛋%MASTERNAME:42%…%UNICODE(0x2665) *1%」
					PRINTW 
				CASE 4
					PRINTFORML 眼前、极那丰满的屁股在不断摇晃着…
					PRINTFORML 在那中央、极下流的小穴、飘散着雌臭的同时垂涎欲滴地不断开闭着
					PRINTW 
					PRINTFORML 「恩恩！？　啊、笨蛋、不要用、手指掰开啊、膣内、不可以看」
					PRINTFORML 双手掰开阴唇后、伴随着Nichi的粘稠声音、阴道口完全暴露出来了
					PRINTFORML 极小嘴的奉仕由此中断、并且慌乱地抗议着
					PRINTW 
					PRINTFORML 就算说了快停下也不可能说好的我明白了这种话。你无视了她的抗议、将舌尖插进了膣口
					PRINTFORML 「Hi呀%UNICODE(0x2665) *1%　舌头、插进、来啦～～～～%UNICODE(0x2665) *1%」
					PRINTW 
					PRINTFORML 「不可以舔膣内呀～～%UNICODE(0x2665) *1%　欧金金、没办法舔了、啊%UNICODE(0x2665) *1%　呀%UNICODE(0x2665) *1%」
					PRINTFORML 「你、你这…哈姆呜%UNICODE(0x2665) *1%　呜噗、噢卟%UNICODE(0x2665) *1%　啾啵、啾啵、啾啵%UNICODE(0x2665) *1%」
					PRINTFORML 是燃起了对抗意识吗、极用舌头比刚才更热情地奉仕、再次开始了口淫
					PRINTW 
					PRINTFORML 这样子的话、不早点让她去的话、这边就要先射出来了…
					PRINTFORML （快点射啊、快射啊%UNICODE(0x2665) *1%　这边已经、要去了啊%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%）
					PRINTW 
				ENDSELECT
			ELSEIF PREVCOM == 81;口交
				SELECTCASE RAND:4
				CASE 0
					PRINTFORML 「我才不会任你摆布、看我反击…%UNICODE(0x2665) *1%　嗯噗…%UNICODE(0x2665) *1%」
					PRINTFORML 「哈、嗯%UNICODE(0x2665) *1%　啾噗、啾噗…%UNICODE(0x2665) *1%　
				CASE 1
					PRINTFORML Nu啾、啾啪、啾噜噜…
					PRINTFORML 「嗯%UNICODE(0x2665) *1%　呼%UNICODE(0x2665) *1%　呼%UNICODE(0x2665) *1%　呼～～～～%UNICODE(0x2665) *1%」
					PRINTW 
				CASE 2
					PRINTFORML 「哈、哈…%UNICODE(0x2665) *1%　嗯…你的、看起来ー很难受嘛…哈姆%UNICODE(0x2665) *1%」
					PRINTFORML 
					PRINTW 
				CASE 3
					PRINTFORML 「哈啊%UNICODE(0x2665) *1%　啊啊%UNICODE(0x2665) *1%」
					PRINTFORML 「太狡猾了、尽是%MASTERNAME:42%在做…哈姆%UNICODE(0x2665) *1%」
					PRINTFORML 「嗯%UNICODE(0x2665) *1%　呼呜%UNICODE(0x2665) *1%　嗯嗯嗯%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%」
					PRINTW 
				ENDSELECT
			ENDIF
		ENDIF
		RETURN 1
	;それ以外
	ELSE
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF
