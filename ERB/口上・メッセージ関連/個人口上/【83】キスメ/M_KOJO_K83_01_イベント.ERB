	;疲惫
		PRINTFORML 「哈、哈啊……累、累了……」
		PRINTFORMW 「抱、抱歉哦……%CALLNAME:MASTER%……」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
	;満足して解放
		PRINTFORMW 「哈、哈啊……舒、舒服了……」
		PRINTFORMW 「%CALLNAME:MASTER%……非、非常感谢……」
		TCVAR:[[琪斯美]]:351 = 0
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
	;満足したところを被逆推た
		PRINTFORMW 「咿啊！！」
		IF TALENT:[[琪斯美]]:恋人
			SELECTCASE RAND:2
			CASE 1
				CALL HPH_PRINT(@"「嗯……请惩罚我吧……HPH」", "W")
			CASEELSE
				CALL HPH_PRINT(@"「嗯……角色逆转了呢……HPH」", "W")
			ENDSELECT
		ELSE
			SELECTCASE RAND:2
			CASE 1
				PRINTFORMW 「呜……十、十分抱歉……！」
			CASEELSE
				PRINTFORMW 「呜……请、请原谅我……！」
			ENDSELECT
		ENDIF
		TCVAR:[[琪斯美]]:351 = 0
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
	;壊れるまでしてあげる
		IF TALENT:[[琪斯美]]:恋人
			SELECTCASE RAND:2
			CASE 1
				CALL HPH_PRINT(@"「%CALLNAME:MASTER%……很喜欢这样呢……HPH」", "W")
			CASEELSE
				CALL HPH_PRINT(@"「那么，来一起舒服个够吧……HPH」", "W")
			ENDSELECT
		ELSE
			SELECTCASE RAND:2
			CASE 1
				CALL HPH_PRINT(@"「现在后悔的话太迟了哦……HPH」", "W")
			CASEELSE
				CALL HPH_PRINT(@"「呵呵……那么我就不客气了……HPH」", "W")
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1

