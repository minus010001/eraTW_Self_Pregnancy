	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORML 「…………」
		PRINTFORML 往地藏菩萨模样的成美嘴里，插入了肉棒…… 
		PRINTFORMW 成美困惑着，似乎还打算保持地藏菩萨的架势……。
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;それ以外
	ELSE
		PRINTFORML 「…………」
		PRINTFORMW 虽然成美作为地藏纹丝不动，舌头却在嘴里淫乱地蠕动……。
		IF NOWEX:PLAYER:射精
			CALL HPH_PRINT,@"「呜呣……HPH………………咕库…………」","L"
			PRINTFORMW 成美的喉咙默默地发出声音……。
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1

