;==================================================
;80,手淫
;==================================================
@M_KOJO_MESSAGE_COM_K121_80
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K121_80_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K121_80_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORML 「……………」
		PRINTFORML 成美双手合十，让肉棒插入……
		PRINTFORMW 成美装作没看见地看着……。
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	ELSE
		;この後射精する(射精寸前の描写)
		IF NOWEX:PLAYER:射精
			CALL HPH_PRINT,@"「…………可以射出来哦、%CALLNAME:MASTER%……HPH」","L"
			PRINTFORMW 成美小声地嘟哝着，两手加强了力量……。
		ELSE
			PRINTFORML 「………………♪」
			PRINTFORMW 每当成美咻哩咻哩摩擦的时候，射精欲就会高涨……
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1

