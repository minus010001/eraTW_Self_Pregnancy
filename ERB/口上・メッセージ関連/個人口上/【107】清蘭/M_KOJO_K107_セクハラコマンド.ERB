;==================================================
@M_KOJO_MESSAGE_COM_K107_330
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K107_330_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K107_330_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF FLAG:70
		SELECTCASE RAND:2
			CASE 0
				PRINTFORMDL 绕到清蘭身后,掀起裙子。 

				PRINTFORMDW 一种仿佛回到了童年,怀念的感觉... 

			CASE 1
				;ダンスは…苦手だな
				PRINTFORMDL "什~么呀什~么呀!今天的内裤是这个!" 

				PRINTFORMDW 一人虚伪地跳着谜之舞蹈,同时掀起清蘭的裙子...

		ENDSELECT
	ELSEIF CFLAG:诶嘿嘿 == 2
		CALL HPH_PRINT,@"「呵呵...HPH小鸡鸡...全部进去了...HPH」","L"
		PRINTFORMDW 裙子下面是美妙的景象...!

	ELSE
		SELECTCASE TFLAG:193
			;ビンタ
			CASE -5
				PRINTFORML 「不行啊啊啊!!」

				PRINTFORMDW 在想着发出干燥的响声的下一秒,%CALLNAME:MASTER%的脸颊传来锐利的痛感... 

			;つねられる
			CASE -3
				IF TALENT:恋人 || TALENT:恋慕
					PRINTFORMW 「%CALLNAME:MASTER%!!再稍微考虑考虑地点之类的!」

				ELSE
					PRINTFORMW 「要做坏事的手我"净化"给你好不好」

				ENDIF
			;嫌がる
			CASE -1
				IF TALENT:恋人 || TALENT:恋慕
					PRINTFORMW 「等...稍微!不要...!」

				ELSE
					PRINTFORML 「............。」  

					PRINTFORMDW 一脸"被扫兴"的表情看着%CALLNAME:MASTER%...

				ENDIF
			;嫌がる（嬉しげ）
			CASE 0
				IF TALENT:恋人 || TALENT:恋慕
					PRINTFORMW 「这、这里稍微......羞耻所以......」

				ELSE
					PRINTFORMW 「——'掀起裙子'!当然不可以!」

				ENDIF
			;成功
			CASE 1
				IF ABL:露出癖 <= 2
					PRINTFORML 「~~~!!」

					PRINTFORMDW 不知道是愤怒还是羞耻,清蘭的脸红透了... 

					;穿いてる
					IF TEQUIP:0 > 1
						PRINTFORML 「到、到底看多久!够了吧!」

						PRINTFORMDW 虽然时间很短,但享受清蘭的内裤视奸... 

					;穿いてない
					ELSE
						PRINTFORMDL 裙子下本应在那的内裤却不见了...

						PRINTFORMW 「为什么不穿......这样的我也想知道啊~...!」

					ENDIF
				ELSEIF ABL:露出癖 <= 4
					PRINTFORML 「等...不可以......!」

					PRINTFORMDW 虽然脸红,但似乎并不生气...

					;穿いてる
					IF TEQUIP:0 > 1
						PRINTFORML 「......说起来...!够了吧...」

						PRINTFORML 「再稍微...!」

						PRINTFORMDW 清蘭可爱的内裤深深烙在眼中。 

					;穿いてない
					ELSE
						PRINTFORMDL 裙子下本应在那的内裤却不见了... 

						PRINTFORML 「......什么!坏!我又不喜欢不穿内裤――唔!」 

						PRINTFORMDW 在被别人发现前,慌忙堵住清蘭的嘴......

					ENDIF
				ELSE
					CALL HPH_PRINT,@"「......色HPH」 ","L"
					PRINTFORMDW 清蘭似乎有点高兴... 

					;穿いてる
					IF TEQUIP:0 > 1
						CALL HPH_PRINT,@"(%CALLNAME:MASTER%那么认真地看我的内裤...HPH)","L"
						PRINTFORMDW 尽情浏览清蘭可爱的内裤。

					;穿いてない
					ELSE
						PRINTFORMDL 裙子下本应在那的内裤却不见了... 

						CALL HPH_PRINT,@"「呃~不好吗反正做那个的时候也会脱掉......HPH」","W"
					ENDIF
				ENDIF
		ENDSELECT
	ENDIF
ENDIF
RETURN 1

