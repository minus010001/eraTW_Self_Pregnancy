			CASE 0
				CALL HPH_PRINT,@"「这样...可以吗HPH」 ","L"
				PRINTFORMDL 清蘭屁股高高提起,双手撑开菊门。 

				PRINTFORMDL 扩张得如此之大的屁眼,不禁目不转睛...... 

				CALL HPH_PRINT,@"「够了!羞耻死了,快进来啊...HPH」 ","L"
				PRINTFORMDW %CALLNAME:MASTER%插入屁眼,清蘭呼出一口气,身体震颤... 

			CASE 1
				CALL HPH_PRINT,@"「......说起来...虽然提前清理过屁股...可、可以吗...HPH」  ","L"
				PRINTFORMDL %CALLNAME:MASTER%点头,清蘭有点害羞地转过屁股趴下。

				CALL HPH_PRINT,@"「恩咕...HPH是...哈HPH小鸡鸡...HPH屁股...撑开了...HPH」","L"
		ENDSELECT
	ENDIF
RESETCOLOR
ENDIF
RETURN 1

