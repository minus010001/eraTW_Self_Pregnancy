			SELECTCASE RAND:4
			CASE 1
				IF K2_SEXUAL_FRANKNESS()
					PRINTFORMDL 大幅度地挥动着腰，拔出来的时候肛门的粘膜\@ TALENT:MASTER:性別 & 2 ?%K2_EXPRESSION_VARIATION("PENIS")%#张開的样子\@吸附着，露出了粉红色的肉。
					CALL K2_HPH_PRINT,@"「哈HPH 啊、呜、呜HPH 屁股啪嗒啪嗒的HPH 呼啊HPH」","W"
				ELSE
					GOTO PICK_SIGMOID_COLON_LOOP
				ENDIF
			CASE 2
				IF K2_SEXUAL_FRANKNESS() && ABL:Ａ感覚 >= 5
					CALL K2_HPH_PRINT,@"「啊、嗯哈HPH 嗯，哈，哈HPH 变成了屁股有感觉的变态HPH」","W"
				ELSE
					GOTO PICK_SIGMOID_COLON_LOOP
				ENDIF
			CASE 3
				CALL K2_HPH_PRINT,@"「唔、哈、嗯HPH 在屁股小穴里、很深的地方HPH」","W"
			CASEELSE
				SIF TALENT:MASTER:性別 & 2
					PRINTFORMDL %CALLNAME:MASTER%插入肠子的深处，括约肌有节奏的收缩，把%K2_EXPRESSION_VARIATION("PENIS")%的根部紧紧地缠住了。
