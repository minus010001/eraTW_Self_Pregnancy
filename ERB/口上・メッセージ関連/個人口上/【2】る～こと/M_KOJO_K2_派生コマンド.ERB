;==================================================
;5,手淫口交
;==================================================
@M_KOJO_MESSAGE_SCOM_K2_5
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_SCOM_K2_5_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K2_5_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	$HAND_BLOWJOB_LOOP
	IF K2_BASIC_CONDITION_IN_MATING()
		IF TALENT:恋慕 || TALENT:愛欲
			SELECTCASE RAND:6
			CASE 1
				IF ABL:技巧 >= 4
					CALL K2_HPH_PRINT,@"「啾噜、呜呼HPH 偶尔碰一下HPH」","L"
					CALL K2_HPH_PRINT,@"「感觉有点可爱HPH」","W"
				ELSE
					GOTO HAND_BLOWJOB_LOOP
				ENDIF
			CASE 2
				IF ABL:舌 >= 3 && ABL:指 >= 2
					PRINTFORMDL %CALLNAME:TARGET%把龟头贴在脸颊内侧，像栗子鼠一样鼓起脸颊。
					CALL K2_HPH_PRINT,@"「嗯……呼……嗯……唔HPH …嗯…呣啊……嗯……咕嘟……呸咯HPH」","L"
					PRINTFORMDW 从%CALLNAME:TARGET%的嘴里溢出的大量口水，垂下来，在落下的地方留下了痕迹。
				ELSE
					GOTO HAND_BLOWJOB_LOOP
				ENDIF
			CASE 3
				IF !TEQUIP:MASTER:避孕套
					CALL K2_HPH_PRINT,@"「才一開始就有液体流出来了HPH 啾噜噜HPH」","W"
				ELSE
					GOTO HAND_BLOWJOB_LOOP
				ENDIF
			CASE 4
				IF !TEQUIP:MASTER:避孕套
					CALL K2_HPH_PRINT,@"「呜呣……啾………嗯呼呼HPH 很好吃HPH」","W"
				ELSE
					GOTO HAND_BLOWJOB_LOOP
				ENDIF
			CASE 5
				PRINTFORMDL %CALLNAME:TARGET%在梦中与%CALLNAME:MASTER%嬉戏着。
				CALL K2_HPH_PRINT,@"「啾噜……啵……嗯……啾噜噜HPH 啾啵…啾啵HPH」","W"
			CASEELSE
				CALL K2_HPH_PRINT,@"「嗯唔、啾啵、嗯、小鸡鸡的根部，舒服吗？HPH」","W"
			ENDSELECT
			IF NOWEX:MASTER:射精 && ( TCVAR:精飲経験 > 0 || TEQUIP:MASTER:避孕套 )
				PRINTL
				PRINTFORMDL %CALLNAME:TARGET%从收缩的阴囊附近察觉到射精，激烈地动手引导着。
				CALL K2_HPH_PRINT,@"「啊哈HPH 我的手和%CALLNAME:MASTER%大人的小鸡巴，紧紧地咬着，发出了很H的声音HPH」","L"
				CALL K2_HPH_PRINT,@"「欸HPH 欸HPH \@ !TEQUIP:MASTER:避孕套 && RAND:2 ?在我的舌头上射出来吧 #请把漂亮的白汁射出来吧 \@HPH」","W"
			ENDIF
		ELSE
			SELECTCASE RAND:2
			CASE 1
				IF ABL:技巧 >= 4
					PRINTFORMW 「恩，嗯，阴囊也一起吗？知道了。哈哈」
				ELSE
					PRINTFORMW 「哼哼，哈哈……手和嘴同时动很难」
				ENDIF
			CASEELSE
				PRINTFORMW 「啊呣……啾…啵……嗯…哈」
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
;未記入
;CALLFORM M_KOJO_MESSAGE_COM_K2_{SELECTCOM}_1
RETURN 1

