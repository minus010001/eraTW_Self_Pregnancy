			SELECTCASE RAND:4
			CASE 1
				;真正風騷、Ｖ拡張経験あり、主人に阴茎あり、随机６分の１
				IF CFLAG:1001 && EXP:Ｖ拡張経験 > 0 && TALENT:MASTER:性別 & 2 && !RAND:6
					PRINTFORML 「那个，我的女阴是不是很松？」
					PRINTFORMDL 不安地询问的%CALLNAME:TARGET%被说没有那样的事的话，開心地夹紧了阴道。
					CALL K2_HPH_PRINT,@"「我会努力做的、请不要讨厌二手小穴HPH」","W"
				;真正風騷、主人の腰３以上、今回あたえる快Ｖが４０００以上
				ELSEIF CFLAG:1001 && ABL:MASTER:腰 >= 3 && CUP:快Ｖ >= 4000
					CALL K2_HPH_PRINT,@"「%CALLNAME:MASTER%大人的腰技、很棒HPH 非常熟练HPH」","W"
				ELSE
					GOTO PICK_G_SPOT_LOOP
				ENDIF
			CASE 2
				IF ABL:Ｖ感覚 >= 5
					CALL K2_HPH_PRINT,@"「呀HPH 为什么会、这么舒服HPH 欸、嗯HPH」","W"
				ELSE
					GOTO PICK_G_SPOT_LOOP
				ENDIF
			CASE 3
				IF K2_SEXUAL_FRANKNESS()
					CALL K2_HPH_PRINT,@"「就是这样，好厉害! 啊、那里、哇HPH」","W"
				ELSE
					GOTO PICK_G_SPOT_LOOP
				ENDIF
			CASEELSE
				CALL K2_HPH_PRINT,@"「那里被摩擦得咯吱咯吱响，越来越舒服了HPH」","W"
			ENDSELECT
		ELSE
			SELECTCASE RAND:2
			CASE 1
				PRINTFORMW 「恩、啊、恩那里、好奇怪、啊」
			CASEELSE
				PRINTFORMW 「恩恩恩那里，啊，好麻呀，恩」
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
