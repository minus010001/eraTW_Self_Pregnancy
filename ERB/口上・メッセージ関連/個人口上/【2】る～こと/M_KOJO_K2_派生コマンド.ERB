			ENDSELECT
		ELSE
			SELECTCASE RAND:3
			CASE 1
				PRINTFORMW 「哼，哈哈啊，哈哈，好深啊。」
			CASE 2
				PRINTFORMW 「啊，呵呵，呼呼呼呼啊」
			CASEELSE
				PRINTFORMW 「啊，嗯，屁股，哈，呵呵」
			ENDSELECT
		ENDIF
	ENDIF
