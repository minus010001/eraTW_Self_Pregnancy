;==================================================
;7,挿入子宮口玩弄
;==================================================
@M_KOJO_MESSAGE_SCOM_K2_7
IF K2_BASIC_CONDITION_IN_MATING() && CFLAG:1001 && TALENT:体型 >= -1 && TALENT:MASTER:性別 & 2 && TALENT:MASTER:形状 < 3
	DRAWLINE
	IF TEQUIP:吸乳头
		CALL MESSAGE_EQUIP_COM11
	ELSEIF TEQUIP:揉胸
		CALL MESSAGE_EQUIP_COM12
	ENDIF
	PRINTFORMDL %CALLNAME:MASTER%拼命地摆动腰，想要捅进%CALLNAME:TARGET%的子宫。
	PRINTFORMDL 可是，%CALLNAME:MASTER%的阴茎扎不太深。
	SIF !NOWEX:Ｖ絶頂
		PRINTFORMDL 几乎无法得到深处感觉的%CALLNAME:TARGET% %TEXTR("有点扫兴/稍有不满/看上去并不满足")%\@ TEQUIP:眼罩 ?的样子#表情\@。
	PRINTW
	SIF FLAG:口上色
		TRYCALLFORM M_KOJO_COLOR_K{TARGET}
	IF ( TALENT:施虐狂 && !TALENT:受虐狂 ) || ABL:施虐属性 >= 10
		IF TALENT:MASTER:形状 == 1
			LOCALS = %TEXTR("お子様/ちびっこお/未熟お/幼稚お")%
		ELSEIF TALENT:MASTER:形状 == 2
			LOCALS = %TEXTR("お粗末/ちっさいお/親指お/ちんちくりんお")%
		ENDIF
	ELSE
		IF TALENT:MASTER:形状 == 1
			LOCALS = %TEXTR("ちいさめお/こどもお/小不点お")%
		ELSEIF TALENT:MASTER:形状 == 2
			LOCALS = %TEXTR("ちっちゃいお/短小お/おちび")%
		ENDIF
	ENDIF

	;今回Ｖ絶頂無
	IF !NOWEX:Ｖ絶頂
		SELECTCASE RAND:3
		CASE 1
			PRINTFORMDL 大幅度地挥动腰的话，就会%TEXTR("啵地一声/噗地一声")%，阴道和阴茎脱离了。
			CALL K2_HPH_PRINT,@"「やんHPH %CALLNAME:MASTER%大人的小鸡巴，很容易就出来了」","L"
			IF TEQUIP:体位 == 1 || TEQUIP:体位 == 2 || TEQUIP:縄 || TEQUIP:拘束
				CALL K2_HPH_PRINT,@"「是的HPH %LOCALS%请您再插进去吧HPH」","L"
				IF TEQUIP:縄 || TEQUIP:拘束
					PRINTFORMD 露出了下流地张開着的蜜罐
				ELSEIF TEQUIP:体位 == 1
					PRINTFORMD %TEXTR("把腿张開/让我看看秘唇深处的红色嫩肉")%
				ELSE
					PRINTFORMD 摇晃屁股
				ENDIF
				PRINTFORMDW %CALLNAME:TARGET% %TEXTR("乞求着/央求着")%插到里面，%K2_EXPRESSION_VARIATION("PENIS")%更加激烈地勃起。
			ELSE
				CALL K2_HPH_PRINT,@"「哟咻HPH %LOCALS%又要再插进去了HPH」","L"
				PRINTFORMDW %CALLNAME:TARGET%一边显出秘处一边叼住%K2_EXPRESSION_VARIATION("PENIS")%，频繁地摆动着腰部。
			ENDIF
		CASE 2
			CALL K2_HPH_PRINT,@"「嗯，想插去更深一点的地方HPH 请不要留情HPH %LOCALS%加油HPH」","W"
		CASEELSE
			PRINTFORML 「啊，%CALLNAME:MASTER%大人的%LOCALS%小鸡鸡怎么可能进到里面去呢」
			CALL K2_HPH_PRINT,@"「来吧HPH 请更加努力地摆动下腰部吧，让你在最里面打起精神HPH」","W"
		ENDSELECT
	;今回Ｖ絶頂
	ELSE
		SELECTCASE RAND:3
		CASE 1
			CALL K2_HPH_PRINT,@"「噢HPH 虽然还没到最里面、不过也差不多快要到了HPH」","L"
			CALL K2_HPH_PRINT,@"「啊、啊、%CALLNAME:MASTER%大人的%LOCALS%小鸡巴HPH 要去了HPH」","W"
		CASE 2
			CALL K2_HPH_PRINT,@"「啊哈HPH 拼命的%CALLNAME:MASTER%大人很可爱HPH」","L"
			CALL K2_HPH_PRINT,@"「无论%CALLNAME:MASTER%大人的%LOCALS%鸡鸡有多小%CALLNAME:TARGET%也会为之着迷的HPH 请放心吧HPH」","W"
		CASEELSE
			CALL K2_HPH_PRINT,@"「哈HPH 小鸡巴，终于，要停下来了HPH」","L"
			CALL K2_HPH_PRINT,@"「啊、哈HPH 去了HPH 好舒服啊HPH」","W"
		ENDSELECT
	ENDIF
	RETURN 1
ENDIF
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_SCOM_K2_7_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K2_7_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		PRINTFORMW 
		RETURN 1
	ENDIF

	$PICK_OS_UTERI_LOOP
	IF K2_BASIC_CONDITION_IN_MATING()
		SELECTCASE EXP:子宮口開発経験
		CASE IS < 25
			SELECTCASE RAND:2
			CASE 1
				PRINTFORMW 「有、有点、痛苦、呜」
			CASEELSE
				PRINTFORMW 「嗯、呜呜　呼、哈、哈」
			ENDSELECT
		CASE 25 TO 50
			SELECTCASE RAND:2
			CASE 1
				PRINTFORMW 「稍微有点习惯了。」
			CASEELSE
				PRINTFORMW 「欸、奇怪、那样的感觉、嗯」
			ENDSELECT
		CASE IS > 50
			IF TALENT:恋慕 || TALENT:愛欲 || ( TALENT:風騷 && ABL:Ｖ感覚 >= 5 )
				IF NOWEX:MASTER:射精 && ABL:膣射中毒 >= 4 && !TEQUIP:MASTER:避孕套
					SELECTCASE RAND:2
					CASE 1
						CALL K2_HPH_PRINT,@"「呀、有点奇怪HPH 好像会漏出来吧？HPH」","L"
						CALL K2_HPH_PRINT,@"「请吧HPH 请进到最深处HPH」","W"
					CASEELSE
						CALL K2_HPH_PRINT,@"「在最里面射会很舒服HPH 请直接来吧HPH」","L"
						CALL K2_HPH_PRINT,@"「请给最喜欢精液的子宫配种吧HPH」","W"
					ENDSELECT
				ELSE
					SELECTCASE RAND:3
					CASE 1
						IF K2_SEXUAL_FRANKNESS()
							CALL K2_HPH_PRINT,@"「呀HPH 咕咚咕咚地灌进去了、真好HPH」","W"
						ELSE
							GOTO PICK_OS_UTERI_LOOP
						ENDIF
					CASE 2
						CALL K2_HPH_PRINT,@"「好深的地方HPH 呜、呜HPH」","W"
					CASEELSE
						CALL K2_HPH_PRINT,@"「哈、哈HPH 全身都在发颤HPH 呼HPH」","W"
					ENDSELECT
				ENDIF
			ELSE
				SELECTCASE RAND:2
				CASE 1
					PRINTFORMW 「嗯、啊呀、进去了　啊」
				CASEELSE
					PRINTFORMW 「呵呵，碰到最里面了」
				ENDSELECT
			ENDIF
		ENDSELECT
	ENDIF
ENDIF
