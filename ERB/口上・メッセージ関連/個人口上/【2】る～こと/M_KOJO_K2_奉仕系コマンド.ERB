		IF TALENT:恋慕 || TALENT:愛欲 || TALENT:風騷
			SELECTCASE RAND:5
			CASE 1
				IF !TEQUIP:継続接吻 && !( TALENT:性別 & 2 ) && TALENT:MASTER:形状 != 2
					PRINTFORMDL 让%CALLNAME:TARGET%站直，在背后从胯间摩擦着%K2_EXPRESSION_VARIATION("PENIS")%。
					CALL K2_HPH_PRINT,@"「呼呼HPH 就像自己长出来了一样、好有趣HPH」","L"
					SIF !TEQUIP:縄 && !TEQUIP:拘束
						PRINTFORMDL %CALLNAME:TARGET%兴致勃勃地摆弄着%CALLNAME:MASTER%的%K2_EXPRESSION_VARIATION("PENIS")%。
					WAIT
				ELSE
					GOTO FEMORAL_SEX_LOOP
				ENDIF
			CASE 2
				IF !( CFLAG:育児人数 > 0 && !TEQUIP:MASTER:避孕套 && !TCVAR:口服避孕薬 && CFLAG:生理周期 >= 3 )
