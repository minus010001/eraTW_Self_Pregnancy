	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF

	$BLOWJOB_LOOP
	IF K2_BASIC_CONDITION_IN_MATING()
		IF TALENT:恋慕 || TALENT:愛欲
			SELECTCASE RAND:4
			CASE 1
				SELECTCASE ABL:舌
				CASE 0,1
					PRINTFORML 「嗯……哈哈……那个，我能做好吗？」
					PRINTFORMDL 对用嘴巴服务不习惯的%CALLNAME:TARGET%用，看起来不安地询问了。
					PRINTFORMW 「想让%CALLNAME:MASTER%大人心情变好」
				CASE 2,3
					CALL K2_HPH_PRINT,@"「啾啵……啾呼…… 欸嘿HPH 感觉真不错HPH」","W"
				CASE IS >= 4
					PRINTFORMDL %CALLNAME:TARGET%用嘴唇刺激的同时，也用舌头狠狠地责备阴茎小带。
					CALL K2_HPH_PRINT,@"「咕啵…啾、啾啵HPH ……啾噜……呸咯……啾啵……呸咯咯HPH」","W"
				ENDSELECT
			CASE 2
				;フル勃起
				IF CFLAG:MASTER:勃起度 >= 1500
					CALL K2_HPH_PRINT,@"「硬邦邦的样子、我的服务还可以吧？ 就这么出来也可以哦HPH」","W"
				;挿入可能な勃起
				ELSEIF CFLAG:MASTER:勃起度 >= 1000
					IF !TEQUIP:MASTER:避孕套
						CALL K2_HPH_PRINT,@"「啊、有什么要漏出来了HPH」","W"
					ELSE
						GOTO BLOWJOB_LOOP
					ENDIF
				;半勃ち
				ELSEIF CFLAG:MASTER:勃起度 > 500
					CALL K2_HPH_PRINT,@"「用嘴、让它更硬一些HPH」","W"
				;ふにゃちん
				ELSE
					CALL K2_HPH_PRINT,@"「软绵绵的小鸡，在嘴袋里快乐地发出咕噜咕噜的声音HPH」","W"
				ENDIF
			CASE 3
				IF TEQUIP:MASTER:避孕套
					PRINTFORML 「%CALLNAME:MASTER%大人的小不点，想亲身体验一下……」
					PRINTFORMDW %CALLNAME:TARGET%似乎对越过橡胶的口淫不满。
				ELSE
					PRINTFORMDL %CALLNAME:TARGET%一边舔着竿子，一边将鼻子贴在内侧，不停地嗅着香味。
					CALL K2_HPH_PRINT,@"「哈…HPH 好下流的味道……我想一直闻HPH」","W"
				ENDIF
			CASEELSE
				CALL K2_HPH_PRINT,@"「噗啾……呸咯……啾啵……咕啵……啾噜噜HPH」","W"
			ENDSELECT
			IF NOWEX:MASTER:射精
				PRINTL
				SELECTCASE RAND:2
				CASE 1
					CALL K2_HPH_PRINT,@"「小鸡pikupiku的HPH 很舒服吧HPH 请便HPH」","L"
					IF EXP_UP(15,TARGET) == NOWEX:PLAYER:射精 * 3 && !TEQUIP:MASTER:避孕套 && ABL:精液中毒 >= 5 && TCVAR:精飲経験 >= 20
						CALL K2_HPH_PRINT,@"「请让我成为最喜欢精液的女仆吧HPH」","W"
					ELSEIF !TEQUIP:MASTER:避孕套 && TCVAR:精飲経験 >= 20 
						CALL K2_HPH_PRINT,@"「把biubiu地射出来的精液赏给%CALLNAME:TARGET%吧HPH」","W"
					ELSE
						CALL K2_HPH_PRINT,@"「请射进我的嘴里HPH」","W"
					ENDIF
				CASEELSE
					PRINTFORMDL %CALLNAME:MASTER%的%K2_EXPRESSION_VARIATION("PENIS")%已经快要爆炸了。
					IF EXP_UP(15,TARGET) == NOWEX:PLAYER:射精 * 3 && !TEQUIP:MASTER:避孕套 && ABL:精液中毒 >= 5 && TCVAR:精飲経験 >= 20
						CALL K2_HPH_PRINT,@"「让我都喝下去吧HPH 可以吧? 嗯？HPH 好想要您的精液啊HPH」","W"
					ELSEIF !TEQUIP:MASTER:避孕套 && TCVAR:精飲経験 >= 20 
						CALL K2_HPH_PRINT,@"「咕啾……呸咯……啾啵……咕啵……啾噜噜HPH biubiu、哈、呜呼HPH」","W"
					ELSE
						CALL K2_HPH_PRINT,@"「嗯啾、啵、啾HPH 就这样做到最后HPH」","W"
					ENDIF
				ENDSELECT
			ENDIF
		ELSEIF TALENT:思慕 || TALENT:炮友
			SELECTCASE RAND:4
			CASE 1
				IF ABL:舌 < 2
					PRINTFORMW 「啾……呸咯……嗯……太难受了……」
				ELSE
					GOTO BLOWJOB_LOOP
				ENDIF
			CASE 2
				PRINTFORMW 「嗯、啾噜……噗噜………啾噗……啾啵…」
			CASE 3
				PRINTFORMW 「嗯……啊，怎么样？舒服吗？
			CASEELSE
				PRINTFORMW 「呜，好难受。快要出来的话请告诉我」
			ENDSELECT
		ELSE
			IF ABL:奉仕精神 >= 2
				;主人の阴茎に、精液か肛門か粘液か血の有污垢、かつ避孕套無
				IF STAIN:MASTER:Ｐ & 108 && !TEQUIP:MASTER:避孕套
					PRINTFORML 「……好脏……太、太臭了……呜呣……啾噜」
					PRINTFORMDW 一边说着%CALLNAME:TARGET%很臭，一边淡淡地继续着服务。
				ELSE
					PRINTFORMW 「呜呣、嗯……噗噜……噗啾……呸咯………啾噜」
				ENDIF
			ELSE
				PRINTFORMW 「嗯，要舔吗？呜……」
			ENDIF
		ENDIF
	ENDIF
ENDIF
RETURN 1

