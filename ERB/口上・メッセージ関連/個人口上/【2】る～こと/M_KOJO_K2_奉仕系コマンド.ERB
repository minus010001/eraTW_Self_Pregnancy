;80,手淫
;==================================================
@M_KOJO_MESSAGE_COM_K2_80
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K2_80_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K2_80_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && K2_BASIC_CONDITION_IN_MATING() && CFLAG:1001 && FIRSTTIME(SELECTCOM)
		PRINTFORM 「呵呵，
		SELECTCASE TALENT:MASTER:形状
		CASE 1
			PRINTFORM 可爱
		CASE 2
			PRINTFORM 好小啊
		CASE 3
			PRINTFORM 健壮
		CASE 4
			PRINTFORM 晃眼
		CASE 5
			PRINTFORM 改变了形态
		CASE 6
			PRINTFORM 气派
		CASE 10
			PRINTFORM 非常大
		CASEELSE
			PRINTFORM 小鬼
		ENDSELECT
		PRINTFORMW 真是小不点儿啊」
		PRINTFORMDW %CALLNAME:TARGET%脸上露出厌恶的笑容的同时，一只手在玩弄阴茎。
		PRINTFORMW 「那么，我就为你服务了。」
		RETURN 1
	ENDIF
	
	$HANDJOB_LOOP
	IF K2_BASIC_CONDITION_IN_MATING()
		IF TALENT:恋慕 || TALENT:愛欲
			SELECTCASE RAND:6
			CASE 1
				IF ABL:技巧 >= 4 && !NOWEX:MASTER:射精
					CALL K2_HPH_PRINT,@"「要我用手再刺激一下吗？ 什么时候出来都可以哦HPH」","W"
				ELSE
					GOTO HANDJOB_LOOP
				ENDIF
			CASE 2
				IF ABL:指 >= 4
					CALL K2_HPH_PRINT,@"「%CALLNAME:MASTER%大人的这里只要咯吱咯吱地一动就要出来了呢HPH」","L"
					PRINTFORMDW %CALLNAME:TARGET%用巧妙的指技责备%CALLNAME:MASTER%的弱点。
				ELSE
					GOTO HANDJOB_LOOP
				ENDIF
			CASE 3
				IF !TEQUIP:眼罩 && !TEQUIP:吸乳头 && !TEQUIP:継続接吻 && !TEQUIP:六九式 && ABL:受虐属性 >= 3
					PRINTFORMDL 跪下的%CALLNAME:TARGET%用手服务的同时，眼神看着%CALLNAME:MASTER%的脸。
					PRINTFORMW 「一边被%CALLNAME:MASTER%大人俯视一边这样做，感觉像是在为%CALLNAME:MASTER%大人服务。」
				ELSE
					GOTO HANDJOB_LOOP
				ENDIF
			CASE 4
				IF ABL:指 >= 2 && ABL:施虐属性 >= 4
					PRINTFORMDL 由于%CALLNAME:TARGET%的激烈的责备，%CALLNAME:MASTER%情不自禁地退缩了。
					CALL K2_HPH_PRINT,@"「啊、别想跑。还没结束呢HPH」","L"
					SIF !TEQUIP:吸乳头 && !TEQUIP:揉胸 && !TEQUIP:拘束 && !TEQUIP:六九式
						PRINTFORMDL %CALLNAME:TARGET%绕到%CALLNAME:MASTER%的后面，为了不让你逃跑而紧紧地、从背后更加激烈地抱住你。
					WAIT
				ELSE
					GOTO HANDJOB_LOOP
				ENDIF
			CASE 5
				CALL K2_HPH_PRINT,@"「能感觉到它在手里跳动HPH」","W"
			CASEELSE
				SELECTCASE ABL:指
				CASE 0
					PRINTFORMW 「啊，是这样的感觉吗？」
				CASE 1,2
					PRINTFORMW 「怎么样？做得舒服吗？」
				CASE IS >= 3
					PRINTFORMDL %CALLNAME:TARGET%巧妙控制着缓急的同时，引导着%CALLNAME:MASTER%射精。
					;フル勃起
					IF CFLAG:MASTER:勃起度 >= 1500
						CALL K2_HPH_PRINT,@"「啊哈、好硬啊HPH 该怎么做呢？ 就这样用手射精吗? 还是要留在后面？HPH」","W"
					;挿入可能な勃起
					ELSEIF CFLAG:MASTER:勃起度 >= 1000
						CALL K2_HPH_PRINT,@"「啊HPH 硬度很棒HPH 但是，%CALLNAME:MASTER%大人还可以变得更硬吧？HPH」","W"
					;半勃ち
					ELSEIF CFLAG:MASTER:勃起度 > 500
						CALL K2_HPH_PRINT,@"「嘿咻、嘿咻HPH 看呀HPH 再大一点的话、就可以做了HPH」","W"
					;ふにゃちん
					ELSE
						CALL K2_HPH_PRINT,@"「还是软绵绵的样子。嘿、加油HPH 加油HPH」","W"
					ENDIF
				ENDSELECT
			ENDSELECT
			IF NOWEX:MASTER:射精 && ABL:技巧 >= 4
				PRINTL
				PRINTFORMDL 被%CALLNAME:TARGET%的手弄脏了的%K2_EXPRESSION_VARIATION("PENIS")%快要射精了。
				SELECTCASE RAND:2
				CASE 1
					CALL K2_HPH_PRINT,@"「啊HPH 怎么了？HPH 只是用手就biubiu地射出来了吗？HPH」","W"
				CASEELSE
					CALL K2_HPH_PRINT,@"「啊，已经出来了吧？不错HPH 我的手上都黏糊糊的了HPH」","W"
				ENDSELECT
			ENDIF
		ELSEIF TALENT:思慕 || TALENT:炮友
			IF ABL:指 >= 2
				SELECTCASE RAND:2
				CASE 1
					PRINTFORMW 「我渐渐明白了该怎么做比较好了。」
				CASEELSE
					PRINTFORML 「嗯……嗯……没关系吧？不疼吗？
					PRINTFORMDW %CALLNAME:TARGET%在发出咕嘟咕嘟的声音的同时，以快速的动作给予%CALLNAME:MASTER%快感。
				ENDSELECT
			ELSE
				PRINTFORMW 「呜……胳膊累了」
			ENDIF
		ELSE
			SELECTCASE RAND:2
			CASE 1
				PRINTFORMW 「用手摩擦就可以了吧。我明白了」
			CASEELSE
				PRINTFORML 「……」
				PRINTFORMDW %CALLNAME:TARGET%沉默着，一味地动着手。
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
RETURN 1

