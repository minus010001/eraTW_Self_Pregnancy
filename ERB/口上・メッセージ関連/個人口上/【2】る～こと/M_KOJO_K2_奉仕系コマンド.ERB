					ELSE
						PRINTFORMW 「真的可以不插吗？向你摆出姿势就可以了吗？」
					ENDIF
				ELSE
					GOTO FEMORAL_SEX_LOOP
				ENDIF
			CASE 3
				IF K2_SEXUAL_FRANKNESS()
					CALL K2_HPH_PRINT,@"「嗯啊HPH 摩擦着鸡鸡HPH 要出来了哟HPH」","W"
				ELSE
					GOTO FEMORAL_SEX_LOOP
				ENDIF
			CASE 4
				CALL K2_HPH_PRINT,@"「用温热的大腿和%CALLNAME:MASTER%大人的小鸡巴吧唧吧唧地摩擦HPH 啊HPH 声音好下流HPH」","W"
			CASEELSE
				CALL K2_HPH_PRINT,@"「我的腰技如何？HPH 舒服吗？HPH」","W"
			ENDSELECT
		ELSE
			SELECTCASE RAND:3
			CASE 1
				PRINTFORMDL 让%CALLNAME:TARGET%坐在上面，让她分開双腿。
				PRINTFORMW 「没事吧…我很担心会不会压坏……」
