					PRINTFORMW 「深い……っ……駄目、奥駄目っ…」
				ENDSELECT
				PRINTFORML
			CASE 41,42,43,44,51,52,53,54,71,72,73,74
				;恋慕OR愛欲
				SELECTCASE M_KOJO_K103_RANDNR(4,73)
				CASE 1
					PRINTFORMW 「%M_KOJO_K103_AEGI()%…%M_KOJO_K103_AEGI()%…%M_KOJO_K103_HEART()%」
				CASE 2
					PRINTFORMW 「あっ…あんっ…子宮の入り口に当たってるの、わかるの…っ…%M_KOJO_K103_HEART()%」
				CASE 3
					PRINTFORMW 「…奥に…奥に頂戴…たくさん欲しい…っ…%M_KOJO_K103_HEART()%」
				CASEELSE
					PRINTFORMW 「%M_KOJO_K103_AEGI()%…%M_KOJO_K103_HEART()% ああぁ…こじ開けないで……！%M_KOJO_K103_HEART()%」
				ENDSELECT
				PRINTFORML
			ENDSELECT
		RETURN 1
	ENDIF
ENDIF
RETURN 1
