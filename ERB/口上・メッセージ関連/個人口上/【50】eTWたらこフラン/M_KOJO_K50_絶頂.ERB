@M_KOJO_MESSAGE_PALAMCNG_A_K50_1(ARG)
CALL PALAM_MESSAGE_A
;-------------------------------------------------
;膣内
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	SIF !SHIRAHU(TARGET)
		RETURN 0
	IF 充填率(TARGET,1) >= 100
		;筹码は株だからおNISAまは優遇されるの
		PRINTFORML 「哥哥、%CALLNAME:TARGET%已經肚子滿滿了喲」
		PRINTFORMW 「變得不能吃飯了喲」
	ELSE
		PRINTFORMW 「嗯……\@ TALENT:恋慕 ? %UNICODE(0x2661)% #  \@」
		PRINTFORML 「哥哥的小雞雞在肚子裡頭，咚咚咚做著」
		IF GETBIT(CFLAG:1500,1)
			PRINTFORMW 「在%CALLNAME:TARGET%中射精著呢」
		ENDIF
		IF ABL:Ｖ感覚 > 2
			PRINTFORMW 「誒嘿嘿%UNICODE(0x2661)%在咚咚咚做的時候，每次哥哥的小雞雞鬧騰，%CALLNAME:TARGET%也很舒服喲」
		ELSE
			PRINTFORMW 「哥哥好像很舒服」
		ENDIF
		IF GETBIT (CFLAG:1500,3)
			PRINTFORMW 「哥哥高興嗎？在%CALLNAME:TARGET%肚子裏\@ GETBIT (CFLAG:1500,1) ? 射精 # 咚咚地做着 \@開心吗？」
			SIF 充填率(TARGET,1) < 50
				PRINTFORMW 「繼續做也可以哦」
		ENDIF
	ENDIF
ENDIF
RETURN 1

