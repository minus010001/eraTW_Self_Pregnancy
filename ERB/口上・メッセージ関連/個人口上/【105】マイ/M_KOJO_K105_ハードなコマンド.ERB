;==================================================
;140,深喉插入
;==================================================
@M_KOJO_MESSAGE_COM_K105_140
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K105_140_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K105_140_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;特殊状況
	IF FLAG:70 ;時姦
		PRINTFORMW 
		RETURN 1
	ELSEIF CFLAG:MASTER:恶作剧 == 2
		PRINTFORMW 
		RETURN 1
	ELSEIF TCVAR:烂醉 == 1
		PRINTFORML 
		RETURN 1
	;通常
	ELSE
		IF ABL:受虐属性 < 2
			IF TALENT:恋人 || TALENT:恋慕
				PRINTFORML 「ん゛っ、う！？　ぐっ、ん……ん……」
				PRINTFORMDW 突然の激しい行為に​舞​は非難がましい目を向けているが、特に抵抗様子は無い
			ELSE
				PRINTFORML 「ん゛ぐっ！　ん～っ！」
				PRINTFORMDW ​舞​は顔を遠ざけようともがいている……
			ENDIF
		ELSE
			IF TALENT:恋人 || TALENT:恋慕
				IF CFLAG:105:1006
					PRINTFORMDL ​舞​は自ら%CALLNAME:MASTER%の陰茎を飲み込むかのように顔を押し付け、喉奥を使って奉仕し始めた
					PRINTFORMDL 苦しそうに呻きつつもその声には甜音色が混じっており、どこか嬉しそうな様子だ…
					PRINTFORML 「ん゛ぅぅ、ん゛っ、ごぇ%UNICODE(0x2665) *1%　ぐっ、ぐぷ……ん゛%UNICODE(0x2665) *1%」
				ELSE
					PRINTFORML 「ん゛っ、ぐっ……%UNICODE(0x2665) *1%　ぉご、ぷ……%UNICODE(0x2665) *1%」
					PRINTFORMDW ​舞​は苦しそうにしながらも、顔や舌を回すように動かし積極的に奉仕している
				ENDIF
			ELSE
				PRINTFORMDL ​舞​は反射的に僅かな抵抗を示したものの、すぐに大人しく行為を受け入れた
				PRINTFORMW 「ぐ、ん……んぶ、ぅぐ……」
				SIF TALENT:思慕
					PRINTFORMW （苦しい……苦しい、けど……意外と、これ……）
			ENDIF
		ENDIF
	ENDIF
ENDIF
RETURN 1

@M_KOJO_EXTRASOURCE_COM_K105_140
IF TALENT:恋人 || TALENT:恋慕
	SELECTCASE CFLAG:105:1006
	CASE 0
		SOURCE:反感 = 300
	CASE 1
		SOURCE:屈従 += ABL:105:受虐属性 * 150
		SOURCE:反感 = 0
		SOURCE:快Ｍ += ABL:105:受虐属性 * 100
		TIMES DOWNBASE:体力 , 0.50
		TIMES DOWNBASE:気力 , 0.50
	CASE 2
		SOURCE:屈従 += ABL:105:受虐属性 * 300
		SOURCE:反感 = 0
		SOURCE:快Ｍ += ABL:105:受虐属性 * 200
		TIMES DOWNBASE:体力 , 0.30
		TIMES DOWNBASE:気力 , 0.30
	CASE 3
		SOURCE:屈従 += ABL:105:受虐属性 * 500
		SOURCE:反感 = 0
		SOURCE:快Ｍ += ABL:105:受虐属性 * 300
		SOURCE:快Ｖ += ABL:105:受虐属性 * 30
		TIMES DOWNBASE:体力 , 0.10
		TIMES DOWNBASE:気力 , 0.10
	CASEELSE
	ENDSELECT
ENDIF
RETURN 1
