;14,玩弄Ｓ状結腸
;==================================================
@M_KOJO_MESSAGE_SCOM_K105_14
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_SCOM_K105_14_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K105_14_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;特殊状況
	IF FLAG:70 ;時姦
		PRINTFORML 
		RETURN 1
	ELSEIF CFLAG:MASTER:恶作剧 == 2
		PRINTFORML 
		RETURN 1
	ELSEIF TCVAR:烂醉 == 1
		PRINTFORML 
		RETURN 1
	ELSEIF TCVAR:媚薬 || TCVAR:発情
		IF TCVAR:105:351 >= 4
			PRINTFORML 「ぁぐ、これっ……どこぉ？　ぁへ……からだっ、つきぬけちゃったぁ%UNICODE(0x2665) *1%」
			PRINTFORMW 「ん゛っ……ふ、へ%UNICODE(0x2665) *1%　もっと、もっとぉ……めちゃくちゃにぃ……%UNICODE(0x2665) *1%」
		ELSEIF TALENT:恋人 || TALENT:恋慕
			PRINTFORML 「突き刺しすぎ、ぃ……%UNICODE(0x2665) *1%　お尻っ、ダメになっちゃうぅ……%UNICODE(0x2665) *1%」
			PRINTFORMW 「ぅふ、ふふ……わたし、こんなところまで……ふふふふ、あは……%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORML 「ん゛っ……稍微っ、苦しぃ……%UNICODE(0x2665) *1%」
			PRINTFORMW 「……け、ど……これ、ぇ……っ%UNICODE(0x2665) *1%　あ、あぁ……%UNICODE(0x2665) *1%」
		ENDIF
	ELSEIF ABL:肛門 > 3 ;このへんの蔷薇ンス感覚サッパリわからん
		IF TALENT:恋人 || TALENT:恋慕
			PRINTDATAW
				DATAFORM 「嗯、好棒……来吧、插到不能插入为止……嗯%UNICODE(0x2665) *1%」
				DATAFORM 「啊、嘎……没问题、插得更深的……很棒%UNICODE(0x2665) *1%」
			ENDDATA
		ELSE
			PRINTFORMW 「好、深……这个、到底要插到多深……%UNICODE(0x2665) *1%」
			SIF TALENT:思慕
				PRINTFORMW （明明是不可以、插进去的地方……但是却感觉舒服起来了、啊……%UNICODE(0x2665) *1%）
		ENDIF
	ELSE
		IF TALENT:恋人 || TALENT:恋慕
			PRINTDATAW
				DATAFORM 「快停、下……再继续做的话、感觉会、很不妙的……」
				DATAFORM 「呜唉……绝对、碰到了、奇怪的地方……呜」
			ENDDATA
		ELSE
			PRINTFORMW 「等……稍微、拔出去些啊……」
			SIF TALENT:思慕
				PRINTFORMW （好危险……这家伙把我的身体、当作什么了啊……）
		ENDIF
	ENDIF
ENDIF
