;==================================================
;6,挿入Ｇスポ責め
;==================================================
@M_KOJO_MESSAGE_SCOM_K105_6
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_SCOM_K105_6_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K105_6_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;特殊状況
	IF FLAG:70 ;時姦
		PRINTFORML 
		RETURN 1
	ELSEIF CFLAG:MASTER:恶作剧 == 2
		PRINTFORML 
		RETURN 1
	ELSEIF TCVAR:烂醉 == 1
		PRINTDATAW
			DATAFORM 「！　嗯呜呜呜%UNICODE(0x2665) *1%　啊啊啊嗯%UNICODE(0x2665) *1%　ki、咿咕、呜%UNICODE(0x2665) *1%」
			DATAFORM 「嗯咿！　不行不行、那里、是%UNICODE(0x2665) *1%　呀啊、啊啊啊！」
		ENDDATA
		RETURN 1
	ELSEIF TCVAR:媚薬 || TCVAR:発情
		IF TCVAR:105:351 >= 4
			PRINTFORML 「っ、やあ゛ああぁ！！　ひっい゛っ、ぐぅぅ%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%　そこっやばいのぉ゛ぉ%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%」
			PRINTFORMW 「も゛おっむ゛りい゛ぃぃ%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%　お゛っお、おかしくなるぅ゛ぅ%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%」
		ELSEIF TALENT:恋人 || TALENT:恋慕
			PRINTFORML 「あぁん%UNICODE(0x2665) *1%　そこぉ%UNICODE(0x2665) *1%　良いとこ当たってるのっ%UNICODE(0x2665) *1%」
			PRINTFORMW 「いいっ、いひぃぃ%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%　だめっ、感じすぎちゃうぅ%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORML 「ひあぁっ%UNICODE(0x2665) *1%　待っ、て……っ%UNICODE(0x2665) *1%　そこっ、やばぁ……%UNICODE(0x2665) *1%」
			PRINTFORMW 「だっ、稍微っ%UNICODE(0x2665) *1%　待って、ってぇ%UNICODE(0x2665) *1%」
		ENDIF
	ELSE
		IF TALENT:恋人 || TALENT:恋慕
			PRINTDATAW
				DATAFORM 「嗯、那里、不行、啊……等！　等下、噫咕、嗯嗯%UNICODE(0x2665) *1%」
				DATAFORM 「没错、那里……呀、呜啊啊啊！　咿咿、嗯咿咿%UNICODE(0x2665) *1%」
			ENDDATA
		ELSE
			PRINTFORMW 「！？　等、那里是……啊%UNICODE(0x2665) *1%」
			SIF TALENT:思慕
				PRINTFORMW （舒服的地方、已经、暴露了啊……库呜%UNICODE(0x2665) *1%）
		ENDIF
	ENDIF
ENDIF
;未記入
;CALLFORM M_KOJO_MESSAGE_COM_K105_{SELECTCOM}_1
RETURN 1

