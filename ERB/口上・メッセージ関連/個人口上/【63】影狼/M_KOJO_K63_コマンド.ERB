				DISH_TASTE += "卵/赏月/"
			ELSEIF STRCOUNT(DISH_TASTE, ESCAPE("魚料理/"))
				GOTO OSAKANA
			ELSEIF STRCOUNT(DISH_TASTE, ESCAPE("肉料理/"))
				GOTO ONIKU
			ELSEIF STRCOUNT(DISH_TASTE, ESCAPE("甜点/"))
				PRINTFORMW 「点心吗～。嗯、好羡慕呢」
				PRINTFORMW 「我也能学做这个吗…？」
			ELSEIF STRCOUNT(DISH_TASTE, ESCAPE("野菜/"))
