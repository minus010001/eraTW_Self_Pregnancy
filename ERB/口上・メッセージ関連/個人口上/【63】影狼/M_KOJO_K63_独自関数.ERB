@K63_RETRIEVE
#DIM 獲物
#DIM RETRIEVE
TCVAR:356 = 1
RETRIEVE = 0
FOR LOCAL,1,4
	SIF !TRAP_POINT:LOCAL
		CONTINUE
	;地底は無理
	SIF TRAP_POINT:LOCAL / 100 == 9
		CONTINUE
	IF TRAP_DURABILITY:LOCAL == 0 || TRAP_CAPTURE:LOCAL
		RETRIEVE = LOCAL
		BREAK
	ENDIF
NEXT
;なかった場合もう一度回す
IF !RETRIEVE
	FOR LOCAL,1,4
		SIF !TRAP_POINT:LOCAL
			CONTINUE
		;地底は無理
		SIF TRAP_POINT:LOCAL / 100 == 9
			CONTINUE
		IF TRAP_DURABILITY:LOCAL <= 20
			RETRIEVE = LOCAL
			BREAK
		ENDIF
	NEXT
ENDIF
SIF RETRIEVE == 0
	RETURN

SELECTCASE TRAP_POINT:RETRIEVE / 100
	CASE 0
		LOCALS = 神社の森
	CASE 4
		LOCALS = 竹林
	CASE 5
		LOCALS = 森林的入口
	CASE 7
		LOCALS = 山の中
	CASE 8
		LOCALS = 山のてっぺん
	CASEELSE
		LOCALS = どこだったか
ENDSELECT

PRINTFORML 「啊%CALLNAME:MASTER%、之前在%LOCALS%设了陷阱吧？」
IF TRAP_DURABILITY:RETRIEVE == 0 || TRAP_CAPTURE:RETRIEVE == 11
	;野狗がかかってた場合嘘ついて逃がしちゃう
	PRINTFORMW 「那个坏掉了诶」
	CALL TRAP_V_RESET(RETRIEVE)
	RETURN 0
ENDIF
;東南アジアで実在する狩猟法
IF TRAP_CAPTURE:RETRIEVE == 50 && K63_CHICKENNAME == ""
	PRINTFORMW 「オスのトリだったらバラさないでって言ってたけど、何に使うの？目覚まし？」
	PRINTFORMW 「……へえ、他のオスがナワバリ守ろうと寄ってくるんだ！人間は色々考えつくのねえ」
	PRINTFORMW 「ならこれ、私にくれない？%CALLNAME:MASTER%の罠猟より私の方が有効活用できそうよ」
	PRINTFORMW 「大丈夫大丈夫！食べないから！」
	K63_CHICKENNAME '= @"%TEXTR("スナギモ/ハツ/カシワ/セセリ/チューリップ/ボンジリ/ヤゲン/オタフク/フリソデ")%"
	PRINTFORML 「そうね、名前は…%K63_CHICKENNAME%でどうかしら」
	PRINTFORMW 不安になるネーミングだ…
	CALL TRAP_V_RESET(RETRIEVE)
	RETURN 0
ENDIF
IF TRAP_CAPTURE:RETRIEVE == 0 && TRAP_DURABILITY:RETRIEVE
	PRINTFORML 「已经\@ TRAP_DURABILITY:RETRIEVE < 11? 坏的很严重# 有点坏掉\@了」
	PRINTFORML 「想着帮你回收了会不会比较好来着」
	RETURN 0
ENDIF

獲物 = TRAP_CAPTURE:RETRIEVE
CAPTURE_RECORD:獲物 ++
