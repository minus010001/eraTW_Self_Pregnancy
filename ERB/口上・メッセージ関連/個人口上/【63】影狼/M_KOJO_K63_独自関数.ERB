@影狼起床
;満月の日
SELECTCASE DAY:3
CASE 15
	;満月の下
	IF MOON_WATCH(DAY:3) == 9
		;しかも中秋の名月
		SELECTCASE DAY:2
		CASE 3
			PRINTFORMW 「哎呀、%CALLNAME:MASTER%你好呀……」
			PRINTFORMW 「今晚是……中秋夜呢……」
			PRINTFORMW 「那个、可以一起去赏月吗…？」
			PRINTFORML 「虽、虽然有点不好意思、但如果是%CALLNAME:MASTER%的话」
			PRINTFORMW 「把真实的姿态、展现出来……」
			PRINTFORMW 「不过这样的话……」
			PRINTFORMW 「不没什么、接下来就这样一起去赏月吧！」
			BASE:63:理性 = 0
			TCVAR:発情 = 1
		;普通の満月
		CASEELSE
			PRINTFORMW 「哎呀、%CALLNAME:MASTER%你好呀……」
			PRINTFORMW 「快看、今晚是満月哦」
			PRINTFORML 「虽然有点不好意思、但如果%CALLNAME:MASTER%…」
			PRINTFORMW 「想看看、今天的我的话……」
			PRINTFORMW 「呵呵、我竟然会去想这种事情真是不可思議呢」
			BASE:63:理性 = 0
			TCVAR:発情 = 1
		ENDSELECT
	;まだ昼で晴天てない
	ELSEIF TIME:2 >=1 && TIME:2 <= 4 && TIME:5 > 1
		PRINTFORMW 「哎呀、%CALLNAME:MASTER%、早安」
		PRINTFORMW 「那个……今晚是満月呢……」
		PRINTFORMW 「虽然不知道天気会变的怎样」
		PRINTFORMW 「如果是晴天的话要不要和我一起赏月呢？」
		CALL K63_ASK_OTSUKIMI
	;まだ昼
	ELSEIF TIME:2 >=1 && TIME:2 <= 4
		PRINTFORMW 「哎呀、%CALLNAME:MASTER%、早安」
		PRINTFORMW 「那个……今晚是満月呢……」
		PRINTFORMW 「虽然不知道天気会变的怎样」
		PRINTFORMW 「总之先祈祷会是个大晴天吧」
	;夜だけど晴天てない
	ELSEIF TIME:5 > 1
		;中秋の名月
		SELECTCASE DAY:2
		CASE 3
			PRINTFORMW 「啊啦、%CALLNAME:MASTER%早上好…」
			PRINTFORMW 「明明是难得的中秋佳节、这种天气真是气人呢……」
			PRINTFORMW 「呐%CALLNAME:MASTER%、如果放晴了的话要和我一起赏月吗？」
			CALL K63_ASK_OTSUKIMI
		;普通の満月
		CASEELSE
			PRINTFORMW 「哎呀、%CALLNAME:MASTER%、早安」
			PRINTFORMW 「明明是难得的満月、这种天气真是气人呢……」
			PRINTFORML 「……嘛因为正好是种族的生理周期」
			PRINTFORMW 「可以变身、不过现在体毛变到一半就……」
			PRINTFORMW 「喂、你在看哪里啊！」
		ENDSELECT
	;夜で晴天てるけど室内（秋）
	ELSEIF DAY:2 == 3
		PRINTFORMW 「哎呀、%CALLNAME:MASTER%你好呀……」
		PRINTFORMW 「今晚是……中秋夜呢……」
		PRINTFORMW 「那个、可以一起去赏月吗…？」
		PRINTFORML 「虽、虽然有点不好意思、但如果是%CALLNAME:MASTER%的话」
		PRINTFORMW 「把真实的姿态、展现出来也没什么关系吧……」
		PRINTFORMW 「不过这样的话……」
		PRINTFORMW 「不没什么、接下来就这样一起去赏月吧！」
	;夜で晴天てるけど室内
	ELSE
		PRINTFORMW 「啊…%CALLNAME:MASTER%你好呀……」
		PRINTFORMW 「呐、今晚是満月哦」
		PRINTFORML 「虽然有点不好意思、但如果%CALLNAME:MASTER%…」
		PRINTFORMW 「让你看看今天的我、也没什么吧……」
		PRINTFORMW 「不过那个……」
		PRINTFORMW 「不、什么事也没有哦！」
	ENDIF
;それ以外の日
CASEELSE
	SELECTCASE TIME:2
	;朝は寝てるんで死に分岐ですが
	CASE 1,2
		PRINTFORMW 「早安～…」
		PRINTFORMW 「从早上就很有精神呢……」
		PRINTFORMW 「我对早上有点不太适应……」
	;昼
	CASE 3,4
		PRINTFORMW 「哎呀%CALLNAME:MASTER%、早安！」
		PRINTFORMW 「中午是一天的開始呢」
	SIF TIME:0 < 901
		PRINTFORMW 「……呜哇～…还想睡……」
	;夜は分岐
	CASE 5,6,7
		SELECTCASE TIME:0
		;19～22時
		CASE 1140 TO 1320
			SELECTCASE TIME:3
			;18時以降起き
			CASE 1080 TO 1320
				PRINTFORMW 「哎呀%CALLNAME:MASTER%、早安！」
				PRINTFORMW 「呵呵、你也是夜行性的人类吗」
				PRINTFORMW 「难道是勉强自己跟上我的节拍吗？」
				PRINTFORML 「虽、虽然很開心、不过这样对你的健康并没有好处」
