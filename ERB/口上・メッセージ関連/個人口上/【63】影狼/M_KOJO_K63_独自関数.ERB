;くんくん影狼
;━━━━━━━━━━━━━━━━━━━━━━━━━
@KUNKUN_KAGEROU
#LOCALSIZE 人物数量上限
#DIM LOCAL_ID
;一緒に作った料理は食べない
IF FLAG:20 && !TCVAR:353
	PRINTFORMW 「唔！这是%DISHNAME%的气味呢！」
	PRINTL 
	PRINTFORML 影狼用肉眼难以捕捉的速度将%CALLNAME:MASTER%身上带着的%DISHNAME%给拿走了
	PRINTFORMW 然后没过一眨眼的工夫就吃光了。
	;EVENTCOMENDからコピペ。変更があったら対応の事。
	IF DISH_DRG
		CALL MESSAGE_EAT_DISH_DRG(TARGET)
		WAIT
	ENDIF
	;CALL KOJO_MESSAGE_SEND("EVENT",14,TARGET,-1,-1)
	CALL 吃飯(TARGET)
	CALL RESET_DISH
ELSE
	CALLF FUNC_FISHER_YATES_SHAFFLE(LOCAL, CHARANUM)
	;影狼ちゃんと路人娘とMASTERは抽選対象から外します
	ARRAYREMOVE LOCAL, FINDELEMENT(LOCAL, 63), 1
	ARRAYREMOVE LOCAL, FINDELEMENT(LOCAL, MASTER), 1
	ARRAYREMOVE LOCAL, FINDELEMENT(LOCAL, RANDOM_CHARANUM), 1
	;FUNC_FISHER_YATES_SHAFFLE内で-1で初期化されるのでこれで全配列対象処理になる
	;場所判定がよくわからんことになってたのでとりあえず近場の角色？にしてみるよ
	FOR LOCAL_ID, 0, FINDELEMENT(LOCAL, -1)
		;同じMapにいる角色限定
		SIF GET_MAPID(CFLAG:(LOCAL:LOCAL_ID):現在位置) != GET_MAPID(CFLAG:63:現在位置)
			CONTINUE
		;同じ場所の角色は除外
		SIF CFLAG:(LOCAL:LOCAL_ID):現在位置 == CFLAG:63:現在位置
			CONTINUE
		PRINTFORMW 「嗅嗅……这是%CALLNAME:(LOCAL:LOCAL_ID)%的味道呢」
		BREAK
	NEXT
ENDIF
RETURN 1
