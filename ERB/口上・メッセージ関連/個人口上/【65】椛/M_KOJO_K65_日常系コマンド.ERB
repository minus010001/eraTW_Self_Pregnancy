				CASE 1
					CALL HPH_PRINT,@"「呜…HPH%CALLNAME:MASTER%桑…HPH」","W"
				CASE 2
					CALL HPH_PRINT,@"「欸嘿嘿、%CALLNAME:MASTER%桑…HPH」","W"
					PRINTFORMW 椛依偎在%CALLNAME:MASTER%的身上、用身体不断地擦蹭着…
				CASE 3
					PRINTFORML 趁椛不备之时突然摸了摸她的尾巴。
					CALL HPH_PRINT,@"「呼啊啊姆…HPH」","L"
					PRINTFORML 「真是的…为什么会发出这种声音啊…」
					PRINTFORMW 与埋怨的话语相反、椛的尾巴像是在期待情爱一般地摇晃着。
			ENDSELECT
		ELSE
			SELECTCASE RAND:2
				CASE 0
					PRINTFORML 「啊啊啊啊、那个！现在请不要和我靠得太近！！」
					PRINTFORMW 椛满脸通红地说着…
				CASE 1
					PRINTFORML 趁椛不备之时突然摸了摸她的尾巴。
					CALL HPH_PRINT,@"「呼啊啊姆…HPH」","L"
					PRINTFORML 
					PRINTFORMW 然后被椛骂惨了。
			ENDSELECT
		ENDIF
	ELSE
