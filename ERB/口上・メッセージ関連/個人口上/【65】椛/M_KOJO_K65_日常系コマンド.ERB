;301,泡茶
;TFLAG:193(1=大成功 0=成功 -1=失敗)
;==================================================
@M_KOJO_MESSAGE_COM_K65_301
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K65_301_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K65_301_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF CFLAG:ブチギレ > 0
		PRINTFORML 「…谢谢」
		PRINTFORMW 椛默默地喝着你沏的茶。
	;仕事中
	ELSEIF CFLAG:行動 == 5
		SELECTCASE RAND:2
			CASE 0
			PRINTFORMW 「谢谢。巡逻的工作总是让人口渴呢…」
			CASE 1
			PRINTFORML 「谢、谢谢」
			PRINTFORMW 「难得来一次、就在这附近休息一下吧」
		ENDSELECT
	ELSE
		;大成功
		IF TFLAG:193 == 1
			SELECTCASE RAND:2
				CASE 0
				PRINTFORML 「好香啊…」
				PRINTFORML 「光是这样闻着就有一种幸福的感觉」
				PRINTFORMW 椛陶醉地闭上眼睛、享受着馥郁的茶香。
				CASE 1
				PRINTFORML 「是茶吗？谢谢」
				PRINTFORML 「……非常好喝」
				PRINTFORMW 「泡茶的时候有什么窍门吗？」
			ENDSELECT
		;失敗
		ELSEIF TFLAG:193 == -1
		PRINTFORML 「是茶吗？谢谢」
		PRINTFORML 「……呜！？…烫烫烫…」
		PRINTFORMW 椛喝茶的时候好像呛到了……
		;成功
		ELSE
			SELECTCASE RAND:2
				CASE 0
				PRINTFORMW 「是茶吗？谢谢」
				CASE 1
				PRINTFORML 「啊、不好意思、那我就不客气啦…」
				PRINTFORMW 椛恭恭敬敬地接过茶杯、開始慢慢喝茶。
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
RETURN 1
