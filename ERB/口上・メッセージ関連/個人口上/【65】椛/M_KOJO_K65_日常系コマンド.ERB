@M_KOJO_MESSAGE_COM_K65_621_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;釣れなかった
	IF TFLAG:193 == 0
		SELECTCASE RAND:4
			CASE 0
				PRINTFORML 「嘛、也会有这种时候」
				PRINTFORMW 「耐心点吧、%CALLNAME:MASTER%桑」
			CASE 1
				PRINTFORML 「嗯、刚才是有微风吹过来吗…」
				PRINTFORML 「…像这样、悠闲地眺望着浮云也是很好的」
				PRINTFORMW 椛和%CALLNAME:MASTER%并排拿着鱼竿、一边听着水的潺潺细流、一边悠闲地度过了时光。
			CASE 2
				PRINTFORML 「………」
				PRINTFORML 「……」
				PRINTFORML 「…Zzz」
				PRINTFORMW 椛一边划桨一边垂杆。真是灵巧。
			CASE 3
				;椛の初期能力は教養1
				PRINTFORML 「听说以前有一位非常喜欢钓鱼的名军师」
				PRINTFORMW 「如果我也这样垂钓的话、会不会就能想出好的将棋法…之类的。呵呵呵」
		ENDSELECT
	;椛が釣った
	ELSEIF TFLAG:194 >= 1000
		SELECTCASE TFLAG:193
			;木材
			CASE 610
				PRINTFORMW 「嗯…我觉得是可以接受的…」
			;守矢之守札
			CASE 131
				PRINTFORML 「好像是守矢神社的守札呢」
				SELECTCASE RAND:2
					CASE 0
						PRINTFORMW 「…但是、为什么会在这种地方？」
					CASE 1
						PRINTFORML 「据说收集几张就可以抽签…」
						PRINTFORMW 「%CALLNAME:MASTER%桑、你试过吗？」
					CASE 2
						PRINTFORML 「1回、3000円…」
						PRINTFORMW 椛望着远方喃喃自语。
				ENDSELECT
			;跳蛋
			CASE 21
				PRINTFORML 「这是什么东西、粉红色的…」
				PRINTFORML 「啊！？～～～～～～啊！！」
				PRINTFORML 椛涨红了脸、把跳蛋扔了出去。
				PRINTFORMW …太可惜了、我把它捡了回来。
			;素材
			CASE 609,606
				PRINTFORMW 「哇、看起来是很漂亮的东西呢」
			;其他の魚
			CASEELSE
				SELECTCASE RAND:3
					CASE 0
						PRINTFORMW 「有反应了！就是那里！」
					CASE 1
						PRINTFORMW 「啊、上钩了」
					CASE 2
						PRINTFORMW 「不要以为你能逃过我的眼睛！」
				ENDSELECT
		ENDSELECT
	;你が釣った
	ELSE
		SELECTCASE TFLAG:193
			;木材
			CASE 610
				PRINTFORMW 「啊哈哈…也有那样的时候。对吧？」
			;守矢之守札
			CASE 131
				PRINTFORML 「好像是守矢神社的守札呢」
				SELECTCASE RAND:2
					CASE 0
						PRINTFORMW 「…但是、为什么会在这种地方？」
					CASE 1
						PRINTFORML 「据说收集几张就可以抽签…」
						PRINTFORMW 「%CALLNAME:MASTER%桑、你试过吗？」
					CASE 2
						PRINTFORMW 「…听说、以前有一只天狗非常沉迷于那个抽签…」
				ENDSELECT
			;跳蛋
			CASE 21
				PRINTFORML 「钓到了什么？请给我看一下…嗯！」
				PRINTFORMW 看到了%CALLNAME:MASTER%手里的椭球形物体、椛面红耳赤地挪開了视线。
			;素材
			CASE 609,606
				PRINTFORML 「哇、看起来是很漂亮的东西呢」
				PRINTFORMW 「可以用来做什么呢…」
			;其他の魚
			CASEELSE
				SELECTCASE RAND:3
					CASE 0
						PRINTFORMW 「哇…上钩了呢！%CALLNAME:MASTER%桑！」
					CASE 1
						PRINTFORMW 「干得漂亮！」
					CASE 2
						PRINTFORML 「哦、太好了！」
						PRINTFORMW 「既然机会难得、趁着鱼新鲜的时候来做菜吧？我去拿酒来！」
				ENDSELECT
		ENDSELECT
	ENDIF
ENDIF
