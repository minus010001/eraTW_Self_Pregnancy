@M_KOJO_MESSAGE_COM_K65_300_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;憤怒
	IF CFLAG:ブチギレ > 0
		PRINTFORML 「……是吗」
		PRINTFORMW 椛把脸扭向另一边。
	
	;荒天かつ屋外
	;この分岐が活きるタイミングってほぼほぼないんじゃなかろうか
	ELSEIF GROUPMATCH(TIME:5,5,9,13) && OUTROOF(CFLAG:MASTER:現在位置)
		PRINTFORMW 「这、这种天气、不用特意在外面说话也可以的！」
	
	;発情
	ELSEIF TCVAR:発情
		IF CFLAG:既成事實 & 合意_诶嘿嘿
			SELECTCASE RAND:2
				CASE 0
					PRINTFORML 「唔、那个…」
					PRINTFORMW 椛看着%CALLNAME:MASTER%、在飘忽不定的眼神中可以感觉出她有什么想要和%CALLNAME:MASTER%好好倾诉一下…
				CASE 1
					PRINTFORML 「……嗯…刚刚、你在说什么来着」
					PRINTFORML 椛心不在焉地听着%CALLNAME:MASTER%说话。
					PRINTFORMW …虽然不知道是不是错觉、不过似乎有的时候股间能感觉到椛的视线。
			ENDSELECT
		;ELSEIF CFLAG:65:好感度 >= 1500
		ELSE
			SELECTCASE RAND:2
				CASE 0
					PRINTFORML 「嗯姆……」
					PRINTFORMW 椛一副坐立不安的样子…
				CASE 1
					PRINTFORML 「……嗯…刚刚、你在说什么来着」
					PRINTFORMW 椛心不在焉地听着%CALLNAME:MASTER%说话…
			ENDSELECT
		ENDIF
	;陪睡中
	;ELSEIF CFLAG:陪睡中
	
	;约会中
	
	
	;大成功
	ELSEIF TFLAG:193 == 1
		SELECTCASE RAND:2
			CASE 0
				PRINTFORML 「那然后呐…」
				PRINTFORMW 椛啪嗒啪嗒地摇着尾巴、愉快地继续说着话。
			CASE 1
				PRINTFORML 「啊哈哈」
				PRINTFORMW 「这个笑话、下次我也可以用吗？」
		ENDSELECT
	
	;会話累積値が閾値を超えての失敗
	ELSEIF TCVAR:302
		PRINTFORML 「…唔、是这样吗」
		PRINTFORMW 好几次听到同样的话题、椛看上去有点心不在焉的样子…
	
	;失敗
	ELSEIF TFLAG:193 < 0
		SELECTCASE RAND:2
			CASE 0
				PRINTFORML 「啊、啊哈哈…」
				PRINTFORMW 椛和颜悦色地笑着…
			CASE 1
				PRINTFORML 「……啊、唔姆、那个…」
				PRINTFORMW 椛急急忙忙地找着话题。
		ENDSELECT
	
	;お仕事中 (1/2で普通のトークへ逃がす)
	ELSEIF CFLAG:行動 == 5 && RAND:2 == 0
		;おなかたぷたぷ
		IF EX:膣内精液 + EX:肛門内精液 >= 100
			CALL PRINT_IMAGE("65_精液垂れ0")
			CALL PRINT_IMAGE("65_精液垂れ1")
			CALL PRINT_IMAGE_ANIMATION("65_精液垂れ",2,3,1,0)
			CALL PRINT_IMAGE("65_精液垂れ5")
			WAIT
			;自覚あり
			IF TCVAR:350
				SELECTCASE RAND:3
					CASE 0
						PRINTFORML （肚子里…还是热热的…）
						PRINTFORMW ……椛心不在焉地伫立着。
					CASE 1
						PRINTFORML （得比平时更加在意别人的视线才行……）
						PRINTFORML 大概是很在意身体内被大量注入的精液吧、
						PRINTFORMW 椛在和%CALLNAME:MASTER%说着话的时候、也不时确认着周围的情况…
					CASE 2
						PRINTFORML 「…好过分啊。今天明明是我的工作日…」
						PRINTFORML 椛有些埋怨地看着%CALLNAME:MASTER%。
						PRINTFORMW ……%CALLNAME:MASTER%也看着椛、椛不由得满脸通红地低下了头。
				ENDSELECT
			;自覚無
			ELSE
				SELECTCASE RAND:2
					CASE 0
						PRINTFORML 「………」
						PRINTFORMW 椛看起来在警戒着周围…
					CASE 1
						PRINTFORML 「……比平时更有杀气…？在说我嘛？」
						PRINTFORMW 「…对不起、是这样的…嗯…因为有件比较在意的事…」
				ENDSELECT
			ENDIF
		ELSE
			SELECTCASE RAND:5
				CASE 0
				PRINTFORMW 「今天也没什么异常呢」
				CASE 1
					PRINTFORML 「巡逻这个工作嘛、空闲时间还是挺多的」
					PRINTFORMW 「有你这个可以聊天的对象真是太好了呢」
				CASE 2
					PRINTFORML 「巡逻的时候会不会用千里眼东张西望…？」
					PRINTFORMW 「才、才没有！才没有这回事哦！」
				CASE 3
					PRINTFORMW 「其实呢、几乎没有人会心怀不轨地来到山上的」
				CASE 4
					PRINTFORMW 「%CALLNAME:MASTER%桑喜欢登山吗？」
			ENDSELECT
		ENDIF	
	;RANDで並列にする普通のトーク
	;5.酒が入っていい気分トーク
	ELSEIF BASE:酒気 >= 400 && RAND:5 == 0
		IF CFLAG:既成事實 & 合意_诶嘿嘿 && EXP:精液経験 >= 20 && RAND:2
			SELECTCASE RAND:2
				CASE 0
					PRINTFORML 「这么说来、以前在酒席上似乎有谈到过关于%CALLNAME:MASTER%桑的事情…」
					PRINTFORMW 「…唔」
					PRINTFORML 「………」
					PRINTFORML 「啊…我的那群白狼天狗同事应该没有给你添什么麻烦吧…是吧！啊哈哈…」
					PRINTFORML 椛有些难为情地笑着。
					PRINTFORMW …到底是什么样的话题呢。
				CASE 1
					PRINTFORML 「唔、好像有些醉了呢…」
					PRINTFORML 「对方是%CALLNAME:MASTER%桑的话、不知不觉就放松下来了…」
					IF TALENT:恋人 && RAND:2
						PRINTFORMW 她应该是在为她自己的事而苦恼着吧…%CALLNAME:MASTER%温柔地抚摸着椛的头。
						CALL HPH_PRINT,@"「嗯、嗯嗯…HPH…呼呼。真的、很舒服呢…」","L"
						PRINTFORMW 「可以再让我这样撒一会儿娇吗…？」
					ELSE
						PRINTFORMW 椛面红耳赤地向%CALLNAME:MASTER%微笑着。
					ENDIF
			ENDSELECT
		ELSE
			SELECTCASE RAND:2
				CASE 0
					PRINTFORML 「呼呼、总感觉心情变得畅快起来了…」
					PRINTFORMW 因酒气而脸色微微红润的椛開心地微笑着。
				CASE 1
					;晴天た夜
					IF GROUPMATCH(TIME:5,0,1) && TIME:2 >= 5 && TIME:2 <= 7
						PRINTFORMW 「举杯对月、这种说法还真是风雅呢」
					ELSEIF GROUPMATCH(TIME:5,8,10,11)
						PRINTFORMW 「唔ー嗯、冬天下雪的时候喝一杯赏雪酒真是再好不过了呢…」
					ELSE
						PRINTFORMW 「和%CALLNAME:MASTER%桑一边喝酒一边聊聊天、感觉也很不错呀…」
					ENDIF
			ENDSELECT
		ENDIF
	;4.お天気トーク
	ELSEIF RAND:4 == 0
		SELECTCASE TIME:5
			;晴天,晴朗
			CASE 0,1
				SELECTCASE RAND:2
					CASE 0
						IF TIME:2 >= 5 && TIME:2 <= 7
							PRINTFORMW 「我也很喜欢就这样仰望星空」
						ELSE
							PRINTFORMW 「天气好的话心情也会随之变得舒畅起来呢」
						ENDIF
					CASE 1
						PRINTFORML 「呼啊…」
						PRINTFORML 椛捂着嘴、打了一个可爱的哈欠。
						PRINTFORMW 「啊、不好意思、今天天气真不错啊…」
				ENDSELECT
			;多雲り
			CASE 2
				IF TIME:2 >= 5 && TIME:2 <= 7
					PRINTFORMW 「明月浮于薄雾之上、这样也很不错啊」
					SIF CFLAG:2 >= 1000
						PRINTFORMW 「…机会难得、不如来一次赏月酒吧？」
				ELSE
					PRINTFORMW 「真是微妙的天气啊…」
				ENDIF
			;陰天
			CASE 3
				SELECTCASE RAND:2
					CASE 0
						IF TIME:2 >= 5 && TIME:2 <= 7
							PRINTFORMW 「云这么厚、连夜晚的天空也看不见了」
						ELSE
							PRINTFORMW 「天阴沉沉的呢ー」
						ENDIF
					CASE 1
						PRINTFORML 「但愿不要下雨…」
						PRINTFORMW 椛仰望着乌云密布的天空。
				ENDSELECT
			;雨、大雨、霧雨
			CASE 4,5,6
				SELECTCASE RAND:3
					CASE 0
						PRINTFORMW 「雨下得好大啊…」
					CASE 1
						PRINTFORMW 「下雨的时候、一想到要出门就感觉好麻烦啊…」
					CASE 2
						IF DAY:5 == 5
							PRINTFORMW 「真的就像是水桶被打翻一样的倾盆大雨啊…」
						ELSE
							PRINTFORMW 「湿气太重的话尾巴也会变得很潮湿闷热呢」
						ENDIF
				ENDSELECT
			;霧
			CASE 7
				SELECTCASE RAND:2
					CASE 0
						PRINTFORML 「真是很浓的雾啊」
						PRINTFORMW 「请十分注意脚下哦、%CALLNAME:MASTER%桑」
					CASE 1
						PRINTFORML 「我吗？这种程度的雾对我来说无足轻重哦」
						PRINTFORML 「白狼天狗的千里眼可不是摆设哦！」
						PRINTFORM 椛的
						SIF TALENT:胸圍 > 0
							PRINTFORM 和小小的身体不相称的丰满胸部
						SIF TALENT:胸圍 < 0
							PRINTFORM 和小小的身体相符的胸部
						SIF TALENT:胸圍 == 0
							PRINTFORM 和小小的身体相比起来发育得格外良好的胸部
						PRINTFORMW 自豪地挺起来了。
				ENDSELECT
			;雪、細雪、霧雪
			CASE 8,10,11
				SELECTCASE RAND:2
					CASE 0
						PRINTFORML 「每次一到下雪的时候、心里就会感到不可思议的激动」
						PRINTFORMW 「…这是为什么呢」(因为是狗狗的原因啦)
					CASE 1
						PRINTFORML 「下了这场雪之后看来要去铲雪才行了」
						PRINTFORMW 椛不知为何看起来很開心…。
				ENDSELECT
			;吹雪
			CASE 9
				SELECTCASE RAND:2
					CASE 0
						PRINTFORML 「每次一到下雪的时候、心里就会感到不可思议的激动」
						PRINTFORMW 「…这是为什么呢」(因为是狗狗的原因啦)
					CASE 1
						PRINTFORML 「正在刮暴风雪呢…」
						PRINTFORMW 「但愿不会有遇难者」
				ENDSELECT
			;雨雪交加
			CASE 12
				PRINTFORML 「现在的天气是雨夹雪啊」
				PRINTFORMW 「夹杂在雪中的雨水能不能全都变成雪呢…」
			;冰雹
			CASE 13
				PRINTFORML 「哇啊、外面下冰雹了…」
				PRINTFORMW 「现在最好不要外出哦…」
		ENDSELECT
	;3.季節トーク
	ELSEIF RAND:3 == 0
		SELECTCASE DAY:2
			;春
			CASE 1
				SELECTCASE RAND:2
					CASE 0
						PRINTFORML 「春天到了呢。一到春天、山上的各个地方就会陆陆续续地绽開花朵」
						PRINTFORML 「即使是同样的花、生长的地方不同的话開花的时间也会不同…」
						PRINTFORMW 「这也是巡逻时的一个小小乐趣吧」
					CASE 1
						PRINTFORMW 「像这样不冷不热的天气真是舒服呀」
				ENDSELECT
			;夏
			CASE 2
				SELECTCASE RAND:2
					CASE 0
						PRINTFORML 「我空闲时偶尔去山脚下的河童那里、有时候她们会请我吃黄瓜」
						PRINTFORMW 「在河水里冰过的黄瓜简直是绝品」
					CASE 1
						PRINTFORML 「好热啊ー…」
						PRINTFORMW 椛的尾巴无力地垂着。
				ENDSELECT
			;秋
			CASE 3
				SELECTCASE RAND:2
					CASE 0
						PRINTFORML 「当山峦也被染上秋天的颜色时、就会有种秋天来临的感觉呢」
						PRINTFORMW 「巡逻的时候不能东张西望…不行！不可以！」
					CASE 1
						PRINTFORML 「秋天是一个很棒的季节呢。生活度过得很愉快舒适、最重要的是有好多好吃的」
						PRINTFORMW 「呼呼、今天的晚餐吃什么比较好呢…」
				ENDSELECT
			;冬
			CASE 4
				SELECTCASE RAND:2
					CASE 0
						PRINTFORMW 「虽说已经习惯了巡逻工作、但还是觉得好冷啊…」
					CASE 1
						PRINTFORMW 「太干燥的话、尾巴上就会积有静电…」
				ENDSELECT
		ENDSELECT
	;2.親密トーク
	ELSEIF RAND:2 == 0 && CFLAG:2 >=1000
		SELECTCASE RAND:5
			CASE 0
				IF BASE:MASTER:TSP >= 1000
					PRINTFORML 「说起来、你有的时候会突然瞬间移动？是怎么一回事呢…」
					PRINTFORML 「是使用了什么特殊的魔法吗？」
					PRINTFORMW …因为被追究这种不必要的问题也很麻烦、所以就敷衍过去了。
				ELSE
					PRINTFORMW 「老实说、巡逻的工作也挺悠闲的…」
			ENDIF
			CASE 1
				PRINTFORML 「老实说、巡逻的工作也挺悠闲的…」
				PRINTFORMW 「以后也可以多来找我聊聊天吗？」
			CASE 2
				PRINTFORML 「文桑吗？唔ー嗯、嘛、该怎么说才好呢…」
				PRINTFORMW 椛一副含糊其辞的模样。
			CASE 3
				PRINTFORML 「和荷取桑关系如何吗？嗯姆、关系很好哦」
				PRINTFORMW 「就是有的时候会被拉过去做一些意想不到的发明测试、真是让人困扰呢…」
			CASE 4
				PRINTFORML 「极桑？唔嗯嗯、这个、该怎么说呢…」
				PRINTFORMW 椛一副含糊其辞的模样。
		ENDSELECT
	;1.通常トーク
	ELSE
		SELECTCASE RAND:6
			CASE 0
				PRINTFORML 「啊！那个、不好意思」
				PRINTFORML 椛从怀里拿出手账、開始奋笔疾书着什么…
				PRINTFORMW 「没什么、只是想到了将棋的新规则而已」
			CASE 1
				PRINTFORML 「我经常和河童们下将棋」
				PRINTFORMW 「为了能玩得更久一点、所以制定了一些自己的规则」
			CASE 2
				PRINTFORML 「巡逻的工作能充分发挥千里眼和嗅觉的作用、
				PRINTFORMW 　对我这种白狼天狗来说是再好不过的了」
			CASE 3
				PRINTFORMW 「我也是天狗、就算对手是妖怪、我也不会落下风哦！」
			CASE 4
				PRINTFORMW 「这里也没有什么大的骚动、真是平静啊」
			CASE 5
				PRINTFORMW 「以前山上也发生过一场大动乱呢…」
		ENDSELECT
	ENDIF
ENDIF
RETURN 1
