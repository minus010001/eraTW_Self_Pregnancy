					PRINTFORMW 椛谄媚地摇晃着身体、索求%CALLNAME:MASTER%的嘴唇…
				ELSE
					CALL HPH_PRINT,@"「%K65_AEGI()%」","L"
					CALL HPH_PRINT,@"「%K65_AEGI("控えめ",2)%、噗啊…%CALLNAME:MASTER%桑…HPH」","L"
					PRINTFORMW 椛眼睛湿润、偷瞄着%CALLNAME:MASTER%的脸…
				ENDIF
			ELSE
				CALL HPH_PRINT,@"「%K65_AEGI("控えめ")%」","L"
				CALL HPH_PRINT,@"「%K65_AEGI("控えめ",1)%、噗啊…%CALLNAME:MASTER%桑…」","L"
				IF TALENT:恋慕 && RAND:2
					PRINTFORMW 「你对我很温柔呢…我、很高兴…」
				ELSE
					PRINTFORMW 「我、我…没关系、的、所以……请动起、来吧…」
				ENDIF
			ENDIF
			RETURN 1
		;イキすぎ
		ELSEIF EX:Ｖ絶頂 > 15 && BASE:体力 < MAXBASE:体力/2 && BASE:気力 <= 0
			;SELECTCASE RAND:2
				;CASE 0
					CALL HPH_PRINT,@"「哈啊…嗯…%K65_AEGI("控えめ")%」","L"
					PRINTFORML 椛带着荡漾的表情接受了%CALLNAME:MASTER%的亲吻与插入的阴茎…
					CALL HPH_PRINT,@"「%K65_AEGI()%」","L"
					PRINTFORMW 两个人一边交换着彼此的体温、一边享受着片刻充满幸福的时光…
				;CASE 1
			;ENDSELECT
			RETURN 1
		;挿入中かつ発情
		ELSEIF TCVAR:発情
			;後背位、背面座位でない
			IF !GROUPMATCH(TEQUIP:体位２,2,5)
				SELECTCASE RAND:2
					CASE 0
						CALL HPH_PRINT,@"「%K65_AEGI()%」","W"
					CASE 1
						CALL HPH_PRINT,@"「%K65_AEGI()%」","L"
						CALL HPH_PRINT,@"「嗯…呼呜HPH…%K65_AEGI("控えめ")%」","L"
						PRINTFORML 舌头交缠的浓厚深吻结束后、又開始像下雨一般不断多次亲吻着脸。
						PRINTFORMW 发情的椛无比甘甜的唾液的香气侵犯着鼻腔、让阴茎变得更加坚硬。
						CALL HPH_PRINT,@"「嗯嗯HPH嗯…HPH…%K65_AEGI("控えめ")%」","L" 
						PRINTFORMW 椛因为对%CALLNAME:MASTER%的反应非常高兴、所以更加热情地反复接着吻…
