					CASE 1
						CALL HPH_PRINT,@"「%K65_AEGI(,4)%…～～～HPH」","L"
						PRINTFORML 一边用力插到最里面一边接吻的话、膣壁就会紧紧收缩起来
						PRINTFORML 煽动着%CALLNAME:MASTER%的射精欲。
						CALL HPH_PRINT,@"「嗯呜呜呜HPH%K65_AEGI()%」","W"
				ENDSELECT
			ENDIF 
		;挿入中かつ非発情
		ELSE
			;後背位、背面座位でない
			IF TEQUIP:体位２ != 2 && TEQUIP:体位２ != 5
				SELECTCASE RAND:2
					CASE 0
						CALL HPH_PRINT,@"「%K65_AEGI()%」","W"
					CASE 1
							CALL HPH_PRINT,@"「%K65_AEGI(,2)%…%CALLNAME:MASTER%桑…HPH」","L"
							PRINTFORML 椛抱着%CALLNAME:MASTER%、一边交缠着舌头\@ TALENT:恋慕&&RAND:2 ? 一边发出幸福的吐息 # 一边眯着眼睛 \@…
							CALL HPH_PRINT,@"「%K65_AEGI("控えめ",2)%…%K65_AEGI(,3)%」","W"
				ENDSELECT
			;後背位or背面座位
			ELSE
				SELECTCASE RAND:2
					CASE 0
						CALL HPH_PRINT,@"「%K65_AEGI()%」","W"
					CASE 1
						CALL HPH_PRINT,@"「哈啊、啊…%K65_AEGI(,2)%」","L"
						PRINTFORML %CALLNAME:MASTER%像是要把椛娇小的身体整个压在身下一般弯下了腰、
						PRINTFORML 一边搅弄着她的狭窄阴道一边接吻…
						CALL HPH_PRINT,@"「%K65_AEGI("控えめ",2)%…%K65_AEGI(,3)%」","L"
				ENDSELECT
			ENDIF
		ENDIF
		RETURN 1
	;通常
	ELSE
		IF TCVAR:発情
			IF TALENT:恋慕 && RAND:2
				SELECTCASE RAND:2
					CASE 0
						CALL HPH_PRINT,@"「%K65_AEGI()%」","L"
						PRINTFORML 椛用双手按着%CALLNAME:MASTER%的脸颊、专心致志地用舌头彼此纠缠…
						CALL HPH_PRINT,@"「唔呣、呼ー%CALLNAME:MASTER%桑…HPH%K65_AEGI(,3)%」","W"
						PRINTFORML ……即使想挪開脸、被抓住的脸颊也会被用力按住而动弹不得。
						PRINTFORML 这与其说是接吻、也许不说是捕食更为贴切
						CALL HPH_PRINT,@"「…%K65_AEGI(,2)%…HPH…%K65_AEGI(,2)%」","W"
					CASE 1
						CALL HPH_PRINT,@"「%K65_AEGI(,2)%…呼HPH…%K65_AEGI(,2)%」","L"
						PRINTFORML 椛一边喘气、一边索求%CALLNAME:MASTER%的唾液…
				ENDSELECT
			ELSE
				SELECTCASE RAND:2
					CASE 0
						CALL HPH_PRINT,@"「%K65_AEGI()%」", "W"
					CASE 1
						CALL HPH_PRINT,@"「嗯、啾…%K65_AEGI("控えめ,2")%…%K65_AEGI(",2")%」","L"
						CALL HPH_PRINT,@"「%CALLNAME:MASTER%桑、还要…HPH%K65_AEGI("控えめ,4")%」","W"
				ENDSELECT
			ENDIF
		ELSE
			IF TALENT:恋慕 && RAND:2
				SELECTCASE RAND:2
					CASE 0
						CALL HPH_PRINT,@"「%K65_AEGI(,3)%」", "L"
						PRINTFORML 「呃…那个、请不要…一直看着…」
						PRINTFORMW 「我现在一定是一脸不像样子的表情…」
					CASE 1
						CALL HPH_PRINT,@"「%K65_AEGI()%」", "L"
						PRINTFORML 「ふ呵呵。明明都接过\@ EXP:接吻経験>20 ? 好多次 # 几次 \@吻了、但到现在却还是每次都会心跳加速。」
						CALL HPH_PRINT,@"「……好啦。再来、亲一次吧…唔、%K65_AEGI("控えめ",2)%」","L"
						PRINTFORMW %CALLNAME:MASTER%和椛拥抱在一起、感受着彼此的心跳、慢慢地接吻…
				ENDSELECT
			ELSE
				SELECTCASE RAND:2
					CASE 0
						CALL HPH_PRINT,@"「%K65_AEGI()%」", "W"
					CASE 1
						CALL HPH_PRINT,@"「%K65_AEGI(,2)%…唔…」", "L"
						PRINTFORMW 一边接吻一边抚摸着顺滑的头发、椛\@ TALENT:恋慕 && RAND:2 ? 用满含春意的目光注视着这边 # 開心地微笑着 \@。
				ENDSELECT
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1


