;100,打屁股
;==================================================
@M_KOJO_MESSAGE_COM_K111_100
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K111_100_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K111_100_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM) && !FLAG:時間停止
		PRINTFORMW 「啊咦っ！好痛！！」
		PRINTFORMW 「为什么……要打我……」
		RETURN 1
	ENDIF
	IF FLAG:時間停止
	ELSE
		IF EXP:111:苦痛快楽経験 >= 50
			PRINTFORMW 「啊啊！ 不要っ、饶了我吧，是我错了……」
		ELSE
			PRINTFORMW 「好痛啊！ 为什么……%CALLNAME:MASTER%住手啊……」
		ENDIF
	ENDIF
ENDIF
RETURN 1

