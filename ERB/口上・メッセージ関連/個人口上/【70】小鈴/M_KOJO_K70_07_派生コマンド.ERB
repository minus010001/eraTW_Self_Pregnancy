@M_KOJO_MESSAGE_SCOM_K70_9_1
#DIM CHARA
CHARA = MASTER_POSE(4,1,1)
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = CFLAG:70:1006
;-------------------------------------------------
IF LOCAL
	IF CHARA == 80
		SELECTCASE M_KOJO_K70_BALANCE(70, 80)
		CASE 8, 5
			SELECTCASE RAND:2
			CASE 1
				PRINTFORMW 「阿求，一起来让%CALLNAME:MASTER%先生舒服起来吧%UNICODE(0x2764)%」
			CASEELSE
				PRINTFORMW 「呵呵%UNICODE(0x2764)% 和阿求一起做%UNICODE(0x2764)%」
			ENDSELECT
		CASE 7, 6
			SELECTCASE RAND:2
			CASE 1
				PRINTFORMW 「那个，那个阿求！ 来比一比谁先让%CALLNAME:MASTER%先生舒服起来吧…！」
			CASEELSE
				PRINTFORMW 「嗯嗯……只有阿求太狡猾了……！即使是对我来说……！」
			ENDSELECT
		CASE 4, 3
			SELECTCASE RAND:2
			CASE 1
				PRINTFORMW 「我会比阿求更能让你舒服起来的！」
			CASEELSE
				PRINTFORMW 「我不会输给阿求的…っ！ 哼哼%UNICODE(0x2764)%」
			ENDSELECT
		ENDSELECT
	ELSE
		SELECTCASE RAND:2
		CASE 1
			PRINTFORMW 「嗯嗯……%CALLNAME:CHARA%先生总是很狡猾……！即使对我来说……！」
		CASEELSE
			PRINTFORMW 「嗯ー！ %CALLNAME:CHARA%先生，也请给我吧……っ！」
		ENDSELECT
