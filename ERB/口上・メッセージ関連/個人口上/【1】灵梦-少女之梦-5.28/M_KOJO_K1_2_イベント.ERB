	;気まずそうに立ち去る
	;看到你先生的交合尴尬的走开了
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG:1 == 3
		PRINTFORML
		RETURN 1
	ENDIF
	;その他
	PRINTFORML 
ENDIF
RETURN 1

