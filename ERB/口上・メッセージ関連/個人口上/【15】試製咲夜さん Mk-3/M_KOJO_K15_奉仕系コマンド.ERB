;80,手淫
;==================================================
@M_KOJO_MESSAGE_COM_K15_80
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K15_80_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K15_80_1
;-------------------------------------------------
;填写检查（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 时间停止中，一手抓住咲夜如青葱一般又细又白的小手摩擦着阴茎
		PRINTFORMW 被柔滑又娇嫩的手侵犯，%CALLNAME:MASTER%非常兴奋
		PRINTFORMW 为了追求更多的快感、调整着咲夜的手指，紧紧的缠绕在阴茎上……
		RETURN 1
	;恋慕
	ELSEIF TALENT:恋慕
		PRINTFORMW 「要用手做？」
		PRINTFORMW 咲夜问着、两手涂上口水，一手握住肉棒上下套弄，一手托着睾丸轻轻地揉
		PRINTFORMW 小小的嘴含住亀頭转来转去的舔着……
		PRINTFORMW 「嗯……什么时候出来都可以噢」
		RETURN 1
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		CALL HPH_PRINT("「手淫吗、我明白了HPH」", "L")
		PRINTFORMW 咲夜一边说、一边集中口水垂落在肉棒上、両手用巧妙的力道上下套弄
		PRINTFORMW 一口含住亀頭、舌头毫不厌倦的爱抚鈴口……
		RETURN 1
	;それ以外
	ELSE
		PRINTFORMW 「手、是吗……我知道了」
		PRINTFORMW 咲夜把口水涂在双手上，均匀的抹開、
		PRINTFORMW 一手握住肉棒、另一手轻轻的揉着睾丸
		PRINTFORMW 「……」
		PRINTFORMW 咲夜冷淡的为%CALLNAME:MASTER%做性処理……
		RETURN 1
	ENDIF
ENDIF
RETURN 1

