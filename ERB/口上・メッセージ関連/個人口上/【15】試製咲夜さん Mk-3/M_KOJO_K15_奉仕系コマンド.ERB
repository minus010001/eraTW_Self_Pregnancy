LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW %CALLNAME:MASTER%把咲夜的嘴当做自慰套使用
		PRINTFORMW 抓住时停中静止不动的咲夜的下巴、强行把嘴张開
		PRINTFORMW %CALLNAME:MASTER%就这样毫不考虑后果用阴茎突入、無防備的舌头迎接阴茎的进入
		PRINTFORMW %CALLNAME:MASTER%的阴茎感受着嘴里柔软又湿滑的快感、对稍微吸附上来的咲夜的口穴非常满意……
		PRINTFORMW 于是%CALLNAME:MASTER%就这样用腰拍打着咲夜的脸，坚挺的阴茎深深地侵犯咲夜的喉咙深处
		RETURN 1
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		IF PREVCOM == 81
			SELECTCASE RAND:2
				CASE 0
				CALL HPH_PRINT("「啾咕HPH 啾噜HPH 咧噜HPH」", "L")
				PRINTFORMW 咲夜一心一意的用阴茎把嘴塞满……
				CASEELSE
				CALL HPH_PRINT("「嗯… 咕噗、嗯……HPH」", "L")
				PRINTFORMW 咲夜让肉棒探进喉咙的深处、不但使用舌根、整个喉咙包覆着亀頭愛撫起来
				ENDSELECT
		ELSE
		CALL HPH_PRINT("「失礼了……HPH」", "L")
		PRINTFORML 分開%CALLNAME:MASTER%的双腿、咲夜一边说一边開始侍奉
		CALL HPH_PRINT("「嗯……哈啊、嘶～～……HPH」", "L")
		CALL HPH_PRINT("（好浓的味道……HPH 让我成为雌性、“男人”的气味……HPHHPH）", "L")
		PRINTFORML 用嘴拉下裤子的拉链、阴茎露出后散发的浓厚气味被深深地吸入肺中、咲夜流露出陶醉的表情
		CALL HPH_PRINT("「那么……開动了HPH 哈噗HPH 嗯、呣呜……HPH」", "L") 
		PRINTFORML 就这样吸吮着亀頭、冠状沟也用舌头细心的清扫，之后深深地吞入直到肉棒根部……
		RETURN 1
		ENDIF
	;恋慕
	ELSEIF TALENT:恋慕
		PRINTFORMW 「嗯、咧噜……如何？舒服吗？嗯啾」
		PRINTFORMW 对把脸埋进%CALLNAME:MASTER%大腿中间侍奉的咲夜表示感觉很舒服
		PRINTFORMW 咲夜一边用阴茎把嘴塞满一边開心地微笑
		RETURN 1
	;それ以外
	ELSE
		SELECTCASE RAND:2
			CASE 0
			PRINTFORMW 拜托咲夜帮我口交
			PRINTFORMW 「嗯……嗯啾……咧噜……」
			PRINTFORMW 确实咲夜的口交感觉很舒服，不过感受不到热情
			PRINTFORMW 咲夜就这样机械性的侍奉着……
			CASEELSE
			PRINTFORMW 「唔……有好好的洗过吗、这个？」
			PRINTFORMW 咲夜的脸靠近阴茎后、男性器特有的味道令她皱起了眉头
			PRINTFORMW 「……我知道了啦。……嗯、咧噜……唔……啾……」
			PRINTFORMW 看着阴茎的咲夜，短暫的停止不动、在%CALLNAME:MASTER%的催促下開始侍奉……
			ENDSELECT
	RETURN 1
	ENDIF
ENDIF
RETURN 1

