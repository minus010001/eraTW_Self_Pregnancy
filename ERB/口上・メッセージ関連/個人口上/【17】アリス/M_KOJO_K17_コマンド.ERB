RETURN 1

@M_KOJO_MESSAGE_COM_K17_189_1
#DIM iType = 0
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;タイプ保存
	;-------------------------------------------------
	IF TEQUIP:新妻PLAY
		;拘束のみが入った時　つまり装着した時タイプ一時保存
		iType = CFLAG:K17C_ID:K17C_CF_喘ぎタイプ
	ELSE
		;拘束が入ってないでここ来た場合は外した時
		;縄ありなら先に解除しないとこっちもできないみたい
		CFLAG:K17C_ID:K17C_CF_喘ぎタイプ = iType
	ENDIF
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初回　装着時
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(@"{SELECTCOM}口上") && TEQUIP:新妻PLAY
		;初回　付けた時
		PRINTFORML 「圍裙？把这个穿上就可以了么？」
		
		PRINTFORMDL %CALLNAME:MASTER%拜托%CALLNAME:TARGET%当新妻
		PRINTFORML 「新妻……%K17_BLUSH()%　知道啦、就配合你吧」
		
	ELSEIF TEQUIP:新妻PLAY
		;二回目以降　付けた時
		PRINTFORML 「又要%K17_RS("用围裙了/假扮新妻了")%么……%K17_HEART(2)%」
	ELSE
		;外した時
		PRINTFORML 「呼唔……%K17_RS("已经结束了么？/已经够了么？")%」
		RETURN 1
	ENDIF
	
	;現在的服装検査
	CALLF K17_GET_CLOTHNAME_TOP(TARGET,1)
	IF RESULT == 2
		;服着てる
		PRINTFORMDL %CALLNAME:TARGET%在%K17_GET_CLOTHNAME_TOP()%之上套上了围裙
		PRINTFORML 「嗯唔、%K17_RS("很普通/这不是一直做家务的装扮")%呢」
	ELSEIF RESULT == 1
		;内衣のみ
		PRINTFORMDL %CALLNAME:TARGET%在%K17_GET_CLOTHNAME_TOP_UNDER()%之上穿上了
		PRINTFORML 「总、总觉得、%K17_RS("冷飕飕的/感觉怪怪的")%……喜欢这种样子么？」
		PRINTFORML （总感觉、比裸体还要H……）
	ELSE
		;上半身裸
		PRINTFORMDL %CALLNAME:TARGET%裸体穿上了围裙
		PRINTFORML 「%K17_RS("虽然在书上看到过/虽然听说过是有人喜欢这样")%……没想到、还真有呢……」
		PRINTFORML （总感觉、比裸体还要H……）
	ENDIF
	
	
ENDIF
RETURN 1
