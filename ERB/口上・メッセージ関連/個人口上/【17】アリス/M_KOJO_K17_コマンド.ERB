	CALL TRAIN_MESSAGE
	CALL M_KOJO_MESSAGE_COM_K17_睡眠
ELSE
	CALL TRAIN_MESSAGE
	CALL M_KOJO_MESSAGE_COM_K17_189_1
	PRINTW
