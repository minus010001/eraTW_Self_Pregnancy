IF LOCAL
	CALL K17_SET_AUTO_PVOICE(K17_GRT_EX(),K17C_性技_胸)
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && K17_GRT(TARGET) >= K17C_恋慕 && CFLAG:K17C_CF_喘ぎタイプ == K17C_受け_おねだり系 && FIRSTTIME(@"{SELECTCOM}口上",1)
		CALLF FIRSTTIME(@"{SELECTCOM}口上")
		;性別判定
		IF TALENT:MASTER:性別 == 2
			PRINTFORML 「用胸部夹住就行了么……？　我知道的、男人都喜欢这样的吧？」
		ELSE
			;フタナリの女
			PRINTFORML 「用胸部夹住就行了么……？」
		ENDIF
	ENDIF
	;二回目以降
	IF !(CFLAG:K17C_CF_喘ぎタイプ & K17C_喘ぎF_襲い)
		;ねだり系
		;汎用　性技判定
		CALL K17_MESSAGE
		CALL K17_CLL_COUNTER_REACTIONMESSAGE
		
		;乳夾口交の時喘ぎ箇所を変える
		IF TCVAR:Counter行動的派生 == 4
			CALL K17_SET_AUTO_PVOICE(-1,K17C_部位M_口交)
		ELSE
			SELECTCASE ABL:TARGET:胸
				CASE IS < 2
					;PRINTL あんまり気持ちよくない…
					PRINTFORML 「%K17_SPLIT_SHUFFLE(@"这个%K17_RS(",还真是有些困难呐/、做起来还真难呢")%…\\%K17_AP_NUM(2)%\\%K17_AP_NUM(1)%","\\")%%K17_AP_NUM(1,1)%」
				CASE IS  > 2
					;PRINTFORML %CALLNAME:TARGET%はこなれた奉仕で%CALLNAME:PLAYER%を悦ばせた…
					PRINTFORML 「%K17_SPLIT_SHUFFLE(@"这个%K17_RS(",很舒服吧/、是不是有所长进了呢")%？　…\\%K17_AP_NUM(2)%\\%K17_AP_NUM(1)%","\\")%%K17_AP_NUM(1,1)%」
				CASEELSE
					;PRINTFORML %CALLNAME:TARGET%の奉仕は%CALLNAME:PLAYER%を気持よくさせている…
					PRINTFORML 「%K17_SPLIT_SHUFFLE(@"这个%K17_RS(",也许已经习惯了呢/、已经很擅长了呢")%…\\%K17_AP_NUM(2)%\\%K17_AP_NUM(1)%","\\")%%K17_AP_NUM(1,1)%」
			ENDSELECT
		ENDIF
	ELSE
		;襲い系
		PRINTFORMDL %CALLNAME:MASTER%将%TINKO(PLAYER)%插入%CALLNAME:TARGET%的谷间不断扭动着
		;Counter追加
		IF TCVAR:Counter行動的派生
			PRINTFORMDL 顺势对%CALLNAME:TARGET%张開的小嘴突刺了进去
			CALL K17_SET_AUTO_PVOICE(-1,K17C_性技_舌)
			TCVAR:Counter行動的派生 = 0
		ENDIF
	ENDIF
	
	PRINTFORML 「%K17_AUTO_PVOICE()%」
	PRINTW
	;汎用　感覚判定
	PRINTFORMDL %CALLNAME:MASTER%的%TINKO(PLAYER)%在%OPPAI_DESCRIPTION(TARGET)%%K17_RS("刺激下/摩擦着")%、在侍奉的%CALLNAME:TARGET%看上去也很舒服得样子
	CALL K17_SET_AUTO_PVOICE(K17_GRT_EX(),K17C_部位B)
	PRINTFORML 「%K17_AUTO_PVOICE()%」
	
ENDIF
RETURN 1

