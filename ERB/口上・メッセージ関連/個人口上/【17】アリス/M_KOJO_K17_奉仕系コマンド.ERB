@M_KOJO_MESSAGE_COM_K17_80_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	CALL K17_SET_AUTO_PVOICE(K17_GRT_EX(),K17C_性技_指)
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && K17_GRT(TARGET) >= K17C_恋慕 && CFLAG:K17C_CF_喘ぎタイプ == K17C_受け_おねだり系 && FIRSTTIME(@"{SELECTCOM}口上",1)
		PRINTFORML 「用手么……知道了、我试试吧……但是……」
		PRINTFORML 「……看过书有过了了解、要摸这个的话、明明还是感觉很肮脏……」
		PRINTFORML 「但是一想到%K17_CNAME()%……就觉得好可爱呐……」
		PRINTFORMDL %CALLNAME:TARGET%温柔的抚摸着%CALLNAME:MASTER%的%TINKO(PLAYER)%
		PRINTW
		PRINTFORML 「……那个、这样的感觉……嗯唔……怎样呢？」
		;通常あるけど一応
		IF ABL:指 >= 5
			PRINTFORML 「嗯唔%K17_HEART()%　嗯唔%K17_HEART()%　唔%K17_HEART()%　……」
			PRINTFORMDL %CALLNAME:TARGET%的指法杀伤力了得……
			PRINTFORMDL %CALLNAME:MASTER%舒服得腰都挺直了起来
			PRINTW
			PRINTFORML 「诶？　诶？　那就么……这样、就可以了么？」
			PRINTFORMDL %CALLNAME:TARGET%依偎在%CALLNAME:MASTER%的身旁不安的询问着
			PRINTFORMDL 在此期间手指也在无规则的运动着、给与%CALLNAME:MASTER%无比的快感
			PRINTFORML 「总感觉……稍微有些自信了……」
			PRINTW
			CALLF FIRSTTIME(@"{SELECTCOM}口上")
		ENDIF
		;汎用　性技判定
		CALL K17_MESSAGE
		CALL K17_CLL_COUNTER_REACTIONMESSAGE
	ELSE
		;二回目以降
		IF CFLAG:K17C_CF_喘ぎタイプ & K17C_喘ぎF_襲い
			;襲い系
			PRINTFORMDL %CALLNAME:TARGET%握住了%TINKO(PLAYER)%
			IF TCVAR:Counter行動的派生
				PRINTFORMDL %CALLNAME:MASTER%将%TINKO(PLAYER)%抵入了%CALLNAME:TARGET%膣穴的同时晃动起了腰……
				TCVAR:Counter行動的派生 = 0
			ENDIF
		ELSE
			;合意系
			;汎用　性技判定
			CALL K17_MESSAGE
			CALL K17_CLL_COUNTER_REACTIONMESSAGE
		ENDIF
	ENDIF
	PRINTFORML 「%K17_AUTO_PVOICE()%」
ENDIF
RETURN 1

