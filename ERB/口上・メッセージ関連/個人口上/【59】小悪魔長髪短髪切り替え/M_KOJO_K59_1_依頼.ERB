	IF CANNOT_DANMAKU() == 1
	;屋内のとき（勝負できない）
	;【◆口上】
	;【W 屋外で挑もう…】
		PRINTFORMW 「なんだかわかりませんが、仕方ない、やりますか」
		PRINTFORMW 「…って、室内だと危ないですって?巫女が乗り込んできた時は普通にやったというのに?」
		RETURN 1
	ELSEIF CANNOT_DANMAKU() == 2
	;仕事中（勝負できない）
	;【◆口上】
	;【W お取り込み中だ…】
		PRINTFORMW 「弾幕勝負?今は…見てわかりませんか?」
		RETURN 1
	ELSEIF CANNOT_DANMAKU() == 3
	;你が疲労中（勝負できない）
	;【◆口上】
	;【W 今は疲れている…】
		PRINTFORMW 「%MASTERNAME:59%…你、どこからどう見ても疲れてますよ」
		;関係性次第で、「貴方…你～」というセリフになるが気にして是けない
		RETURN 1
	ELSEIF CANNOT_DANMAKU() == 4
	;口上主が疲労竜（勝負できない）
	;【◆口上】
	;【W 今挑むのはフェアじゃない…】
		PRINTFORMW 「ん?弾幕勝負?いやあ、私の今の姿、よく見てくださいよ…」
		RETURN 1
	ELSEIF CANNOT_DANMAKU() == 5
	;既に今日一度弾幕勝負をしている（勝負できない）
	;【◆口上】
	;【W 今日はもう挑めない…】
		PRINTFORMW 「何度もやる気にはなりませんよ」
		RETURN 1
	ELSE
	;【◆口上】
	;【W 勝負開始！！】
	;【以降はそのまま弾幕口上が呼ばれます】
		PRINTFORMW 「弾幕勝負?しかもあるお方に代わって?ナンデ?」
		PRINTFORMW 「…まあいいです、相手しますよ」
		RETURN 1
	ENDIF
	;【▼そのまま下の口上へ】
CASE "依頼実行直後"
;○この依頼では討伐される側の口上です
	IF CFLAG:CLIENT_ID:依頼状況 == 2
		IF CANNOT_DANMAKU() == 0
			;败犬
			;【▲上の口上から続き】
			;【L ［彼女］の討伐に失敗した……】
			;【W ……こうなったら後日出直すしかない】
			;【◆口上】
			PRINTFORMW 「二時間前に出直してきなさい!」
			PRINTFORMW 「…言ってみたかったんですよ、これ」
			RETURN 1
		ELSE
			;挑めなかった場合
			;【▲上の口上から続き】
			;【◆口上】
			PRINTFORMW 「後にしましょう。後に」
			RETURN 1
		ENDIF
	ELSEIF CFLAG:CLIENT_ID:依頼状況 == 3
	;勝利
	;【▲上の口上から続き】
	;【W ［彼女］を懲らしめた！】
	;【◆口上】
		PRINTFORMW 「ぐえー!」
		PRINTFORMW 「…%MASTERNAME:59%をよこした方には、「小悪魔は美しく倒れた」とお伝えください」
		RETURN 1
	ENDIF
CASE "成功報告時"
;報酬：賞金（時価）
	PRINTFORMW 「なるほど…そうでしたか」
	PRINTFORMW 「これは参考になりそうです。ありがとうございました!」
	RETURN 1
CASE "失敗報告時"
	PRINTFORMW 「成果は得られませんでしたか…残念です」
	RETURN 1
;CASE "依頼報告不要"
;＜この依頼にはありません＞
;	RETURN 0
ENDSELECT

