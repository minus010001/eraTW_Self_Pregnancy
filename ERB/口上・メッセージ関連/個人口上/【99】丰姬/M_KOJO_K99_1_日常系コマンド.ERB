	IF TFLAG:193 == 0
		;用意したけどここには飛ばない
		FOR COUNT, 1, 11
			IF ITEM:(100 + COUNT) == 1
				IF TALENT:恋慕
					PRINTFORMW 「如果缺什么的话，可以问问我」
				ELSEIF TALENT:思慕
					PRINTFORMW 「好学是件好事」
				ELSE
					PRINTFORML 丰姬留在了外面的商店街
					PRINTFORMW 「不怎么喜欢这种封闭的环境，感觉周围的污秽一下子变多了」
				ENDIF
				RETURN 1
			ENDIF
		NEXT
		;貸し出し中を選択
		PRINTDATAW
			DATAFORM 「那么，要去别的地方逛逛吗？」
		ENDDATA
	;借りた
	ELSEIF TALENT:恋慕
		IF ITEM:101 == 1
			PRINTFORML 「战术指南书…精进战斗的艺术」
			PRINTFORMW 「战斗的环境不同，差异还是蛮大的」
