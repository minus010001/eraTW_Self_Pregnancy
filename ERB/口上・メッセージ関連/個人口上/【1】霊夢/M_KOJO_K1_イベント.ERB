;==================================================
@M_KOJO_SPEVENT_K1_3(ARG,ARG:1)
;口上主はTARGET
;地の文表示
CALL SPEVENT_MESSAGE_3(ARG,ARG:1)
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;通常帰宅
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		IF TALENT:恋人
			PRINTW
			PRINTFORML 「今天很開心呐」
			PRINTFORML 「下次，还要一起出来玩哦」
			PRINTFORML 「呼呼，下次去哪里呢」
			PRINTFORML 灵梦想去哪就去哪吧，%CALLNAME:MASTER%这么说道
			PRINTFORML 「和%CALLNAME:MASTER%一起的话，到哪里都好。」
			PRINTFORMW 像这样，灵梦幸福的说。
			RETURN 0
		ELSEIF TALENT:恋慕
			PRINTW
			PRINTFORML 「今天、很開心呐」
			PRINTFORML 「那个……下次再……」
			PRINTFORML 可以啊。%CALLNAME:MASTER%这么回答。
			PRINTFORML 「真的吗？那么，我很期待哦」
			PRINTFORMW 灵梦笑着说道。
		ELSEIF TALENT:思慕
			PRINTW
			PRINTFORML 「今天……嘛、挺開心的呢」
			PRINTFORML 「哪天有兴致的话，再出来玩也可以哦」
			PRINTFORML 那什么时候会有兴致呢，%CALLNAME:MASTER%这样说道。
			PRINTFORML 「……说不定，很快就有了呢」
			PRINTFORML 这样，灵梦小声的说。
			PRINTW
		ELSE
			PRINTW
			PRINTFORML 「哈……好累」
			PRINTFORML 「果然还是在神社里优哉游哉比较好啊」
			PRINTFORML 偶尔出门也不错吧。%CALLNAME:MASTER%说道、
			PRINTFORML 「我想出去时会自己去的哦。」
			PRINTFORMW 灵梦这么说着。
		ENDIF
		RETURN 0
	ENDIF
	;-------------------------------------------------
	;反発刻印消去イベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF TALENT:恋人
			PRINTFORML 「——唉、算了、好吧」
			PRINTFORML 「知道了，这次就原谅你」
			PRINTFORML 「可是，如果还有下次的话，我就要生气了哦？」
			PRINTFORMW 灵梦笑着这样说道。
			RETURN 0
		ELSE
			PRINTFORML 「——唉，真没办法呢」
			PRINTFORML 「好吧，原谅你吧」
			PRINTFORML 「姑且，再相信你一回吧」
			PRINTFORML 灵梦面色稍雯、这样说道。
			PRINTW
		RETURN 0
		ENDIF
	ENDIF
	;-------------------------------------------------
	;反発刻印2>1レベルダウンイベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		IF TALENT:恋人
			PRINTFORML 「……嘛，就原谅你一点点吧」。
			PRINTFORML 「听好了？只是「一点点」哦？」
			PRINTFORML 「还没说完全原谅你」
			PRINTFORMW 灵梦稍微板着脸说道。
			RETURN 0
		ELSE
			PRINTFORML 「･･･就稍微就原谅你吧」
			PRINTFORML 「但是，如果再得意忘形的话，下场你知道的吧？」
			PRINTFORML 灵梦板着脸说道。
			PRINTW
		ENDIF
		RETURN 0
	ENDIF
	;-------------------------------------------------
	;反発刻印3>2レベルダウンイベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		IF TALENT:恋人
			PRINTFORML 「做得太过分的话、就干掉你哦」
			PRINTFORML 「……但是，就稍微原谅你好了」
			PRINTFORML 「不用谢」
			PRINTFORMW 灵梦板着脸对%CALLNAME:MASTER%说道。
			RETURN 0
		ELSE
			PRINTFORML 「%CALLNAME:MASTER%、听我一言」
			PRINTFORML 「与其向我撒娇、不如停止那些行为哦」
			PRINTFORML 「这次、稍微原谅你了」
			PRINTFORML 「但是、%CALLNAME:MASTER%、以后会怎么样就看你的行动了」
			PRINTW
		ENDIF
		RETURN 0
	ENDIF
	PRINTFORML
ENDIF
RETURN 0

