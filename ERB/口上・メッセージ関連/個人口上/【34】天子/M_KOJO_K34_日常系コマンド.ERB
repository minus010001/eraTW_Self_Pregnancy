;==================================================
;621,釣魚
;TFLAG:193(0=釣れない 1～4=魚 11～15=好吃的魚 610=木材 131=守矢之守札 21=跳蛋 609=氷之鱗 606=龍之爪)
;TFLAG:194(1000以上ならTARGETが釣った)
;==================================================
@M_KOJO_MESSAGE_COM_K34_621
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K34_621_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K34_621_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	SELECTCASE TFLAG:193
	;釣れない
	CASE 0
		IF TALENT:恋慕
			PRINTFORMW 「……欸……%CALLNAME:MASTER%真的很喜欢钓鱼呢……」
			PRINTFORMW 天子随手拔起了一根狗尾巴草，无聊地叼在嘴里看着%CALLNAME:MASTER%钓鱼。
			PRINTFORMW 「什么？我也一起来吗？啊啊……算了，试一试吧……」
			PRINTFORMW 天子效仿着%CALLNAME:MASTER%拿起了钓竿，開始钓鱼……
		ELSE
			PRINTFORMW 「………去别处玩吗」
			PRINTFORMW 天子无聊地坐在旁边
			PRINTFORMW 「……这个很有意思吗？」
		ENDIF
		RETURN 1
	;魚
	CASE 1 TO 8, 10 TO 20, 22, 25, 26, 27, 29
		IF TFLAG:194 > 1000
			PRINTFORMW 「哦，有鱼上钩了……」
		ELSE
			IF RAND:2 == 1
				PRINTFORMW 「嗯嗯，随处可见的小鱼呢」
			ELSE
				PRINTFORMW 「可以了吗？可以走了吗？」
			ENDIF
		ENDIF
		RETURN 1
	;大きい魚＠FISHING_SIZEでm級定義の魚
	CASE 9, 23, 24, 28, 30, 31
		IF TFLAG:194 > 1000
			PRINTFORMW 「……哦哦！竟然是这种鱼呢！」
		ELSE
			IF RAND:2 == 1
				PRINTFORMW 「据说是肉质相当鲜美的鱼呢。衣玖经常带回来这种鱼，不过因为她不会做饭所以做的很难吃」
			ELSE
				PRINTFORMW 「能钓上这种品质的鱼，今天已经非常圆满了呢，我们是不是可以走了？」
			ENDIF
		ENDIF
	;守矢之守札
	CASE 131
		IF TFLAG:194 > 1000
			PRINTFORMW 「啊，是神社的东西呢」
			PRINTFORMW 「当时我就说没人会要的，果然被扔掉了呢」
		ELSE
			IF RAND:2 == 1
				PRINTFORMW 「要试着收集起来吗？据说她们准备了神秘大奖的样子。」
				PRINTFORMW 「不过从来没人抽到过，所以慢慢地也没人愿意花钱买这玩意儿了，到现在很多人见到这东西都会无视掉呢」
			ELSE
				PRINTFORMW 「这种东西在这里的话，就算是鱼也不会想要靠近的」
				PRINTFORMW 「这里不会再有收获了哦？我们去别的地方玩吧。」
			ENDIF
		ENDIF
		RETURN 1
	CASE 21
		LOCALS = 跳蛋
		IF TFLAG:194 > 1000
			PRINTFORMW 「……哈？为什么这种东西也能钓到啊？」
			PRINTFORMW 「……该说你是真的技术娴熟还是别的什么呢……」
		ELSE
			PRINTFORMW 「啊，这东西……那些河童又乱扔垃圾了吗。」
			PRINTFORMW 「……说起来，河童所谓的“尻子玉”不会就是这玩意儿吧？」
		ENDIF
		RETURN 1
	;お宝
	CASE 606, 609, 643
		IF TFLAG:194 > 1000
			PRINTFORMW 「唔哇……这种东西也能钓上来…！？」
		ELSE
			PRINTFORMW 「哼哼……果然是因为我在旁边呢，感谢我给你带来的强运吧~~」
		ENDIF
		RETURN 1
	;木材
	CASE 610
		IF TFLAG:194 > 1000
			PRINTFORMW 「嗯嗯，预料之中呢……」
		ELSE
			IF RAND:2 == 1
				PRINTFORMW 「……不要灰心嘛%CALLNAME:MASTER%，我们去别的地方玩吧！」
				PRINTFORMW 「诶？为什么不走啊！」
			ELSE
				PRINTFORMW 「走吧走吧，大好时光才不该浪费在这种事情上~」
			ENDIF
		ENDIF
		RETURN 1
	;青蛙
