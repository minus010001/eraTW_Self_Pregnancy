;==================================================
;80,手淫
;==================================================
@M_KOJO_MESSAGE_COM_K118_80
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K118_80_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K118_80_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORML 「こ、こういう玩耍をするの……？ 誰かに見られてない……？」
		PRINTFORML 「……但是，这个可能有点像我的触角……」
		PRINTFORMW 拉瓦爾下定决心后，战战兢兢地摸到了%CALLNAME:MASTER%的肉棒……。
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:時間停止
		PRINTFORMW %CALLNAME:MASTER%在无法动弹的拉瓦爾的腋下用肉棒摩擦着……。
		RETURN 1
	;それ以外
	ELSE
		IF NOWEX:PLAYER:射精
			PRINTFORML 「%CALLNAME:MASTER%，舒服吗……？」
			PRINTFORMW 拉瓦爾突然加快了手淫的速度……。
		ELSE
			PRINTFORML 「这个，总觉得握着很安心……」
			PRINTFORMW 拉瓦爾红着耳朵继续手淫……。
		ENDIF
	ENDIF
ENDIF
RETURN 1

;==================================================
;81,口交
;==================================================
@M_KOJO_MESSAGE_COM_K118_81
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K118_81_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K118_81_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORML 「这、舔这个的游戏……？ 比我的触角还要奇怪的气味……」
		PRINTFORMW 拉瓦爾一边踌躇着一边開始啾噜啾噜地舔龟头……。
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORML 把肉棒插进说不了话的拉瓦爾的口内……。
		PRINTFORMW 被小小的牙齿嘎吱嘎吱地摩擦着感觉很好……。
		RETURN 1
	;それ以外
	ELSE
		PRINTFORML 「ちゅるちゅる…… なんらかこれ、蜜すってるみらい……」
		PRINTFORMW 拉瓦爾像吮吸着受精管一样地持续着口交……。
		IF NOWEX:PLAYER:射精
				PRINTFORML 「！！…………嗯……%UNICODE(0x2764)%」
				PRINTFORMW 拉瓦爾似乎喜欢精液的苦味……。
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1

