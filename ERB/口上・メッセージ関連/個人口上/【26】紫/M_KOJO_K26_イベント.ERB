;==================================================
;相手が推倒を解除
;==================================================
@M_KOJO_EVENT_K26_30(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------

IF LOCAL
	;-------------------------------------------------
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
	;疲惫
		PRINTFORMW 「唔…不可以…%UNICODE(0x2665) *1%」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
	;満足して解放
		PRINTFORMW 「呼…舒服多了…%UNICODE(0x2665) *1%」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
	;満足したところを被逆推た
		PRINTFORMW 「嘛…美妙绝伦…%UNICODE(0x2665) *1%」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
	;壊れるまでしてあげる
		PRINTFORMW 「别再想跑掉了…%UNICODE(0x2665) *1%」
		RETURN 1
	ENDIF
ENDIF
RETURN 1


