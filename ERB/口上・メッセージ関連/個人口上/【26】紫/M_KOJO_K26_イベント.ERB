;==================================================
;相手から约会のお誘い
;==================================================
@M_KOJO_EVENT_K26_27(ARG,ARG:1)
;口上主はLOCAL
;ARG:1は目的地
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	SELECTCASE ARG
		;-------------------------------------------------
		CASE 0
		;誘い文句
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF TALENT:恋人
				PRINTFORMW 「喂，%CALLNAME:MASTER%，要不要约个会？」
			ELSEIF TALENT:恋慕
				PRINTFORMW 「%CALLNAME:MASTER%，要去玩吗？」
			ELSEIF TALENT:思慕
				PRINTFORMW 「%CALLNAME:MASTER%，现在有空吗？」
			ELSE
				PRINTFORMW 「%CALLNAME:MASTER%，可以陪我玩吗？」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 1
		;受けた
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF TALENT:恋人
				PRINTFORMW 「要是今晚过得開心就好了，%CALLNAME:MASTER%」
			ELSEIF TALENT:恋慕
				PRINTFORMW 「呵呵，不要被误认为有什么异变就好。」
			ELSEIF TALENT:思慕
				PRINTFORMW 「对灵梦保密哦？会被退治的」
			ELSE
				PRINTFORMW 「来吧，在幻视之夜里彷徨吧」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 2
		;断った
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF TALENT:恋人
				PRINTFORMW 「呵呵，那等下次再有机会吧。」
			ELSEIF TALENT:恋慕
				PRINTFORMW 「哎呀，真遗憾啊」
			ELSEIF TALENT:思慕
				PRINTFORMW 「哎呀呀，说服的方法，我去请教蓝吧」
			ELSE
				PRINTFORMW 「真可惜，被甩了。」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 3
		;部屋に誘われた
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF TALENT:恋人
				PRINTFORMW 「这样的好机会、不来我都房间吗？」
			ELSEIF TALENT:恋慕
				PRINTFORMW 「也许能看到有趣的东西哦、不进来吗？」
			ELSEIF TALENT:思慕
				PRINTFORMW 「要来家庭访问吗？」
			ELSE
				PRINTFORMW 「不进来喝点茶吗？」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 4
		;部屋に誘われたのを受けた
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF TALENT:恋人
				PRINTFORMW 「今天一整天都能和%CALLNAME:MASTER%一起度过呢♪」
			ELSEIF TALENT:恋慕
				PRINTFORMW 「呋呋、不许后悔哦？」
			ELSEIF TALENT:思慕
				PRINTFORMW 「%CALLNAME:MASTER%、不知道有什么好怕的」
			ELSE
				PRINTFORMW 「那么，就通过这个间隙——」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 5
		;部屋に誘われたのを断った
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF TALENT:恋人
				PRINTFORMW 「真可惜、但你什么时候来都可以哦」
			ELSEIF TALENT:恋慕
				PRINTFORMW 「啊啦？真可惜」
			ELSEIF TALENT:思慕
				PRINTFORMW 「错过了难得的机会呢」
			ELSE
				PRINTFORMW 「嘛、很明智」
			ENDIF
			RETURN 1
		ENDIF
	ENDSELECT
ENDIF
RETURN 1

