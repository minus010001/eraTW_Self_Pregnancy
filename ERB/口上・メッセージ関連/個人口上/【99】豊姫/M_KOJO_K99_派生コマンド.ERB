;==================================================
;1,六九式
;==================================================
@M_KOJO_MESSAGE_SCOM_K99_1
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_SCOM_K99_1_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K99_1_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;通常
	ELSE
		;恋慕
		IF TALENT:恋慕
			;この指令で你が射精
			IF NOWEX:MASTER:射精
				SELECTCASE RAND:3
					CASE 0
						CALL HPH_PRINT,@"「れろっ……れろっれるっ……くぷ……っHPH」","L"
						CALL HPH_PRINT,@"「んっ、んっ……ちゅうっ……んぷ……っHPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL 舌を器用に遣い亀頭を舐め回し、%CALLNAME:MASTER%が物足りなさそうにしていれば深くまで竿を咥え込む
							PRINTFORMDW ただ任憑擺佈ことしかできない。%CALLNAME:MASTER%は奉仕するのも忘れ、その快楽に口の端から涎を滴下去
						ENDIF
					CASE 1
						SIF !CFLAG:99:1998
							PRINTFORMDL 限界が来た%CALLNAME:MASTER%は腰を浮かせ射精した
						CALL HPH_PRINT,@"「ん……っHPH ふぅ……ぢゅる……っHPH」","L"
						CALL HPH_PRINT,@"「はぁ……HPH ご馳走様。美味しかったわ……HPH」","L"
						SIF !CFLAG:99:1998
							PRINTFORMDW %CALLNAME:TARGET%は口の端から垂れた精液を小指で拭うと、それを口に含んだ
					CASE 2
						CALL HPH_PRINT,@"「ちゅ……ちゅぷ……っHPH んる……れろ……っHPH」","L"
						CALL HPH_PRINT,@"「一緒に……んっHPH 気持ち良くなりましょ……HPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL 時に激烈地、時に加減しながら……
							PRINTFORMDW 二人は同時に絶頂するため、互相用性器を舐め合った……
						ENDIF
				ENDSELECT
				RETURN 1
			;それ以外
			ELSE
				SELECTCASE RAND:3
					CASE 0
						CALL HPH_PRINT,@"「んんっHPH ふっ……ふぅ……っHPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL ふやけた蜜穴に舌を滑り込ませ、慢慢地粘膜を擦り上げる
							PRINTFORMDW 次に口をつけ『ジュルッ』と吸い上げてやると、豊姫は小さく腰を跳ねさせた
						ENDIF
					CASE 1
						CALL HPH_PRINT,@"「それじゃあ、始めるわね……HPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL 豊姫は%CALLNAME:MASTER%の顔に跨ると、美味しそうに阴茎をしゃぶりだした
							PRINTFORMDW きっと下品な顔をしているのだろう。こちらから見えないのが少し残念だった
						ENDIF
					CASE 2
						CALL HPH_PRINT,@"「んふ……ふぅ……っHPH んく……ぢゅぷ……っHPH」","L"
						SIF !CFLAG:99:1998
							PRINTFORMDL 顔を揺すって阴茎をしゃぶり、口内に溜まった先走りを飲み下し、その味に膣口をヒクつかせる
						CALL HPH_PRINT,@"「ごく……ん……っHPH はぁ……はむ……っHPH」","L"
						SIF !CFLAG:99:1998
							PRINTFORMDW 商売女のように、豊姫は%CALLNAME:MASTER%を気持ち良くさせることに全力を出している……
				ENDSELECT
				RETURN 1
			ENDIF
		;それ以外
		ELSE
			;この指令で你が射精
			IF NOWEX:MASTER:射精
				SELECTCASE RAND:3
					CASE 0
						CALL HPH_PRINT,@"「くぷっ……くふっぷ……っHPH」","L"
						CALL HPH_PRINT,@"「んっふぅ……ふぅ……んっく……っHPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL 豊姫は目を閉じて%CALLNAME:MASTER%の阴茎を舐めることに集中している
							PRINTFORMDW %CALLNAME:MASTER%は身体の力を抜くと、射精に意識を集中させた
						ENDIF
					CASE 1
						CALL HPH_PRINT,@"「ふぅ……ぢゅる……っHPH ん……っHPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL 熱心に阴茎をしゃぶる豊姫に射精を予告する
							PRINTFORMDW すると豊姫は口を離すことなく、より深い所まで肉棒を咥え込んだ
						ENDIF
					CASE 2
						CALL HPH_PRINT,@"「くぷっ、ちゅっ……イキそう……？HPH」","L"
						CALL HPH_PRINT,@"「いいわよ、このまま……ん、ちゅぱっHPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL 阴茎を口に咥えたまま小刻みに顔を揺する豊姫
							PRINTFORMDW %CALLNAME:MASTER%が射精すると豊姫は動きを止め、吐き出される精液を口内で受け止めた
						ENDIF
				ENDSELECT
				RETURN 1
			;それ以外
			ELSE
				SELECTCASE RAND:3
					CASE 0
						CALL HPH_PRINT,@"「ほら、舌が止まってるわよ……？HPH」","L"
						SIF !CFLAG:99:1998
							PRINTFORMDL %CALLNAME:MASTER%の顔に跨った豊姫が、腰を動かし秘所を口に擦り付けてくる……
						CALL HPH_PRINT,@"「お互いに気持ち良く……ねHPH」","W"
					CASE 1
						CALL HPH_PRINT,@"「ふふっ、ここが弱いのね……HPH」","L"
						CALL HPH_PRINT,@"「んれる……ちゅ……っHPH れる、れろ……っHPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL 円を描くように舌を動かし、亀頭中心に責め立てる豊姫
							PRINTFORMDW %CALLNAME:MASTER%も負けじと舌を動かすが、主導権は完全に豊姫に握られていた
						ENDIF
					CASE 2
						CALL HPH_PRINT,@"「上手よ……HPH でも、まだまだ……HPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL 「見ていなさい」とばかりに腰を下ろし、%CALLNAME:MASTER%の奉仕を止めさせる
							PRINTFORMDL 次に豊姫は%CALLNAME:MASTER%の阴茎を根本まで咥え込むと……
						ENDIF
						CALL HPH_PRINT,@"「ん……ん……っHPH んぷ……ぐぽ……っHPH」","L"
						SIF !CFLAG:99:1998
							PRINTFORMDW 喉を締め的同时、慢慢地顔を揺すり始めた……
				ENDSELECT
				RETURN 1
			ENDIF
		ENDIF
	ENDIF
ENDIF
;未記入
;CALLFORM M_KOJO_MESSAGE_COM_K99_{SELECTCOM}_1
RETURN 1

