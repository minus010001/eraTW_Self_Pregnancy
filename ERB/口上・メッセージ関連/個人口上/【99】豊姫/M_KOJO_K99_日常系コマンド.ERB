@M_KOJO_SUCCESS_COM_K99_300
;成否判定
;TFLAG:192(-2=指令終了、-1＝強制失敗、0=COM依存、1=強制成功or大成功
TFLAG:192 =0

@M_KOJO_MESSAGE_COM_K99_300
;指令共通メッセージ(TRAIN_MESSAGE) 
;削除すると「～说着话。」とかが消えます。また、削除した場合は自動で口上色が付きません。
CALL TRAIN_MESSAGE
;口上メイン呼出
CALL M_KOJO_MESSAGE_COM_K99_300_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K99_300_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;初回会話
	IF FIRSTTIME(SELECTCOM)
		PRINTFORML 「好久没像这样和地上的人说话了」
		PRINTFORML 「上一次是距今一千五百多年前……他的名字叫水江浦岛子」
		PRINTFORMW 「啊、说浦岛太郎更容易理解吧？」
	;憤怒
	ELSEIF CFLAG:ブチギレ > 0  || MARK:反発刻印 >= 3
		PRINTFORML 「……………」
		PRINTFORMDW 丰姬用扇子遮住嘴角、目不转睛地注视着%CALLNAME:MASTER%
	;会話不能
	ELSEIF TCVAR:302
		;恋慕
		IF TALENT:恋慕
			PRINTFORML 「………」
			PRINTFORMDW 丰姬微笑着、默默倾听着%CALLNAME:MASTER%的话
		;それ以外
		ELSE
			PRINTFORML 「算了、不用硬找话题了」
			PRINTFORMW 「先放松一会儿吧」
		ENDIF
	;通常
	ELSE
		;恋慕
		IF TALENT:恋慕
			SELECTCASE RAND:10
				CASE 0
					;１５分の１の確率でフェムトファイバーについて語る
					IF !RAND:15
						PRINTFORML 「是不是快忘了、我之前和你提过的“费米纤维”的性质？」
						PRINTFORMW 「那么、就在这里复习一下吧」
						PRINTFORML 
						;選択肢
						CALL ASK_YN("好的","不想听")
						;判定
						SELECTCASE RESULT
							;是
							CASE 0
								GOTO FEMTOFIBER
							;否
							CASE 1
								PRINTFORMW 「我就知道你会这么说、没得选择哦」
								GOTO FEMTOFIBER
						ENDSELECT
					;通常
					ELSE
						PRINTFORMDL 连着说了一段时间、两人稍微喘了口气
						PRINTFORMDL %CALLNAME:MASTER%轻轻搂住丰姬的肩膀、对她甜言蜜语着
						PRINTFORMDL 丰姬羞得面红耳赤、却又高兴地微笑着
						PRINTFORMW 「……呵呵、最喜欢你了、%CALLNAME:MASTER%♪」
					ENDIF
				CASE 1
					PRINTFORML 「总有一天、你也会……」
					PRINTFORML 「……不、没什么」
					PRINTFORMW 「还是来讲一讲你的故事吧？」
				CASE 2
					PRINTFORML 「欸？欸欸欸欸————你在看哪里呢？」
					PRINTFORML 「和人说话的时候要看着对方的眼睛」
					PRINTFORMDW 但是、说这话的同时、丰姬解開了衬衫的扣子、故意拢起双乳、露出乳沟
				CASE 3
					PRINTFORML 「……噗……噗噗」
					PRINTFORML 「啊哈哈、对不起」
					PRINTFORMW 「你的话……哈哈……真的很有意思」
				CASE 4
					PRINTFORML 「那个故事、应该还有下文吧？」
					PRINTFORML 「请说出来吧、我会认真倾听的♪」
					PRINTFORMDW 丰姬听着%CALLNAME:MASTER%的话、開心地笑了起来
				CASE 5
					PRINTFORML 「你听说过吗？」
					PRINTFORML 「月人和地上人之间的传闻」
					PRINTFORML 「即使超过了75天、也始终没有消失的人」
					PRINTFORMW 「呼呼……说的到底是谁和谁呢」
				CASE 6
					PRINTFORML 「我在想、有一天、我们可以一起去旅行」
					PRINTFORML 「去了一个谁也不认识我们的地方、在那里……」
					PRINTFORML 「我们可以举办一个小型的婚礼」
					PRINTFORML 「要把依姬叫过来、啊，还有八意大人……」
					PRINTFORMDW 总觉得话题突然加速了……
				CASE 7
					PRINTFORML 「嗯？想听我讲以前的事吗？」
					PRINTFORML 「这样啊……那么……」
					PRINTFORMDW %CALLNAME:MASTER%从丰姬那里听到了%TEXTR("令人怀念的往事/与依姬的回忆/有趣的故事/")%
				CASE 8
					CALL HPH_PRINT,@"「………HPH」","L"
					PRINTFORMDL 丰姬一边聆听着%CALLNAME:MASTER%的话语、一边投以热切的视线
					PRINTFORML 「……啊？没关系、请继续吧？」
					PRINTFORMW 「你的话、我一直都在听哦」
				CASE 9
					PRINTFORML 「呐……可以叫我一声『丰姬（とよねぇ）』吗？」
					PRINTFORML 「………啊呀呀♪」
					PRINTFORMW 「心脏……怦怦直跳……♪」
			ENDSELECT
		;炮友
		ELSEIF TALENT:炮友
			SELECTCASE RAND:10
				CASE 0
					PRINTFORML 「嘿，和我在一起开心吗？」
					PRINTFORML 「——啊，不，只是随口问问而已」
					PRINTFORMW 「没什么。真的哦？」
				CASE 1
					PRINTFORML 「本来想成为可靠的姐姐」
					PRINTFORML 「但现实却总是靠着别人。无论是依姬还是你」
					PRINTFORMW 「——为什么会谈起这种话题呢？呵呵，不过想来的话，你还不错」
				CASE 2
					PRINTFORML 「话说回来……你是怎么看依姬的？」
					PRINTFORML 「你看，她那样的性格，对男人一点兴趣都没有」
					PRINTFORML 「你的话，我觉得可以信赖，可以放心交给你」
					PRINTFORMDW 豊姬似乎想要把依姬和自己联系起来……
				CASE 3
					; 豊姬对待兔子的善意与池塘里的鲤鱼一样，这是个考察
