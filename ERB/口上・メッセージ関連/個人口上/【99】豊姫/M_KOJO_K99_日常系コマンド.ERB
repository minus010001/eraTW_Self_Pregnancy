@M_KOJO_MESSAGE_SCOM_K99_60_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;恋慕
	IF TALENT:恋慕
		;判定
		SELECTCASE TFLAG:193
			;大成功
			CASE 1
				PRINTFORML 「……好、完成了！」
				PRINTFORML 「……%CALLNAME:MASTER%？完成了哦？」
				PRINTFORMDL 丰姬探了下头、看向%CALLNAME:MASTER%的脸、发现他正发出低低的鼾声
				PRINTFORMW 「……呵呵……那就再睡一会儿吧」
			;成功
			CASE 0
				PRINTFORML 「嗯……好、变干净了」
				PRINTFORML 「你再想要的话、随时告诉我哦」
				PRINTFORMDW %CALLNAME:MASTER%起身向丰姬道谢
			;失敗
			CASE -1
				PRINTFORML 「啊、对不起……很痛吧？」
				PRINTFORML 「我该更仔细一些的……」
				PRINTFORMDW %CALLNAME:MASTER%安慰了自责的丰姬
		ENDSELECT
	;それ以外
	ELSE
		;判定
		SELECTCASE TFLAG:193
			;大成功
			CASE 1
				PRINTFORML 「啊、积攒了不少啊」
				PRINTFORML 「好了、躺下……不要动哦」
				PRINTFORML 「……呵呵、怎么样？很舒服吧？」
				PRINTFORML 「哇！这么多……好大……♪」
				PRINTFORMDL %CALLNAME:MASTER%让丰姬打扫了耳朵
				PRINTFORMDW 非常健全、没有色色
			;成功
			CASE 0
				PRINTFORML 「……好、完成了！」
				PRINTFORML 「你看、弄出来这么多哦」
				PRINTFORMW 「自己也得好好收拾干净才行啊？」
			;失敗
			CASE -1
				PRINTFORML 「哦……欸！？……」
				PRINTFORML 「没问题吧？痛不痛？」
				PRINTFORMDW 丰姬担心地看着%CALLNAME:MASTER%
		ENDSELECT
	ENDIF
ENDIF
RETURN 1
