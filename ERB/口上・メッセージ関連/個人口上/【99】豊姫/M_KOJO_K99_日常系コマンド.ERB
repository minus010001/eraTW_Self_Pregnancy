@M_KOJO_MESSAGE_COM_K99_411_1

#DIM 正解
#DIM 経験値
#DIM 制限時間
#DIM 倍率
#DIM FEINT
#DIM イカサマ
VARSET LOCAL
イカサマ = 0
制限時間 = 2000 - (ABL:99:戦闘能力 - ABL:MASTER:戦闘能力) * 100

;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;特訓デイリーからお借りしました
	CALL M_KOJO_COLOR_K99
	PRINTFORML 「玉不琢不成器、人不学不知义」
	PRINTFORML 「宝石也好、才能也好、不磨练就不会发光……」
	PRINTFORMDL 丰姬拿着扇子刺向%CALLNAME:MASTER%
	PRINTFORML 「来吧、拿出你所有的力量」
	PRINTFORMW 「向死而生！地上人！」
	RESETCOLOR
	PRINTFORML 
	CALL ASK_YN("開始战斗！","算了吧")
	IF RESULT
		CALL M_KOJO_COLOR_K99
		PRINTFORML 「哦呀、不来吗？」
		PRINTFORML 「那就一起喝茶吧」
		PRINTFORMDW 刚才的气魄消失得无影无踪、丰姬完全恢复了往日的状态
		RESETCOLOR
	ELSE
		正解 = RAND:4
		経験値 = MIN(ABL:99:戦闘能力 * 5, 5)
		SELECTCASE RAND:20
			CASE IS <= 11
			;普通に
				LOCALS:0 = 上
				LOCALS:1 = 右
				LOCALS:2 = 左
				LOCALS:3 = 下
				倍率 = 100
			CASE 12 TO 15
			;ローマ字
				LOCALS:0 = UE
				LOCALS:1 = MIGI
				LOCALS:2 = HIDARI
				LOCALS:3 = SHITA
				倍率 = 160
			;英語
			CASE 16,17,18
				LOCALS:0 = UP
				LOCALS:1 = RIGHT
				LOCALS:2 = LEFT
				LOCALS:3 = DOWN
				倍率 = 120
			;スペイン語
			CASE 19
				LOCALS:0 = encima de
				LOCALS:1 = a la derecha
				LOCALS:2 = a la izquierda
				LOCALS:3 = abajo de
				倍率 = 300
		ENDSELECT
		;你の戦闘能力が高いと５分の一の確率でフェイントをかけてくる
		IF ABL:MASTER:戦闘能力 >= 5 && !RAND:5
			FEINT = 1
			制限時間 += 400
			倍率 += 80
		ELSE
			FEINT = 0
		ENDIF
		CALL FISHER_YATES_SHAFFLE(4)
		PRINTFORMW 開始躲避%CALLNAME:TARGET%的攻击（点击開始）
		PRINTFORM %CALLNAME:TARGET%的
		SETCOLOR C_YELLOW
		IF FEINT
			PRINTFORM %LOCALS:(RAND:4)%
			RESETCOLOR
			PRINTFORM 到来了假象！从%LOCALS:(3 - 正解)%
		ELSE
			PRINTFORM %LOCALS:(3 - 正解)%
			RESETCOLOR
		ENDIF
		PRINTFORML 到来的攻击！
		$LOOP_IKASAMA
		FOR LOCAL:10,0,4
			LOCAL = SHAFFLE_ARRAY:(LOCAL:10)
			PRINTFORML [{LOCAL}] - 从%LOCALS:LOCAL%方避開
		NEXT
		SIF !イカサマ && BASE:MASTER:TSP > 500
			PRINTFORML [{4}] - 時間停止
		TINPUT 制限時間, -1, 1
		IF RESULT == 正解
			PRINTFORML %CALLNAME:MASTER%躲開了丰姬的攻击！
			CALL M_KOJO_COLOR_K99
			IF !イカサマ
				PRINTFORMDL ……………
				PRINTFORMDL ………
				PRINTFORMDW …
				PRINTFORML 「好了、先停一下吧」
				PRINTFORMDL 看着完全喘不过气来的%CALLNAME:MASTER%、丰姬很坦然的样子
				PRINTFORML 「很累了吧？」
				PRINTFORMW 「练习就到此为止、可以休息一下了」
			ELSE
				PRINTFORML 「呜哇！啊！？」
				PRINTFORMDL %CALLNAME:MASTER%停止时间回避攻击、然后趁机将丰姬推倒
				PRINTFORML 「啊……我输了。真厉害啊」
				PRINTFORML （不过、这个能力……在什么地方……）
				PRINTFORMDW 丰姬掸去沾在衣服上的污渍、惊讶地看着%CALLNAME:MASTER%
			ENDIF
			RESETCOLOR
			BASE:MASTER:体力 -= 100 * (1 + ABL:99:戦闘能力)
			BASE:MASTER:気力 -= 100 * (1 + ABL:99:戦闘能力)
			経験値 = 経験値 * 倍率 / 100
			EXP:MASTER:戦闘経験 += 経験値
			EXP:99:戦闘経験 += 経験値 * 2
			CFLAG:99:好感度 += 経験値 * 2
			CFLAG:99:信賴度 += 経験値 * 2
			PRINTFORML 戦闘経験 ＋{経験値}（%CALLNAME:MASTER%）
			PRINTFORML 戦闘経験 ＋{経験値 * 2}（%CALLNAME:TARGET%）
			PRINTFORML 好感度 ＋{経験値 * 2}（%CALLNAME:TARGET%）
			PRINTFORML 信赖度 ＋{経験値 * 2}（%CALLNAME:TARGET%）
		ELSEIF RESULT == 4 && !イカサマ && BASE:MASTER:TSP > 500
			イカサマ = 1
			倍率 /= 5
			PRINTFORMW %CALLNAME:MASTER%用时间停止能力看清了攻击!
			GOTO LOOP_IKASAMA
		ELSE
			CALL M_KOJO_COLOR_K99
			PRINTFORMDL 失败了……
			PRINTFORML 「没问题吧？……哪里受伤了吗？」
			PRINTFORMDL 丰姬走到了被华丽地吹飞的%CALLNAME:MASTER%身边
			PRINTFORML 「嗯……看来没受什么伤」
			PRINTFORML 「怎么样？能起来吗？」
			PRINTFORMDW %CALLNAME:MASTER%在丰姬的帮助下、勉强支撑起颤抖的身体……
			RESETCOLOR
			BASE:MASTER:体力 -= 500 * (1 + ABL:99:戦闘能力)
			BASE:MASTER:気力 -= 500 * (1 + ABL:99:戦闘能力)
			PRINTFORML 戦闘経験 ＋{経験値}（%CALLNAME:TARGET%）
