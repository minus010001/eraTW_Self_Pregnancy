;				IF GET_MAPID(CFLAG:現在位置) == MAIN_MAP
;					CALL RAN_NORMALTALK
;				;そうじゃなかったら外。どっかで偶然出会った場合でも约会会話になるけど今は妥協
;				ELSE
;					CALL RAN_DATETALK
;				ENDIF
