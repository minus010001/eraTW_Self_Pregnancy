		ELSEIF RAND:2 == 0
			PRINTFORMW 「嗯%UNICODE(0x2665) *1%舔%UNICODE(0x2665) *1%啾%UNICODE(0x2665) *1%咕噜%UNICODE(0x2665) *1%嗯%UNICODE(0x2665) *1%舔%UNICODE(0x2665) *1%嗯%UNICODE(0x2665) *1%啾%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「舔%UNICODE(0x2665) *1%嗯%UNICODE(0x2665) *1%」
			PRINTFORM 「
			CALL MASTER_CALLNAME_K95
			PRINTFORMW 舒服吗…？」
		ENDIF
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		IF RAND:3 == 0
			PRINTFORM 「
			CALL MASTER_CALLNAME_K95
			PRINTFORMW 、用嘴给你做吧…%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
