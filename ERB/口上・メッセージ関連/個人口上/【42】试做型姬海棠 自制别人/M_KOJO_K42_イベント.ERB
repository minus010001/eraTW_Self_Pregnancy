;==================================================
;部屋で遭遇
;==================================================
@M_KOJO_中文自制_EVENT_K42_1(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;MASTERが入ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		;一日一回
		IF TCVAR:42:350 == 1
			PRINTFORMDW %CALLNAME:TARGET%简单打了个招呼。
			RETURN 1
		ENDIF
		IF TALENT:恋慕
			;DAY:2で季節を選択
			SELECTCASE DAY:2
			;春
			CASE 1
				;TIME:2で朝昼晩を選択
				SELECTCASE TIME:2
				;朝
				CASE 1,2
					PRINTFORMW 「哎呀，%CALLNAME:MASTER%，早上好」
					PRINTFORMDW %CALLNAME:TARGET%微笑着，看起来很有精神。
				;昼
				CASE 3,4
					PRINTFORMW 「唔，%CALLNAME:MASTER%啊」
					PRINTFORMW 「在这个季节，挑这个时间出门最合适呢」
					PRINTFORMW 「想跟我一起走走吗？」
					PRINTFORMDW %CALLNAME:TARGET%笑着问道。
				;夜
				CASE 5,6,7
					PRINTFORMW 「%CALLNAME:MASTER%，晚上好啊」
					PRINTFORMW 「温差较大的天气里，晚上要多注意保暖哦」
				ENDSELECT
			;夏
			CASE 2
				SELECTCASE TIME:2
				;朝
				CASE 1,2
					PRINTFORMW 「早上好，%CALLNAME:MASTER%……」
					PRINTFORMW 「昨晚睡得好吗？毕竟已经是这种炎热的天气了呢」
					PRINTFORMDW %CALLNAME:TARGET%有点燥热的样子，领带下的领口敞的很開……
					PRINTFORMDW ……没忍住多看了两眼。
				;昼
				CASE 3,4
					PRINTFORMW 「%CALLNAME:MASTER%，嘿……」
					PRINTFORMDW %CALLNAME:TARGET%把手里的矿泉水瓶贴在%CALLNAME:MASTER%的额头上。
					PRINTFORMDW ……好冰！
					PRINTFORMW 「哼哼，拿去喝吧。别中暑了哦」 
				;夜
				CASE 5,6,7
					PRINTFORMW 「%CALLNAME:MASTER%」
					PRINTFORMDW %CALLNAME:TARGET%走过来，靠在%CALLNAME:MASTER%身上。
					PRINTFORMDW 取出手机，和%CALLNAME:MASTER%一起看着最近拍的照片。
					PRINTFORMW 「吶，你觉得彩页的插图用萤火虫或者烟花，哪个比较浪漫？」
				ENDSELECT
			;秋
			CASE 3
				SELECTCASE TIME:2
				;朝
				CASE 1,2
					PRINTFORMW 「脸有点红啊，%CALLNAME:MASTER%，还好吗？」
					PRINTFORMW 「因为这个季节容易生病，甚至也有妖怪会得的病呢」
					PRINTFORMW 「需要我带你去看看竹林里的医生吗？」
				;昼
				CASE 3,4
					PRINTFORMW 「是枫叶啊……%CALLNAME:MASTER%采的吗？」
					PRINTFORMW 「嗯？没什么，只是想起了某个令人不爽的家伙」
					PRINTFORMW 「……%CALLNAME:MASTER%没有错哦」
				;夜
				CASE 5,6,7
					PRINTFORMW 「嗯？%CALLNAME:MASTER%啊」
					PRINTFORMW 「我在听虫鸣……还有一些妖怪在唱歌，相得益彰……」
					PRINTFORMW 「明明是美好的东西，却没办法刊登到报纸上呢」
				ENDSELECT
			;冬
			CASE 4
				SELECTCASE TIME:2
				;朝
				CASE 1,2
					PRINTFORMW 「早上好……等等，%CALLNAME:MASTER%这不是在发抖吗」
					PRINTFORMW 「…………借你一会儿，帮你系好了哦」
					PRINTFORMDW ……%CALLNAME:MASTER%裹上了带着%CALLNAME:TARGET%体温的围巾。
				;昼
				CASE 3,4
					PRINTFORMW 「啊，%CALLNAME:MASTER%」
					PRINTFORMW 「我正在工作哦，请你稍等一下」
					PRINTFORMDW %CALLNAME:TARGET%活动了一下有些僵硬的手指，熟练地操作着手机。
				;夜
				CASE 5,6,7
					PRINTFORMW 「%CALLNAME:MASTER%，不觉得突然变得好冷吗……」
					PRINTFORMW 「单靠围巾根本没办法保暖了呢」
					PRINTFORMDW %CALLNAME:TARGET%搂住了%CALLNAME:MASTER%的手。
					PRINTFORMW 「……这样就好多了」
				ENDSELECT
			ENDSELECT
		ELSEIF TALENT:思慕
			SELECTCASE TIME:2
				;朝
				CASE 1,2
					PRINTFORMW 「%CALLNAME:MASTER%，早上好」
				;昼
				CASE 3,4
					PRINTFORMW 「%CALLNAME:MASTER%，中午好啊」
				;夜
				CASE 5,6,7
					PRINTFORMW 「%CALLNAME:MASTER%，这么晚了还没休息啊」
				ENDSELECT
		ELSEIF TALENT:愛欲
			PRINTFORMW 「……是%CALLNAME:MASTER%啊」
			PRINTFORMDW	%CALLNAME:TARGET%稍微有些不自然地收拢了一下双腿。
			PRINTFORMW 「有何、贵干呢？」
		ELSE
			PRINTFORMW 「嗯？是%CALLNAME:MASTER%啊」
		ENDIF
		IF TALENT:恋人
			PRINTFORMDW 从靠近的%CALLNAME:TARGET%身上闻到一股好闻的水果香味……
			SELECTCASE RAND:6
				CASE 0
					PRINTFORMDW 苹果的清香……
				CASE 1
					PRINTFORMDW 草莓的清香……
				CASE 2
					PRINTFORMDW 柑橘的清香……
				CASE 3
					PRINTFORMDW 柠檬的清香……
				CASE 4
					PRINTFORMDW 菠萝的清香……
				CASE 5
					PRINTFORMDW 桃子的清香……
			ENDSELECT
			PRINTFORMDW 难以分清是香水还是体香————或是两种都有呢？
		ENDIF
		TCVAR:42:350 = 1
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが入ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		IF TALENT:恋慕
			PRINTFORML 「你在这啊，%CALLNAME:MASTER%」
			PRINTFORMDW %CALLNAME:TARGET%带着微笑靠近了%CALLNAME:MASTER%。
		ELSEIF TALENT:思慕
			PRINTFORML 「打扰了……」
			PRINTFORMDL %CALLNAME:MASTER%和%CALLNAME:TARGET%眼神相交的一瞬间。
			PRINTFORMDW %CALLNAME:TARGET%下意识地移開了目光。
		ELSEIF TALENT:愛欲
			PRINTFORML 「……打扰了」
			PRINTFORMDL %CALLNAME:MASTER%和%CALLNAME:TARGET%眼神相交。
			PRINTFORMDW 似乎是想到了什么，%CALLNAME:TARGET%的呼吸频率乱了。
		ELSE
			PRINTFORMW 「打扰了」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて一緒に入る
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		IF TALENT:恋人
			SELECTCASE RAND:2
				CASE 0
					PRINTFORMW 「啊，%CALLNAME:MASTER%已经在洗了吗？」
					PRINTFORMDW 将门拉開，用手提住浴巾的%CALLNAME:TARGET%进来了。
					PRINTFORMW 「那，我和你一起洗吧」
					PRINTFORMDW %CALLNAME:TARGET%身上的浴巾不能完全遮掩身体，不同部位的肌肤随着她的移动若隐若现。
					PRINTFORMW 「……你一直在盯着看哦？有那么好看吗……」
					PRINTFORMDW %CALLNAME:TARGET%微红着脸，坐在%CALLNAME:MASTER%的身边。
				CASE 1
					PRINTFORMW 「诶？好大的雾气……」
					PRINTFORMDW 门被拉開，隐约可以看见一个窈窕的身影走进了房间，是%CALLNAME:TARGET%。
					PRINTFORMW 「啊……」
					PRINTFORMDW 发现了浴室里的人，%CALLNAME:TARGET%手中提着的浴巾滑落下来。
					PRINTFORMDW 大片晃眼的肌肤裸露在潮湿的空气中，\@ TALENT:42:105 >= 1 ? 饱满的乳房上， # 胸前的 \@两点红晕依稀可见……
					PRINTFORMW 「什么啊……%CALLNAME:MASTER%你在啊，那就一起洗吧」
					PRINTFORMDW %CALLNAME:TARGET%红着脸蹲下身，将浴巾捡起裹在身上。
					PRINTFORMW 「%CALLNAME:MASTER%？眼睛都直了啊……傻瓜！」
			ENDSELECT
		ELSE
			PRINTFORMW 「我进来了哦……」
			PRINTFORMDW 门被打開，紧紧提住包裹在身上的浴巾的%CALLNAME:TARGET%走了进来。
			PRINTFORMDW 她没意识到正是因为裹的太紧，美丽的曲线已经被%CALLNAME:MASTER%一览无余……
			IF (TALENT:MASTER:2 == 2)
				PRINTFORMW 「既然是混浴，那也没办法啊……」
			ELSE
				PRINTFORMW 「都是女孩子的话，一起洗澡也没什么吧……」
			ENDIF
			PRINTFORMDW 嘴上这样小声嘟囔着，%CALLNAME:TARGET%\@ TALENT:42:恋慕 ? 在靠近%CALLNAME:MASTER%的位置 # 背对着%CALLNAME:MASTER% \@坐了下来。
			PRINTFORMDW 在浴室的氛围中，%CALLNAME:TARGET%渐渐放松了……
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて出て行った
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		PRINTFORMW 「诶？」
		PRINTFORMDW 随意包裹着浴巾的%CALLNAME:TARGET%走了进来。
		PRINTFORMW 「失、失礼了……」
		PRINTFORMDW %CALLNAME:TARGET%有点尴尬地退了出去。
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて追い出された
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		PRINTFORMW 「额……%CALLNAME:MASTER%怎么在这里……」
		PRINTFORMDW 发现浴室已经有人的%CALLNAME:TARGET%尴尬地站在那里。
		PRINTFORMW 「唔，啧……算了我等会儿再……」
		PRINTFORMDW 看着提着浴巾一脸烦闷的%CALLNAME:TARGET%，%CALLNAME:MASTER%表示自己已经洗好了。
		PRINTFORMW 「这样啊？那就——」
		PRINTFORMDW %CALLNAME:TARGET%对着门外做了个『请』的手势。
		PRINTFORMDW ……还真不客气啊。
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;外出先で遭遇した
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		IF TALENT:恋慕
			PRINTFORML 「啊，%CALLNAME:MASTER%」
			PRINTFORMDW %CALLNAME:TARGET%的嘴角勾起了微笑。
		ELSE
			SELECTCASE TIME:2
				;朝
				CASE 1,2
					PRINTFORMW 「%CALLNAME:MASTER%，早上好」
				;昼
				CASE 3,4
					PRINTFORMW 「%CALLNAME:MASTER%，中午好」
				;夜
				CASE 5,6,7
					PRINTFORMW 「%CALLNAME:MASTER%，晚上好」
				ENDSELECT
		ENDIF
	ENDIF
	;-------------------------------------------------
	;他角色と约会中に遭遇した
	;ARG:1に约会的对象
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 7
		;一日一回
		IF TCVAR:42:351 == 1
			RETURN 1
		ENDIF
		IF TALENT:恋人
			IF CFLAG:42:4 >= 3000 
				IF (ARG:1 == 29)
					PRINTFORMDW 从%CALLNAME:TARGET%那里感受到一道锐利的眼神……
					PRINTFORMDW 今天的%CALLNAME:TARGET%出乎意料的充满攻击性？
					PRINTFORMDW 是因为文吗……
					PRINTFORMDW 已经对%CALLNAME:MASTER%有了长足的了解的%CALLNAME:TARGET%哼了一声，扭过头去。
					PRINTFORMDW 看起来并不打算追究太多的样子。
				ELSEIF (ARG:1 == 65)
					PRINTFORMDW 趁着椛转移注意力的片刻，%CALLNAME:TARGET%悄悄凑了上来。
					PRINTFORMW 「我一直想和这孩子打好关系呢」
					PRINTFORMW 「就是不知道有你夹在中间是好还是坏……」
					PRINTFORMW 「……谁要你帮忙啊？还是先把你这沾花惹草的毛病改一改吧」
					PRINTFORMW 「被当成情敌就很尴尬了啊……」
					PRINTFORMDW %CALLNAME:TARGET%无奈地叹了口气。
				ELSE
					PRINTFORMDW %CALLNAME:MASTER%悄悄对%CALLNAME:TARGET%打了个招呼。
					PRINTFORMW 「笨蛋，别在这种时候跟我打招呼啊」
					PRINTFORMDW %CALLNAME:TARGET%只做口型却没有发出声音，这样说着。
					PRINTFORMW 「在搞外遇的时候和正牌女友打招呼你是傻瓜吗？是傻瓜吧！傻瓜！」
					PRINTFORMDW	经过短暂的无声交流后，%CALLNAME:TARGET%扭过头去不再搭理\@ TALENT:MASTER:性別 == 2 ? 他 # 她 \@……
				ENDIF
			ELSE
				PRINTFORMDW 从%CALLNAME:TARGET%那里传来一道复杂的眼神……
				PRINTFORMDW %CALLNAME:TARGET%的不快和抑郁各增加了500……
				PRINTFORMDW %CALLNAME:TARGET%的心情变差了……
				Juel:42:31 += 500
				Juel:42:32 += 500
				CFLAG:42:322 = 1
			ENDIF
		ELSEIF TALENT:恋慕
			IF (ARG:1 == 29)
				PRINTFORMDW %CALLNAME:TARGET%远远地看了这边一眼，没有上来打招呼……
				PRINTFORMDW %CALLNAME:TARGET%的抑郁增加了200……
				Juel:42:32 += 500
			ELSEIF (ARG:1 == 65)
				PRINTFORMW 「%CALLNAME:MASTER%和椛在约会啊」
				PRINTFORMW 「要玩的開心点哦」
				PRINTFORMDW %CALLNAME:TARGET%的笑容似乎有些复杂。
			ELSE
				PRINTFORMW 「%CALLNAME:MASTER%……在约会啊」
				PRINTFORMW 「那，那我就不打扰你们了……」
				PRINTFORMDW %CALLNAME:TARGET%看起来有些没精神……
			ENDIF
		ELSEIF TALENT:思慕
			PRINTFORMW 「%CALLNAME:MASTER%这是……在约会啊」
			IF (ARG:1 == 29)
				PRINTFORMW 「啊，我突然想起来我还有工作……不用在意我」
				PRINTFORMDW %CALLNAME:TARGET%看了文一眼，取出了手机低头操作着……
			ELSE
				PRINTFORMW 「要玩的開心点哦……」
			ENDIF
		ELSEIF TALENT:愛欲 || TALENT:風騷
			PRINTFORMW 「%CALLNAME:MASTER%这是……约会中吗？」
			IF (ARG:1 == 29)
				PRINTFORMDW 拿出手机，%CALLNAME:TARGET%投来了十分感兴趣的眼神。
			ELSEIF (ARG:1 == 29)
				PRINTFORMW 「啊呀啊呀……」
				PRINTFORMDW %CALLNAME:TARGET%露出了意味深长的笑容。
			ENDIF
		ELSE
			PRINTFORMW 「哼哼，感情不错嘛，你们两个」
		ENDIF
		TCVAR:42:351 = 1
		RETURN 1
	ENDIF
	;其他
	PRINTFORMDL 
ENDIF
RETURN 1


