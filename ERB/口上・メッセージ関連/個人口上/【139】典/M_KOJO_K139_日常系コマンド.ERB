;==================================================
;621,釣魚
;TFLAG:193(0=釣れない 1～4=魚 11～15=好吃的魚 610=木材 131=守矢之守札 21=跳蛋 609=氷之鱗 606=龍之爪)
;TFLAG:194(1000以上ならTARGETが釣った)
;==================================================
@M_KOJO_MESSAGE_COM_K139_621
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K139_621_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K139_621_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF TALENT:139:心情 == -1 || CFLAG:139:ブチギレ == 1
		PRINTFORML 「…………」
		PRINTFORML 典一脸不悦地钓着鱼…
		PRINTW 
	ELSE
		PRINTFORML 「鱼儿啊ー、鱼儿啊ー♪　何时上钩呢ー♪」
		PRINTFORML 钓鱼时的典很天真
		PRINTW 
	ENDIF
	
	SELECTCASE TFLAG:194
	;典が釣った
	CASE IS >= 1000
		SELECTCASE TFLAG:193
		CASE 0
			PRINTFORML 「哎哎！　欸、什么也没钓上来啊…」
			PRINTFORML 看见典什么也没钓上来的%CALLNAME:MASTER%大笑起来、典却一脸严肃
			PRINTFORML 「唔ー。下次我要钓条大鱼给你看ー」
			PRINTW 
		;普通の魚
		CASE 1 TO 4
			PRINTFORML 「请看。%CALLNAME:MASTER%阁下、典钓到鱼了呢♪」
			PRINTFORML 典一边展示着扑哧扑哧跳着的%FISH_NAME(TFLAG:193)%、一边露出得意的笑容
			PRINTW 
		;好吃的魚
		CASE 11 TO 15
			PRINTFORML 「%CALLNAME:MASTER%阁下！　%CALLNAME:MASTER%阁下！　是%FISH_NAME(TFLAG:193)%啊！」
			PRINTFORML 「钓到了%FISH_NAME(TFLAG:193)%哟！（ﾄﾞﾔｧ）」
			PRINTW 
		;木材
		CASE 610
			PRINTFORML 「这是怎么回事！」
			PRINTFORML 「觉得很重、结果是木材啊…这样的话、那就不能用了呢」
			PRINTW 
		;守矢之守札
		CASE 131
			PRINTFORML 「哇、钓到垃圾了」
			PRINTFORML 「…失礼了、不小心说了脏话」
			PRINTW 
		;跳蛋
		CASE 21
			PRINTFORML 「啊咧、这个是跳蛋吧…是谁把它放到河里的」
			PRINTFORML 「…你要用吗？」
			PRINTW 
		;氷之鱗
		CASE 609
			PRINTFORML 「真是漂亮的鳞片啊…都是很珍贵的材料」
			PRINTFORML 「不过、不溶于河水真是不可思议啊」
			PRINTW 
		;龍之爪
		CASE 606
			PRINTFORML 「不不不…没想到龙真的存在吗」
			PRINTFORML 「虽说是龙的爪、但并不是龙大人的爪」
			PRINTW 
		ENDSELECT
	;你が釣った
	CASEELSE
		IF TALENT:139:心情 == -1 || CFLAG:139:ブチギレ == 1
			SELECTCASE TFLAG:193
			CASE 0
				PRINTFORML 典看见%CALLNAME:MASTER%什么也没钓上来、讥笑了一声
			CASE 1 TO 4
				PRINTFORML 看见%CALLNAME:MASTER%钓到了%FISH_NAME(TFLAG:193)%、典露出了生气的表情
			CASE 11 TO 15
				PRINTFORML 看着%CALLNAME:MASTER%得意洋洋地炫耀着%FISH_NAME(TFLAG:193)%、
				PRINTFORML 典的钓竿開始剧烈颤抖起来…
			CASE 21
				PRINTFORML 拿着震动的跳蛋靠近典、典用力把你推開了
				PRINTFORML 结果打在了%CALLNAME:MASTER%的脸上
				PRINTFORML ┏━━━━━━┓
				PRINTFORML ┃　好痛！　┃
				PRINTFORML ┗━━━━━━┛
			CASEELSE
				PRINTFORML 把钓到的鱼拿给典看、典若无其事地「嗯ー」了一声
			ENDSELECT
			PRINTW 
		ELSE
			SELECTCASE TFLAG:193
			CASE 0
				SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「…嘛、也有这种情况」
					PRINTFORML 典露出为难的笑容、安慰着你
				CASE 1
					PRINTFORML 「说不定下次就能钓到了哦。不要放弃、加油吧」
				CASE 2
					PRINTFORML 「啊啊、%CALLNAME:MASTER%阁下钓上来的是『只有傻瓜才看不见的鱼』…」
					PRINTFORML 「因为典是个愚者…所以才看不见鱼的身影」
					PRINTFORML 这家伙明明知道什么也没钓上来…
				ENDSELECT
				PRINTW 
			;普通の魚
			CASE 1 TO 4
				PRINTFORML 「请看。%CALLNAME:MASTER%阁下、典钓到鱼了呢♪」
				PRINTFORML 典一边展示着扑哧扑哧跳着的%FISH_NAME(TFLAG:193)%、一边露出得意的笑容
				PRINTW 
			;好吃的魚
			CASE 11 TO 15
				PRINTFORML 「%CALLNAME:MASTER%阁下！　%CALLNAME:MASTER%阁下！　是%FISH_NAME(TFLAG:193)%啊！」
				PRINTFORML 「钓到了%FISH_NAME(TFLAG:193)%哟！（ﾄﾞﾔｧ）」
				PRINTW 
			;跳蛋
			CASE 21
				PRINTFORML 「那是…新品种的鱼…是吗」
				PRINTFORML 咕噜噜噜噜噜！！
				PRINTFORML 「…很有活力呢」
				PRINTW 
			CASEELSE
				PRINTFORML 「哎呀、钓到什么稀有的东西了」
				PRINTW 
			ENDSELECT
		ENDIF
	ENDSELECT
ENDIF
RETURN 1
