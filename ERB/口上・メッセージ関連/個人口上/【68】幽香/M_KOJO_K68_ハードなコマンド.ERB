;==================================================
;140,深喉插入
;==================================================
@M_KOJO_MESSAGE_COM_K68_140
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K68_140_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K68_140_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;時姦中
	IF FLAG:70
		RETURN 1
	;睡姦中
	ELSEIF CFLAG:MASTER:恶作剧 == 2
		RETURN 1
	ELSEIF TCVAR:68:烂醉 == 1
		
	;それ以外
	ELSE
		SELECTCASE RAND:2
			CASE 0
				PRINTFORML 「嗯……呼！？呼！？啊、啊、可是、呼……啊！！」
				SIF ABL:68:受虐属性 >= 2
					PRINTFORML （明明很痛苦，却很舒服……为什么……？）
			CASEELSE
				PRINTFORML 「唔！？唔！？嗯、啊、是…………！」
				SIF ABL:68:受虐属性 >= 2
					PRINTFORML （这么点事，算不了什么……）
		ENDSELECT
		PRINTFORMDW 在痛苦中扭曲着的同时、幽香用泛着泪花的眼睛盯着%CALLNAME:MASTER%
	ENDIF
ENDIF

RETURN 1
