;80,手淫
;==================================================
@M_KOJO_MESSAGE_COM_K125_80
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K125_80_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K125_80_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		IF TALENT:恋人 || TALENT:恋慕
			PRINTFORMW 「用手做吗？」
			PRINTFORMW 「嘛、交给我吧」
			PRINTFORMW 女苑手势非常麻利、将手放在%CALLNAME:MASTER%的肉棒上
			PRINTFORMW 「哇……硬邦邦的……%UNICODE(0x2764)%」
		ENDIF
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;恋人
	ELSEIF TALENT:恋人 || TALENT:恋慕
		IF RAND:3 == 0
			PRINTFORMW 「嗯……就是这种感觉吧……」
			PRINTFORMW 「怎、如何？心情很好吧」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「这和我心中的…」
			PRINTFORMW 「这、根本没有想象过！」
		ELSE
			PRINTFORMW 「哇哇……战战兢兢」
			PRINTFORMW 「没什么好害怕的！」
		ENDIF
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		PRINTFORMW 
		RETURN 1
	;それ以外
	ELSE
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF
RETURN 1

