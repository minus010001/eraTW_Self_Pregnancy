;### ＶＡ共通対面座位口上用関数 ################
@DIALOGUE_K78_VA_FACE_TO_FACE_SITTING,ARGS

IF K78_BASIC_CONDITION_IN_MATING()
	$VA_FACE_TO_FACE_SITTING_LOOP
	IF TALENT:恋慕 || TALENT:愛欲
		SELECTCASE RAND:6
		CASE 1
			IF !TEQUIP:縄 && !TEQUIP:拘束 && !TEQUIP:継続接吻 && !TEQUIP:吸乳头 && ( K78_ARMPIT_EXPOSURE() || K78_SWEATY_UNDERARMS() )
				IF K78_ARMPIT_EXPOSURE()
					PRINTFORMD 在膝盖上扭动腰%CALLNAME:TARGET%举起了手臂，
					IF K78_ARMPIT_SWEAT() && RAND:10 >= 7
						PRINTFORMD 用舌头舔着
					ELSE
						PRINTFORMD %K78_ARMPIT_HAIR_MODIFIER()%
					ENDIF
					PRINTFORMDL 沾满汗水的腋下。
					CALL K78_HPH_PRINT,@"「嗯唔HPH 虽然有点痒，但还是很舒服HPH哈啊HPH」","W"
				ELSE
					PRINTFORMDL 让坐在膝盖上的%CALLNAME:TARGET%举起一只手臂，一边将脸贴在湿透的腋下一边动着腰。
					CALL K78_HPH_PRINT,@"「呀，等下HPH 还真是变态呀HPH 呀，别再闻了HPH」","W"
				ENDIF
			ELSE
				GOTO VA_FACE_TO_FACE_SITTING_LOOP
			ENDIF
		CASE 2
			IF TEQUIP:継続接吻
				PRINTFORMDL %CALLNAME:TARGET%一边摇着腰，一边贴紧%CALLNAME:MASTER%，不断地向其索吻。
				CALL K78_HPH_PRINT,@"「はむっ、ん…… んちゅ… はぁHPH ちゅっHPH ちゅぱ…あむ…… ん、もっとぉHPH」","W"
			ELSE
				GOTO VA_FACE_TO_FACE_SITTING_LOOP
			ENDIF
		CASE 3
			IF !TEQUIP:継続接吻
				CALL K78_HPH_PRINT,@"「哈啊 哈啊HPH 嗯、啊哈、哈、哈HPH」","L"
				PRINTFORMDW 从%CALLNAME:TARGET%的嘴里散发出比任何时候都更香甜的吐息，喷到了%CALLNAME:MASTER%的脸上。
			ELSE
				GOTO VA_FACE_TO_FACE_SITTING_LOOP
			ENDIF
		CASE 4
			IF !TEQUIP:縄 && !TEQUIP:拘束
				PRINTFORMDL %CALLNAME:MASTER%搂着%CALLNAME:TARGET%的腰部，紧紧地抱住%CALLNAME:TARGET%，为了回应，%CALLNAME:TARGET%将手臂绕在其脖子上。
				CALL K78_HPH_PRINT,@"「唉嘿嘿♪这样紧紧的抱在一起HPH 不管是%CALLNAME:MASTER%的心跳声也好还是其他东西也好，感觉都能感受到了HPH」","W"
			ELSE
				GOTO VA_FACE_TO_FACE_SITTING_LOOP
			ENDIF
		CASE 5
			PRINTFORMDL 互相之间不怎么动腰的焦急感反而更加激情，在耳边不停地倾诉着情爱
			CALL K78_HPH_PRINT,@"「哈、哈、喜欢、最喜欢了HPH我爱你HPH想和你这样就这样一直呆下去的HPH」","W"
		CASEELSE
			PRINTFORMDL 慢慢摇着腰做圆周运动的%CALLNAME:TARGET%的煽情姿态、点燃了%CALLNAME:MASTER%的欲火。
			CALL K78_HPH_PRINT,@"「嘿嘿HPH 这样动如何？ 如果舒服的话我也会很開心的HPH」","W"
		ENDSELECT
	ELSE
		SELECTCASE RAND:3
		CASE 1
			IF ABL:腰 >= 3
				PRINTFORMDL %CALLNAME:TARGET%在缓慢扭动的同时确实提高了%CALLNAME:MASTER%的快感。
				PRINTFORMW 「这样慢慢的H感觉也不错呐」
			ELSE
				PRINTFORMW 「我也要动吗？唔…还真是有点难呀…」
			ENDIF
		CASE 2
			PRINTFORML 「大丈夫？ 阿，会不会很重？」
			PRINTFORMDW %CALLNAME:TARGET%毕竟也是个年轻的女孩子，还是很在意自己的体重的。
		CASEELSE
			PRINTFORMW 「这，这样子离这么近…有点不好意思…」
		ENDSELECT
	ENDIF
ENDIF


