;奴隷の絶頂時の口上
;NOWEX:0～4 CVABM絶頂
;NOWEX:6 二重絶頂
;NOWEX:7 三重絶頂
;NOWEX:8 四重絶頂
;NOWEX:9 五重絶頂
;NOWEX:10 噴乳
;NOWEX:11 射精
;NOWEX:12 放尿
;随意組み合わせて使ってください
;-------------------------------------------------
@M_KOJO_MESSAGE_PALAMCNG_B_K78
CALL PALAM_MESSAGE_B
;-------------------------------------------------
;噴乳
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL && NOWEX:噴乳
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;射精
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL && NOWEX:射精
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;放尿
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL && NOWEX:放尿
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;絶頂
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF K78_CONSCIOUS_AND_TOLERANT() && !TALENT:無知
		;-------------------------------------------------
		;五重絶頂
		;-------------------------------------------------
		IF NOWEX:五重絶頂
			SELECTCASE RAND:3
			CASE 1
				CALL K78_HPH_PRINT,@"「おっ、んぉっHPH んはぁHPH あぁ、あっ、ああ゛あ゛あぁっHPH い、い゛ぐううぅぅHPH」","L"
				PRINTFORMDL 在%CALLNAME:TARGET%大声娇喘之后，如同断了线一般全身松弛，呼哧呼哧地在靠在肩上喘着气。
				PRINTFORMW 「呼、呼、呼 ……够…够了…饶了我吧…」
			CASE 2
				CALL K78_HPH_PRINT,@"「ひゃっ、あぁっ、あっHPH んああぁっHPH んぃ、んひいぃぃHPH」","L"
				PRINTFORMDL %CALLNAME:TARGET%的脸上满是眼泪鼻涕和口水，全身无力的抽搐着
				CALL K78_HPH_PRINT,@"「はぁ、はっ、はぇHPH ぉ、ぅあ、はぁHPH ふぅ、ふぅHPH んぅHPH」","W"
			CASEELSE
				CALL K78_HPH_PRINT,@"「要死了、要死掉了HPH 不行了HPH不行HPH ほぉっ、んほおお゛お゛ぉぉHPH」","L"
				PRINTFORMDL 发出了象悲鸣一样的哭喊的%CALLNAME:TARGET%，筋疲力尽的靠在%CALLNAME:TARGET%身上。
				CALL K78_HPH_PRINT,@"「哈、哈……太，太厉害了HPH」","W"
			ENDSELECT
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;四重絶頂
		;-------------------------------------------------
		IF NOWEX:四重絶頂
			SELECTCASE RAND:3
			CASE 1
				CALL K78_HPH_PRINT,@"「啊啊，不行HPH 饶了我吧HPH 哈、恩啊啊啊HPH」","W"
			CASE 2
				CALL K78_HPH_PRINT,@"「已经不行了HPH太舒服了，要变成白痴了HPH脑袋都一片空白了HPH」","W"
			CASEELSE
				CALL K78_HPH_PRINT,@"「不要HPH 已经要去了HPH 太舒服了HPH 感觉要HPH 飞起来了HPH」","W"
			ENDSELECT
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;三重絶頂
		;-------------------------------------------------
		IF NOWEX:三重絶頂
			SELECTCASE RAND:3
			CASE 1
				CALL K78_HPH_PRINT,@"「啊啊HPH这个好厉害啊HPH啊啊啊啊HPH啊啊啊啊」","W"
			CASE 2
				CALL K78_HPH_PRINT,@"「恩、恩HPH不行啊HPH什么很厉害的要来了HPH」","W"
			CASEELSE
				CALL K78_HPH_PRINT,@"「嗯阿HPH 呜阿HPH 不行HPH 咿，咿咿～～っHPH」","W"
			ENDSELECT
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;二重絶頂
		;-------------------------------------------------
		IF NOWEX:二重絶頂
			SELECTCASE RAND:3
			CASE 1
				CALL K78_HPH_PRINT,@"「啊，啊哈。哈HPH 要去了 去了啊啊HPH」","W"
			CASE 2
				CALL K78_HPH_PRINT,@"「哈啊、啊啊HPH 啊、啊、去了　要去了HPH」","W"
			CASEELSE
				CALL K78_HPH_PRINT,@"「嗯哈、哈啊、昂HPH %CALLNAME:MASTER%　好舒服啊HPH」","W"
			ENDSELECT
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;単部位の絶頂
		;-------------------------------------------------	
		IF NOWEX:Ｃ絶頂
			;超強絶頂Ｃ
			IF NOWEX:Ｃ絶頂 == 3
				SELECTCASE RAND:3
				CASE 1
					CALL K78_HPH_PRINT,@"「\@ TALENT:性別 & 2 ?おちんちん #クリ \@きもちいいっHPH あんっHPH ん、んぃっHPH」","W"
				CASE 2
					CALL K78_HPH_PRINT,@"「んふぅHPH んんぅHPH おっ、\@ TALENT:性別 & 2 ?おちんちん #お豆 \@キちゃうぅHPH」","W"
				CASEELSE
					CALL K78_HPH_PRINT,@"「\@ TALENT:性別 & 2 ?おちんちん #お豆 \@すごっ、すごいのぉっHPH あぁ、はぁっ、はんぅっHPH」","W"
				ENDSELECT
			;強絶頂Ｃ
			ELSEIF NOWEX:Ｃ絶頂 == 2
				CALL K78_HPH_PRINT,@"「呼啊　哈、咿、高潮了HPH」","W" 
			;絶頂Ｃ
			ELSE
				PRINTFORMW 「啊、嗯、哈那里、对、就是那里」
			ENDIF
			RETURN 1
		ENDIF
		IF NOWEX:Ｖ絶頂
			;超強絶頂Ｖ
			IF NOWEX:Ｖ絶頂 == 3
				SELECTCASE RAND:3
				CASE 1
					CALL K78_HPH_PRINT,@"「嗯阿、小穴好厉害HPH 不行了、啊啊HPH」","W"
				CASE 2
					CALL K78_HPH_PRINT,@"「好厉害HPH小穴要去了HPH 嗯，库～～ッHPH」","W"
				CASEELSE
					CALL K78_HPH_PRINT,@"「好舒服HPH小穴咕啾咕啾的响着HPH」","W"
				ENDSELECT
			;強絶頂Ｖ
			ELSEIF NOWEX:Ｖ絶頂 == 2
				CALL K78_HPH_PRINT,@"「啊啊、啊、啊哈、里面、去了HPH」","W"
			;絶頂Ｖ
			ELSE
				PRINTFORMW 「嗯，啊，就是那里，要来了」
			ENDIF
			RETURN 1
		ENDIF
		IF NOWEX:Ａ絶頂
			;超強絶頂Ａ
			IF NOWEX:Ａ絶頂 == 3
				SELECTCASE RAND:3
				CASE 1
					CALL K78_HPH_PRINT,@"「嗯哦HPH 啊啊HPH 屁股，有什么厉害的东西，要来了！HPH」","W"
				CASE 2
					CALL K78_HPH_PRINT,@"「恩，恩嘿HPH 屁穴HPH 屁穴要去了HPH」","W"
				CASEELSE
					CALL K78_HPH_PRINT,@"「啊，呀啊HPH 屁股洞要去了HPH 唔，啊啊啊！HPH」","W"
				ENDSELECT
			;強絶頂Ａ
			ELSEIF NOWEX:Ａ絶頂 == 2
				CALL K78_HPH_PRINT,@"「啊，屁股要，呜阿，啊哈HPH」","W"
			;絶頂Ａ
			ELSE
				PRINTFORMW 「啊、哼、哼、啊啊、啊啊」
			ENDIF
			RETURN 1
		ENDIF
		IF NOWEX:Ｂ絶頂
			;超強絶頂Ｂ
			IF NOWEX:Ｂ絶頂 == 3
				SELECTCASE RAND:3
				CASE 1
					CALL K78_HPH_PRINT,@"「啊哈HPH 好，好舒服HPH 胸部要去了HPH 」","W"
				CASE 2
					CALL K78_HPH_PRINT,@"「乳头酥酥麻麻的HPH 好厉害HPH 要去了，去了～～っHPH」","W"
				CASEELSE
					CALL K78_HPH_PRINT,@"「胸部，胸部好热HPH 啊啊、胸部要高潮了HPH」","W"
				ENDSELECT
			;強絶頂Ｂ
			ELSEIF NOWEX:Ｂ絶頂 == 2
				CALL K78_HPH_PRINT,@"「唔唔唔　啊啊，胸部不行了，咿咿咿！HPH」","W"
			;絶頂Ｂ
			ELSE
				PRINTFORMW 「嗯，哈，嗯，啊嗯」
			ENDIF
			RETURN 1
		ENDIF
		[SKIPSTART]
		IF NOWEX:Ｍ絶頂
			;超強絶頂Ｍ
			IF NOWEX:Ｍ絶頂 == 3
				PRINTFORMW 
			;強絶頂Ｍ
			ELSEIF NOWEX:Ｍ絶頂 == 2
				PRINTFORMW 
			;絶頂Ｍ
			ELSE
				PRINTFORMW 
			ENDIF
		ENDIF
		[SKIPEND]
	ENDIF
ENDIF
RETURN 1
