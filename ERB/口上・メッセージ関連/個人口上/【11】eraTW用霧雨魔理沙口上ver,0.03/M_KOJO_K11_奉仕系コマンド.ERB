;80,手淫
;==================================================
@M_KOJO_MESSAGE_COM_K11_80
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K11_80_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K11_80_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM) && !FLAG:時間停止
		PRINTFORML 「想要用手做吗？好吧」
		PRINTFORMW 「唔嗯、是这样子的感觉的吗？」
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;恋慕
	ELSEIF TALENT:恋慕
		PRINTFORMW 
		RETURN 1
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		PRINTFORMW 
		RETURN 1
	;それ以外
	ELSE
		PRINTFORML 「呼呼~……已经一颤一颤的了呢……」
		PRINTFORML 魔理沙用小小的手套弄着%CALLNAME:MASTER%的下体…
		PRINTFORML 「怎样……舒服吗……%CALLNAME:MASTER%？」
		PRINTFORMW 「忍耐对身体不好呢，没有必要忍耐哦~」
		RETURN 1
	ENDIF
ENDIF
RETURN 1

