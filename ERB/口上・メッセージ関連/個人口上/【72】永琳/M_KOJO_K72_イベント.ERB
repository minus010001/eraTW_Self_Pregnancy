;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
	;疲惫
		PRINTFORMW 「……唔，啊♥　——不愧是你，稍微有點累了」
		PRINTFORMDL 永琳把頭靠在你的胸前，溫柔地微笑著。
		PRINTFORMW 「謝謝%CALLNAME:MASTER%。…願意和我交往」
		PRINTFORMDL 用摸頭代替回答，使她像貓一樣眯起了眼睛。
		PRINTFORMDL 就這樣，兩人之後在溫馨的氣氛中慢慢睡著了。
		PRINTFORML 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
	;満足して解放
		PRINTFORMW 「ふぅ…♥」
		PRINTFORMDL 永琳發出了滿意的吐息，撲在你身上緊緊地蓋住了你。
		PRINTFORMW 「唔......我覺得非常舒服哦…♥」
		PRINTFORMDL 你摸了摸她的頭，她舒服地眯起了眼睛。
		PRINTFORMW 「呵呵。…謝謝%CALLNAME:MASTER%。能和我這樣纏綿哦」
		PRINTFORMW 「善後工作由我來做。…你，好好地休息吧……♥」
		PRINTFORMDL 聽從了她的話，你享受著永琳的侍奉。
		PRINTFORML 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
	;満足したところを被逆推た
		PRINTFORMW 「きゃ――」
		PRINTFORMDL 在永琳流露出滿意喘息的間隙，你推倒了她。
		PRINTFORMDL 束縛著她奄奄一息、四肢無力的身體，粗暴地舔舐著乳頭。
		PRINTFORMW 「%CALLNAME:MASTER%…？　我已經…♥」
		PRINTFORMW 「ぁ――♥　等等啊♥　讓我稍微休息一下——咿呀♥」
		PRINTFORMDL 享受著永琳慘叫般的嬌叫著的反應的同時，你盡情地向對方報復著。
		PRINTFORML
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
	;壊れるまでしてあげる
		PRINTFORMW 「哎呀，居然敢說出這種話呢…？」
		PRINTFORMDL 永琳眯起了眼睛。
		PRINTFORMW 「那麼——%CALLNAME:MASTER%的希望之路」
		PRINTFORMDL 妖豔地微笑著，舌尖微微舔舐著嘴唇，靠近你的耳邊說道
		PRINTFORMW 「——沒關係。我會好好準備的必須的藥品的」
		PRINTFORMW 「%CALLNAME:MASTER%等下，不管變成什麼樣子……我都會馬上給你治好…♥」
		PRINTFORMDL 永琳用令人毛骨悚然的嬌豔聲調，溫柔地低語著。
		PRINTFORML
		RETURN 1
	ENDIF
ENDIF
RETURN 1
;==================================================
;一日の終了時(AFTERTRAニーの前)、面識がなければ呼び出されない
;==================================================
@M_KOJO_EVENT_K72_31(ARG,ARG:1)
;口上主はTARGET
;ARGは会ってない日数
;何日以上会ってなかったら愚痴を言うとかできます
;CFLAG:オナバレで分岐入れてもよさそう
;映姫様口上を参照
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;今日会っていない
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG
		;映姫様口上に同意して頻度を少なく。
		IF ARG % 30 == 0 && ARG != 0 && !TALENT:72:恋慕 && !TALENT:72:思慕 && !TALENT:72:愛欲 && !TALENT:72:炮友 == 2 && !TALENT:72:炮友
			PRINTFORMW 「說起來——」
			PRINTFORMDL 入睡前，永琳突然想起了%CALLNAME:MASTER%的臉。
			PRINTFORMW 「好久當面見過他了，他還好吧」
			PRINTDATADL
				DATAFORM 「…没生病就好了」
				DATAFORM 「…不会受伤了吧」
				DATAFORM 「…但愿没有卷入什么异变当中」
			ENDDATA
			PRINTFORMDL 經過一番擔心，她終於入睡了。
			PRINTFORML 
		;愛欲は十日に一度。
		ELSEIF ARG % 10 == 0 && ARG != 0 && TALENT:72:愛欲
			PRINTFORMW 「唔、%CALLNAME:MASTER%…。你現在在做什麼呢」
			PRINTFORMW 「好想要擁抱你……一直沒來，是在捉弄我麼？　誒嘿嘿。真是，殘酷的人――」
			PRINTFORMDL 永琳呼出熾熱的吐息，睡著了。
			PRINTFORML 
		;愛人も十日に一度。
		ELSEIF ARG % 10 == 0 && ARG != 0 && TALENT:72:炮友 == 2 && TALENT:72:思慕
			PRINTFORMW 「%CALLNAME:MASTER%為什麼不來看我呢…」
			PRINTFORMW 「這樣的話，我就只能一直自慰了——要不要去見見他呢。呵呵，那也不錯啊」
			PRINTFORMDL 在這樣想著的同時，永琳睡著了。
			PRINTFORML 
		;炮友も十日に一度。
		ELSEIF ARG % 10 == 0 && ARG != 0 && TALENT:72:炮友
			PRINTFORMW 「%CALLNAME:MASTER%為什麼不來看我呢」
			PRINTFORMW 「你對我的身體厭煩了嗎。如果是這樣的話——我會覺得寂寞的啊…」
			PRINTFORMDL 永琳輕輕地歎著氣，慢慢入睡了。
			PRINTFORML 
		;思慕は十四日に一度。
		ELSEIF ARG % 14 == 0 && ARG != 0 && TALENT:72:思慕 && !TALENT:72:炮友 == 2
			PRINTFORMW 「%CALLNAME:MASTER%――」
			PRINTFORMDL 想起了好久沒見的思念之人的臉，永琳歎著氣。
			PRINTFORMW 「生病了，受傷了。你現在到底怎麼樣了」
			PRINTFORMW 「如果能讓我看到你健康的樣子，我會很高興的…」
			PRINTFORMDL 苦悶的思考著的同時，睡著了。
			PRINTFORML 
		;恋慕は七日に一度。
		ELSEIF ARG % 7 == 0 && ARG != 0
			IF TALENT:72:恋人
				PRINTFORMW 「…%CALLNAME:MASTER%身體健康麼」
				PRINTFORMW 「下次休息的時候，我去看看他吧。就當是，去進行患者的家訪吧」
				PRINTFORMW 「順便也想採集一下那個。…是啊，就這樣吧」
				PRINTFORMDL 想到幾個藉口之後，永琳就睡著了。
				PRINTFORML 
			ELSEIF TALENT:72:恋慕
				PRINTFORMW 「%CALLNAME:MASTER%身體健康麼」
				PRINTFORMW 「……只是有段時間沒看到他的臉，就這麼不安」
				PRINTFORMW 「事到如今，我——，已經忍耐不下去了呢。」
				PRINTFORMDL 永琳自嘲似的笑著閉上眼睛，入睡了。
				PRINTFORML 
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;今日会っている
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		IF CFLAG:72:1000 >= 100
			IF FIRSTTIME("母乳発覚")
				CALL PRINT_FACE, [[永琳]], "発情", "服", "", ""
				PRINTFORMW 「這是——」
				PRINTFORMDL 半夜三更。永琳感到身體的一部分有些違和感，永琳緊鎖眉頭。
				PRINTFORMDL 摸著乳房的尖端，她聞著味道，確認分泌液的詳細情況——然後歎了口氣。
				PRINTFORMDL 理解了事態的永琳，用手巾把溢出的液體擦乾淨了
				PRINTFORMW 「明天去和%CALLNAME:MASTER%見一面吧」
					IF TALENT:恋人 || TALENT:恋慕 || TALENT:思慕
						PRINTFORMW 「呵呵。到時候他一定會嚇一跳吧…」
						PRINTFORMDL 一邊浮現出思念的人的臉一邊睡著了。
						PRINTFORML 
					ELSEIF TALENT:愛欲 || TALENT:炮友
						PRINTFORMW 「誒嘿嘿。他一定會嚇一跳的…♥」
						PRINTFORMDL 一邊浮現出思念的人的臉一邊睡著了。
						PRINTFORML 
					ELSEIF TALENT:炮友 == 2 && TALENT:思慕 
						PRINTFORMW 「誒嘿嘿。他一定會嚇一跳的…♥」
						PRINTFORMDL 一邊浮現出思念的人的臉一邊睡著了。
						PRINTFORML 
					ELSE
						PRINTFORMW 「總之，到時候要和他說一下…」
						PRINTFORMDL 想著某人的臉的同時，永琳睡著了。
						PRINTFORML 
					ENDIF
				CFLAG:72:1010 = 1
			ELSE
			ENDIF
		ELSE
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1
;==================================================
;自由行動中
;ARG:1      0=行動中,1=完了時
;技能系の行動は技能レベルが1以上なら発生
;==================================================
@M_KOJO_EVENT_K72_32(ARG,ARG:1)
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	SELECTCASE ARG
	CASE 0
		;▼考え事,（～は考えに耽っている。）
		IF !ARG:1
			;行動中
			PRINTFORMW 「最近經常發生異變呢，果然還是——」
			PRINTFORML 
		ELSE
			;完了時
			PRINTFORMW 「嗯，再看看情況吧」
			PRINTFORML 
		ENDIF
	CASE 1
		;▼玩遊戲,（～は走り回っている。）
		;※[幼稚]持ちのみ
		IF !ARG:1
			;行動中
			PRINTFORML 
		ELSE
			;完了時
			PRINTFORML 
		ENDIF
	CASE 2
		;▼吃飯,（～は吃飯を摂っている。）
		IF !ARG:1
			;行動中
			PRINTFORMW 「嗯。味道很好呢，非常美味」
			PRINTFORML 
		ELSE
			;完了時
			PRINTFORMW 「謝謝您的款待」
			PRINTFORML 
		ENDIF
	CASE 3
		;▼つまみ食い（～はこっそりつまみ食いをしている。）
		;※[妖精]持ちと露米亜、恋、幽幽子、紫苑のみ
		IF !ARG:1
			;行動中
			PRINTFORML 
		ELSE
			;完了時
			PRINTFORML 
		ENDIF
	CASE 4
		;▼掃除,（～はテキパキと部屋の掃除をしている。）
		;※[掃除係]持ちまたは汚臭耐性-1以下のみ
		IF !ARG:1
			;行動中
			PRINTFORMW 「偶爾也幫忙做些鈴仙的工作吧」
			PRINTFORML 
		ELSE
			;完了時
			PRINTFORMW 「好，打掃完成」
			PRINTFORML 
		ENDIF
	CASE 5
		;▼運動,（～は張り切って運動している。）
		;※[音楽知識]持ちは除く（演奏になる）
		IF !ARG:1
			;行動中
			PRINTFORMW 「偶爾，也需要，像這樣運動下。如果，一直不活動身體的話——唔」
			PRINTFORML 
		ELSE
			;完了時
			PRINTFORMW 「嗯。流了不少汗水」
			PRINTFORML 
		ENDIF
	CASE 6
		;▼読書,（～は黙々と本を読んでいる。）
		IF !ARG:1
			;行動中
			PRINTFORMW 「嗯…。很有意思的病例啊…」
			PRINTFORML 
		ELSE
			;完了時
			PRINTFORMW 「——哎呀。不知不覺就讀入迷了嗎」
			PRINTFORML 
		ENDIF
	CASE 7
		;▼料理,
		IF !ARG:1
			;行動中
			IF Activity_Duration:TARGET > 30
				;料理中（～は軽快に料理をしている。）
				PRINTFORMW 「偶爾做一次的話，料理也是很有樂趣的嘛」
				PRINTFORML 
			ELSE
				;吃飯中（～は作った料理を食べている。）
				PRINTFORMW 「嗯，雖然材料不充足，但我覺得做得相當好。」
				PRINTFORML 
			ENDIF
		ELSE
			;完了時
			PRINTFORML 
		ENDIF
	CASE 8
		;▼演奏
		;※[音楽知識]持ちのみ（TALENT:音楽知識で変化）
		;1,～は小気味よく演奏している。
		;2,～は気ままに歌っている。
		;3,～は華麗に踊っている。
		IF !ARG:1
			;行動中
			PRINTFORMW 「呵呵，很有趣呢——」
			PRINTFORML 
		ELSE
			;完了時
			PRINTFORMW 「啊。很開心♪」
			PRINTFORML 
		ENDIF
	CASE 9
		;▼採集,（～使える素材を探している。）
		IF !ARG:1
			;行動中
			PRINTFORMW 「嗯，這附近的話……」
			PRINTFORML 
		ELSE
			;完了時
			PRINTFORMW 「……嗯。到此為止吧」
			PRINTFORML 
		ENDIF
	CASE 10
		;▼釣魚,（～はのんびり釣魚をしている。）
		IF !ARG:1
			;行動中
			PRINTFORMW 「…♪　――♪」
			PRINTFORML 
		ELSE
			;完了時
			PRINTFORMW 「那麼。就這樣吧」
			PRINTFORML 
		ENDIF
	CASE 11
		;▼実験,（～は可疑実験をしている。）
		IF !ARG:1
			;行動中
			SELECTCASE RAND:10
				CASE 9
					PRINTFORMW 「好，就這樣——」
					PRINTFORML 
				CASE 8
					PRINTFORMW 「…哎呀，天平在哪里？」
					PRINTFORML 
				CASE 7
					PRINTFORMW 「這個和這個，好像可以用來做別的藥…」
					PRINTFORML 
				CASE 6
					PRINTFORMW 「這樣的話，果然還是需要臨床實驗啊…」
					PRINTFORML 
				CASE 5
					PRINTFORMW 「嗯。這個反應是——」
					PRINTFORML 
				CASE 4
					PRINTFORMW 「哎呀。會變成這樣呢…」
					PRINTFORML 
				CASE 3
					PRINTFORMW 「嗯…。再稍微提高一下溫度吧」
					PRINTFORML 
				CASE 2
					PRINTFORMW 「呵呵。感覺不錯啊…」
					PRINTFORML 
				CASE 1
					PRINTFORMW 「唔，這稍微有些出乎意料…」
					PRINTFORML 
				CASE 0
					PRINTFORMW 「嗯，閾值的數字記在哪里了」
					PRINTFORML 
			ENDSELECT
		ELSE
			;完了時
			PRINTFORMW 「今天就到這裏吧」
			PRINTFORML 
		ENDIF
	CASE 12
		;▼のんびり,（～はのんびりしている。）
		IF !ARG:1
			;行動中
			PRINTFORMW 「唔～嗯。好舒服啊…」
			PRINTFORML 
		ELSE
			;完了時
			PRINTFORMW 「話說回來。也不能這樣玩太久吧」
			PRINTFORML 
		ENDIF
	CASE 13
		;▼晩酌,（～はまったりと酒を飲んでいる。）
		;※酒耐性0以上のみ
		IF !ARG:1
			;行動中
			PRINTFORMW 「嗯…。呵呵。這酒真好喝啊」
			PRINTFORML 
		ELSE
			;完了時
			PRINTFORMW 「喝多了也不好啊」
			PRINTFORML 
		ENDIF
	CASE 14
		;▼買い物,（～は気ままに買い物している。）
		IF !ARG:1
			;行動中
			PRINTFORMW 「還有什麼要買的東西嗎」
			PRINTFORML 
		ELSE
			;完了時
			PRINTFORMW 「購物——結束。那麼，回去吧」
			PRINTFORML 
		ENDIF
	CASE 15
		;▼説教,（～はじっと聞いている…）
		;※神霊・蓬莱人は除く
		IF !ARG:1
			;行動中
			PRINTFORML 
		ELSE
			;完了時
			PRINTFORML 
		ENDIF
	CASE 16
		;▼落とし穴,（～は落とし穴を増やしている）
		;※三月精・琪露諾・克勞恩皮絲のみ
		;それ以外は落ちる側
		IF !ARG:1
			;行動中
			PRINTFORML 
		ELSE
			;完了時,
			;（仕掛け人（失敗時）,誰も引っかからないので～は飽きてしまったようだ…）
			;（落ちた側（戦闘能力2～4）,～はすぐに穴から飛び出して周りをキョロキョロ見回している…）
			PRINTFORMW 「唔—哇—。被—打—敗—了—」
			PRINTFORML 
		ENDIF
	CASE 17
		;▼雪だるま作り
		IF !ARG:1
			;行動中
			PRINTFORMW 「呵呵，好不容易來了場雪，一定要做得大一點」
			PRINTFORML 
		ELSE
			;完了時
			PRINTFORMW 「嗯，做得不錯嘛」
			PRINTFORML 
		ENDIF
	ENDSELECT
ENDIF
RETURN 1
