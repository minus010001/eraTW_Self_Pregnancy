@M_KOJO_MESSAGE_COM_K108_1_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;膣内射精直後
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && TCVAR:100 == PLAYER
		IF TCVAR:102
			;時姦中
			ELSEIF FLAG:70
			PRINTFORML 
		ELSE
			SELECTCASE RAND:3
				CASE 0
					CALL HPH_PRINT,@"「ふあぁっHPH あんっHPHだ、だめぇ…HPH」","L"
					CALL HPH_PRINT,@"「イッたばっかでっHPH敏感になってるからぁHPH HPH」","L"
					PRINTFORMDW 鈴瑚の膣穴から溢れ出る精液を吸い出した……
				CASE 1
					PRINTFORML 「えっ！？%CALLNAME:MASTER%、まさか舐め―」
					CALL HPH_PRINT,@"「うああぁぁっHPH HPHあっHPH あ゛ぁぁーーーっHPH HPH HPH」","L"
					PRINTFORMDW 膣内射精したばかりの自分の精液を、音を立てて吸い上げた……！
				CASE 2
					CALL HPH_PRINT,@"「んんっ…HPHキミ…よく自分の精液舐められるね……」","L"
					CALL HPH_PRINT,@"「……HPH ねえ、私にも…HPH」","L"
					CALL HPH_PRINT,@"「あ…HPHぁ…むっHPH じゅるるっHPH ……ごくんっHPH」","L"
					PRINTFORMDW 鈴瑚にも少し"お裾分け"した。
			ENDSELECT
			RETURN 1
		ENDIF
	ENDIF
	;-------------------------------------------------
	;絶頂した
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && NOWEX:0
		;Ｃ感覚５以上
		IF ABL:Ｃ感覚 >= 5
			SELECTCASE RAND:4
				CASE 0
					CALL HPH_PRINT,@"「はぁーHPH はぁーHPH …%CALLNAME:MASTER%、ごめんねっ」","L"
					PRINTFORMDL "何が"と問おうとした瞬間― 鈴瑚に後頭部を掴まれ、顔を秘部に押し付けられた…！
					CALL HPH_PRINT,@"「もうちょっとでイケそうだからっ…HPHいっぱい舐めてっHPHお願いHPH」","L"
					PRINTFORMDW 溢れ出る愛液に溺れそうになりながらも、%CALLNAME:MASTER%は必死に舐め続け―
				CASE 1
					CALL HPH_PRINT,@"「%CALLNAME:MASTER%っHPH もっHPH もう、イキそう……！」","L"
					PRINTFORMDL "わかった"と一言告げ、%CALLNAME:MASTER%は陰核に噛みついた。
					PRINTFORMDL 鈴瑚は仰け反り、大きな嬌声を上げた……！
				CASE 2
					CALL HPH_PRINT,@"「%CALLNAME:MASTER%の舌っHPH HPH ナカで動いてHPH 気持ちいいHPH HPH」","L"
					CALL HPH_PRINT,@"「ああっHPHああぁHPHイクHPHもうイッちゃうHPH」","L"
					CALL HPH_PRINT,@"「%CALLNAME:MASTER%飲んでっHPH わたしのお汁飲んでぇぇHPH HPH」","L"
					PRINTFORMDW %CALLNAME:MASTER%は女陰にかぶりつき、音を立てて吸い上げた―
				CASE 3
					CALL HPH_PRINT,@"「%CALLNAME:MASTER%っHPH もっと舐めてHPH 気持ちよく、してぇHPH」","L"
					PRINTFORMDL 鈴瑚もその気になってきたようだ。
					PRINTFORMDL %CALLNAME:MASTER%は彼女の腰をしっかりと掴み、更に膣内を激しく舐めまわした…！
					CALL HPH_PRINT,@"「ぅあああぁぁっHPH HPHそれHPH好きぃHPH気持ちいいぃHPH HPH」","W"
			ENDSELECT
			RETURN 1
		;それ以外
		ELSE
			SELECTCASE RAND:4
				CASE 0
					PRINTFORML 「%CALLNAME:MASTER%…！も、もうそれ位でいいから……！」
					CALL HPH_PRINT,@"「それ以上されたら―ああぁっHPH で、出ちゃう！出ちゃうからあぁぁっHPH HPH」","L"
					PRINTFORMDW 鈴瑚の言う事を無視して、一心不乱に膣内を舐め続けた……！
				CASE 1
					PRINTFORMDL 絶頂が近いのか、鈴瑚の膣壁が痙攣し始めた。
					PRINTFORMDL %CALLNAME:MASTER%は息を大きく吸って雌臭を堪能した後、再び舐め始めた…！
					CALL HPH_PRINT,@"「あぁぁっHPH もうだめっHPHくるっHPHおっきいのくるぅHPH HPH」","W"
				CASE 2
					CALL HPH_PRINT,@"「はぁっHPH あっHPHあんっHPHあぁぁっHPH HPH」","L"
					PRINTFORMDL 鈴瑚が激しく喘ぐ度に、膣内がビクンビクンと震える。
					PRINTFORMDW %CALLNAME:MASTER%は舌を引き抜き、クリトリスを吸い上げた…！
				CASE 3
					CALL HPH_PRINT,@"「んんっHPH ふ…うぅっ…！%CALLNAME:MASTER%……」","L"
					PRINTFORMDL 鈴瑚が切ない声を上げながら見つめてくる……
					PRINTFORMDL %CALLNAME:MASTER%は舌を激しく動かし、鈴瑚を絶頂へと導いた……！
					CALL HPH_PRINT,@"「あ゛ぁっHPH それっHPHだめHPHイクっHPHイッちゃうっHPH HPH」","W"
			ENDSELECT
			RETURN 1
		ENDIF
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;Ｃ感覚５以上
	ELSEIF ABL:Ｃ感覚 >= 5
		SELECTCASE RAND:5
			CASE 0
				CALL HPH_PRINT,@"「んんっHPH お、美味しい…？私のおまんこ…HPH」","L"
				CALL HPH_PRINT,@"「あぁっHPH HPH く、口付けたまま喋らないでぇぇHPH HPH」","W"
			CASE 1
				CALL HPH_PRINT,@"「はぁっHPH あぁHPH あぁっHPH 気持ちいいっHPH HPH」","L"
				CALL HPH_PRINT,@"「%CALLNAME:MASTER%っHPH もっとしてHPH もっとおまんこ舐めてぇHPH HPH」","L"
				PRINTFORMDW 鈴瑚のワレメに舌を這わせ、溢れ出る愛液を舐め取った……！
			CASE 2
				CALL HPH_PRINT,@"「あぁっ！？ふぁあっHPHあ゛ぁぁーっHPH おまんこ、かじらないでぇぇHPH HPH」","L"
				PRINTFORMDW 鈴瑚の恥部を甘噛みした。うっすらと刻まれた自分の歯型を見て、言い様の無い征服感を得た…！
			CASE 3
				CALL HPH_PRINT,@"「%CALLNAME:MASTER%……HPHおまんこばっかりじゃなくて……HPH」","L"
				CALL HPH_PRINT,@"「ふぁああぁぁっHPH HPH クリHPHクリきたあぁぁっHPH HPH」","L"
				PRINTFORMDW %CALLNAME:MASTER%が更に強く女陰を吸い上げると、鈴瑚は嬉しそうな嬌声を上げた。
			CASE 4
				CALL HPH_PRINT,@"「んんっHPHんぅっ…HPH …ふふっ♪」","L"
				CALL HPH_PRINT,@"（%CALLNAME:MASTER%、一所懸命におまんこ舐めてくれてる…HPH）","W"
		ENDSELECT
		RETURN 1
	;それ以外
	ELSE
		SELECTCASE RAND:4
			CASE 0
				CALL HPH_PRINT,@"「はっ…HPH はっ……ああぁっ…HPH舌が、ナカで動い…てえぇっHPH」","L"
				PRINTFORMDW 鈴瑚の膣内を舐め回した。
			CASE 1
				PRINTFORML 「%CALLNAME:MASTER%……そこは、あんまり舐めない方が―」
				CALL HPH_PRINT,@"「ああぁぁっHPH HPH だ、ダメだってぇ…！汚いから…あぁっHPH」","W"
			CASE 2
				CALL HPH_PRINT,@"「あぁぁっHPH そこは……HPH っぐぅっ…HPHびんかん、だからぁぁHPH HPH」","L"
				PRINTFORMDW 陰核を舐められ、鈴瑚は悶えている……！
			CASE 3
				CALL HPH_PRINT,@"「ああ…っHPH HPH い、いやぁ……止めて…恥ずかしい…！んんっHPH」","L"
				PRINTFORMDW 鈴瑚は音を聞きたくないのか、秘部を舐められながらも耳を塞いでいる……
			CASE 4
				CALL HPH_PRINT,@"「んん…HPH んっHPH んぅぅっ…HPH」","L"
				PRINTFORML （おまんこって、そんなに舐めたくなる物なの？）
				CALL HPH_PRINT,@"（…でも、こんなに一所懸命に舐めてくれるのは嬉しいかも……HPH）","W"
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF
RETURN 1
