@M_KOJO_MESSAGE_COM_K108_0_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;挿入中
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	;挿入中分岐
	IF LOCAL:1 && (TEQUIP:50 == PLAYER || TEQUIP:51 == PLAYER)
		;Ｃ＆Ｂ感覚が一定以上かつＶ挿入中ならＶ感覚５以上Ａ挿入中ならＡ感覚４以上
		IF ABL:Ｃ感覚 >= 5 && ABL:Ｂ感覚 >= 5 && ((TEQUIP:50 == PLAYER && ABL:Ｖ感覚 >= 5) || (TEQUIP:51 == PLAYER && ABL:Ａ感覚 >= 4))
			;射精した
			IF NOWEX:PLAYER:11
				CALL RINGO_SEX
			;それ以外
			ELSE
				SELECTCASE RAND:3
					CASE 0
						CALL HPH_PRINT,@"「あっHPH んっHPH ああぅっHPH それっHPH 気持ちよすぎるからダメぇっHPH」","L"
						CALL HPH_PRINT,@"「動きながらするの…HPH はっHPH 反則ぅっHPH HPH」","L"
						PRINTFORMDW 内と外から同時に攻め立てられ、鈴瑚は身体をガクガクと震わせる…！
					CASE 1
						PRINTFORML 「え……あっ！ダメ！今触られたら―」
						CALL HPH_PRINT,@"「ああぁぁぁ～～っHPH HPH HPH んっHPHんんんぅぅぅ～～HPH HPH」","L"
						PRINTFORMDW 鈴瑚の膣襞が、痙攣しながら締め付けてくる……！
					CASE 2
						CALL HPH_PRINT,@"「%CALLNAME:MASTER%…HPH その…耳を―」","L"
						CALL HPH_PRINT,@"「ふぁあぁ～っHPH HPH みみぃっHPHかまれるの、好きぃっHPH HPH」","W"
				ENDSELECT
			ENDIF
			RETURN 1
		;それ以外
		ELSE
			;射精した
			IF NOWEX:PLAYER:11
				CALL RINGO_SEX
			;それ以外
			ELSE
				SELECTCASE RAND:3
					CASE 0
						CALL HPH_PRINT,@"「ふあっHPHああっHPH ちょっ…！今はダメだってぇ……っHPH」","L"
						PRINTFORMDW 抽挿しつつ、鈴瑚の身体を愛撫した。
					CASE 1
						CALL HPH_PRINT,@"「ふぁっ…HPHふぅ…っHPHあっHPHんんっHPH」","L"
						CALL HPH_PRINT,@"（%CALLNAME:MASTER%の息が……耳にかかってぇ…っHPH）","W"
					CASE 2
						CALL HPH_PRINT,@"「んぅっ…HPH あんっHPH 触られながらするの、好きかも…HPH」","W"
				ENDSELECT
			ENDIF
			RETURN 1
		ENDIF
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;Ｃ感覚・Ｂ感覚が５以上
	ELSEIF ABL:Ｃ感覚 >= 5 && ABL:Ｂ感覚 >= 5
		SELECTCASE RAND:4
			CASE 0
				CALL HPH_PRINT,@"「んぅっHPH あっHPHはあぁっ……%CALLNAME:MASTER%……っHPH」","L"
				PRINTFORMDL 鈴瑚の脇腹や太股をつつ…と指でなぞって愛撫していく。
				PRINTFORMDL 度重なる愛撫のお陰か、鈴瑚は以前と比べて甘い声を上げるようになった。
				CALL HPH_PRINT,@"「%CALLNAME:MASTER%…その……こっちも…HPH」","L"
				PRINTFORMDW 鈴瑚は%TEXTR("陰核/乳房/膣穴/")%を触って欲しいようだ。
			CASE 1
				CALL HPH_PRINT,@"「あぁぁっHPH HPH 耳だめぇっHPH HPHそこHPHそこは弱いのおっHPH」","L"
				PRINTFORMDL はむはむ、と鈴瑚のウサ耳を甘噛みした。鈴瑚はとても気持ちよさそうだ……
				PRINTFORMDL 更なる反応を見ようと、%CALLNAME:MASTER%は耳の穴を穿るように舌で舐めた。
				CALL HPH_PRINT,@"「ひああぁぁっっHPH HPH HPH ぞわぞわするうぅぅっHPH HPH」","W"
			CASE 2
				CALL HPH_PRINT,@"「んん…HPH んっ…ああっHPH んうぅっHPH」","L"
				CALL HPH_PRINT,@"「…わ、私も……HPH HPH」","L"
				PRINTFORMDW 鈴瑚とお互いの身体を愛撫し合った。
			CASE 3
				CALL HPH_PRINT,@"「あぁっHPH HPHしっぽぉっHPHしっぽこするのっHPHずるいぃHPH」","L"
				CALL HPH_PRINT,@"「ふぁっHPHあっHPHあっHPHもっとHPHこしゅるの、もっとぉぉっHPH HPH」","L"
				PRINTFORMDW 尻尾を愛部するコツが少し分かってきた気がする……
		ENDSELECT
		RETURN 1
	;それ以外
	ELSE
		SELECTCASE RAND:4
			CASE 0
				PRINTFORML 「んっ！……ふふっ♪ちょっと…くすぐったいってば～♪」
				PRINTFORMDW 鈴瑚は身を捩って愛撫を受け入れている……
			CASE 1
				CALL HPH_PRINT,@"「はぁんっHPH HPH　……～～ッ！」","L"
				PRINTFORML （へ、変な声出ちゃった……）
				PRINTFORMDW 鈴瑚はウサ耳を舐められ、恥ずかしそうに顔を伏せた。
			CASE 2
				PRINTFORML 「んん……！気持ちいい…のかな？」
				PRINTFORML 「くすぐったいて言うか……ごめん、よく分かんない……」
				PRINTFORMDW 鈴瑚は他人に愛撫される事にまだ慣れていないようだ。
			CASE 3
				CALL HPH_PRINT,@"「ふあぁっHPH HPHし、尻尾は敏感だから触らないでぇ……！」","L"
				CALL HPH_PRINT,@"「あぁんっHPH HPHちょっ…！%CALLNAME:MASTER%、聞いて…んんんっHPH HPH」","W"
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF
RETURN 1

