IF TFLAG:24 == 11 || TFLAG:24 == 31
	IF CFLAG:诶嘿嘿
		CALL HPH_PRINT,@"「はぁ…HPHあっHPHあぁ…HPH」","L"
		CALL HPH_PRINT,@"「%CALLNAME:MASTER%…HPH　もっと激しくシてぇ…HPH」","W"
