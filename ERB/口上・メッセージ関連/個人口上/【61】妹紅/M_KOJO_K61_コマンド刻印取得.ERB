;調教後の文本
;-------------------------------------------
@M_KOJO_MESSAGE_PALAMCNG_C_K61
CALL PALAM_MESSAGE_C
;処女喪失
IF TCVAR:破瓜
	;正真正銘処女喪失
	IF TCVAR:破瓜 == 1
		IF 陥落状態() >= 4
			PRINTFORML 「……在与辉夜的战斗中、……虽然也很痛」
			PRINTFORML 「这个、完全不一样的疼痛啊……太痛了……」
			PRINTFORML 「但是、我很高兴。向%MASTERNAME:61%献上处女也是证明吧」
			PRINTFORML 「……嗯、眼泪？嗯、很高兴、所以」
			PRINTFORML 「啊、我想、眼泪什么的、舔了也觉得不好吃啊」
			PRINTFORML 「但是、总觉得、真的很開心」
		ELSEIF 陥落状態() == 3
			PRINTFORML 「啊疼疼疼……虽然活得很长、但这是第一次的疼痛」
			PRINTFORML 「但是、%CALLNAME:MASTER%能成为第一个的人。我很高兴。」
			PRINTFORML 「因为我觉得这跟我没什么缘分。」
			PRINTFORML 「呵呵、从今往后、也请多多疼爱哦」
		ELSEIF 陥落状態() >= 1
			PRINTFORML 「疼……、咿~、这……、这个、好痛啊、很辛苦」
			PRINTFORML 「为什么这样……」
		ELSE
			PRINTFORML 「咕…………」
		ENDIF
	;再生処女喪失
	ELSEIF TCVAR:破瓜 == 2
		IF 陥落状態() >= 4
			PRINTFORML 「嗯、这个、有点痛的、特意让我再生、真是恶趣味」
			PRINTFORML 「……看起来比以前习惯了吗？只有身体的疼痛啊」
			PRINTFORML 「真的第一次的时候、感觉是刻在内心深处的感觉」
		ELSEIF 陥落状態() == 3
			PRINTFORML 「嗯、你这个色狼……。特意让我再生、有什么好開心呢」
			PRINTFORML 「只是让我痛而已」
		ELSE
			PRINTFORML 「痛…………、变态……」
		ENDIF
	;無自覚非処女喪失
	ELSEIF TCVAR:破瓜 == -1
		IF 陥落状態() >= 4
			PRINTFORML 「……嗯、不疼吗？为什么一開始问我疼吗」
			PRINTFORML 「难道是时停时抢走了吗？」
			PRINTFORML 「%MASTERNAME:61%笨蛋、再怎么说那也太过分了吧。」
		ELSEIF 陥落状態() == 3
			PRINTFORML 「啊、不疼……好像松了一口气、有点遗憾」
			PRINTFORML 「我说、那种为难的表情是怎样的呢？」
		ELSE
			PRINTFORML 「嗯……为什么不疼呢……」
		ENDIF
	ENDIF
	PRINTFORMW 
ENDIF

;Ａ処女喪失
IF TCVAR:Ａ破瓜
	;正真正銘Ａ処女喪失
	IF TCVAR:Ａ破瓜 == 1
		IF 陥落状態() >= 4
			PRINTFORML 「……啊、啊……嗯、你这个、变态」
			PRINTFORML 「屁股什么的、明明不是受欢迎的地方」
			PRINTFORML 「%MASTERNAME:61%这样的变态行为让你满意吗？」
			PRINTFORML 「……别吵了、让人舒服的那个地方不好」
			PRINTFORML 「咿！　现在不要碰那里！！」
			PRINTFORML 「嗯、你个、变态」
		ELSEIF 陥落状態() == 3
			PRINTFORML 「哈、哈啊、嗯……、有点、难受」
			PRINTFORML 「叫出声了、屁股有光泽吗？闭嘴、想被烧掉吗？」
			PRINTFORML 「……在屁股上、舒服的不是%CALLNAME:MASTER%吗」
			PRINTFORML 「你这个变态」
		ELSE
			PRINTFORML 「咕、就在、这边什么的……、嗯……」
		ENDIF
	;無自覚Ａ処女喪失
	ELSEIF TCVAR:Ａ破瓜 == -1
		IF 陥落状態() >= 4
			PRINTFORML 「哈啊哈啊、嗯、为什么呢、并不是、那么难受」
			PRINTFORML 「……难道、你做了？……变态」
			PRINTFORML 「哈啊、嗯、变态……、我觉得我很舒服什么的」
		ELSEIF 陥落状態() == 3
			PRINTFORML 「哈呼、嗯、很舒服、或者说、并不是、那么难受」
			PRINTFORML 「因为%CALLNAME:MASTER%好好习惯了」
		ELSE
			PRINTFORML 「嗯、呼……、总觉得、不比起想象的疼、嗯」
		ENDIF
	ENDIF
	PRINTFORMW 
ENDIF
	
;口交で射精後のお掃除
IF (TCVAR:MASTER:112 == 1 || TCVAR:MASTER:112 == 2) && SELECTCOM == 81
	IF ABL:LOCAL:精液中毒 < 3
		IF 陥落状態() >= 4
			PRINTFORML 「嗯、啾……、咕、这样就行了吗？」
			PRINTFORML 「嗯、但是、果然还是讨厌这个。虽然%MASTERNAME:61%想让我喝」
			PRINTFORML 「舔进入自己里面的东西、感觉不是很好。」
		ELSEIF 陥落状態() == 3
			PRINTFORML 「舔、舔……诶、变得干净了」
			PRINTFORML 「但是、中出之后、用嘴打扫什么的、真是太可怕了」
		ELSE
			PRINTFORML 「嗯、啾……、咕、这样就行了吧、结束了」
		ENDIF
	;Ｖ出し
	ELSEIF TCVAR:MASTER:112 == 1
		IF 陥落状態() >= 4
			PRINTFORML 「哈啊、嗯、舔、啾啪、咕噜噜、舔……」
			PRINTFORML 「这就是、舔、啾、和小穴混在一起、哈啊哈啊」
			PRINTFORML 「咕噜噜噜、舔、舔、啾啪、啾噜、哈啊、更多更多」
		ELSEIF 陥落状態() == 3
			PRINTFORML 「啾啪、舔、啾噜、舔着、进入、小穴、的东西、舔」
			PRINTFORML 「总觉得、啾噜、变得兴奋、了。舔、噜、咕噜噜」
		ELSE
			PRINTFORML 「舔……、好了、结束了。……舔进入自己里面的东西是不可能的吧」
		ENDIF
	ELSEIF TCVAR:MASTER:112 == 2
		IF 陥落状態() >= 4
			PRINTFORML 「嗯、啾噜、嗯、……舔」
			PRINTFORML 「呐、%MASTERNAME:61%、这样、请不要这样」
			PRINTFORML 「不管怎么说、要舔我进入后面的东西、不想这样做」
		ELSEIF 陥落状態() == 3
			PRINTFORML 「……嗯、啾噜、嗯、好的、结束了」
			PRINTFORML 「不管怎么说、后面被放入之类的。如果可以的话、我希望你能原谅我。」
		ELSE
			PRINTFORML 「……啊、结束了。不管怎么说、这是不可能的。」
		ENDIF
	ENDIF
	PRINTFORMW 
ENDIF

;-------------------------------------------
;刻印取得
;-------------------------------------------
@M_KOJO_MESSAGE_MARKCNG_K61
;このままだと地の文が表示されません
;地の文を表示したい方は、下記のコメントアウトを削除して下さい
CALL MARK_MESSAGE
IF !FLAG:70
	;苦痛刻印
	IF TFLAG:22 == 1
		PRINTFORMW 「咕呜、明明、已经习惯、疼痛了」
	ELSEIF TFLAG:22 == 2
		PRINTFORMW 「为什么、这样的事、好痛」
	ELSEIF TFLAG:22 == 3
		PRINTFORMW 「真的要生气了、痛死了」
	ENDIF

	;快楽刻印
	IF TFLAG:23 == 1
		PRINTFORMW 「呜啊～……、什、什么事也没有」
	ELSEIF TFLAG:23 == 2
		PRINTFORMW 「啊呜、嗯咕呜～！　还、还算挺舒服的吧」
	ELSEIF TFLAG:23 == 3
		PRINTFORMW 「呀啊啊啊～～！！」
	ENDIF

	;不埒刻印
	;快楽による刻印
	;LV1
	IF TFLAG:24 == 11
		PRINTFORMW 「唉～、真是的、拿你没办法」
	;LV2
	ELSEIF TFLAG:24 == 12
		PRINTFORMW 「呜啊～！　嗯、呵呵、呐、这么想要吗？」
	;LV3
	ELSEIF TFLAG:24 == 13
		PRINTFORMW 「咿啊啊～！！　还、还想、再要、嗯咕呜～！」
	;屈服、恭順による刻印
	;LV1
	ELSEIF TFLAG:24 == 31
		PRINTFORMW 「呵呵、只能让你看哦」
	;LV2
	ELSEIF TFLAG:24 == 32
		PRINTFORMW 「呐、想要的话不用客气尽管告诉我吧」
	;LV3
	ELSEIF TFLAG:24 == 33
		PRINTFORMW 「要我做什么都可以、嗯、我好想要啊」
	ENDIF

	;反発刻印
	IF TFLAG:21 == 1
		PRINTFORMW 「……哼～」
	ELSEIF TFLAG:21 == 2
		PRINTFORMW 「……哈啊？……你说什么？」
	ELSEIF TFLAG:21 == 3
		PRINTFORMW 「…………想死吗？」
	ENDIF
ENDIF

