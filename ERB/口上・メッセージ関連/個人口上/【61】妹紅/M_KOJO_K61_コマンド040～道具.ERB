LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)

		PRINTFORMW 
		RETURN 1
	ENDIF
	IF TEQUIP:PLAYER:避孕套 && EXP:出産経験 && !FLAG:70
		IF 陥落状態() >= 4
			PRINTFORML 「……%MASTERNAME:61%、你在想什么呢？」
			PRINTFORML 「笑脸很可怕？　……不是理所当然的嘛。为什么要戴避孕套、不生孩子什么的」
			PRINTFORML 「哎！焚烧处分完成、喂、好好地播种、让宝宝孕育吧？」
		ELSEIF 陥落状態() == 3
			PRINTFORML 「……为什么要用避孕套之类的东西呢？你不想让我怀孕吗？」
			PRINTFORML 「不管%CALLNAME:MASTER%怎么说、我希望能生孩子、所以没收了」
		ELSEIF 陥落状態() == 2
			PRINTFORML 「因为我想要孩子、所以这样的东西不能使用。」
		ENDIF
		TEQUIP:PLAYER:避孕套 = 0
		PRINTFORML 
		PRINTFORMW 避孕套被妹红强制拿走了
	;基本セット
	;時姦中
	ELSEIF FLAG:70
		PRINTFORMW 
		RETURN 1
	;恋慕
	ELSEIF 陥落状態() == 3
		PRINTFORMW 
		RETURN 1
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		PRINTFORMW 
		RETURN 1
	;それ以外
	ELSE
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF
RETURN 1

