			CASE IS < 4
				IF 低気圧位置 == "台風最接近"
					PRINTW 雲在周围高速地循环飛走着
					PRINTL 
					;CALL ASK_DIARY("进入了台風眼")
				ELSEIF DAY:2 == 2
					SELECTCASE 積乱雲位置
						CASE "積乱雲遠方", "積乱雲未発生"
							IF IS_COLD_LOW || 低気圧位置 == "台風勢力圏"
								PRINTW 远处可以看到很多入道雲、也是积雨云
								PRINTL 
							ELSEIF POWER_WARM_AIR <= 1
								PRINTW 天空的模様不是很整洁
								PRINTL 
							ELSE
								SELECTCASE CAN_CUMULUS:0
									CASE 0
										PRINTW 是清爽的夏日天空
										PRINTL 
									CASE 1
										PRINTW 遠處飄著綿雲
										PRINTL 
									CASE 2, 3
										PRINTW 遠處飄著入道雲
										PRINTL 
									CASE 4
										PRINTW 遠處飄著砧雲
										PRINTL 
									CASE 5
										PRINTW 雲在遠處盤旋
										PRINTL 
								ENDSELECT
							ENDIF
						CASE "積乱雲視認"
							SELECTCASE CAN_CUMULUS:0
								CASE 1
									PRINTW 一朵綿雲出現了
									PRINTL 
								CASE 2, 3
									PRINTW 入道雲在滾滾成長
									PRINTL 
								CASE 4
									PRINTW 入道雲的頂端變平了 現在是砧雲！
									PRINTL 
								CASE 5
									PRINTW 遠處的雲塊卷成了漩渦……
									PRINTL 
								CASEELSE
									PRINTW 是清爽的夏日天空
									PRINTL 
							ENDSELECT
						CASE "積乱雲接近"
							SELECTCASE CAN_CUMULUS:0
								CASE 1
									PRINTW 綿雲在天上飄著
									PRINTL 
								CASE 2
									PRINTW 雲的底部微暗
									PRINTL 
								CASE 3
									PRINTW 飄著的雲越來越近了
									PRINTL 
								CASE 4
									PRINTW 飄著的烏雲越來越近了
									PRINTL 
									IF CAN_CUMULUS:2 == 1
										PRINTW ……仔細看的話雲上似乎有胸部！
										PRINTL 
										;CALL ASK_DIARY("看到了有胸部的雲")
									ELSEIF CAN_CUMULUS:2 == 2
										PRINTW ……仔細看的話雲上似乎長著小鷄鷄！
										PRINTL 
										;CALL ASK_DIARY("看到了長有小鷄鷄的雲")
									ENDIF
								CASE 5
									PRINTW 拱形雲越來越近了
									PRINTL 
									IF CAN_CUMULUS:2 == 1
										PRINTW ……仔細看的話雲上似乎有胸部！
										PRINTL 
										;CALL ASK_DIARY("看到了有胸部的雲")
									ELSEIF CAN_CUMULUS:2 == 2
										PRINTW ……仔細看的話雲上似乎長著小鷄鷄！
										PRINTL 
										;CALL ASK_DIARY("看到了長有小鷄鷄的雲")
									ENDIF
								CASEELSE
									PRINTW 是清爽的夏日天空
									PRINTL 
							ENDSELECT
						CASEELSE
							PRINTW 綿雲在天上飄著
							PRINTL 
					ENDSELECT
				ELSE
					IF (低気圧位置 == "寒冷渦遠方" && TIME_ENTER_TERRITORY_COLD_LOW:1 == DAY:3 && TIME_ENTER_TERRITORY_COLD_LOW:2 <= TIME:0 + 300) || 低気圧位置 == "疑似好天" || 低気圧位置 == "暖域"
						PRINTW 天空的另一端可以看见像墙壁一样连成一片的雲
						PRINTL 
					ELSEIF 低気圧位置 == "台風最接近"
						PRINTW 雲在周围高速地循环飛走着
						PRINTL 
						CALL ASK_DIARY("进入了台風眼")
					ELSEIF 低気圧位置 == "二つ玉低気圧接近" || 低気圧位置 == "日本海低気圧接近" || 低気圧位置 == "南岸低気圧接近" || 低気圧位置 == "温暖前線接近"
						IF TIME:5 == 3
							PRINTW 乌雲扩散開了
							PRINTL 
						ELSE
							PRINTW 卷积雲扩散開了
							PRINTL 
						ENDIF
					ELSE
						PRINTW 平靜的天空
						PRINTL 
					ENDIF
				ENDIF
			CASE 6, 7, 11
				PRINTW 全都是白色的霧
				PRINTL 
			CASEELSE
				IF 積乱雲位置 == "積乱雲直下"
					 SELECTCASE CAN_CUMULUS:0
					 	CASE IS < 4
					 		PRINTW 天空微暗
					 		PRINTL 
					 	CASE 4
					 		PRINTW 天空一片漆黑
					 		PRINTL 
							IF CAN_CUMULUS:2 == 1
								PRINTW ……仔細看的話雲上似乎有胸部！
								PRINTL 
								;CALL ASK_DIARY("看到了有胸部的雲")
							ELSEIF CAN_CUMULUS:2 == 2
								PRINTW ……仔細看的話雲上似乎長著小鷄鷄！
								PRINTL 
								;CALL ASK_DIARY("看到了長有小鷄鷄的雲")
							ENDIF
					 	CASE 5
					 		PRINTW 黑壓壓的雲層卷了起來
					 		PRINTL 
							IF CAN_CUMULUS:2 == 1
								PRINTW ……仔細看的話雲上似乎有胸部！
								PRINTL 
								;CALL ASK_DIARY("看到了有胸部的雲")
							ELSEIF CAN_CUMULUS:2 == 2
								PRINTW ……仔細看的話雲上似乎長著小鷄鷄！
								PRINTL 
								;CALL ASK_DIARY("看到了長有小鷄鷄的雲")
							ENDIF
					 ENDSELECT
				ELSEIF GET_LOCATION_AREA_TYPHOON() == 1 || 低気圧位置 == "寒冷渦接近"
					PRINTW 雲游动的速度很快
					PRINTL 
				ELSE
					PRINTW 天空完全被雲覆盖了
					PRINTL 
				ENDIF 
		ENDSELECT
	;PM6~8　夕
	CASE 1080 TO 1199
		SELECTCASE TIME:5
			CASE IS < 3
				IF 低気圧位置 == "台風勢力圏"
					PRINTW 比平時更加鮮艷的紅色晚霞
					PRINTL 
				ELSEIF (低気圧位置 == "寒冷渦遠方" && TIME_ENTER_TERRITORY_COLD_LOW:1 == DAY:3 && TIME_ENTER_TERRITORY_COLD_LOW:2 <= TIME:0 + 300) || 低気圧位置 == "疑似好天" || 低気圧位置 == "暖域"
