@IMAGE_I61_別立ち判定_体形
RESULTS = 
IF CFLAG:61:妊娠経過日数 >= 25 ||(CFLAG:61:妊娠経過日数 >= 15 && 充填率(61,3) >= 100)
	IF IMAGE_I61_別立ち判定_futa()
		IF NOWEX:61:射精 > 0
			RESULTS = 扶她射精妊娠二%RESULTS%
		ELSE
			RESULTS = 扶她妊娠二%RESULTS%
		ENDIF
	ELSE
		RESULTS = 妊娠二%RESULTS%
	ENDIF	
ELSEIF CFLAG:61:妊娠経過日数 >= 15 || 充填率(61,3) >= 100
	IF IMAGE_I61_別立ち判定_futa()
		IF NOWEX:61:射精 > 0
			RESULTS = 扶她射精妊娠一%RESULTS%
		ELSE
			RESULTS = 扶她妊娠一%RESULTS%
		ENDIF
	ELSE
		RESULTS = 妊娠一%RESULTS%
	ENDIF
ELSE
	IF IMAGE_I61_別立ち判定_futa()
		IF NOWEX:61:射精 > 0
			RESULTS = 扶她射精%RESULTS%
		ELSE
			RESULTS = 扶她%RESULTS%
		ENDIF
	ENDIF
ENDIF


@IMAGE_I61_別立ち判定_表情
IF NOWEX:61:Ｃ絶頂 || NOWEX:61:Ｂ絶頂 || NOWEX:61:Ｖ絶頂 || NOWEX:61:Ａ絶頂 || NOWEX:61:Ｍ絶頂
	IF EX:61:TotalEX >= 5
		RESULTS:0 = 皱眉睁眼张嘴
		RESULTS:1 = 皱眉闭眼张嘴
	ELSE
		RESULTS:0 = 皱眉微笑
		RESULTS:1 = 皱眉闭眼张嘴
	ENDIF
	RETURN
ENDIF
	
IF CFLAG:61:睡眠
	LOCAL = EX:61:無自覚Ｃ絶頂 + EX:61:無自覚Ｖ絶頂 + EX:61:無自覚Ａ絶頂 + EX:61:無自覚Ｂ絶頂 + EX:61:無自覚Ｍ絶頂
	IF LOCAL >= 5
		RESULTS:0 = 闭眼微笑
		RESULTS:1 = 皱眉闭眼微笑
	ELSE
		RESULTS:0 = 闭眼无表情
		RESULTS:1 = 挑眉闭眼无表情
	ENDIF
	RETURN
ENDIF

IF CFLAG:61:ブチギレ
	RESULTS:0 = 皱眉不悦
	RESULTS:1 = 挑眉不悦
	RETURN
ELSEIF BASE:61:憤怒 > 600 || TALENT:61:心情 < 0 || CFLAG:61:心情不快 == 1
	RESULTS:0 = 皱眉不悦
	RESULTS:1 = 皱眉不悦
	RETURN
ENDIF

IF TALENT:61:恋人
	IF BASE:61:情緒 >= 100
		RESULTS:0 = 微笑
		RESULTS:1 = 闭眼微笑
	ELSE
		RESULTS:0 = 无表情
	ENDIF
ELSEIF TALENT:61:恋慕 || TALENT:61:愛欲
	IF BASE:61:情緒 >= 300
		RESULTS:0 = 微笑
		RESULTS:1 = 闭眼微笑
	ELSE
		RESULTS:0 = 无表情
	ENDIF
ELSEIF TALENT:61:思慕 || TALENT:61:炮友
	IF BASE:61:情緒 >= 600
		RESULTS:0 = 微笑
		RESULTS:1 = 闭眼微笑
	ELSE
		RESULTS:0 = 无表情
	ENDIF
ELSE
	IF BASE:61:情緒 >= 1200
		RESULTS:0 = 闭眼微笑
	ELSE
		RESULTS:0 = 无表情
	ENDIF
ENDIF

IF RESULTS:0 == "无表情"
	IF MARK:61:反発刻印 <= 0
		RESULTS:1 = 微张嘴
	ENDIF
ELSEIF RESULTS:0 == "笑顔"
	IF PALAM:61:欲情 >= PALAMLV:10 || TCVAR:61:発情 || CFLAG:61:积攒度 >= 900
		RESULTS:1 = 张嘴
	ELSEIF MARK:61:反発刻印 <= 0 && (TFLAG:103==109 || TARGET==109)
		RESULTS:1 = 微笑
	ENDIF
ENDIF

RETURN



