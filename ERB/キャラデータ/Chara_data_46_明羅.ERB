@CHARAMOVE_DATA_46(ARG, ARG:2)
LOCAL = 46
SELECTCASE ARG
	;連動設定（RETURN=依存する角色, RESULT:1～3=対等に連動する角色）
	CASE 1
	;特殊生活時間設定（CFLAG:LOCAL:生活時間補正を設定する）
	CASE 3
	;移動傾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;未設定
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 桜花道的鳥居, 神社境内
				RETURN 1
			CASE 広場, 商家町, 呑屋小道
				RETURN 1
		ENDSELECT
	;引力設定（CFLAG:LOCAL:引力点を設定する）
	CASE 8
ENDSELECT

@EXIST_角色データ46
#LOCALSIZE 1
#LOCALSSIZE 1

@角色データ46(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "稱呼"
	CALLF MAKE_STR(V_NAME, "明羅")
;こっから衣装設定
CASE "今天的睡衣"
	CALLF MAKE_STR(V_NAME, "睡衣（襦袢）套")

CASE "便装：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "雪駄"))
CASE "便装：襪子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("襪子", "短布襪"))
CASE "便装：和服"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("和服", "和服"))
CASE "便装：裤子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("裤子", "袴"))

CASE "固有内衣形容詞"
	CALLF MAKE_STR(V_NAME, "古代宽松的")
CASE "固有下半身内衣名"
	CALLF MAKE_STR(V_NAME, "畚兜裆布")
CASE "固有上半身内衣"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身内衣名"
	CALLF MAKE_STR(V_NAME, "裹胸")
CASE "固有内衣着用条件"
	SIF !RAND:(1 + (2 * 勝負内衣判定([[明羅]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣布料厚度"
	CALLF MAKE_INT(V_NAME, 3)

;料理関連
CASE "料理：喜欢的味道"
	CALLF MAKE_STR(V_NAME, "魚料理/米/油炸食品/简易/淡白/豪華/野趣/豪快/年糕/和食/和食/辣/辣/咸/咸/")
CASE "料理：讨厌的味道"
	CALLF MAKE_STR(V_NAME, "洋食/濃厚/甜/甜/青蛙/青蛙/青蛙/虫肉/虫肉/虫肉/謎料理/謎料理/謎料理/謎料理/謎料理/")
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "切れ味/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "赤/美的/和風/別緻/" + "闇/刃物/" * 3 + "博霊の/懐剣/護刀/护臂/闇の/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "奇妙/洋風/活潑/誇張/洛麗塔/")
ENDSELECT

