@CHARAMOVE_DATA_155(ARG, ARG:2)
LOCAL = 155
SELECTCASE ARG
	;連動設定（RETURN=依存する角色, RESULT:1～3=対等に連動する角色）
	CASE 1
	;特殊生活時間設定（CFLAG:LOCAL:生活時間補正を設定する）
	CASE 3
	;移動傾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;外に出る道中は行く
			CASE P930旧血の池地獄, P929地獄的人工太陽, P928灼熱地獄跡, P926中庭, P905一階廊下, P901地霊殿前
				RETURN 0
			;残りの地霊殿には行かない
			CASE P901地霊殿前 TO P930旧血の池地獄
				RETURN -100
			;家には入らない
			CASE P936勇儀的家, P940帕露西的家, P941山女的家, P942琪斯美的家
				RETURN -100
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 地蔵小路, 倒垂的柳樹下, 墓地
				RETURN 1
			CASE 商家町, 呑屋小道
				RETURN 1
			CASE 中有之道, 三途之川, 彼岸, 地獄, 畜生界
				RETURN 1
			CASE 幻想風穴, 地獄的深道, 旧地獄街道, 地霊殿, 灼熱地獄跡
				RETURN 1
		ENDSELECT
	;引力設定
	CASE 8
		;あまりにも奥に居るので昼間は表に出てくる,1/8でそのまま
		SIF INRANGE(TIME, 660, 840) && CFLAG:LOCAL:今日の運勢 % 8
			TCVAR:LOCAL:引力点 = P931大街
ENDSELECT

@EXIST_角色データ155
#LOCALSIZE 1
#LOCALSSIZE 1

@角色データ155(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "稱呼"
	CALLF MAKE_STR(V_NAME, "ちやり")
;こっから衣装設定
CASE "今天的睡衣"
	CALLF MAKE_STR(V_NAME, "パジャマ（裤子＋フード）セット")
CASE "便装：上身衣服２"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上身衣服２", "地獄Ｔ恤衬衫"))
CASE "便装：裤子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("裤子", "ショート裤子"))

CASE "固有内衣形容詞"
	CALLF MAKE_STR(V_NAME, "怨念渦巻く")
CASE "固有下半身内衣名"
	CALLF MAKE_STR(V_NAME, "鬼火パンツ")
CASE "固有上半身内衣"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身内衣名"
	CALLF MAKE_STR(V_NAME, "鬼火ブラ")
CASE "固有内衣着用条件"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣布料厚度"
	CALLF MAKE_INT(V_NAME, 1)

;料理関連
CASE "料理：喜欢的味道"
	CALLF MAKE_STR(V_NAME, "汁物/饮用/咸/咸/酸/酸/垃圾/濃厚/豪快/新鲜/怪诞/粘稠/霊障/地底風/ + 肉料理/ * 3 + 血の魔力/ * 5")
CASE "料理：讨厌的味道"
	CALLF MAKE_STR(V_NAME, "甜/甜/淡白/質素/芸術的/")
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "血の魔力/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "赤/妖術/混沌/可怕的/奇妙/狂熱/ + 火/血/鹫の/チュパ芜菁ラの/ * 3 + 血の/ * 5")
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "青/秩序/時尚/未来/ + 水/ * 3 + 狼の/水獭の/ * 10")
ENDSELECT

