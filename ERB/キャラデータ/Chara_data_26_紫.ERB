@CHARAMOVE_DATA_26(ARG, ARG:2)
LOCAL = 26
SELECTCASE ARG
	;連動設定（RETURN=依存する角色, RESULT:1～3=対等に連動する角色）
	CASE 1
	;特殊生活時間設定（CFLAG:LOCAL:生活時間補正を設定する）
	CASE 3
	;移動傾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;神社の居間
			CASE P9居間
				RETURN 5
			;藍,橙の私室はあまり入らない
			CASE P635橙私室, P638藍私室
				RETURN -5
			;是非曲直庁、畜生界は行かない
			CASE P610是非曲直庁大堂 TO P618空的部屋, P640異界の関所 TO P655尤魔宅の風呂
				RETURN -100
			;他人的家には入らない
			CASE P608潤美的家, P609瓔花的家
				RETURN -100
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 桜花道的鳥居, 神社境内, 神社本堂, 神社走廊
				RETURN 1
			CASE 広場, 商家町, 呑屋小道
				RETURN 1
			CASE 大廳 , 食堂, 大図書館
				RETURN 1
			CASE 森林的入口, 香霖堂, 古木的大樹, 霧雨魔法店, 人偶的洋館, 再思之道, 無縁塚
				RETURN 1
			CASE 中有之道, 三途之川, 迷途之家, 白玉楼庭, 白玉楼
				RETURN 1
			CASE 守矢神社境内, 守矢神社本殿
				RETURN 1
		ENDSELECT
	;引力設定（CFLAG:LOCAL:引力点を設定する）
	CASE 8
		;土曜の夜は白玉楼へ玩耍に行く
		SIF YOUBI_MATCH(DAY, "土") && INRANGE(TIME, 960, 1200)
			TCVAR:LOCAL:引力点 = P628居間
ENDSELECT

@EXIST_角色データ26
#LOCALSIZE 1
#LOCALSSIZE 1

@角色データ26(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "稱呼"
	CALLF MAKE_STR(V_NAME, "紫")
;こっから衣装設定
CASE "今天的睡衣"
	CALLF MAKE_STR(V_NAME, "睡衣（襦袢）套")

CASE "便装：飾品"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("飾品", "傘"))
CASE "便装：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "帽子"))
CASE "便装：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "便装：襪子"
	IF FLAG:2 > 0
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("襪子", "吊帯襪"))
	ELSE
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("襪子", "襪子"))
	ENDIF
CASE "便装：連衣裙"
	IF DAY:2 == 1
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("連衣裙", "道士服"))
	ELSE
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("連衣裙", "晚禮服"))
	ENDIF

CASE "固有内衣形容詞"
	CALLF MAKE_STR(V_NAME, "放出奇怪的光泽")
CASE "固有下半身内衣名"
	CALLF MAKE_STR(V_NAME, "丝绸内裤")
CASE "固有上半身内衣"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身内衣名"
	CALLF MAKE_STR(V_NAME, "丝绸内衣")
CASE "固有内衣着用条件"
	SIF !RAND:(3 - (2 * 勝負内衣判定([[紫]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣布料厚度"
	CALLF MAKE_INT(V_NAME, 1)

;料理関連
CASE "料理：喜欢的味道"
	CALLF MAKE_STR(V_NAME, "野菜/肉料理/葉物/健康/芸術的/上品/家庭的/软绵绵/外界風/和食/和食/甜/甜/辣/辣/")
CASE "料理：讨厌的味道"
	CALLF MAKE_STR(V_NAME, "丰盛/垃圾/野趣/咸/咸/" + "虫肉/天界風/月都風/" * 3)
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "妖力/妖力/得意料理＊和食＊/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "高級/美的/芸術的/懷舊/別緻/知的/威厳/巫術/" + "紫/妖術/护符/式神の/夢幻の/" * 3 + "黒死蝶の/生と死の/境界の/隙間の/幻葬の/傘/夢與現實の/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "活潑/洛麗塔/" + "安物/月/护臂/" * 3 + "费米纤维の/天人の/" * 10)
ENDSELECT

;-------------------------------------------------
;固有料理設定
;DISH_NAME          料理名
;RESULTS            料理の種類（軽食/主食/甜点）
;CALL 材料設定("")  材料の指定。"汎用"を入れると自動で汎用食材になる
;DISH_TASTE         味の設定
;DISH_COMMENT       （任意）一行コメント
;DISH_TIME          （任意）調理にかかる時間
;DISH_MAXLIMIT      （任意）料理の持続時間
;-------------------------------------------------
@SPECIAL_MENU_26
DISH_NAME = 海龟汤
RESULTS = 軽食
CALL 材料設定("來歷不明的肉")
DISH_TASTE = 汁物/肉料理/酸/简易/質素/上品/謎料理/
DISH_COMMENT = 用海龟肉制作的浓汤。但是幻想乡里没有大海啊……。
RETURN 1

;IMAGE_I26_特殊服装(CHARA)-------------------------------------------------------------------
;角色特有の服装表示条件
;エフェクトを共有する服装差分はこちらで指定する
;使用時注意点:
;	RESULTSに特殊服装を格納すること
;	書式は %服装%%特殊服装% 適宜"_"等入れること
;-------------------------------------------------------------------------------------------
@IMAGE_I26_特殊服装(CHARA)
#DIM CHARA
VARSET RESULTS

IF CFLAG:CHARA:差し替え適用
	;替换用画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立絵種類
			;顔絵
		ELSE
			;立絵
		ENDIF
	ELSE
		;服
		IF FLAG:立絵種類
			;顔絵
		ELSE
			;立絵
		ENDIF
	ENDIF
ELSE
	;通常画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立絵種類
			;顔絵
		SIF (TALENT:26:年齢 == -1 || TALENT:26:幼児／幼児退行 || TALENT:26:体型 < 0)
			RESULTS = _幼
		ELSE
			;立絵
		ENDIF
	ELSE
		;服
		IF (TALENT:26:年齢 == -1 || TALENT:26:幼児／幼児退行 || TALENT:26:体型 < 0)
			RESULTS = _幼
		ELSEIF EQUIP:CHARA:連衣裙 == 7
			RESULTS = _晚禮服
		ENDIF
		;SIF EQUIP:CHARA:連衣裙 == 7
			;RESULTS = _晚禮服
	ENDIF
ENDIF

