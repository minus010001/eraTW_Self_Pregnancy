@CHARAMOVE_DATA_25(ARG, ARG:2)
LOCAL = 25
SELECTCASE ARG
	;連動設定（RETURN=依存する角色, RESULT:1～3=対等に連動する角色）
	CASE 1
	;特殊生活時間設定（CFLAG:LOCAL:生活時間補正を設定する）
	CASE 3
	;移動傾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;紫様の私室はあまり入らない
			CASE P633紫私室
				RETURN -5
			;是非曲直庁、畜生界は行かない
			CASE P610是非曲直庁大堂 TO P618空的部屋, P640異界の関所 TO P655尤魔宅の風呂
				RETURN -100
			;他人的家には入らない
			CASE P608潤美的家, P609瓔花的家
				RETURN -100
			;私室等には入らない
			CASE P623幽幽子私室, P624妖夢私室, P627風呂
				RETURN -100
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 桜花道的鳥居, 神社境内, 神社本堂, 神社走廊
				RETURN 1
			CASE 呑屋小道
				RETURN 1
			CASE 中有之道, 三途之川, 迷途之家, 白玉楼庭, 白玉楼
				RETURN 1
			CASE 山麓樹海, 玄武之澤, 大蛤蟆之池, 森の深奥
				RETURN 1
		ENDSELECT
	;引力設定（CFLAG:LOCAL:引力点を設定する）
	CASE 8
ENDSELECT

@EXIST_角色データ25
#LOCALSIZE 1
#LOCALSSIZE 1

@角色データ25(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "稱呼"
	CALLF MAKE_STR(V_NAME, "藍")
;こっから衣装設定
CASE "今天的睡衣"
	CALLF MAKE_STR(V_NAME, "睡衣（襦袢）套")

CASE "便装：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "帽子"))
CASE "便装：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "便装：襪子"
	IF FLAG:2 > 0
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("襪子", "長筒襪"))
	ELSE
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("襪子", "襪子"))
	ENDIF
CASE "便装：連衣裙"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("連衣裙", "道士服"))

CASE "固有内衣形容詞"
	CALLF MAKE_STR(V_NAME, "尾部開了个大洞的")
CASE "固有下半身内衣名"
	CALLF MAKE_STR(V_NAME, "Ｏ洞内裤")
CASE "固有上半身内衣"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身内衣名"
	CALLF MAKE_STR(V_NAME, "全剪子")
CASE "固有内衣着用条件"
	SIF !RAND:(1 + (2 * 勝負内衣判定([[藍]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣布料厚度"
	CALLF MAKE_INT(V_NAME, 2)

;料理関連
CASE "料理：喜欢的味道"
	CALLF MAKE_STR(V_NAME, "肉料理/卵/和食/中華/油炸食品/丰盛/上品/家庭的/機能的/年糕/辣/辣/酸/酸/")
CASE "料理：讨厌的味道"
	CALLF MAKE_STR(V_NAME, "咸/咸/謎料理/謎料理/謎料理/謎料理/謎料理/")
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "妖力/妖力/得意料理＊和食＊/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "青/秩序/美的/和風/別緻/知的/" + "妖術/書物/" * 3 + "狐の/式神の/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "活潑/洛麗塔/狂熱/" + "水/护臂/" * 3 + "狸の/" * 10)

;農民関連
;おあげ
CASE "農民：説明"
	CALLF MAKE_STR(V_NAME, "大豆農家")
CASE "農民：作物"
	CALLF MAKE_STR(V_NAME, "大豆")
CASE "農民：田的种类"
	CALLF MAKE_STR(V_NAME, "大豆畑")
CASE "農民：畑の所在地"
	CALLF MAKE_STR(V_NAME, "人里")

CASE "農民：MapID"
	CALLF MAKE_INT(V_NAME, 2)
CASE "農民：場所ID"
	CALLF MAKE_INT(V_NAME, 205)
CASE "農民：報酬"
	CALLF MAKE_INT(V_NAME, 600)
CASE "農民：金利"
	CALLF MAKE_INT(V_NAME, 5)
ENDSELECT

