@CHARAMOVE_DATA_157(ARG, ARG:2)
LOCAL = 157
SELECTCASE ARG
	;連動設定（RETURN=依存するキャラ, RESULT:1～3=対等に連動するキャラ）
	CASE 1
	;特殊生活時間設定（CFLAG:LOCAL:生活時間補正を設定する）
	CASE 3
	;移動傾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;よく居る
			CASE P605地獄
				RETURN 5
			;是非曲直庁、冥界、八雲邸には行かない（入り口部分は出る時用）
			CASE P612法廷 TO P639八雲邸居間
				RETURN -100
			;霊長園,組長の家には行かない
			CASE P642霊長園 TO P655尤魔宅の風呂
				RETURN -100
			;他人の家には入らない
			CASE P608潤美の家, P609瓔花の家, P606日狭美の家
				RETURN -100
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 桜並木の鳥居, 神社境内, 神社縁側
				RETURN 1
			CASE 中有の道, 三途の川, 彼岸, 地獄
				RETURN 1
			CASE 麓の樹海, 玄武の沢, 大蝦蟇の池, 森の深奥
				RETURN 1
			CASE 九天の滝, 絶景の丘, 山の湖
				RETURN 1
		ENDSELECT
	;引力設定（CFLAG:LOCAL:引力点を設定する）
	CASE 8
ENDSELECT

@EXIST_キャラデータ157
#LOCALSIZE 1
#LOCALSSIZE 1

@キャラデータ157(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "呼び名"
	CALLF MAKE_STR(V_NAME, "残無")
;こっから衣装設定
CASE "今日のパジャマ"
	CALLF MAKE_STR(V_NAME, "パジャマ（襦袢）セット")
CASE "普段着：上半身上着２"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上半身上着２", "スモックブラウス"))
CASE "普段着：ズボン"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("ズボン", "ショートズボン"))
CASE "普段着：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "下駄"))

CASE "固有下着形容詞"
	CALLF MAKE_STR(V_NAME, "ゴールを呼び寄せる")
CASE "固有下半身下着名"
	CALLF MAKE_STR(V_NAME, "無のパンツ")
CASE "固有上半身下着"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身下着名"
	CALLF MAKE_STR(V_NAME, "無のキャミソール")
CASE "固有下着着用条件"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着透ける(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有下着生地厚さ"
	CALLF MAKE_INT(V_NAME, 1)

;料理関連
CASE "料理：好きな味"
	CALLF MAKE_STR(V_NAME, "鍋物/淡白/ジャンキー/豪快/上品/質素/もちもち/霊障/地獄風/辛い/辛い/しょっぱい/しょっぱい/ + 米/スパイス/ * 3")
CASE "料理：嫌いな味"
	CALLF MAKE_STR(V_NAME, "甘い/甘い/ガッツリ/ヘルシー/家庭的/鮮やか/ + パン/ * 3")
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "地獄風/薄味/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "黒/かっこいい/シンプル/レトロ/知的/不思議/ + 闇/妖術/無の/ * 3 + 虚無の/ * 5")
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "かわいい/ポップ/ロリータ/マニアック/ + 機械/ * 5")
ENDSELECT

;-------------------------------------------------
