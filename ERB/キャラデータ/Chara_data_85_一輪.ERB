@CHARAMOVE_DATA_85(ARG, ARG:2)
LOCAL = 85
SELECTCASE ARG
	;連動設定（RETURN=依存する角色, RESULT:1～3=対等に連動する角色）
	CASE 1
	;特殊生活時間設定（CFLAG:LOCAL:生活時間補正を設定する）
	CASE 3
	;移動傾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;命蓮寺メンバーの私室はあんまり入らない
			CASE P113納茲琳私室, P114村紗私室, P115星私室, P117鵺私室, P118白蓮私室, P109女苑私室, P136響子私室
				RETURN -5
			;別屋、猯藏の棲家には入らない,神霊廟内部は行かない
			CASE P119別屋, P137猯藏の棲家, P125屠自古私室 TO P134蔵, P138鐘楼, P139邪仙楼
				RETURN -100
			;居間に行きやすい
			CASE P108居間
				RETURN 5
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 地蔵小路, 倒垂的柳樹下, 命蓮寺境内, 命蓮寺方丈, 墓地
				RETURN 1
			CASE 広場, 商家町
				RETURN 1
			CASE 天界
				RETURN 1
		ENDSELECT
	;引力設定
	CASE 8
		;白蓮が本堂にいる時はやってくる
		SIF CFLAG:[[白蓮]]:現在位置 == P103本堂 && !CFLAG:[[白蓮]]:诶嘿嘿 && SHIRAHU([[白蓮]])
			TCVAR:LOCAL:引力点 = P103本堂
ENDSELECT

@EXIST_角色データ85
#LOCALSIZE 1
#LOCALSSIZE 1

@角色データ85(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "稱呼"
	CALLF MAKE_STR(V_NAME, "一輪")
;こっから衣装設定
CASE "今天的睡衣"
	CALLF MAKE_STR(V_NAME, "睡衣（襦袢）套")

CASE "便装：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "頭巾"))
CASE "便装：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "便装：襪子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("襪子", "襪子"))
CASE "便装：連衣裙"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("連衣裙", "法衣"))

CASE "固有内衣形容詞"
	CALLF MAKE_STR(V_NAME, "闻着香香的")
CASE "固有下半身内衣名"
	CALLF MAKE_STR(V_NAME, "半旧的棉布胖次")
CASE "固有上半身内衣"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身内衣名"
	CALLF MAKE_STR(V_NAME, "半旧的棉布胸罩")
CASE "固有内衣着用条件"
	SIF !RAND:(1 + (2 * 勝負内衣判定([[一輪]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣布料厚度"
	CALLF MAKE_INT(V_NAME, 3)

;料理関連
CASE "料理：喜欢的味道"
	CALLF MAKE_STR(V_NAME, "米/香料/和食/鍋物/甜/简易/健康/淡白/質素/家庭的/豪快/软绵绵/" + "咸/" * 3)
CASE "料理：讨厌的味道"
	CALLF MAKE_STR(V_NAME, "洋食/上品/外界風/酸/酸/青蛙/青蛙/青蛙/虫肉/虫肉/虫肉/")
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "薄味/得意料理＊软绵绵＊/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "青/白/秩序/美的/充滿力量/" + "風/法具/" * 3 + "雲の/天輪の/法輪/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "混沌/繊細/洛麗塔/狂熱/" + "土/" * 3 + "蜘蛛の/" * 5)
ENDSELECT

;-------------------------------------------------
;固有料理設定
;DISH_NAME          料理名
;RESULTS            料理の種類（軽食/主食/甜点）
;CALL 材料設定("")  材料の指定。"汎用"を入れると自動で汎用食材になる
;DISH_TASTE         味の設定
;DISH_COMMENT       （任意）一行コメント
;DISH_TIME          （任意）調理にかかる時間
;DISH_MAXLIMIT      （任意）料理の持続時間
;-------------------------------------------------
@SPECIAL_MENU_85
DISH_NAME = 入道棉花糖
RESULTS = 甜点
CALL 材料設定("甜味料")
DISH_TASTE = 和食/甜/濃厚/夏物/豪快/豪快/软绵绵/妖力/
DISH_COMMENT = 蓬鬆膨脹的特大棉花糖。味道也很濃。
RETURN 1

