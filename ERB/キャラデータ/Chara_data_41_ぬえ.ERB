@CHARAMOVE_DATA_41(ARG, ARG:2)
LOCAL = 41
SELECTCASE ARG
	;連動設定（RETURN=依存する角色, RESULT:1～3=対等に連動する角色）
	CASE 1
	;特殊生活時間設定（CFLAG:LOCAL:生活時間補正を設定する）
	CASE 3
	;移動傾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;命蓮寺メンバーの私室はあんまり入らない
			CASE P113納茲琳私室, P114村紗私室, P115星私室, P116一輪私室, P118白蓮私室, P109女苑私室, P136響子私室
				RETURN -5
			;別屋には入らない,神霊廟内部は行かない
			CASE P119別屋, P125屠自古私室 TO P134蔵, P138鐘楼, P139邪仙楼
				RETURN -100
			;居間、猯藏の棲家に行きやすい
			CASE P108居間, P137猯藏の棲家
				RETURN 5
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 地蔵小路, 倒垂的柳樹下, 命蓮寺境内, 命蓮寺方丈, 墓地
				RETURN 1
			CASE 山麓樹海, 玄武之澤, 大蛤蟆之池, 森の深奥
				RETURN 1
		ENDSELECT
	;引力設定（CFLAG:LOCAL:引力点を設定する）
	CASE 8
		;白蓮が本堂にいる時はやってくる
		SIF CFLAG:[[白蓮]]:現在位置 == P103本堂 && !CFLAG:[[白蓮]]:诶嘿嘿 && SHIRAHU([[白蓮]])
			TCVAR:LOCAL:引力点 = P103本堂
ENDSELECT

@EXIST_角色データ41
#LOCALSIZE 1
#LOCALSSIZE 1

@角色データ41(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "稱呼"
	CALLF MAKE_STR(V_NAME, "鵺")
;こっから衣装設定
CASE "今天的睡衣"
	CALLF MAKE_STR(V_NAME, "睡衣（裤子）套")

CASE "便装：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "靴"))
CASE "便装：襪子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("襪子", "長筒襪"))
CASE "便装：連衣裙"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("連衣裙", "連衣裙"))

CASE "固有内衣形容詞"
	CALLF MAKE_STR(V_NAME, "马赛克模糊的")
CASE "固有下半身内衣名"
	CALLF MAKE_STR(V_NAME, "正体不明胖次")
CASE "固有上半身内衣"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身内衣名"
	CALLF MAKE_STR(V_NAME, "正体不明胸罩")
CASE "固有内衣着用条件"
	SIF !RAND:3
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(前)"
	CALLF MAKE_INT(V_NAME, 0)
CASE "固有内衣半透明(後)"
	CALLF MAKE_INT(V_NAME, 0)
CASE "固有内衣布料厚度"
	CALLF MAKE_INT(V_NAME, 3)

;料理関連
CASE "料理：喜欢的味道"
	CALLF MAKE_STR(V_NAME, "肉料理/香料/麺類/丰盛/垃圾/濃厚/新鲜/怪诞/扑鼻/辣/辣/咸/咸/" + "謎料理/" * 3)
CASE "料理：讨厌的味道"
	CALLF MAKE_STR(V_NAME, "简易/健康/質素/甜/甜/野菜/野菜/野菜/")
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "未知の味/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "黒/混沌/可怕的/奇妙/不思議/" + "闇/妖術/生体品/酒/" * 3 + "謎の/正体不明の/夜空の/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "秩序/樸素/知的/" + "光/書物/照明/领带/" * 3 + "真実の/" * 10)
ENDSELECT

;-------------------------------------------------
;固有料理設定
;DISH_NAME          料理名
;RESULTS            料理の種類（軽食/主食/甜点）
;CALL 材料設定("")  材料の指定。"汎用"を入れると自動で汎用食材になる
;DISH_TASTE         味の設定
;DISH_COMMENT       （任意）一行コメント
;DISH_TIME          （任意）調理にかかる時間
;DISH_MAXLIMIT      （任意）料理の持続時間
;-------------------------------------------------
@SPECIAL_MENU_41
DISH_NAME = 未確認飛行料理
RESULTS = 主食
CALL 材料設定("暗黑物质")
DISH_TASTE = 香料/未知の味/未知の味/未知の味/中立温度/謎料理/妖力/
DISH_COMMENT = 正体不明的宇宙舞蹈料理。非常難以下咽。
RETURN 1


