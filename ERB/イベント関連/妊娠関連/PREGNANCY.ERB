;一日の終わりに呼び出される関数
@GET_CHILD(ARG)
#DIM C_ID
#DIM 妊娠必要値
#DIM 中毒補正
#DIM 妊娠精液判定

#DIM 目前懷孕值
#DIM 懷孕必要值
#DIM 懷孕率

SELECTCASE ABL:ARG:膣射中毒
	CASE 0 TO 2
		中毒補正 = 5
	CASE 3 TO 5
		中毒補正 = 4
	CASEELSE
		中毒補正 = 2
ENDSELECT

EX:ARG:子宮内精液 += EX:ARG:膣内精液 / 中毒補正

;------ ALTER 殘留精液

CFLAG:ARG:膣内精液 = EX:ARG:膣内精液

CFLAG:ARG:子宮内精子 = EX:ARG:子宮内精液

IF TALENT:ARG:幼稚 && !TALENT:ARG:初潮
	RETURN 0
ENDIF

;------

;主人公優先
SIF CFLAG:ARG:2000
	CALL GET_CHILD_SUB(ARG,MASTER)

;キャラ順ランダマイズ
CALL FISHER_YATES_SHAFFLE(CHARANUM - 1)

IF CFLAG:ARG:妊娠経過日数 == 0
	FOR LOCAL , 0 , CHARANUM - 1

		C_ID = SHAFFLE_ARRAY:LOCAL + 1
		
		
		
		SIF TALENT:C_ID:路人
			CONTINUE
		
		SIF CFLAG:ARG:(C_ID + 2000)
			CALL GET_CHILD_SUB(ARG,C_ID)
		
		SIF CFLAG:ARG:妊娠経過日数
			BREAK
		
	NEXT
ENDIF

@GET_CHILD_SUB(ARG,ARG:1)

