	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		IF FLAG:70
			PRINTFORMW 
			RETURN 1
		;サグメなりきり時
		ELSEIF FLAG:なりきり == 110
		ELSE
			PRINTFORMW 「手で…？上手くできるかわかりませんが…」
		ENDIF
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;サグメなりきり時
	ELSEIF FLAG:なりきり == 110
	;出す直前
	ELSEIF BASE:PLAYER:2 > 8000
		SELECTCASE RAND:3
		CASE 0
			CALL HPH_PRINT,@"「出そうですか？我慢しなくていいですよ…HPH」","W"
		CASE 1
			CALL HPH_PRINT,@"「さぁ、力を抜いて…HPH」","W"
		CASE 2
			CALL HPH_PRINT,@"「ザーメンいっぱい出してくださいHPH…なんて」","W"
		ENDSELECT
	;恋慕
	ELSEIF TALENT:恋慕 || TALENT:愛欲 || TALENT:セフレ || TALENT:ヤリマン
		SELECTCASE RAND:7
		CASE 0
			CALL HPH_PRINT,@"「ここは正直ね…HPH」","L"
			PRINTFORMDW 痙攣する肉棒を扱かれながら耳元で囁かれた
		CASE 1
			PRINTFORML 「ここが弱そうですね…？」
			PRINTFORMDW 先走りを塗した指で亀頭を擦られると、%CALLNAME:MASTER%は思わず情けない声が出た
		CASE 2
			PRINTFORML 「気持ち良いですか？息が荒くなってますよ」
			PRINTFORMDW こちらの顔を見て、ドレミーは意地の悪そうな笑みを浮かべた
		CASE 3
			CALL HPH_PRINT,@"「こうされるのが弱いんですよね…？HPHふふ、分かってますよHPH」","L"
