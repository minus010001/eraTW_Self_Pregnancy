@M_KOJO_MESSAGE_COM_K109_88_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		;サグメなりきり時
		IF FLAG:なりきり == 110
		ELSE
			PRINTFORMW 「もっと奥まで？…やってみます」
			RETURN 1
		ENDIF
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;サグメなりきり時
	ELSEIF FLAG:なりきり == 110
	;出す直前
	ELSEIF BASE:PLAYER:2 > 8000
		SELECTCASE RAND:3
		CASE 0
			CALL HPH_PRINT,@"「じゅぽっHPHごぷっHPHん、ふっHPH」","L"
			PRINTFORMDW ドレミーは喉の奥まで咥え込みながら、目で射精を促している
		CASE 1
			CALL HPH_PRINT,@"「ぐぽっHPHぐ、んHPHずるるっHPH」","L"
			PRINTFORMDW ドレミーは射精が近いと見るや激しくしゃぶりだした
		CASE 2
			CALL HPH_PRINT,@"「んぐっ、じゅぷっHPHじゅるるるっHPH」","L"
			PRINTFORMDW 思わず腰が浮くほどの苛烈なフェラチオに強烈な射精欲が沸き上がってくる
		ENDSELECT
	;恋慕
	ELSEIF TALENT:恋慕 || TALENT:愛欲 || TALENT:セフレ || TALENT:ヤリマン
		SELECTCASE RAND:6
		CASE 0
			CALL HPH_PRINT,@"「ぢゅるるっHPH…んふっ…HPH」","L"
			PRINTFORMDW ドレミーは尿道のカウパーを吸い上げようとするかのように吸引してきた
		CASE 1
			CALL HPH_PRINT,@"「ふっHPH…んっ…HPH」","L"
			PRINTFORMDL ドレミーにもっと強く吸ってくれるように頼む
			CALL HPH_PRINT,@"「はいHPH…じゅずるるるっHPH」","L"
			PRINTFORMDW 魂まで吸われるのではというほどの吸引に思わず腰が浮かび上がった
		CASE 2
			CALL HPH_PRINT,@"「ぐぽっHPHぐぽっHPHぐぷっHPH」","L"
			PRINTFORMDW ドレミーは一心不乱に肉棒をしゃぶっている
		CASE 3
			PRINTFORML 「ん、じゅるっ…」
			PRINTFORMDW ドレミーは少し苦しそうにしながらも一生懸命にしゃぶっている
		CASE 4
			PRINTFORML 「んっ…んんんっ…」
			PRINTFORMDW ドレミーは涙目になりながらも喉奥まで咥え入れた
		CASE 5
			PRINTFORML 「ふ、んっ…んぐっ…」
			PRINTFORMDW ドレミーは口の端から涎を垂れ流しながら口一杯に頬張っている
		ENDSELECT
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		SELECTCASE RAND:3
		CASE 0
			PRINTFORML 「ん、じゅるっ…」
			PRINTFORMDW ドレミーは少し苦しそうにしながらも一生懸命にしゃぶっている
		CASE 1
			PRINTFORML 「んっ…んんんっ…」
			PRINTFORMDW ドレミーは涙目になりながらも喉奥まで咥え入れた
		CASE 2
			PRINTFORML 「ふ、んっ…んぐっ…」
			PRINTFORMDW ドレミーは口の端から涎を垂れ流しながら口一杯に頬張っている
		ENDSELECT
	;それ以外
	ELSE
		SELECTCASE RAND:3
		CASE 0
			PRINTFORML 「…じゅるっ…げほっ、ちょっと、苦しい…」
			PRINTFORMDW ドレミーはえずいている
		CASE 1
			PRINTFORML 「…ぐぷっ…ん、んん」
			PRINTFORMDW ドレミーは喉奥まで咥え込もうとして諦めたようだ
		CASE 2
			PRINTFORML 「ちゅぶっ…ふっ…んぷっ…」
			PRINTFORMDW ドレミーはなんとか頑張っている
		ENDSELECT
	ENDIF
ENDIF
RETURN 1