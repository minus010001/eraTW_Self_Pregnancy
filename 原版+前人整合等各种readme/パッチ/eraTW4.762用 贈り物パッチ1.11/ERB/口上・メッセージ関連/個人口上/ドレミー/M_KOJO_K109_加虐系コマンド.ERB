;==================================================
;100,スパンキング
;==================================================
@M_KOJO_MESSAGE_COM_K109_100
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K109_100_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K109_100_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;サグメなりきり時
	ELSEIF FLAG:なりきり == 110
		PRINTFORML
		CALL AUTO_AEGI(NO:TARGET)
		RETURN 1
	;恋慕
	ELSEIF TALENT:恋慕 || TALENT:愛欲 || TALENT:セフレ || TALENT:ヤリマン
		PRINTFORMDL ドレミーの揺れる尻を前に欲求を抑えられなくなった%CALLNAME:MASTER%は手を振り上げると思い切り引っ叩いた
		PRINTFORML 「きゃうっ！？…んぅ…もう、終わりですか…？」
		PRINTFORMDW そう言うドレミーの口元は緩み、潤んだ瞳は続きを期待しているように見えた
		RETURN 1
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		PRINTFORML 「っぅ…うぅ…」
		PRINTFORMDW ドレミーは痛そうにしている
		RETURN 1
	;それ以外
	ELSE
		PRINTFORML 「ぃっ！」
		PRINTFORMDW ドレミーの尻は赤く腫れている
		RETURN 1
	ENDIF
ENDIF
RETURN 1

