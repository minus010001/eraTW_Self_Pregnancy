;-------------------------------------------------
;名前を教えるイベント（関数）
;/見事な響子口上の丸パクだと感心するがどこもおかしくはない
;-------------------------------------------------

@NAME_SET_K39
PRINTL 名前を入力してください(直接入力するか、ボタンで選択してください)
IF NAME:MASTER != "あなた"
	LOCALS = %NAME:MASTER%
ELSE
	LOCALS = %CALLNAME:MASTER%
ENDIF
PRINTBUTTON @"[名前(%LOCALS%)を名乗る]", @"%LOCALS%"
PRINTL 　
;名前を入力するループ
$INPUT_LOOP_ONAMAE
INPUTS
; 行頭行尾の空白文字削除
LOCALS = %RESULTS%
REPLACE LOCALS, "\(\^\\s\+\|\\s\+\$)", ""
;10文字以上は受け付けない
IF STRLENS(LOCALS) > 10
	CLEARLINE 1
	REUSELASTLINE 「…いくらなんでも長すぎだ。もうちょっと短くならないか？」
	GOTO INPUT_LOOP_ONAMAE
;空白は受け付けない
ELSEIF RESULTS == ""
	PRINTL 名前を入力してください
	GOTO INPUT_LOOP_ONAMAE
ELSE
	; 同姓同名
	IF FLAG:なりきり != 39 && STRCOUNT(LOCALS, "ナズーリン")
		CLEARLINE 1
		REUSELASTLINE 「…自分の名前を名乗られるのはあまり愉快じゃないね」
		GOTO INPUT_LOOP_ONAMAE
	; 白蓮が主人ではなく、白蓮というフレーズが含まれている。もしくは
	; 星が主人ではなく、寅丸もしくは星というフレーズが含まれている
	ELSEIF (FLAG:なりきり != 55 && STRCOUNT(LOCALS, "聖\*\\s\*白蓮")) || (FLAG:なりきり != 87 && STRCOUNT(LOCALS, "\(寅丸\)\*\\s\*星\$"))
		CLEARLINE 1
		REUSELASTLINE 「…つまらない冗談はやめなよ」
		GOTO INPUT_LOOP_ONAMAE
	; 命蓮寺勢はじき
	; うんそうだね多々良勝五郎とか教える人もいるかもしれないもんね
	ELSEIF (FLAG:なりきり != 40 && STRCOUNT(LOCALS, "\(多々良\)\*\\s\*小傘")) || (FLAG:なりきり != 85 && STRCOUNT(LOCALS, "\(雲居\)\*\\s\*一輪")) || (FLAG:なりきり != 86 && STRCOUNT(LOCALS, "\(\(村紗\)*\\s\*水蜜\|ムラサ\)"))
		CLEARLINE 1
		REUSELASTLINE 「…うちの者と紛らわしいね。他のにしてくれないか」
		GOTO INPUT_LOOP_ONAMAE
	ELSEIF FLAG:なりきり != 69 && STRCOUNT(LOCALS, "\(二ツ岩\)\*\\s\*マミゾウ")
		CLEARLINE 1
		REUSELASTLINE 「…あいつは尻尾が出るんだよ」
		GOTO INPUT_LOOP_ONAMAE
	ELSEIF (FLAG:なりきり != 41 && STRCOUNT(LOCALS, "\(封獣\)\*\\s\*ぬえ"))
		CLEARLINE 1
		REUSELASTLINE 「…ぬえは今屋根の上にいるけど」
		GOTO INPUT_LOOP_ONAMAE
	; 道教勢力はじき
	ELSEIF (FLAG:なりきり != 89 && STRCOUNT(LOCALS, "\(宮古\)\*\\s\*芳香")) || (FLAG:なりきり != 90 && STRCOUNT(LOCALS, "\^霍\*\\s\*青娥")) || (FLAG:なりきり != 91 && STRCOUNT(LOCALS, "\(蘇我\)\*\\s\*屠自古")) || (FLAG:なりきり != 92 && STRCOUNT(LOCALS, "\(物部\)\*\\s\*布都")) || (FLAG:なりきり != 56 && STRCOUNT(LOCALS, "\(\(豊聡耳\)\*\\s\*神子\|聖徳太子\)"))
		CLEARLINE 1
		REUSELASTLINE 
		GOTO INPUT_LOOP_ONAMAE
	; 博麗霊夢というフレーズが含まれてる。もしくは
	; 東風谷早苗というフレーズが含まれている
	ELSEIF STRCOUNT(LOCALS, "\(博麗\)\*\\s\*霊夢") || STRCOUNT(LOCALS, "\(東風谷\)\*\\s\*早苗")
		PRINTFORML 「あの常識外し巫女の身内かい？まあいいけど」
	; 霧雨魔理沙というフレーズが含まれてる。
	ELSEIF STRCOUNT(LOCALS, "\(霧雨\)\*\\s\*魔理沙")
		PRINTFORML 「あの変な魔法使いとは妙な縁があるな。まあいいけど」
	; 直接入力ならいけると思ったか？弾くぜ！
	ELSEIF FIRSTTIME("U39-1") && ABL:親密 < 4 && STRCOUNT(LOCALS, "\(お\(\(兄\|にい\)\|\(姉\|ねえ\)\)\(さん\|ちゃん\)\)\$")
		CLEARLINE 1
		REUSELASTLINE 「馬鹿か君は」
		GOTO INPUT_LOOP_ONAMAE
	ELSEIF FIRSTTIME("U39-1") && ABL:親密 < 7 && STRCOUNT(LOCALS, "\(お\(\(兄\|にい\)\|\(姉\|ねえ\)\)\(様\|さま\)\)\$")
		CLEARLINE 1
		REUSELASTLINE 「馬鹿か君は」
		GOTO INPUT_LOOP_ONAMAE
	ELSEIF FIRSTTIME("U39-1") && (ABL:従順 < 7 || ABL:奉仕精神 < 7) && STRCOUNT(LOCALS, "\(\(御\|ご\)\(主人\|しゅじん\)\(様\|さま\)\)$")
		CLEARLINE 1
		REUSELASTLINE 「馬鹿か君は」
		GOTO INPUT_LOOP_ONAMAE
	ELSEIF  FIRSTTIME("U39-1") && (!TALENT:恋人 || !TALENT:恋慕) && STRCOUNT(LOCALS, "\(\(旦那\|だんな\)\(様\|さま\)\)\$")
		CLEARLINE 1
		REUSELASTLINE 「馬鹿か君は」
		GOTO INPUT_LOOP_ONAMAE
	ELSE
		PRINTFORML 「%RESULTS%…ね」
	ENDIF
	PRINTL [0] はい
	PRINTL [1] いいえ
	;入力した名前を確定するループ
	$INPUT_LOOP_KAKUTEI_1
	INPUT
	IF RESULT == 0
		;あなたの名前を%MASTERNAME:39%に格納
		MASTERNAME:39 = %RESULTS%
		PRINTFORML 
	ELSEIF RESULT == 1
		PRINTL 名前を入力してください
		GOTO INPUT_LOOP_ONAMAE
	ELSE
		PRINTL 名前を入力してください
		GOTO INPUT_LOOP_ONAMAE
	ENDIF
ENDIF

