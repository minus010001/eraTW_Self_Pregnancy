;==================================================
;朝起きたとき（おはよう口上）
;==================================================
@M_KOJO_EVENT_K86_2(ARG,ARG:1)
CALL MESSAGE_GOODMONING(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	SIF TCVAR:353 == 1
		TCVAR:353 = 0
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;非同棲かつ相手が先に起きてる
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		;-------------------------------------------------
		;TARGET私室にMASTERが入っている（恋慕時添い寝中就寝時のみの筈
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			PRINTFORMW 
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;MASTER私室にTARGETが入ってきてる(恋慕無しでも入ってくる可能性有り
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORMW 
			RETURN 1
		ENDIF
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;同棲かつ相手が起きている
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;同棲かつ相手が寝ている
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;MASTERが起きていて、相手が起きてきた
	;添い寝時TARGET起床はこちら（TARGET衰弱or添い寝中TARGET睡魔負け後そのままMASTERが起き続けた上で相手が起床した場合限定
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		;-------------------------------------------------
		;非同棲
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			IF CFLAG:MASTER:添い寝中
				PRINTFORML 「…ん…… ふぁ…ぁ……っ…」
				PRINTFORMW 「あ…… おはよう%CALLNAME:MASTER%、ふふっ」
				;ムラサ私室以外の部屋を私室に設定している(条件的に有り得ず
				;SIF CFLAG:MASTER:311 != 14
					;PRINTFORMW 「%CALLNAME:MASTER%が来てたのなら… もう少し早く起きていれば良かったかな」
				PRINTL
				PRINTFORMW %CALLNAME:MASTER%と%CALLNAME:TARGET%は温もりを惜しみつつ布団を抜け出した…
			ENDIF
			PRINTFORMW 
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;同棲中
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			IF CFLAG:MASTER:添い寝中
				PRINTFORML 「…ん…… ふぁ…ぁ……っ…」
				PRINTFORMW 「あ…… おはよう%CALLNAME:MASTER%、ふふっ」
				;ムラサ私室以外の部屋を私室に設定している(条件的に有り得ず
				;SIF CFLAG:MASTER:311 != 14
					PRINTFORMW 「%CALLNAME:MASTER%が来てたのなら… もう少し早く起きていれば良かったかな」
				PRINTL
				PRINTFORMW %CALLNAME:MASTER%と%CALLNAME:TARGET%は温もりを惜しみつつ布団を抜け出した…
			ENDIF
			PRINTFORMW 
			RETURN 1
		ENDIF
		PRINTFORMW 
		RETURN 1
	ENDIF
	;その他
	PRINTFORML  
ENDIF
RETURN 1

;==================================================
;夜寝るとき（おやすみ口上）
;==================================================
@M_KOJO_EVENT_K86_3(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入スイッチ
LOCAL = 1
;-------------

IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;自室に帰る（定時になって寝る）
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF BASE:TARGET:体力 <= 500
			IF CFLAG:うふふ
				PRINTFORML 「はぁ… はぁ… もう、だめ……」
				PRINTFORMW %CALLNAME:TARGET%は力無く%CALLNAME:MASTER%にしなだれ掛かり、荒い呼吸と共に肩を上下させている…
				PRINTL
				PRINTFORML 「私の方が先にバテちゃうなんて、ね… ふふっ… 良かったわよ、%CALLNAME:MASTER%」
				PRINTFORMW そう言うと%CALLNAME:TARGET%は%CALLNAME:MASTER%の唇に優しく触れるだけのキスをする…
				PRINTL
			ELSE
				PRINTFORMW 「ん… やっぱりまだ疲れてるみたい」
			ENDIF
			PRINTFORMW 「…私は部屋に戻って休むわね、おやすみなさい」
		ELSE
			PRINTFORMW 「ん… もうこんな時間。私は先に休ませてもらうわね」
			PRINTFORMW 「%CALLNAME:MASTER%もあんまり夜更かししちゃだめよ？ふふっ、おやすみなさい」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;眠そうにしている
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		IF CFLAG:添い寝中
			PRINTL
			PRINTL
			PRINTFORMW 「ふふっ……」
		ELSE
			PRINTFORMW 「ん… ふぁ…ぁ……」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;睡眠中（起きていないので会話というよりは寝言？）
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTL
		PRINTFORMW 「すー… すー… ん…」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;イタズラ中
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		PRINTFORMW 「む… んー…」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;衰弱中（恋慕時、部屋まで運ぶ）
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		IF CFLAG:うふふ
			PRINTFORMW 「はぁ… はぁ… もう、だめ……」
			PRINTFORMW %CALLNAME:TARGET%は力無く%CALLNAME:MASTER%にしなだれ掛かり、荒い呼吸と共に肩を上下させている…
			PRINTL
			PRINTFORMW 「ふふっ… すっごく、良かった。%CALLNAME:MASTER%の事、いっぱい感じられて幸せよ」
			PRINTFORMW そう言うと%CALLNAME:TARGET%は%CALLNAME:MASTER%の唇に優しく触れるだけのキスをする…
			PRINTFORMW そうしてしばらく逢瀬の余韻を楽しむと、%CALLNAME:MASTER%は%CALLNAME:TARGET%を部屋まで送った…
		ELSE
			PRINTFORMW 「ん… やっぱりまだ疲れてるみたい」
			PRINTFORMW 「……送ってくれるの？ふふっ… ありがとう」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;MASTERがダウン（部屋まで運ばれる）
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		PRINTFORMW 「あら… もうお休みかしら」
		PRINTFORMW 「仕方ないわね、お部屋までエスコートしますわ」
		PRINTFORMW そう言うと%CALLNAME:TARGET%は、ひょいと%CALLNAME:MASTER%を抱き上げた…
		PRINTL
		PRINTFORMW 「ふふっ… 驚いた？こう見えても、私結構力あるのよ？」
		PRINTFORMW 「……それじゃあ、おやすみなさい。良い夢を」
		PRINTW
		RETURN 1
	ENDIF
	;その他
	PRINTFORML  
ENDIF
RETURN 1

