;==================================================
;情事を目撃した
;==================================================
@M_KOJO_EVENT_K86_6(ARG,ARG:1)
;口上主はTARGET
;ARGに情事相手
;--------------------------------------------------
;記入スイッチ
LOCAL = 1
;-------------

IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;情事を目撃した（馴れ合い度:低）
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG:1 == 1
		PRINTFORML
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;負けじと参加（馴れ合い度:高）
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG:1 == 2
		;白蓮が居ない
		IF CFLAG:MASTER:現在位置 != CFLAG:55:現在位置
			PRINTFORMW 「…お楽しみのようね？」
			PRINTL
			PRINTFORMW %CALLNAME:ARG%の肩に背後からすうっと白い手が回される
			PRINTL
			PRINTFORML 「ふふ… そんなに驚いた顔しないの、可愛い声が響いていたわよ？」
			PRINTFORML 「とっても気持ち良さそうな… 甘くて切ない声。あんなの聴かされたら私まで参っちゃうわ」
			PRINTFORMW 「だから、私も仲間に入れて下さらない？ねぇ%CALLNAME:MASTER%…」
			PRINTL
			PRINTFORMW そう言って%CALLNAME:TARGET%は%CALLNAME:ARG%の肩越しに%CALLNAME:MASTER%に妖しく微笑んだ…
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;気まずそうに立ち去る
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG:1 == 3
		PRINTFORML 「……お邪魔したようね？」
		PRINTFORMW 「そうね、私は失礼しますのでごゆっくりお楽しみを…」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;MASTERに誘われて参加
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG:1 == 4
		;白蓮が居る
		IF CFLAG:MASTER:現在位置 == CFLAG:55:現在位置
			;ムラサが恋人
			IF TALENT:TARGET:恋人
				PRINTFORML 「ふふっ… 御一緒できて光栄です、聖…」
				PRINTL
				PRINTFORMW そう言うと%CALLNAME:TARGET%は白蓮の手の甲に口付けた…
			ELSE
				PRINTFORMW 「私も一緒に…？」
				PRINTL
				PRINTFORMW %CALLNAME:TARGET%は戸惑うような表情で白蓮を見る
				PRINTL
				PRINTFORMW 「聖は… 構いませんか？」
				PRINTL
				PRINTFORML 「……そうですか、それなら喜んで」
				PRINTFORMW 「御一緒できて光栄です、と言うのも何か変ですよね」
				PRINTL
				PRINTFORMW そう言うと%CALLNAME:TARGET%は嬉しそうに微笑んだ…
			ENDIF
		ELSE
			PRINTFORML 「可愛がって貰ったみたいね、%CALLNAME:ARG%？一人占めなんてずるいわ」
			PRINTFORMW 「ふふ… そう膨れないの、一緒に楽しみましょう？」
		ENDIF
		RETURN 1
	ENDIF
	;その他
	PRINTFORML 
ENDIF
RETURN 1

